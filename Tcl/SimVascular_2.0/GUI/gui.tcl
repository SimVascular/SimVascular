# Procedure: bcGUI_hand_xyplot_velocity
proc bcGUI_hand_xyplot_velocity { widget x y plotdir} {

  set vVectors tmp-bcGUI_hand_xyplot-vectors
  set vScalars tmp-bcGUI_hand_xyplot-scalars
  set vPoints tmp-bcGUI_hand_xyplot-points
  set vPoly tmp-bcGUI_hand_xyplot-poly
  set ds2do tmp-bcGUI_hand_xyplot-ds2d0
  set xyplot3 tmp-bcGUI_hand_xyplot-myxyplot

  set x [expr [winfo pointerx $widget] - [winfo rootx $widget]] 
  set y [expr [winfo pointery $widget] - [winfo rooty $widget]] 

  global bcGUIcurrentFrameNumber
  global bcGUIhanddrawXpressed
  global gBC

  global bcGUImagWindow
  global bcGUIpotWindow

  global symbolicName
  if {[file rootname $widget] == $symbolicName(bcRenWinMag)} {
    set ren $bcGUImagWindow(ren)
  } else {
    set ren $bcGUIpotWindow(ren)
  }

  if {$bcGUIhanddrawXpressed == 1} {
    return
  }
  set bcGUIhanddrawXpressed 1

  # y is flipped upside down
  set height [lindex [$widget configure -height] 4]
  set y [expr $height - $y]

  set coord tmp-add_point_cord
  catch {$coord Delete}
  vtkCoordinate $coord
  $coord SetCoordinateSystemToDisplay
  $coord SetValue $x $y
  set worldValue [$coord GetComputedWorldValue $ren]
  $coord Delete
  #puts "x: $x  y: $y  worldValue: $worldValue"

  foreach param $gBC(phase_contrast_params) {
    set [lindex $param 0] [lindex $param 1]
  }

  set i [expr int(floor([lindex $worldValue 0]/[lindex $voxel_dims 0]))]
  set j [expr int(floor([lindex $worldValue 1]/[lindex $voxel_dims 1]))]

  if {$i < $gBC(voi_x0) || $i > $gBC(voi_x1) ||  $j < $gBC(voi_y0) || $j > $gBC(voi_y1)} {
      puts "$i $j out of range!"
      return
   }

  # only true when entire image is used!
  #set pixel [expr $i+$j*$gBC(ext_i)]
  #set pixelI [expr $pixel%$gBC(ext_i)]
  #set pixelJ [expr $pixel/$gBC(ext_i)]
  #puts "i: $i  j: $j pixelI: $pixelI  pixelJ:  $pixelJ"

  set pixel [expr ($i - $gBC(voi_x0)) + ($j - $gBC(voi_y0))*($gBC(voi_x1)-$gBC(voi_x0)+1)]

  set magslice /gBC/pcdata/mag/$bcGUIcurrentFrameNumber
  set magvalue [[[[repos_exportToVtk -src $magslice] GetPointData] GetScalars] GetTuple1 $pixel]

  catch {$vVectors Delete}
  catch {$vScalars Delete}
  catch {$vPoints Delete}
  catch {$vPoly Delete}

  vtkFloatArray $vVectors; $vVectors SetNumberOfComponents 3
  vtkFloatArray $vScalars
  vtkPoints $vPoints
  vtkPolyData $vPoly

  $vVectors Allocate 100 100
  $vScalars Allocate 100 100
  $vPoints Allocate 100 100
  $vPoly Allocate 100 100

  set totalv 0

  for {set frame 1} {$frame <= $gBC(numSlices)} {incr frame} {
    # slightly different output depending on 1-comp or 3-comp
    # velocity data
    set magslice /gBC/pcdata/mag/$frame
    set magvalue [[[[repos_exportToVtk -src $magslice] GetPointData] GetScalars] GetTuple1 $pixel]
    $vScalars InsertNextTuple1 $magvalue
    if {$gBC(numVelComp) == 1} {
      # only for thruplane
      set velslice /gBC/pcdata/vel/$frame
      set velvalueX 0
      set velvalueY 0
      set velvalueZ [[[[repos_exportToVtk -src $velslice] GetPointData] GetScalars] GetTuple1 $pixel]
      $vPoints InsertNextPoint $frame $velvalueZ 0
      set totalv [expr double($totalv) + $velvalueZ]
      puts "frame: [expr $frame - 1]  mag: $magvalue  velZ: $velvalueZ"
    } elseif {$gBC(numVelComp) == 3} {
      set velsliceX /gBC/pcdata/vel/$frame/r
      set velsliceY /gBC/pcdata/vel/$frame/a
      set velsliceZ /gBC/pcdata/vel/$frame/s
      set velvalueX [[[[repos_exportToVtk -src $velsliceX] GetPointData] GetScalars] GetTuple1 $pixel]
      set velvalueY [[[[repos_exportToVtk -src $velsliceY] GetPointData] GetScalars] GetTuple1 $pixel]
      set velvalueZ [[[[repos_exportToVtk -src $velsliceZ] GetPointData] GetScalars] GetTuple1 $pixel]
      switch $plotdir {
        0 {
          $vPoints InsertNextPoint $frame $velvalueX 0
          set totalv [expr double($totalv) + $velvalueX]
    }
        1 {
          $vPoints InsertNextPoint $frame $velvalueY 0
          set totalv [expr double($totalv) + $velvalueY]
    }
        2 {
          $vPoints InsertNextPoint $frame $velvalueZ 0
          set totalv [expr double($totalv) + $velvalueZ]
    }
        default {
          return -code error "ERROR:  Illegal direction $plotdir"
  }
      }
      puts "frame: [expr $frame -1]  mag: $magvalue  vel: <$velvalueX,$velvalueY,$velvalueZ>"
    } else {
      return -code error "ERROR:  Illegal number of velocity components"
    }
    $vVectors InsertNextTuple3 $velvalueX $velvalueY $velvalueZ
  }

  set avgv [expr double($totalv)/double($gBC(numSlices))]

  # attach the points and point data to the polydata object
  $vPoly SetPoints $vPoints
  [$vPoly GetPointData] SetScalars $vScalars
  [$vPoly GetPointData] SetVectors $vVectors

  # tranform into data object.  I don't really understand
  # this transformation, but it appears to be required
  # (since vtk core dumps if you just AddDataObject the
  # vtkPolyData directly).
  catch {$ds2do Delete}
  vtkDataSetToDataObjectFilter $ds2do
  $ds2do SetInputDataObject $vPoly
  $ds2do GeometryOn
  $ds2do TopologyOff
  $ds2do PointDataOff
  $ds2do CellDataOff
  $ds2do FieldDataOff
  $ds2do Update

  # create the xy plot 2D actor
  catch {$xyplot3 Delete}
  vtkXYPlotActor $xyplot3
    $xyplot3 AddDataObjectInput [$ds2do GetOutput]
    #$xyplot3 SetXRange 0 20
    #$xyplot3 SetYRange -200 600
    #$xyplot3 SetPointComponent 0 1
    $xyplot3 SetDataObjectXComponent 0 0
    $xyplot3 SetDataObjectYComponent 0 1
    $xyplot3 SetPlotColor 0 1 0 0
    $xyplot3 SetPlotLabel 0 "Magnitude"
    [$xyplot3 GetPositionCoordinate] SetValue 0.0 0.0 0
    [$xyplot3 GetPosition2Coordinate] SetValue 1.0 0.33 0;#relative to Position
    #$xyplot3 SetXValuesToIndex
    #$xyplot3 SetNumberOfXLabels 6
    $xyplot3 SetTitle "Velocity (without baseline correction) vs. Frame  (Avg: [format %6.3f $avgv])"
    $xyplot3 SetXTitle "Frame"
    $xyplot3 SetYTitle "Velocity"
    $xyplot3 PlotPointsOn
    $xyplot3 PlotLinesOn
    [$xyplot3 GetProperty] SetColor 0 0 1
    [$xyplot3 GetProperty] SetPointSize 3
    #$xyplot3 LegendOn
    #$xyplot3 SetLegendPosition 0.8 0.35
    #$xyplot3 SetLegendPosition2 0.20 0.20
    #$xyplot3 ReverseYAxisOn
    #$xyplot3 SetDataObjectPlotModeToColumns

  # add the actor to the render window
  $ren AddActor2D $xyplot3
  [$ren GetRenderWindow] Render
}


