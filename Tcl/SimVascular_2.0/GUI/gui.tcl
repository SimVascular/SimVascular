# Procedure: bcGUIwriteVolFlowResults
proc bcGUIwriteVolFlowResults {} {
  global gFlow
  # make vcalc like output
  global gFilenames
  set fp [open $gFilenames(vcalc_flow_file) "w"]
  puts [format "%15s%15s%15s%15s%15s%15s" "Frame" "Flow (ml/min)"  "Avg V" "Min V" "Max V" "area (mm^2)"]
  puts $fp [format "%15s%15s%15s%15s%15s%15s" "Frame" "Flow (ml/min)"  "Avg V" "Min V" "Max V" "area (mm^2)"]
  global bcGUIvcalcFrames
  global gBC
  set allframes [string_parse $bcGUIvcalcFrames 1 $gBC(numSlices)]
  foreach i $allframes {
    if {[info exists gFlow($i)] == 1} {
      set data $gFlow($i)
    } else {
      puts "Warning: flow calculation results does not exist for $i."
      continue
    }
    set flowRate [expr double([lindex $data 0])/1000.0*60.0]
    set AvgVel [lindex $data 1]
    set MinVel [lindex $data 2]
    set MaxVel [lindex $data 3]
    set totalArea [lindex $data 4]
    puts [format "%15i%15.3f%15.3f%15.3f%15.3f%15.3f" $i $flowRate $AvgVel $MinVel $MaxVel $totalArea]
    puts $fp [format "%15i%15.3f%15.3f%15.3f%15.3f%15.3f" $i $flowRate $AvgVel $MinVel $MaxVel $totalArea]
  }
  close $fp
}


# Procedure: createPREOPgrpLoadGroups
proc createPREOPgrpLoadGroups {} {

    global gFilenames
    set grpdir $gFilenames(groups_dir)
    set filename [file join $grpdir group_contents.tcl]

    # check and see if the file exists
    if {[file exists $filename] == 0} {
      puts "ERROR: File $filename does not exist."
      return -code error "ERROR: File $filename does not exist."
    }

    # check for header
    set fp [open $filename "r"]
    set line {}
    catch {gets $fp line}
    close $fp

    if {[regexp geodesic_groups_file $line] == 0} {
      puts "ERROR: File $filename is not of the right format."
      return -code error "ERROR: File $filename is not of the right format."
    }

    if {[regexp 1.0 $line] == 0 && [regexp 2. $line] == 0} {
      puts "ERROR: File $filename is not version 1.0 or 2.x.  File being ignored!"
      return -code error "ERROR: File $filename is not version 1.0 or 2.x.  File being ignored!"
    }
    set version "2.0"
    regexp 2.* $line version sub] != 0


    source $filename

    group_autoload
    if {$version >= "2.3"} {
      seg3d_autoload
    }
    #createPREOPgrpUpdateAvailGrps
}


# Procedure: createPREOPgrpSaveGroups
proc createPREOPgrpSaveGroups {} {

  # create the directory if it doesn't exist
  global gFilenames
  set grpdir $gFilenames(groups_dir)
  set contents_file [file join $grpdir group_contents.tcl]

  if {[file exists $grpdir] == 1} {
    if {[file isdirectory $grpdir] == 0} {
      puts "ERROR:  Group directory $grpdir is apparently a file!"
      return -code error "ERROR:  Group directory $grpdir is apparently a file!"
    }
  } else {
      if [catch {file mkdir $grpdir}] {
      puts "ERROR:  Could not create directory $grpdir."
      return -code error "ERROR:  Could not create directory $grpdir."
    }
  }

  set fp [open $contents_file "w"]
  puts $fp "# geodesic_groups_file 2.3"
  puts $fp ""
  puts $fp "#"

  puts $fp "proc group_autoload {} {"
  puts $fp "  global gFilenames"
  puts $fp "  set grpdir \$gFilenames(groups_dir)"
  puts $fp "  # Group Stuff"
  set names [group_names]
  if {[llength $names] > 0} {
    
    foreach name $names {
  puts $fp "  group_readProfiles \{$name\} \[file join \$grpdir \{$name\}\]"
        group_saveProfiles $name [file join $grpdir $name]
    }
    
  }
  puts $fp "}"


  puts $fp "proc seg3d_autoload {} {"
  puts $fp "  global gFilenames"
  puts $fp "  set grpdir \$gFilenames(groups_dir)"
  set names [seg3d_names]
  puts $names
  puts $fp "# Seg Stuff"
  if {[llength $names] > 0} {
    foreach name $names {
      set fn [file join $grpdir $name.svsurf]
      puts $fp "  seg3d_readSurf \{$name\} \[file join \$grpdir \{$fn\}\]"
      
      puts $fn
      seg3d_saveSurf $name $fn
        
    }
  
  }

  puts $fp "}"
  close $fp
}


