# Procedure: guiMMreadMeshSimScriptFile
proc guiMMreadMeshSimScriptFile {} {
  global symbolicName
  global gFilenames
  global gObjects
  global guiMMvars
  global gOptions

  set fp [open $gFilenames(mesh_script_file) "r"]

  set guiMMvars(meshControlAttributes) {}

  while {[gets $fp line] >=0 } {

     set line [string trim $line]
     catch {set cmd [lindex $line 0]}

     if {$line == ""} {
       # just skip blank line
     } elseif {[string index $line 0] == "#"} {
       # don't preserve comment lines
       #set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd == "newMesh"} {
       # automatically generated
     } elseif {$cmd == "loadModel"} {
       set gFilenames(atdb_solid_file) [lindex $line 1]
       # should we check that the solid model exists here?
     } elseif {$cmd == "msinit"} {
       # automatically generated
     } elseif {$cmd == "msinitbl"} {
       # automatically generated if needed
     } elseif {$cmd == "msexit"} {
       # automatically generated
     } elseif {$cmd == "memoff"} {
       # ignore
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) #IGNORED: $line\n"
     } elseif {$cmd == "deleteMesh"} {
       # automatically generated
     } elseif {$cmd == "deleteModel"} {
       # automatically generated
     } elseif {$cmd == "writeSMS"} {
       puts "WARNING:  writeSMS not compatible with Geodesic."
       puts "          Converting call from writeSMS to writeMesh."
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) #WARNING: converted writeSMS to writeMesh.\n"
       set gFilenames(mesh_file) [lindex $line 1]
     } elseif {$cmd == "writeMesh"} {
       set gFilenames(mesh_file) [lindex $line 1]
     } elseif {$cmd == "writeStats"} {
       set guiMMvars(meshWriteMeshStats) 1
       # what about the filename?
     } elseif {$cmd == "readSMS"} {
       # ignore
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) #IGNORED: $line\n"
     } elseif {$cmd == "constrainSurface"} {
       set guiMMvars(meshConstrainSurface) 1
     } elseif {$cmd == "generateMesh"} {
       set guiMMvars(meshGenerateSurfaceMesh) 1
       set guiMMvars(meshGenerateVolumeMesh) 1
     } elseif {$cmd == "generateSurfaceMesh"} {
       set guiMMvars(meshGenerateSurfaceMesh) 1
     } elseif {$cmd == "generateVolumeMesh"} {
       set guiMMvars(meshGenerateVolumeMesh) 1
     } elseif {$cmd == "tri2quad"} {
       # ignore
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) #IGNORED: $line\n"
     } elseif {$cmd  == "option"} {
       # now process configuration options
       if {[lindex $line 1] == "surface" && [lindex $line 2] == "snap"} {
         # ignore
         set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) #IGNORED: $line\n"
       } elseif {[lindex $line 1] == "surface" && [lindex $line 2] == "optimization"} {
          set guiMMvars(meshOptimizeSurfaceMesh) [lindex $line 3]
       } elseif {[lindex $line 1] == "surface" && [lindex $line 2] == "smoothing"} {
          set guiMMvars(meshSmoothSurfaceMesh) [lindex $line 3]
          set guiMMvars(meshNumSmoothingPasses) [lindex $line 3]
       } elseif {[lindex $line 1] == "surface"} {
          set guiMMvars(meshGenerateSurfaceMesh) [lindex $line 2]
       } elseif {[lindex $line 1] == "volume" && [lindex $line 2] == "optimization"} {
          set guiMMvars(meshOptimizeVolumeMesh) [lindex $line 3]
       } elseif {[lindex $line 1] == "volume" && [lindex $line 2] == "smoothing"} {
          set guiMMvars(meshSmoothVolumeMesh) [lindex $line 3]
       } elseif {[lindex $line 1] == "volume"} {
          set guiMMvars(meshGenerateVolumeMesh) [lindex $line 2]
       } elseif {[lindex $line 1] == "bl" && [lindex $line 1] == "type"} {
          set guiMMvars(meshBoundaryLayerType) [lindex $line 3]
       } else {
          puts "ERROR: line $line ignored in meshSim script file."
       }
     } elseif {$cmd == "nomesh"} {
       # ignore
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) #IGNORED: $line\n"
     } elseif {$cmd == "size"} {
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd == "minsize"} {
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd == "gsize"} {
       set guiMMvars(gsize_type) [lindex $line 1]
       set guiMMvars(gsize) [lindex $line 2]
     } elseif {$cmd == "gminsize"} {
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd == "curv"} {
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd =="gcurv"} {
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd == "mincurv"} {
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd =="gmincurv"} {
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd == "bl"} {
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd == "refine"} {
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd == "match"} {
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd == "setEdgeMesh"} {
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd == "sphereRefinement"} {
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd == "cylinderRefinement"} {
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd == "boundaryLayer"} {
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) $line\n"
     } elseif {$cmd == "newMeshAdapt"} {
       # ignore
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) #IGNORED: $line\n"
     } elseif {$cmd == "deleteMeshAdapt"} {
       # ignore
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) #IGNORED: $line\n"
     } elseif {$cmd == "adapt"} {
       # ignore
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) #IGNORED: $line\n"
     } elseif {$cmd == "refineRandom"} {
       # ignore
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) #IGNORED: $line\n"
     } elseif {$cmd == "refineGFaceRegions"} {
       # ignore
       set guiMMvars(meshControlAttributes) "$guiMMvars(meshControlAttributes) #IGNORED: $line\n"
     } elseif {$cmd == "logon"} {
       # automatically generated
     } elseif {$cmd == "logoff"} {
       # automatically generated
     } else {
       puts "ERROR:  line $line ignored from meshSim script file."
     }
  }
  close $fp
}


# Procedure: guiMMreadScriptFile
proc guiMMreadScriptFile {} {

  global symbolicName
  global gFilenames
  global gObjects
  global guiMMvars
  global gOptions

  puts "Read mesh script file."

  # first we check the file exists


  set kernel $gOptions(meshing_kernel)

  if {$kernel == "MeshSim"} {
    if {[file exists $gFilenames(mesh_script_file)] != 1} {
      puts "ERROR:  File \"$gFilenames(mesh_script_file)\" does not exist!"
