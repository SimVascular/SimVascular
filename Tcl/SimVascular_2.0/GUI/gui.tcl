# Procedure: guiRESULTS3Dshow
proc guiRESULTS3Dshow { side type} {

  global guiRESULTSvars
  global gRenWin_3D_vars
  global gRen3d
  global gOptions

  set useLOD $gRenWin_3D_vars(solid_use_LOD)

  set obj $guiRESULTSvars($side\_selected_solid_model)
  set show $guiRESULTSvars($side\_show)
  set opacity $guiRESULTSvars($side\_opacity_scale_value)
  set color $guiRESULTSvars($side\_color)

  if {$type == "results"} {
    foreach i [repos_subList results_vtkobj_*] {
      catch {vis_lodRm $gRen3d $i}
      catch {vis_pRm $gRen3d $i}
    }
    
    # map string to plot name
    set selected_3D_name $guiRESULTSvars($side\_3D_selected)
    set solid $guiRESULTSvars($side\_selected_solid_model)

    if {$selected_3D_name == "None" || $solid == "None"} {
      # clear and return
      set guiRESULTSvars($side\_3D_displayed) None
      return
    } else {
      set guiRESULTSvars($side\_3D_displayed) None
      set selected_3D_index [lsearch $guiRESULTSvars(3D_names_$solid) $selected_3D_name]
      if {$selected_3D_index < 0} {
        return -code error "ERROR: could not find 3D result for solid name!"
      }

      set showmeobj [lindex $guiRESULTSvars(3D_objs_$solid) $selected_3D_index]
      set srange [lindex $guiRESULTSvars(3D_scalar_ranges_$solid) $selected_3D_index]
      set guiRESULTSvars($side\_3D_displayed) $showmeobj
    }
    if {$useLOD == 1} {
       vis_lodRepos $gRen3d $showmeobj
       #vis_lodSetOpacity $gRen3d $obj $opacity
       #vis_lodSetColor $gRen3d $obj [lindex $color 0] [lindex $color 1] [lindex $color 2]
    } else {
       set p [vis_pRepos $gRen3d $showmeobj]
       #set p [vis_pRepos $g $vtkObj]
       [$p GetMapper] ScalarVisibilityOn
       [$p GetMapper] SetLookupTable gBlueToRedLUT
       #set range [[[[[$p GetMapper] GetInput] GetPointData] GetScalars] GetRange]
       [$p GetMapper] SetScalarRange [lindex $srange 0] [lindex $srange 1]
       vis_render $gRen3d
    }
    return
  }

  set guiRESULTSvars(visopts_solid_$obj) [list show $show opacity $opacity r [lindex $color 0] g [lindex $color 1] b [lindex $color 2]]

  if {$obj == "None"} return

  if {$show == 0} {
    catch {vis_lodRm $gRen3d $obj}
    catch {vis_pRm $gRen3d $obj}
  } else {
    if {$useLOD == 1} {
       vis_lodRepos $gRen3d $obj
       vis_lodSetOpacity $gRen3d $obj $opacity
       vis_lodSetColor $gRen3d $obj [lindex $color 0] [lindex $color 1] [lindex $color 2]
    } else {
       vis_pRepos $gRen3d $obj
       [[vis_pGetActor $gRen3d $obj] GetProperty] SetOpacity $opacity
       [[vis_pGetActor $gRen3d $obj] GetProperty] SetColor [lindex $color 0] [lindex $color 1] [lindex $color 2]
    }
  }
  catch {vis_render $gRen3d}
}


