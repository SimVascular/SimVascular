# Procedure: bcGUI_hand_UpdateQueryInteraction
proc bcGUI_hand_UpdateQueryInteraction { widget x y} {

  global bcGUImagWindow
  global bcGUIpotWindow

  set x [expr [winfo pointerx $widget] - [winfo rootx $widget]] 
  set y [expr [winfo pointery $widget] - [winfo rooty $widget]] 

  global symbolicName
  if {[file rootname $widget] == $symbolicName(bcRenWinMag)} {
    set ren $bcGUImagWindow(ren)
  } else {
    set ren $bcGUIpotWindow(ren)
  }

  # y is flipped upside down
  set height [lindex [$widget configure -height] 4]
  set y [expr $height - $y]

  set coord tmp-add_point_cord
  catch {$coord Delete}
  vtkCoordinate $coord
  $coord SetCoordinateSystemToDisplay
  $coord SetValue $x $y
  set worldValue [$coord GetComputedWorldValue $ren]
  $coord Delete
  #puts "x: $x  y: $y  worldValue: $worldValue"

  global gBC
  foreach param $gBC(phase_contrast_params) {
    set [lindex $param 0] [lindex $param 1]
  }

  set i [expr int(floor([lindex $worldValue 0]/[lindex $voxel_dims 0]))]
  set j [expr int(floor([lindex $worldValue 1]/[lindex $voxel_dims 1]))]

  if {$i < $gBC(voi_x0) || $i > $gBC(voi_x1) ||  $j < $gBC(voi_y0) || $j > $gBC(voi_y1)} {
      puts "$i $j out of range!"
      return
   }

  # only true when entire image is used!
  #set pixel [expr $i+$j*$gBC(ext_i)]
  #set pixelI [expr $pixel%$gBC(ext_i)]
  #set pixelJ [expr $pixel/$gBC(ext_i)]
  #puts "i: $i  j: $j pixelI: $pixelI  pixelJ:  $pixelJ"

  set pixel [expr ($i - $gBC(voi_x0)) + ($j - $gBC(voi_y0))*($gBC(voi_x1)-$gBC(voi_x0)+1)]

  global bcGUIcurrentFrameNumber
  set magslice /gBC/pcdata/mag/$bcGUIcurrentFrameNumber
  set magvalue [[[[repos_exportToVtk -src $magslice] GetPointData] GetScalars] GetTuple1 $pixel]

  # slightly different output depending on 1-comp or 3-comp
  # velocity data
  if {$gBC(numVelComp) == 1} {
    # only for thruplane
    set velslice /gBC/pcdata/vel/$bcGUIcurrentFrameNumber
    set velvalue [[[[repos_exportToVtk -src $velslice] GetPointData] GetScalars] GetTuple1 $pixel]
    set velstring "vel: $velvalue"
  } elseif {$gBC(numVelComp) == 3} {
    set velsliceX /gBC/pcdata/vel/$bcGUIcurrentFrameNumber/r
    set velsliceY /gBC/pcdata/vel/$bcGUIcurrentFrameNumber/a
    set velsliceZ /gBC/pcdata/vel/$bcGUIcurrentFrameNumber/s
    set velvalueX [[[[repos_exportToVtk -src $velsliceX] GetPointData] GetScalars] GetTuple1 $pixel]
    set velvalueY [[[[repos_exportToVtk -src $velsliceY] GetPointData] GetScalars] GetTuple1 $pixel]
    set velvalueZ [[[[repos_exportToVtk -src $velsliceZ] GetPointData] GetScalars] GetTuple1 $pixel]
    set velstring "vel: <$velvalueX,$velvalueY,$velvalueZ>"
  } else {
    return -code error "ERROR:  Illegal number of velocity components"
  }

  vis_textSet $ren "$i $j mag:$magvalue $velstring"
}


# Procedure: bcGUI_hand_add_point
proc bcGUI_hand_add_point { widget x y} {

  global bcGUImagWindow
  global bcGUIpotWindow

  set x [expr [winfo pointerx $widget] - [winfo rootx $widget]] 
  set y [expr [winfo pointery $widget] - [winfo rooty $widget]] 

  global symbolicName
  if {[file rootname $widget] == $symbolicName(bcRenWinMag)} {
    set ren $bcGUImagWindow(ren)
  } else {
    set ren $bcGUIpotWindow(ren)
  }

  set selectedPoints bcGUItmp-handdraw-pts1
  set selectedLines bcGUItmp-handdraw-lines1

  # y is flipped upside down
  set height [lindex [$widget configure -height] 4]
  set y [expr $height - $y]

  set coord tmp-add_point_cord
  catch {$coord Delete}
  vtkCoordinate $coord
  $coord SetCoordinateSystemToDisplay
  $coord SetValue $x $y
  set worldValue [$coord GetComputedWorldValue $ren]
  $coord Delete
  puts "x: $x  y: $y  worldValue: $worldValue"

  #$selectedPoints InsertNextPoint $x $y 0
  $selectedPoints InsertNextPoint [lindex $worldValue 0] [lindex $worldValue 1] 0

  if {[$selectedPoints GetNumberOfPoints] == 1} {return}

  $selectedLines InsertNextCell 2
  $selectedLines InsertCellPoint [expr [$selectedPoints GetNumberOfPoints] - 2]
  $selectedLines InsertCellPoint [expr [$selectedPoints GetNumberOfPoints] - 1]

  foreach renWin {bcRenWinMag bcRenWinPot} {
      global symbolicName
      set widget $symbolicName($renWin).r1
      if {[info commands $widget] != ""} {
           $widget Render
      }
  }
}


# Procedure: bcGUI_hand_make_poly
proc bcGUI_hand_make_poly { widget x y} {

  set selectedPoints bcGUItmp-handdraw-pts1
  set selectedLines bcGUItmp-handdraw-lines1
  set cursorPoints bcGUItmp-handdraw-crsr-pts1

  # create a  closed polygon from the selected points

  set npts [$selectedPoints GetNumberOfPoints]

  if {$npts < 3} {
    return -code error "Need at 3 points to define a closed contour!"
  }

  global bcGUIbaselineAddPolys
  if {$bcGUIbaselineAddPolys == 0} {

    global bcGUIcurrentFrameNumber
    set frame $bcGUIcurrentFrameNumber

    global bcGUIhandContourType
    set type $bcGUIhandContourType

    if {$type == "levelset"} {
      set seg /gBC/pcdata/mag/$frame/ls
    } elseif {$type == "threshold"} {
      set seg /gBC/pcdata/mag/$frame/thr/selected
    } else {
      return -code error "Invalid segmentation type ($type)."
    }

    catch {repos_delete -obj $seg}

    # create the contour
    set pts {}
    for {set i 0} {$i < $npts} {incr i} {
      lappend pts [$selectedPoints GetPoint $i]
    }

    global gBC
    foreach param $gBC(phase_contrast_params) {
      set [lindex $param 0] [lindex $param 1]
    }

    # subsample the loop
    set curveLength [math_curveLength -pts $pts -closed 1]
    set numSamplePoints [expr int(ceil($curveLength/[lindex $voxel_dims 0]))]
    set newpts [math_linearInterpCurve -pts $pts -closed 1 -numInterpPts $numSamplePoints]
    geom_mkLinesFromPts $newpts $seg 1

    # track creation parameters via labels
    foreach obj [list $seg] {
      repos_setLabel -obj $obj -key creation_method     -value "hand_drawn"
      repos_setLabel -obj $obj -key num_hand_picked_pts -value $npts
      repos_setLabel -obj $obj -key creation_type       -value $type
      repos_setLabel -obj $obj -key num_subsample_pts   -value $numSamplePoints
      repos_setLabel -obj $obj -key frame               -value $frame
    }

    # update display
    bcGUIchangeFrame $frame

  } else {

    global bcGUIbaselineRegions

    # find the highest region id and use id+1 for next region number
    set id 0
    if {[llength $bcGUIbaselineRegions] > 0} {
      foreach i $bcGUIbaselineRegions {
        set obj [lindex $i 0]
        set objid [file tail $obj]
        if {$objid > $id} {
         set id $objid
        }
      }
    }
    incr id

    set nextRegion /bcGUI/region/$id
    set visobj /bcGUI/region/vis/$id

    lappend bcGUIbaselineRegions [list $nextRegion {}]

    catch {repos_delete -obj $nextRegion}
    catch {repos_delete -obj $visobj}

    set pts {}
    for {set i 0} {$i < $npts} {incr i} {
      lappend pts [$selectedPoints GetPoint $i]
    }

    geom_polygonFromPts $pts $nextRegion
    geom_triangulate $nextRegion $visobj

    # add to the listbox
    global symbolicName
    set lbox $symbolicName(bcGUIbaselineRegionListbox)
    $lbox insert end [list "region_$id" {}]

    # display the polygon
    global bcGUImagWindow
    set ren $bcGUImagWindow(ren)
    catch {vis_pRm $ren $visobj}
    set actor [vis_pRepos $ren $visobj]
    [$actor GetProperty] SetColor 0 1 0
    [$actor GetProperty] SetOpacity 0.33

  }

  # clear the selection polygon
  $selectedLines Reset
  $selectedPoints Reset

  # clear the cursor if necessary
  $cursorPoints Reset

  foreach renWin {bcRenWinMag bcRenWinPot} {
      global symbolicName
      set widget $symbolicName($renWin).r1
      if {[info commands $widget] != ""} {
           $widget Render
      }
  }
}


# Procedure: bcGUI_hand_remove_last_point
proc bcGUI_hand_remove_last_point { widget x y} {

  set selectedPoints bcGUItmp-handdraw-pts1
  set selectedLines bcGUItmp-handdraw-lines1
  set cursorPoints bcGUItmp-handdraw-crsr-pts1

  set x [expr [winfo pointerx $widget] - [winfo rootx $widget]] 
  set y [expr [winfo pointery $widget] - [winfo rooty $widget]] 

  $cursorPoints Reset

  set desiredNumPts [expr [$selectedPoints GetNumberOfPoints] - 1]
  if {$desiredNumPts <= 0} {
     $selectedLines Reset
     $selectedPoints  Reset
     foreach renWin {bcRenWinMag bcRenWinPot} {
       global symbolicName
       set widget $symbolicName($renWin).r1
       if {[info commands $widget] != ""} {
           $widget Render
       }
     }
     return
  }

  $selectedLines Reset

  foreach renWin {bcRenWinMag bcRenWinPot} {
    global symbolicName
    set widget $symbolicName($renWin).r1
    if {[info commands $widget] != ""} {
       $widget Render
    }
  }

  $selectedPoints SetNumberOfPoints $desiredNumPts
  $selectedPoints Squeeze

  if {$desiredNumPts > 1} {

    # not sure how to remove just the last cell, lets rebuild from scratch
    for {set i 0} {$i < [expr $desiredNumPts - 1]} {incr i} {
      puts "$i: [expr 2*$i] [expr 2*$i+1]"
      $selectedLines InsertNextCell 2
      $selectedLines InsertCellPoint $i
      $selectedLines InsertCellPoint [expr $i+1]
    }
  }

  foreach renWin {bcRenWinMag bcRenWinPot} {
      global symbolicName
      set widget $symbolicName($renWin).r1
      if {[info commands $widget] != ""} {
           $widget Render
      }
  }

  bcGUI_hand_show_cursor $widget $x $y
}


# Procedure: bcGUI_hand_show_cursor
proc bcGUI_hand_show_cursor { widget x y} {


  set selectedPoints bcGUItmp-handdraw-pts1

  set cursorPoints bcGUItmp-handdraw-crsr-pts1

