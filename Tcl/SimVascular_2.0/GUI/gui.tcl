# Procedure: bcGUIaddToGroup
proc bcGUIaddToGroup { type} {

  global bcGUIcurrentFrameNumber
  set frame $bcGUIcurrentFrameNumber
  global bcGUIcurrentGroup
  set group $bcGUIcurrentGroup

  # check that the group exists, if it doesn't offer to
  # create
  if {$group == ""} {
    return -code error "Group name cannot be blank!"
  }

  if {[group_exists $group] == 0} {
    set yesno [tk_messageBox -default yes  -message "Current group ($group) does not exist.  It must exist to be able to add segmentations.  Create it now?"  -title "Current Group Does Not Exist"  -type yesno]
    switch -- $yesno {
       yes {
          group_create $group
       }
       no {
          return -code error "ERROR: group does not exist."
       }
    }
  }

  # need to translate hand/analytic to proper type
  if {$type == "analytic"} {
    global bcGUIanalContourType
    set type $bcGUIanalContourType
  } elseif {$type == "hand"} {
    global bcGUIhandContourType
    set type $bcGUIhandContourType
  }

  set groupItem "/group/$group/$frame"

  if {$type == "levelset"} {
    set seg /gBC/pcdata/mag/$frame/ls
  } elseif {$type == "threshold"} {
    set seg /gBC/pcdata/mag/$frame/thr/selected
  } else {
    return -code error "Invalid segmentation type ($type)."
  }

  if {[repos_exists -obj $seg] == 0} {
    puts "WARNING:  No oriented segmentation found!"
    return
    #return -code error "ERROR:  No oriented segmentation found!"
  }
  if {[group_idtaken $group $frame] == 1} {
     puts "WARNING:  A slice existed at $frame and is being replaced with current selection!"
     group_remove $group $groupItem
  }
  catch {repos_delete -obj $groupItem}
  geom_copy -src $seg -dst $groupItem
  foreach key [repos_getLabelKeys -obj $seg] {
    set value [repos_getLabel -obj $seg -key $key]
    repos_setLabel -obj $groupItem -key $key -value $value
  }
  group_add $group $groupItem $frame
}


