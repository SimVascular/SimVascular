# Procedure: resampleGUIsetup
proc resampleGUIsetup {} {

  set context guiRIB

  upvar #0 $context guiVars

  if {$guiVars(show_image) == 0} {return}

  global symbolicName

  if { [info exists guiVars(use_alt_image)] && $guiVars(use_alt_image) } {
  set repos_image $guiVars(repos_image_obj_alt)  
  } else {
  set repos_image $guiVars(repos_image_obj)   
  }

  # do nothing if we don't have a resampled image
  if {[repos_exists -obj $repos_image] == 0} {return}

  set image [repos_exportToVtk -src $repos_image]

  set ldims [$image GetDimensions]
  set vdims [$image GetSpacing]
  set rng [$image GetScalarRange]

  # set ranges
  $symbolicName(resampleGUI_sliderx) configure -from 0
  $symbolicName(resampleGUI_sliderx) configure -to [expr [lindex $ldims 0] - 1]
  $symbolicName(resampleGUI_slidery) configure -from 0
  $symbolicName(resampleGUI_slidery) configure -to [expr [lindex $ldims 1] - 1]
  $symbolicName(resampleGUI_sliderz) configure -from 0
  $symbolicName(resampleGUI_sliderz) configure -to [expr [lindex $ldims 2] - 1]

  volGUIupdateColorMaps $context

  # update text showing range
  set guiVars(nativeRange) "native \[$rng\]"
}


# Procedure: resampleGUIupdateResample
proc resampleGUIupdateResample { flag} {

  set context guiRIB
  
  # the value of context in the global environment is linked to the local variable, guiVars
  upvar #0 $context guiVars

  set resample_flag $guiVars(resample_flag)
  set show_image $guiVars(show_image)

  # do nothing if the image is not currently
  # displayed and this is not a call by
  # the resample check button
  if {($flag == 0) &&  ($show_image == 0)} {
    return
  }

  # calls from the resample checkbutton

  # turn off displayed resample
  if {($flag == 1) && ($resample_flag == 0)} {

     # remove vol rendering
     set guiVars(show_vol_render) 0
     volGUIupdateVolRender $context all

     # remove isosurface
     set guiVars(show_isosurface) 0
     volGUIupdateIsosurface $context

     # remove point cloud
     set guiVars(show_point_cloud) 0
     volGUIupdatePointCloud $context

     # remove slice planes
     set guiVars(show_RL) 0
     set guiVars(show_AP) 0
     set guiVars(show_SI) 0
     volGUItoggleDisplaySlices $context RL
     volGUItoggleDisplaySlices $context AP
     volGUItoggleDisplaySlices $context SI

     # remove outline
     global gRen3d
     set outline $guiVars(repos_outline_obj)
     catch {vis_pRm $gRen3d $outline}

     set guiVars(show_image) 0

     # if interactor used, remove it
     if {$guiVars(interactor_displayed) == 1} {
       vis_boxWidgetRm $gRen3d resampleGUI
       set guiVars(interactor_displayed) 0
     }
     return

  }

  # make sure we have reasonable box dims
  if {$guiVars(box_min_r) == 0 && $guiVars(box_min_a) == 0 && $guiVars(box_min_s) == 0 &&  $guiVars(box_max_r) == 0 && $guiVars(box_max_a) == 0 && $guiVars(box_max_s) == 0} {
    return -code error "ERROR:  Must specify resample box with non-zero volume!"
  }

  # create resample (and update display if necessary)
  if {($resample_flag == 1)} {

     # set center based on requested method

     if { [info exists guiVars(use_alt_image)] && $guiVars(use_alt_image) } {
    set imgobj $guiVars(repos_image_obj_alt)  
    } else {
    set imgobj $guiVars(repos_image_obj)    
  }
  
     set centerBy $guiVars(box_center_method)
     set size $guiVars(resample_voxel_size)

     set orgctr {}

     if {$centerBy == "pathplan_cursor"} {

       global guiPPchooserXvar
       global guiPPchooserYvar
       global guiPPchooserZvar
       set orgctr [list $guiPPchooserXvar $guiPPchooserYvar $guiPPchooserZvar]

     } elseif {$centerBy == "interactor"} {
       # do work for this later
       # if interactor used, remove it
       if {$guiVars(interactor_displayed) == 0} {
   catch {vis_boxWidgetRm $gRen3d resampleGUI}
         global gRen3d
         vis_boxWidgetAdd $gRen3d [repos_exportToVtk -src volume_image] resampleGUI resampleGUIenableInteraction resampleGUIbeginInteraction resampleGUIinteract resampleGUIendInteraction
         vis_boxWidgetOn $gRen3d resampleGUI
         set guiVars(interactor_displayed) 1
       }
    } elseif {$centerBy == "vol_sliders"} {
       # use of guiVIB is intentional
       global guiVIB
       set orgctr [list $guiVIB(r_coord) $guiVIB(a_coord) $guiVIB(s_coord)]
    } elseif {$centerBy == "path_point"} {
       global lsGUIpositionIdStr
       set orgctr $lsGUIpositionIdStr
    } else {
       return -code error "ERROR:  Illegal centerBy code."
    }

    set dx [expr abs($guiVars(box_max_r)-$guiVars(box_min_r))]
    set dy [expr abs($guiVars(box_max_a)-$guiVars(box_min_a))]
    set dz [expr abs($guiVars(box_max_s)-$guiVars(box_min_s))]

    if {$centerBy != "interactor"} {
      # the user specified center does not correspond to the real center
      # of the box if the user has not specified a box centered around 0,0
      set llx [expr [lindex $orgctr 0] + $guiVars(box_min_r) * $size]
      set lly [expr [lindex $orgctr 1] + $guiVars(box_min_a) * $size]
      set llz [expr [lindex $orgctr 2] + $guiVars(box_min_s) * $size]
      set urx [expr [lindex $orgctr 0] + $guiVars(box_max_r) * $size]
      set ury [expr [lindex $orgctr 1] + $guiVars(box_max_a) * $size]
      set urz [expr [lindex $orgctr 2] + $guiVars(box_max_s) * $size]

      set vdims_x $size
      set vdims_y $size
      set vdims_z $size

    } else {

      set bounds $guiVars(resample_interactor_bounds)
      set llx [lindex $bounds 0]
      set lly [lindex $bounds 2]
      set llz [lindex $bounds 4]
      set urx [lindex $bounds 1]
      set ury [lindex $bounds 3]
      set urz [lindex $bounds 5]
      set vdims_x [expr abs($urx - $llx) / double($dx)]
      set vdims_y [expr abs($ury - $lly) / double($dy)]
      set vdims_z [expr abs($urz - $llz) / double($dz)]
      puts "vdims_x: $vdims_x  vdims_y: $vdims_y  vdims_z: $vdims_z"
    }

    set ctr [list [expr ($llx+$urx)/2.0]  [expr ($lly+$ury)/2.0]  [expr ($llz+$urz)/2.0]]

    # our resampled image has a new origin
    set org_x $llx
    set org_y $lly
    set org_z $llz

    set voi_x0 0
    set voi_y0 0
    set voi_z0 0
    puts "extent $dx $dy $dz"

    puts "create resampled image"
    set rs tmp-resampleGUIupdateResample
    catch {$rs Delete}
    vtkImageReslice $rs
    global guiVIB
    if { [info exists guiVIB(use_alt_image)] && $guiVIB(use_alt_image) } {
    set repos_image $guiVIB(repos_image_obj_alt)  
    } else {
    set repos_image $guiVIB(repos_image_obj)    
    }
    $rs SetInputDataObject [repos_exportToVtk -src $repos_image] ; # use of guiVIB is intentional here!
    $rs SetInterpolationModeToLinear
    $rs SetOutputOrigin $org_x $org_y $org_z
    $rs SetOutputExtent 0 $dx 0 $dy 0 $dz
    $rs SetOutputSpacing $vdims_x $vdims_y $vdims_z
    $rs Update

    catch {repos_delete -obj $imgobj}

    if {$guiVars(resample_gradient) == 0} {
      repos_importVtkImg -src [$rs GetOutput] -dst $imgobj
      $rs Delete
     } else {
       set maggrad tmp-resampleGUI-mag-grad
       catch {$maggrad Delete}
       vtkImageGradientMagnitude $maggrad
       $maggrad SetInputConnection [$rs GetOutputPort]
       $maggrad Update
       repos_importVtkImg -src [$maggrad GetOutput] -dst $imgobj
       $maggrad Delete
       $rs Delete
     }

     set guiVars(show_image) 1
     resampleGUIsetup

     # check  for seeded thr
     volGUIupdateSeed guiRIB manual

     # Outline
     set outline $guiVars(repos_outline_obj)
     set outflt tmp-flt-resampleGUIupdate
     catch {repos_delete -obj $outline}
     catch {$outflt Delete}
     vtkOutlineFilter $outflt
     $outflt SetInputDataObject [repos_exportToVtk -src $imgobj]
     $outflt Update
     repos_importVtkPd -src [$outflt GetOutput] -dst $outline
     $outflt Delete
     global gRen3d
     catch {vis_pRm $gRen3d $outline}
     set actor [vis_pRepos $gRen3d $outline]
     [$actor GetProperty] SetColor 0 1 0

     # slice planes
     vis_img_VolImgBrwsr2 $imgobj $guiVars(vis_basename)
     volGUItoggleDisplaySlices $context RL
     volGUItoggleDisplaySlices $context AP
     volGUItoggleDisplaySlices $context SI
     volGUIupdateSlider $context RL $guiVars(sliderx_value)
     volGUIupdateSlider $context AP $guiVars(slidery_value)
     volGUIupdateSlider $context SI $guiVars(sliderz_value)

     # point cloud
     volGUIupdatePointCloud $context

     # isosurface
     volGUIupdateIsosurface $context

     # vol rendering
     volGUIupdateVolRender $context all

     return

  }
}


# Procedure: smasherGUIcalcFaceArea
proc smasherGUIcalcFaceArea {} {

  global symbolicName
  global gRen3d
  global smasherEntityIdentifier

  # get the selected object name
  set objectname /tmp/smasher/[$symbolicName(smasherEntityIdLabel) get]

  set area [geom_surfArea -src $objectname]
  set AttName [$symbolicName(smasherAttNameLabel) get]
  set ident $smasherEntityIdentifier

  tk_messageBox -title "Surface Area" -type ok  -message "face name: $AttName\nidentifier: $ident\narea: $area"
