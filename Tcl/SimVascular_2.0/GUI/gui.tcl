# Procedure: guiGROUPSreplaceWithSpline
proc guiGROUPSreplaceWithSpline {} {

  global guiGROUPSvars
  global symbolicName
  global gRen3d

  global symbolicName

  set tv $symbolicName(guiSV_group_tree)
  set segs [$tv selection]

  if {$segs == ""} {return}

  # only handle the case of a single spline image for now!
  set segs [lindex $segs 0]

  foreach i $segs {
    set id [string range [file extension $i] 1 end]
    set grp [string range [file extension [file rootname $i]] 1 end]
    puts "id: $id grp: $grp"
    set obj /group/$grp/$id
  }

  if {$obj == ""} {return}

  set yesno [tk_messageBox -icon question -message "This cannot be undone.  Proceed?"  -type yesno -default yes]
  if {$yesno == "no"} {
     puts "Operation cancelled.\n"
     return
  }

  foreach key [repos_getLabelKeys -obj $obj] {
      set tags($key) [repos_getLabel -obj $obj -key $key]
  }
  set tags(modified_as_spline) 1

  repos_delete -obj $obj

  set pts [vis_splineClosedWidgetGetPts $gRen3d guiGROUPS]
  geom_mkLinesFromPts $pts $obj 1

  foreach key [array names tags]  {
    repos_setLabel -obj $obj -key $key -value $tags($key)
  }
}


# Procedure: guiGROUPSshowSpline
proc guiGROUPSshowSpline {} {
  global guiGROUPSvars

  global gRen3d
  global guiGROUPSvars
  set flag $guiGROUPSvars(show_spline)

  catch {vis_splineClosedWidgetRm $gRen3d guiGROUPS}

  if {$flag == 0} {
    return
  }

  vis_splineClosedWidgetAdd $gRen3d  [repos_exportToVtk -src volume_image]  guiGROUPS  guiGROUPSsplineEnableEvent  guiGROUPSsplineStartInteractionEvent  guiGROUPSsplineInteractionEvent  guiGROUPSsplineEndInteractionEvent

  global symbolicName

  set tv $symbolicName(guiSV_group_tree)
  set segs [$tv selection]

  if {$segs == ""} {return}

  # only handle the case of a single spline image for now!
  set segs [lindex $segs 0]

  foreach i $segs {
    set id [string range [file extension $i] 1 end]
    set grp [string range [file extension [file rootname $i]] 1 end]
    puts "id: $id grp: $grp"
    set obj /group/$grp/$id
  }

  set seg /guiGROUPS/segment
  set guiGROUPSvars(spline_id) $id
  set idloc [lsearch [group_get $grp] /group/$grp/$id]

  if {$idloc < 0} {
     return -code error  "ERROR:  could not find id!"
  }
  set idloclength [expr [llength [group_get $grp]] - 1]
  if {$idloc == 0} {
    set guiGROUPSvars(seg_before_id) ""
    set guiGROUPSvars(seg_after_id)  $seg/0
  } elseif {$idloc == $idloclength } {
    set guiGROUPSvars(seg_before_id) $seg/$idloclength
    set guiGROUPSvars(seg_after_id)  ""
  } else {
    set guiGROUPSvars(seg_before_id) $seg/[expr $idloc - 1]
    set guiGROUPSvars(seg_after_id) $seg/$idloc
  }
  global gPathBrowser
  set numHandles $gPathBrowser(solid_sample)

  vis_splineClosedWidgetSetPts $gRen3d guiGROUPS /group/$grp/$id $numHandles

  vis_splineClosedWidgetOn $gRen3d guiGROUPS
  # since it appears the beginInteraction gets called when you turn on the widget,
  # we need to explicitly call the endInteraction the first time
  guiGROUPSsplineEndInteractionEvent
}


