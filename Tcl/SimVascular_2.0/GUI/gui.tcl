# Procedure: geomConnectTwoProfiles
proc geomConnectTwoProfiles { pts1 pts2 dstName} {

  set vpts tmp-geomConnectTwoProfilesPts
  set vconn tmp-geomConnectTwoProfilesConn
  set vpd tmp-geomConnectTwoProfilesPd

  catch {$vpts Delete}
  catch {$vconn Delete}
  catch {$vpd Delete}

  vtkPoints $vpts
  $vpts Allocate 200 400

  vtkIdList $vconn
  $vconn Initialize
  $vconn Allocate 200 400

  vtkPolyData $vpd
  $vpd Initialize
  $vpd Allocate 200 400

  # create vtkPoints array
  set numPts [llength $pts1]

  for {set i 0} {$i < $numPts} {incr i} {
    set pt1 [lindex $pts1 $i]
    $vpts InsertNextPoint [lindex $pt1 0] [lindex $pt1 1] [lindex $pt1 2]
  }
  for {set i 0} {$i < $numPts} {incr i} {
    set pt2 [lindex $pts2 $i]
    $vpts InsertNextPoint [lindex $pt2 0] [lindex $pt2 1] [lindex $pt2 2]
  }

  $vpd SetPoints $vpts
  set numPolys $numPts
  for {set i 0} {$i < $numPolys} {incr i} {
    # create connectivity for polygon
    $vconn InsertNextId $i
    if {$i == [expr $numPolys  - 1]} {
      $vconn InsertNextId 0
      $vconn InsertNextId [expr $numPts]
    } else {
      $vconn InsertNextId [expr $i + 1]
      $vconn InsertNextId [expr $numPts + $i + 1]
    }
    $vconn InsertNextId [expr $numPts + $i]
    set VTK_POLYGON 7
    $vpd InsertNextCell $VTK_POLYGON $vconn
    $vconn Initialize
  }

  repos_importVtkPd -src $vpd -dst $dstName
  $vconn Delete
  $vpts Delete
  $vpd Delete
}


