# Procedure: lsGUIforFrank
proc lsGUIforFrank {} {

  puts "autocalculate missing ellipse parameter."
  global lsGUIellipsePERIMETER
  global lsGUIellipseA
  global lsGUIellipseB

  #
  #  Use approximation to perimeter of an ellipse:
  #    P = pi*(3(a+b) - sqrt((3a+b)*(a+3b)))
  #

  set a $lsGUIellipseA
  set b $lsGUIellipseB
  set p $lsGUIellipsePERIMETER

  if {$a == ""} {
     set d [expr double($p) / [math_pi]]
     set a1 [expr 1.0/6.0*(-4.0*$b+3.0*$d+sqrt(-20.0*$b*$b+12.0*$b*$d+3*$d*$d))]
     set a2 [expr 1.0/6.0*(-4.0*$b+3.0*$d-sqrt(-20.0*$b*$b+12.0*$b*$d+3*$d*$d))]
     puts "  a1: $a1 a2: $a2"
     if {$a1 > $a2} {
      set a $a1
     } else {
      set a $a2
     }
     set lsGUIellipseA $a
  } elseif {$b == ""} {
     set d [expr double($p) / [math_pi]]
     set b1 [expr 1.0/6.0*(-4.0*$a+3.0*$d+sqrt(-20.0*$a*$a+12.0*$a*$d+3*$d*$d))]
     set b2 [expr 1.0/6.0*(-4.0*$a+3.0*$d-sqrt(-20.0*$a*$a+12.0*$a*$d+3*$d*$d))]
     puts "  b1: $b1 b2: $b2"
     if {$b1 > $b2} {
      set b $b1
     } else {
      set b $b2
     }
     set lsGUIellipseB $b
  } else {
     set lsGUIellipsePERIMETER [expr [math_pi]*(3*($a+$b)-sqrt((3*$a+$b)*($a+3*$b)))]
 }
}


