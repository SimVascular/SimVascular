# Procedure: lsGUIsplineInteractionEvent
proc lsGUIsplineInteractionEvent {} {

  global gRen3d
  global gPathBrowser

  global lsGUInumFacetsForSurface
  set sample $lsGUInumFacetsForSurface

  global gInteractorRen

  set pts [vis_splineClosedWidgetGetPts $gInteractorRen lsGUI]

  global lsGUIorgSplinePts
  set orgSplinePts $lsGUIorgSplinePts

  set fixAxisX 0
  set fixAxisY 0
  set fixAxisZ 0
  set fixPlane 1

  for {set i 0} {$i < [llength $pts]} {incr i} {
    set orgpt [lindex $orgSplinePts $i]
    set newpt [lindex $pts $i]
    if {$fixAxisX != 0} {
       set x [lindex $orgpt 0]
    } else {
       set x [lindex $newpt 0]
    }
    if {$fixAxisY != 0} {
       set y [lindex $orgpt 1]
    } else {
       set y [lindex $newpt 1]
    }
    if {$fixAxisZ != 0} {
       set z [lindex $orgpt 2]
    } else {
       set z [lindex $newpt 2]
    }
    lappend fixedpts [list $x $y $z]
  }

  if {$fixPlane == 0} {
    vis_splineClosedWidgetUpdatePts $gRen3d lsGUI $fixedpts
  } else {
    vis_splineClosedWidgetUpdatePtsPlaneOnly $gRen3d lsGUI $fixedpts
  }

  global lsGUIspline_id
  global lsGUIseg_before_id
  global lsGUIseg_after_id
  global lsGUIsurfaceOpacity
  global lsGUIshowSurfaceFlag


  if {$lsGUIshowSurfaceFlag != 0} {
    set fixedpts [vis_splineClosedWidgetGetSamplePts $gRen3d lsGUI $sample]
    global gRen3dFreeze
    set gRen3dFreeze 1
    if {$lsGUIseg_before_id != ""} {
      set vtkobj [repos_exportToVtk -src $lsGUIseg_before_id]
      set ptobj [$vtkobj GetPoints]
      for {set i 0} {$i < [llength $fixedpts]} {incr i} {
        eval $ptobj SetPoint [expr $i + [llength $fixedpts]] [lindex $fixedpts $i]
      }
      vis_pRm $gRen3d $lsGUIseg_before_id
      set a [vis_pRepos $gRen3d $lsGUIseg_before_id]
      [$a GetProperty] SetOpacity $lsGUIsurfaceOpacity
    }
    if {$lsGUIseg_after_id != ""} {
      set vtkobj [repos_exportToVtk -src $lsGUIseg_after_id]
      set ptobj [$vtkobj GetPoints]
      for {set i 0} {$i < [llength $fixedpts]} {incr i} {
        eval $ptobj SetPoint $i [lindex $fixedpts $i]
      }
      vis_pRm $gRen3d $lsGUIseg_after_id
      set a [vis_pRepos $gRen3d $lsGUIseg_after_id]
      [$a GetProperty] SetOpacity $lsGUIsurfaceOpacity
    }
    set gRen3dFreeze 0
    vis_render $gRen3d
  }
}


