    catch {repos_delete -obj /tmp/bcGUIwriteVTKinletVel/ras}
    geom_applyTransformMatrix $inletMeshFace $mymatrix /tmp/bcGUIwriteVTKinletVel/ras
    repos_writeVtkPolyData -file [file join $mydir inlet-mesh-with-vel-$frame.vtk] -obj /tmp/bcGUIwriteVTKinletVel/ras -type ascii

  }

    catch {repos_delete -obj /tmp/bcGUIwriteVTKinletVel/ras}
    catch {$mymatrix Delete}
}


