      puts -nonewline $fp "\t[expr [lindex $mean($step) $j] * 10.0/1333.27]"
    }
    puts $fp ""
  }

  puts $fp "\n"

  puts $fp "flow"

  for {set i 0} {$i < [expr [llength $flow_title] - 1]} {incr i} {
    if {$skipped($i) == 0} {
      puts -nonewline $fp "[lindex $flow_title $i]\t"
    }
  }
  puts $fp ""

  set numSteps [llength [array names flow]]
  for {set i 0} {$i < $numSteps} {incr i} {
    if {$i == [expr $numSteps -1]} {
