# Procedure: guiMMmodelSaveModel
proc guiMMmodelSaveModel {} {

  global symbolicName
  global gFilenames
  global gObjects
  global gOptions

  if {$gOptions(meshing_solid_kernel) == "Parasolid" || $gOptions(meshing_solid_kernel) == "Discrete"} { 
    set filename $gFilenames(atdb_solid_file)
    set modelname $gObjects(atdb_solid)
  } elseif {$gOptions(meshing_solid_kernel) == "PolyData"} {
    set filename $gFilenames(polydata_solid_file)
    set modelname $gObjects(polydata_solid)
  }

  if {[repos_exists -obj $modelname] == 0} {
    puts "Error:  Object $modelname does not exist!"
    return -code error "ERROR:  Object $modelname does not exist!"
  }
  if {[repos_type -obj $modelname] != "SolidModel"} {
    puts "Error:  Object $modelname is not a solid model!"
    return -code error "ERROR:  Object $modelname is not a solid model!"
  }
  if {[file exists $filename] == "1"} {
    puts "Warning:  File $filename exists and will be overwritten!"
  }

  $modelname WriteNative -file [file rootname $filename]
}


