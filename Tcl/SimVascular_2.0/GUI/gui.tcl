# Procedure: bcGUIbaselineDeleteRegion
proc bcGUIbaselineDeleteRegion {} {

  global symbolicName
  set lbox $symbolicName(bcGUIbaselineRegionListbox)
  set select [$lbox curselection]

  # check for a selection
  if {$select == ""} {
    return
  }

  set line [$lbox get $select]
  set name [string trim [lindex $line 0]]
  if {[scan $name "region_%i" id] != 1} {
    return -code error "ERROR finding region id."
  }

  set nextRegion /bcGUI/region/$id
  set visobj /bcGUI/region/vis/$id

  # remove the polygon from display
  global bcGUImagWindow
  set ren $bcGUImagWindow(ren)
  catch {vis_pRm $ren $visobj}

  global bcGUIbaselineRegions
  set keepem {}

  foreach i $bcGUIbaselineRegions {
      set obj [lindex $i 0]
      set objid [file tail $obj]
      if {$objid != $id} {
       lappend keepem $i
      }
  }
  set bcGUIbaselineRegions $keepem

  $lbox delete $select $select

  # update display
  [$ren GetRenderWindow] Render
}


