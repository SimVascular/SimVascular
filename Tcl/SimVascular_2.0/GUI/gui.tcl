    puts "  kthr: $kt  klow: $klow  kupp: $kupp"

  }

  global lsGUIrunInParallel
  if {$lsGUIrunInParallel == 0} {

    set starttime [clock seconds]

    catch {lset2_main $mag $pot  $gridType $maxTS $gridFactor $stopV  $isotropicFlag $rebuildPhiFreq  $goodnessStop $acInterval  $radius $x $y $z $kt  $calcDecayConstBy $magGradFactor $potentialValueDecay  $klow $kupp   $calcPotConstBy $pFactor $potentialValuePotential  $lsres_unclean}

    #catch {lset_nate $mag $pot $radius $x $y $z  $kt $klow $kupp $magGradFactor  $gridType $maxTS $gridFactor  $isotropicFlag $goodnessStop $lsres_unclean}

    set totaltime  "[expr [clock seconds] - $starttime] seconds"

    # save the ls params with this slice
    if {[repos_exists -obj $lsres_unclean]} {

       geom_mergePts -src $lsres_unclean -dst $lsres -tol 0.001

       repos_delete -obj $lsres_unclean

       # orient profile

       global symbolicName
       global lsGUIcurrentPathNumber
       set pathId $lsGUIcurrentPathNumber
       global gPathPoints
       set path $gPathPoints($pathId,splinePts)

       global lsGUIcurrentPositionNumber
       set posId $lsGUIcurrentPositionNumber

       array set items [lindex $path $posId]
       set pos [TupleToList $items(p)]
       set nrm [TupleToList $items(t)]
       set xhat [TupleToList $items(tx)]
       geom_orientProfile -src $lsres -path_pos $pos -path_tan $nrm -path_xhat $xhat -dst $lsoriented

       foreach tagobj [list $lsres $lsoriented] {
         repos_setLabel -obj $tagobj -key radius                 -value $radius
         repos_setLabel -obj $tagobj -key center_x               -value $x
         repos_setLabel -obj $tagobj -key center_y               -value $y
         repos_setLabel -obj $tagobj -key center_z               -value $z
         repos_setLabel -obj $tagobj -key kt                     -value $kt
         repos_setLabel -obj $tagobj -key klow                   -value $klow
         repos_setLabel -obj $tagobj -key kupp                   -value $kupp
         repos_setLabel -obj $tagobj -key mag_grad_factor        -value $magGradFactor
         repos_setLabel -obj $tagobj -key grid_type              -value $gridType
         repos_setLabel -obj $tagobj -key max_num_time_steps     -value $maxTS
         repos_setLabel -obj $tagobj -key grid_factor            -value $gridFactor
         repos_setLabel -obj $tagobj -key isotropic_flag         -value $isotropicFlag
         repos_setLabel -obj $tagobj -key goodness_stop_criteria -value $goodnessStop
         repos_setLabel -obj $tagobj -key center_x_in_pixels     -value $lsGUIctrX
         repos_setLabel -obj $tagobj -key center_y_in_pixels     -value $lsGUIctrY
         repos_setLabel -obj $tagobj -key pathId                 -value $pathId
         repos_setLabel -obj $tagobj -key posId                  -value $posId
         repos_setLabel -obj $tagobj -key pos                    -value $pos
         repos_setLabel -obj $tagobj -key nrm                    -value $nrm
         repos_setLabel -obj $tagobj -key xhat                   -value $xhat
         repos_setLabel -obj $tagobj -key creation_method        -value "levelset"
         repos_setLabel -obj $tagobj -key creation_type          -value "levelset"
         repos_setLabel -obj $tagobj -key simulation_runtime     -value $totaltime
   repos_setLabel -obj $tagobj -key stop_velocity          -value $stopV
         repos_setLabel -obj $tagobj -key rebuild_phi_interval   -value $rebuildPhiFreq
         repos_setLabel -obj $tagobj -key area_check_interval    -value $acInterval
         repos_setLabel -obj $tagobj -key calc_decay_const_mtd   -value $calcDecayConstBy
         repos_setLabel -obj $tagobj -key explicit_pot_for_decay -value $potentialValueDecay
         repos_setLabel -obj $tagobj -key calc_pot_const_mtd     -value $calcPotConstBy
         repos_setLabel -obj $tagobj -key pFactor                -value $pFactor
         repos_setLabel -obj $tagobj -key explicit_pot_for_pot   -value $potentialValuePotential
       }

    }
    # update display
    lsGUIchangeFrame 0

  } else {

    global tcl_platform
    if {$tcl_platform(platform) != "windows"} {
       return -code error "ERROR:  Running in Parallel not supported on UNIX."
    }
    set servers [getTclServers]
    if {[lsearch $servers "geodesic_batch_server"] < 0} {
      # check if the user wants to start the batch server
      set choicebs [tk_messageBox -title "No batch server found." -message "Start a new batch server?" -icon question -type yesno]
      switch -- $choicebs {
        yes {
          batchGUIstartBatchServer
          # wait up to 30 seconds for the server to start, then return error
    for {set waitforserver 0} {$waitforserver < 30} {incr waitforserver} {
            after 1000
            set servers [getTclServers]
      if {[lsearch $servers "geodesic_batch_server"] > 0} {
              break
      }
    }
          if {$waitforserver == 30} {
            return -code error "ERROR:  server starting slowly!"
    }
  }
        no {
          return -code error "ERROR:  No Geodesic batch server running!"
  }
      }
    }

    global symbolicName
    global lsGUIcurrentPathNumber
    set pathId $lsGUIcurrentPathNumber
    global gPathPoints
    set path $gPathPoints($pathId,splinePts)

    global lsGUIcurrentPositionNumber
    set posId $lsGUIcurrentPositionNumber

    array set items [lindex $path $posId]
    set pos [TupleToList $items(p)]
    set nrm [TupleToList $items(t)]
    set xhat [TupleToList $items(tx)]

    # set up directories
    global env
    global gFilenames
    file mkdir $gFilenames(tmp_dir)
    set batchdir [file join [pwd] $gFilenames(tmp_dir) batch-$pathId]
    file mkdir $batchdir
    set baseScriptName $env(USER)-[file tail [pwd]]-[pid]-$pathId-$posId
    set fullPathBSN [file join $batchdir $baseScriptName]


    # write out image slices
    repos_writeVtkStructuredPoints -obj $mag -type ascii -file $fullPathBSN-mag.vtk
    repos_writeVtkStructuredPoints -obj $pot -type ascii -file $fullPathBSN-pot.vtk

    # remove any previous seg results
    file delete $fullPathBSN-seg.vtk

    # create a script file to run in parallel
    set fp [open $fullPathBSN.tcl "w"]
    puts $fp "# geodesic_segment_autoscript 1.1"
    puts $fp ""
