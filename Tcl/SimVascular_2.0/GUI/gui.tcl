# Procedure: gui3DchooserUpdateIncrement
proc gui3DchooserUpdateIncrement {} {
   global symbolicName
   global gui3Dvars
   set chooserIncrement $gui3Dvars(chooserIncrement)
   global gImageVol
   global gOptions

   if {$gOptions(orientImgVolToRAS) == 0} {
     # old xyz system
     set org_x [lindex $gImageVol(vtk_org_xyz) 0]
     set org_y [lindex $gImageVol(vtk_org_xyz) 1]
     set org_z [lindex $gImageVol(vtk_org_xyz) 2]
     set ext_i $gImageVol(ext_i)
     set ext_j $gImageVol(ext_j)
     set ext_k $gImageVol(ext_k)
     set vdims_x $gImageVol(vdims_x)
     set vdims_y $gImageVol(vdims_y)
     set vdims_z $gImageVol(vdims_z)
   } else {
     # properly oriented volume in ras space
     set org_x [lindex $gImageVol(vtk_org_ras) 0]
     set org_y [lindex $gImageVol(vtk_org_ras) 1]
     set org_z [lindex $gImageVol(vtk_org_ras) 2]
     set ext_i [lindex $gImageVol(ext_ras) 0]
     set ext_j [lindex $gImageVol(ext_ras) 1]
     set ext_k [lindex $gImageVol(ext_ras) 2]
     set vdims_x [lindex $gImageVol(vdims_ras) 0]
     set vdims_y [lindex $gImageVol(vdims_ras) 1]
     set vdims_z [lindex $gImageVol(vdims_ras) 2]
   }
   set axes_x $gImageVol(axes-x)
   set axes_y $gImageVol(axes-y)
   set axes_z $gImageVol(axes-z)
   set increment $gui3Dvars(chooserIncrement)

   set scalex $symbolicName(gui3DchooserXscale)
   set scaley $symbolicName(gui3DchooserYscale)
   set scalez $symbolicName(gui3DchooserZscale)

   set curx $gui3Dvars(chooserXvar)
   set cury $gui3Dvars(chooserYvar)
   set curz $gui3Dvars(chooserZvar)

   $scalex configure -to [expr int($ext_i/$increment)]
   $scaley configure -to [expr int($ext_j/$increment)]
   $scalez configure -to [expr int($ext_k/$increment)]

   # adding the vdims/4 just to avoid rounding issues
   set gImageVol(axes-x) [expr int(($curx-$org_x+$increment*$vdims_x/4.0)/($increment*$vdims_x))]
   set gImageVol(axes-y) [expr int(($cury-$org_y+$increment*$vdims_y/4.0)/($increment*$vdims_y))]
   set gImageVol(axes-z) [expr int(($curz-$org_z+$increment*$vdims_z/4.0)/($increment*$vdims_z))]
   $scalex set $gImageVol(axes-x)
   $scaley set $gImageVol(axes-y)
   $scalez set $gImageVol(axes-z)
}


# Procedure: gui3DchooserUpdateX
proc gui3DchooserUpdateX { scale} {
  #puts "X: $scale"
  global gImageVol
  set gImageVol(axes-x) $scale
  gui3DchooserUpdateXYZ
}


# Procedure: gui3DchooserUpdateXYZ
proc gui3DchooserUpdateXYZ {} {

  global gImageVol
  global gui3Dvars
  global symbolicName
  global gOptions


  if {$gOptions(orientImgVolToRAS) == 0} {
    if {[llength $gImageVol(vtk_org_xyz)] == 0} {
      return
    }
    # old xyz system
    set org_x [lindex $gImageVol(vtk_org_xyz) 0]
    set org_y [lindex $gImageVol(vtk_org_xyz) 1]
    set org_z [lindex $gImageVol(vtk_org_xyz) 2]
    set ext_i $gImageVol(ext_i)
    set ext_j $gImageVol(ext_j)
    set ext_k $gImageVol(ext_k)
    set vdims_x $gImageVol(vdims_x)
    set vdims_y $gImageVol(vdims_y)
    set vdims_z $gImageVol(vdims_z)
    set voi_x0 $gImageVol(voi_x0)
    set voi_x1 $gImageVol(voi_x1)
    set voi_y0 $gImageVol(voi_y0)
    set voi_y1 $gImageVol(voi_y1)
    set voi_z0 $gImageVol(voi_z0)
    set voi_z1 $gImageVol(voi_z1)
  } else {
    if {[llength $gImageVol(vtk_org_ras)] == 0} {
      return
    }
    # properly oriented volume in ras space
    set org_x [lindex $gImageVol(vtk_org_ras) 0]
    set org_y [lindex $gImageVol(vtk_org_ras) 1]
    set org_z [lindex $gImageVol(vtk_org_ras) 2]
    set ext_i [lindex $gImageVol(ext_ras) 0]
    set ext_j [lindex $gImageVol(ext_ras) 1]
    set ext_k [lindex $gImageVol(ext_ras) 2]
    set vdims_x [lindex $gImageVol(vdims_ras) 0]
    set vdims_y [lindex $gImageVol(vdims_ras) 1]
    set vdims_z [lindex $gImageVol(vdims_ras) 2]
    set voi_x0 [lindex $gImageVol(voi_ras) 0]
    set voi_x1 [lindex $gImageVol(voi_ras) 1]
    set voi_y0 [lindex $gImageVol(voi_ras) 2]
    set voi_y1 [lindex $gImageVol(voi_ras) 3]
    set voi_z0 [lindex $gImageVol(voi_ras) 4]
    set voi_z1 [lindex $gImageVol(voi_ras) 5]
  }

  set axes_x $gImageVol(axes-x)
  set axes_y $gImageVol(axes-y)
  set axes_z $gImageVol(axes-z)

  puts  "$axes_x $axes_y $axes_z"

  set increment $gui3Dvars(chooserIncrement)

  set gui3Dvars(chooserXvar) [format "%.3f" [expr double($org_x+$axes_x*$vdims_x*$increment)]]
  set gui3Dvars(chooserYvar) [format "%.3f" [expr double($org_y+$axes_y*$vdims_y*$increment)]]
  set gui3Dvars(chooserZvar) [format "%.3f" [expr double($org_z+$axes_z*$vdims_z*$increment)]]

  puts "inrc $increment, gui3Dvars: $gui3Dvars(chooserXvar) $gui3Dvars(chooserYvar) $gui3Dvars(chooserZvar)"
  global guiVIB

  set gui3Dvars(pixelValue) [lindex [img_computeStructuredCoord -image $guiVIB(repos_image_obj) -pt [list $gui3Dvars(chooserXvar) $gui3Dvars(chooserYvar) $gui3Dvars(chooserZvar)]] 2]
  if {$gui3Dvars(pixelValue) == ""} {
     set gui3Dvars(pixelValue) "error"
  } else {
     set gui3Dvars(pixelValue) [format "%6.0f" $gui3Dvars(pixelValue)]
  }
  # display distance map value if map exists
  if {[repos_exists -obj $guiVIB(repos_distancemap_obj)]} {
    set distanceval [lindex [img_computeStructuredCoord -image $guiVIB(repos_distancemap_obj) -pt [list $gui3Dvars(chooserXvar) $gui3Dvars(chooserYvar) $gui3Dvars(chooserZvar)]] 2]
    if {$distanceval == ""} {
      set gui3Dvars(pixelValue) "$gui3Dvars(pixelValue),error"
    } else {
  set gui3Dvars(pixelValue) "$gui3Dvars(pixelValue),[format %05i [expr int($distanceval)]]"
    }
  }

  # move slice planes if desired
  global symbolicName
  # Ken's sliders vary from 0 to ext, so we subtract off
  # voi lower offset.
  if {$gui3Dvars(chooserAttachX) == 1} {
    $symbolicName(volGUI_sliderx) set [expr int($axes_x*$increment-$voi_x0)]
  }
  if {$gui3Dvars(chooserAttachY) == 1} {
    $symbolicName(volGUI_slidery) set [expr int($axes_y*$increment-$voi_y0)]
  }
  if {$gui3Dvars(chooserAttachZ) == 1} {
    $symbolicName(volGUI_sliderz) set [expr int($axes_z*$increment-$voi_z0)]
  }

  #global gInteractorRen
  # intentionally use gRen3d since we want all views
  # to sync up
  global gRen3d
  if {$gui3Dvars(chooserShowCursorFlag) != 0} {
    vis_pointWidgetSetPosition $gRen3d gui3D $gui3Dvars(chooserXvar) $gui3Dvars(chooserYvar) $gui3Dvars(chooserZvar)
  }

  global gRen3d
  gui3DupdateRadiusScale 0
  vis_render $gRen3d
}


