# Procedure: guiTETGENestimateMeshSize
proc guiTETGENestimateMeshSize {} {
  global guiTGvars
  global guiPDvars
  global gPolyDataFaceNames
  global gPolyDataFaceNamesInfo
  global smasherInputName
  global gOptions
  global gObjects

  set gOptions(meshing_kernel) TetGen
  set gOptions(meshing_solid_kernel) PolyData

  set solid $gObjects(polydata_solid)
  if {[repos_exists -obj $solid] == 0} {
      set yesno [tk_messageBox -default yes -message "You have not loaded the solid model to use for meshing.  Load it now?" -title "Missing Solid Model" -type yesno]   
      switch -- $yesno {
        yes {
              guiTETGEN_select_solid_model
        }
        no {
        }
      }
  }
  if {[repos_exists -obj $solid] == 0} {
       return -code error "ERROR:  you need to manually load meshing solid first!"
  }
  set smasherInputName $solid
  smasherGUIupdateViewWindow

  set min [expr 2^30]
  set faceIds [$solid GetFaceIds]
  foreach faceId $faceIds {
    set staticFaceId $faceId
    set area [geom_surfArea -src /tmp/smasher/$faceId/$staticFaceId]

    puts "Area: $area"
    if {$area < $min} {
      set min $area
    }
  } 
  set edgesize [expr sqrt(($min)/(3.14159))/(2.5)]
  set edgesize [expr {double(round(10000*$edgesize))/10000}]
  set guiTGvars(meshMaxEdgeSize) $edgesize
}


