# Procedure: guiVISupdateLegend
proc guiVISupdateLegend {} {

  set legend legend2dActor
  set lut gBlueToRedLUTlegend

  global guiVISvars
  global guiVISvarsPrev

  set ren $guiVISvars(ren)

  # want to use legend for guiCS, so don't require mesh to be loaded
  #if {$guiVISvars(mesh_loaded) == 0} {
  #  return GDSC_OK
  #}

  if {$guiVISvars(legend_showLegend) == 0 &&  $guiVISvarsPrev(legend_showLegend) == 0} {
     puts "legend: off off nothing to do."
     return
  }
  if {$guiVISvarsPrev(legend_showLegend) == 1 &&  $guiVISvars(legend_showLegend) == 0} {
      $ren RemoveActor2D $legend
      set guiVISvarsPrev(legend_showLegend) $guiVISvars(legend_showLegend)
      vis_render $ren
      return
  }

  if {$guiVISvarsPrev(legend_showLegend) == 0 &&  $guiVISvars(legend_showLegend) == 1} {

    catch {$lut Delete}
    vtkLookupTable $lut
    $lut SetHueRange 0.6667 0.0
    $lut SetSaturationRange 1.0 1.0
    $lut SetValueRange 1.0 1.0
    $lut SetAlphaRange 1.0 1.0
    $lut SetNumberOfColors $guiVISvars(legend_numColors)
    $lut Build
    $lut SetTableRange $guiVISvars(legend_minValue) $guiVISvars(legend_maxValue)
    catch {$legend Delete}
    vtkScalarBarActor $legend
    $legend SetLookupTable $lut
    [$legend GetPositionCoordinate] SetCoordinateSystemToNormalizedViewport
    $legend SetTitle $guiVISvars(legend_title)
    $legend SetNumberOfLabels $guiVISvars(legend_numLabels)
    if {$guiVISvars(legend_orientation) == "horizontal"} {
       [$legend GetPositionCoordinate] SetValue 0.1 0.01
       $legend SetOrientationToHorizontal
       $legend SetWidth 0.8
       $legend SetHeight 0.17
    } elseif {$guiVISvars(legend_orientation) == "vertical"} {
       [$legend GetPositionCoordinate] SetValue 0.8 0.1
       $legend SetOrientationToVertical
       $legend SetWidth 0.17
       $legend SetHeight 0.8
    } else {
      return -code error "ERROR:  illegal orientation"
    }
    $legend SetLabelFormat $guiVISvars(legend_labelFormat)
    $ren AddActor2D $legend
    vis_render $ren
    set guiVISvarsPrev(legend_title) $guiVISvars(legend_title)
    set guiVISvarsPrev(legend_numLabels) $guiVISvars(legend_numLabels)
    set guiVISvarsPrev(legend_labelFormat) $guiVISvars(legend_labelFormat)
    set guiVISvarsPrev(legend_minValue) $guiVISvars(legend_minValue)
    set guiVISvarsPrev(legend_maxValue) $guiVISvars(legend_maxValue)
    set guiVISvarsPrev(legend_numColors) $guiVISvars(legend_numColors)
    set guiVISvarsPrev(legend_showLegend) 1
    return
  }

  set updateDisplay 0
  if {$guiVISvarsPrev(legend_numColors) != $guiVISvars(legend_numColors)} {
      $lut SetNumberOfColors $guiVISvars(legend_numColors)
      set guiVISvarsPrev(legend_numColors) $guiVISvars(legend_numColors)
      set updateDisplay 1
  }
  if {$guiVISvarsPrev(legend_title) != $guiVISvars(legend_title)} {
      $legend SetTitle $guiVISvars(legend_title)
      set guiVISvarsPrev(legend_title) $guiVISvars(legend_title)
      set updateDisplay 1
  }
  if {$guiVISvarsPrev(legend_numLabels) != $guiVISvars(legend_numLabels)} {
      $legend SetNumberOfLabels $guiVISvars(legend_numLabels)
      set guiVISvarsPrev(legend_numLabels) $guiVISvars(legend_numLabels)
      set updateDisplay 1
  }
  if {$guiVISvarsPrev(legend_labelFormat) != $guiVISvars(legend_labelFormat)} {
      $legend SetLabelFormat $guiVISvars(legend_labelFormat)
      set guiVISvarsPrev(legend_labelFormat) $guiVISvars(legend_labelFormat)
      set updateDisplay 1
  }
  if {($guiVISvarsPrev(legend_minValue) != $guiVISvars(legend_minValue)) ||  ($guiVISvarsPrev(legend_maxValue) != $guiVISvars(legend_maxValue))} {
      $lut SetTableRange $guiVISvars(legend_minValue) $guiVISvars(legend_maxValue)
      set guiVISvarsPrev(legend_minValue) $guiVISvars(legend_minValue)
      set guiVISvarsPrev(legend_maxValue) $guiVISvars(legend_maxValue)
      set updateDisplay 1
  }

  if {$guiVISvarsPrev(legend_orientation) != $guiVISvars(legend_orientation)} {
    if {$guiVISvars(legend_orientation) == "horizontal"} {
       [$legend GetPositionCoordinate] SetValue 0.1 0.01
       $legend SetOrientationToHorizontal
       $legend SetWidth 0.8
       $legend SetHeight 0.17
    } elseif {$guiVISvars(legend_orientation) == "vertical"} {
       [$legend GetPositionCoordinate] SetValue 0.8 0.1
       $legend SetOrientationToVertical
       $legend SetWidth 0.17
       $legend SetHeight 0.8
    } else {
      return -code error "ERROR:  illegal orientation"
    }
    set guiVISvarsPrev(legend_orientation) $guiVISvars(legend_orientation)
    set updateDisplay 1
  }

  if {$updateDisplay == 1} {
     vis_render $ren
  }
}


