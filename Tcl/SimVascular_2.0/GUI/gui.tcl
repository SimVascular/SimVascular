}


# Procedure: guiVISppInteract
proc guiVISppInteract {} {
  global guiVISvars
  global gInteractorRen
  set guiVISvars(p1) [vis_planeWidgetGetOrigin $gInteractorRen guiVIS]
  set guiVISvars(p2) [vis_planeWidgetGetPoint1 $gInteractorRen guiVIS]
  set guiVISvars(p3) [vis_planeWidgetGetPoint2 $gInteractorRen guiVIS]
