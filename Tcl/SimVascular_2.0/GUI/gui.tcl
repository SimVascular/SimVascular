  global bcGUIbaselineRegions

  set index 0
  foreach i $bcGUIbaselineRegions {
      set obj [lindex $i 0]
      set uses [lindex $i 1]
      set objid [file tail $obj]
      if {$objid == $id} {

       # here we find the current settings and toggle
       # the desired flag
       set x 0
       set y 0
       set z 0
       foreach j $uses {
          set $j 1
       }
       set $direction [eval expr !\$$direction]
       set newuses {}
       if {$x == 1} {
         lappend newuses x
       }
       if {$y == 1} {
         lappend newuses y
       }
       if {$z == 1} {
         lappend newuses z
       }

       set newstr [list "region_$id" $newuses]
       set region [list /bcGUI/region/$id $newuses]

       set bcGUIbaselineRegions [lreplace $bcGUIbaselineRegions  $index $index $region]

       $lbox selection clear $index
       $lbox delete $index

       $lbox insert $index $newstr
       puts "$lbox insert $index $newstr"
       puts "[$lbox size]"
       $lbox selection set $index

       puts "newuses: $newuses  x: $x y: $y z: $z"
       return
      }
      incr index
  }
  puts "variable: $bcGUIbaselineRegions"
  puts "listbox: [$lbox size]"
  return -code error "Could not find region."
}


# Procedure: bcGUIchangeFrame
proc bcGUIchangeFrame { value} {

  # note:  value is ignored

  global bcGUImagWindow
  global bcGUIpotWindow

  global bcGUIcurrentFrameNumber
  set value $bcGUIcurrentFrameNumber

  foreach loop {bcGUImagWindow bcGUIpotWindow} {

    # create some local variables containing the options
    # contained in the loop array
    foreach i [array names $loop] {
      set $i [subst $[subst $loop]($i)]
    }

    if {$showWindow == "0"} {
       continue
    }

    # this is sloppy, but easy
    catch {crd_ren $ren}
    catch {vis_warpRm $ren}
    catch {vis_imgRm $ren}
    set displayedImgObj {}
    if {$loop == "bcGUImagWindow"} {
      if {$bcGUImagWindow(showMagImg) == 1} {
        gdscGeneralView $ren /gBC/pcdata/mag/$value
        set displayedImgObj /gBC/pcdata/mag/$value
      } elseif {$bcGUImagWindow(showVelImg) != 0} {
        global gBC
  if {$gBC(numVelComp) == 1} {
          gdscGeneralView $ren /gBC/pcdata/phase/$value
          set displayedImgObj /gBC/pcdata/phase/$value
  } elseif {$gBC(numVelComp) == 3} {
          gdscGeneralView $ren /gBC/pcdata/phase/$value/$bcGUImagWindow(showVelImg)
          set displayedImgObj /gBC/pcdata/phase/$value/$bcGUImagWindow(showVelImg)
  }
      }
    } else {
      gdscGeneralView $ren /gBC/pcdata/pot/$value
      set displayedImgObj /gBC/pcdata/pot/$value
    }

    # allow the user to hard code the displayed image scalar range
    if {$displayedImgObj != ""} {
      set usegray {}
      set grayrange [[repos_exportToVtk -src $displayedImgObj] GetScalarRange]
      set lookupGrayscale vis_img_g8bitGrayscaleLUT_$ren
      if {[string trim $img_min_val] != ""} {
         set usegray $img_min_val
      } else {
         set usegray [lindex $grayrange 0]
      }
      if {[string trim $img_max_val] != ""} {
         lappend usegray $img_max_val
      } else {
         lappend usegray [lindex $grayrange 1]
      }
      #puts "grays: $usegray"
      $lookupGrayscale SetTableRange [lindex $usegray 0] [lindex $usegray 1]
      [$ren GetRenderWindow] Render
    }

    # display threshold
    if {$showThr == "1"} {
      if {[repos_exists -obj /gBC/pcdata/mag/$value/thr] == 1} {
        gdscGeneralView $ren /gBC/pcdata/mag/$value/thr
      }
    }

    # display selected threshold
    if {$showSelectedThr == "1"} {
      if {[repos_exists -obj /gBC/pcdata/mag/$value/thr/selected] == 1} {
        set throbj /gBC/pcdata/mag/$value/thr/selected
        gdscGeneralView $ren $throbj
        # make the line more visible
        set actor [vis_pGetActor $ren $throbj]
        [$actor GetProperty] SetLineWidth 5
        [$ren GetRenderWindow] Render
      }
    }

    # display center sphere
    if {$showInitSphere == "1"} {
      set sphere /tmp/bcGUI/sphere
      catch {repos_delete -obj $sphere}
      global bcGUIradius
      global bcGUIctrX
      global bcGUIctrY
      global gBC
      # calculate center from voxel dims
      set imageInfo $gBC(phase_contrast_params)
      foreach i $imageInfo {
        set [lindex $i 0] [lindex $i 1]
      }
      set center [list [expr double([lindex $voxel_dims 0])*double($bcGUIctrX)]  [expr double([lindex $voxel_dims 1])*double($bcGUIctrY)] 0]
      solid_sphere -r $bcGUIradius -ctr $center -result $sphere
      repos_setLabel -obj $sphere -key color -value green
      repos_setLabel -obj $sphere -key opacity -value 0.33
      gdscGeneralView $ren $sphere
    }

    # set up warp displays
    if {$showWarp == 1 || $showMeshWarp == 1} {
      vis_warpSetup $ren
    }

    # display warp
    if {$showWarp == 1} {
      set warpVel /gBC/pcdata/warpVel/$value
      if {[repos_exists -obj $warpVel] == 1} {
        set warpactor [vis_warpShowRepos $ren $warpVel]
        vis_warpScaleFactor $ren $warpFactor
        vis_warpSetOpacity $ren 1.0
        #vis_warpSetColor $ren 0 1 0
        vis_warpColorize $ren
        set warprangeorg [[$warpactor GetMapper] GetScalarRange]
  if {$warp_min_val == ""} {
          set warprange [lindex $warprangeorg 0]
  } else {
          set warprange $warp_min_val
  }
        if {$warp_max_val == ""} {
          lappend warprange [lindex $warprangeorg 1]
  } else {
          lappend warprange $warp_max_val
  }
  [$warpactor GetMapper] SetScalarRange [lindex $warprange 0] [lindex $warprange 1]
        [$ren GetRenderWindow] Render
      }
    }

    # display warp
    if {$showRawVelWarp == 1} {
      global gBC
      if {$gBC(numVelComp) == 1} {
          set warpVel /gBC/pcdata/phase/$value
      } elseif {$gBC(numVelComp) == 3} {
          set warpVel /gBC/pcdata/phase/$value/$bcGUImagWindow(showVelImg)
      }
      if {[repos_exists -obj $warpVel] == 1} {
        set warpactor [vis_warpShowRepos $ren $warpVel]
        vis_warpScaleFactor $ren $warpFactor
        vis_warpSetOpacity $ren 1.0
        vis_warpColorize $ren
        set warprangeorg [[$warpactor GetMapper] GetScalarRange]
  if {$warp_min_val == ""} {
          set warprange [lindex $warprangeorg 0]
  } else {
          set warprange $warp_min_val
  }
        if {$warp_max_val == ""} {
          lappend warprange [lindex $warprangeorg 1]
  } else {
          lappend warprange $warp_max_val
  }
  [$warpactor GetMapper] SetScalarRange [lindex $warprange 0] [lindex $warprange 1]
        [$ren GetRenderWindow] Render
      }
    }

    # display mesh warp
    if {$showMeshWarp == 1} {
      set inletMeshFace /gBC/pcdata/inletMeshFace/$value
      if {[repos_exists -obj $inletMeshFace] == 1} {
        set warpactor [vis_warpShowRepos $ren $inletMeshFace]
        vis_warpScaleFactor $ren $warpFactor
        vis_warpSetOpacity $ren 1.0
        #vis_warpSetColor $ren 0 1 0
        vis_warpColorize $ren
        vis_warpTranslate $ren $inletMeshFace $meshOffset
        set warprangeorg [[$warpactor GetMapper] GetScalarRange]
  if {$warp_min_val == ""} {
          set warprange [lindex $warprangeorg 0]
  } else {
          set warprange $warp_min_val
  }
        if {$warp_max_val == ""} {
          lappend warprange [lindex $warprangeorg 1]
  } else {
          lappend warprange $warp_max_val
  }
  [$warpactor GetMapper] SetScalarRange [lindex $warprange 0] [lindex $warprange 1]
        [$ren GetRenderWindow] Render
      }
    }

    # display level set result
    if {$showLS == "1"} {
      set lsres /gBC/pcdata/mag/$value/ls
      if {[repos_exists -obj $lsres] == 1} {
        gdscGeneralView $ren $lsres
        # make the line more visible
        set actor [vis_pGetActor $ren $lsres]
        [$actor GetProperty] SetLineWidth 5
        [$ren GetRenderWindow] Render
      }
    }

    # display levelset evolution
    global gOptions
    set lset_rens $gOptions(lsetDisplayWindows)
    set loc [lsearch $lset_rens $renderWindow]
    if {$showIntermediateLS ==  1} {
       if {$loc < 0} {
         lappend gOptions(lsetDisplayWindows) $renderWindow
       }
    } else {
   if {$loc >= 0} {
           set gOptions(lsetDisplayWindows) [lreplace $lset_rens $loc $loc]
   }
     }

     # display tagged pixels for velocity calculation
     if {$showTaggedPixels == 1} {
        global gTaggedPixels
  if {[info exists gTaggedPixels] == 1} {
          global gBC
          foreach param $gBC(phase_contrast_params) {
            set [lindex $param 0] [lindex $param 1]
          }
          # vis the tagged pixel
          foreach pixel [lsort -dictionary [array names gTaggedPixels]] {
            set magslice /gBC/pcdata/mag/$value
            set pt [[repos_exportToVtk -src $magslice] GetPoint $pixel]
            set box /tmp/bcGUI/taggedPixels/pixel/$pixel
            set boxpd /tmp/bcGUI/taggedPixels/pixel/$pixel/pd
            catch {repos_delete -obj $box}
            catch {repos_delete -obj $boxpd}
            solid_box3d -result $box -ctr [[repos_exportToVtk -src $magslice] GetPoint $pixel] -dims [list [expr [lindex $voxel_dims 0]]  [expr [lindex $voxel_dims 1]] [expr [lindex $voxel_dims 0]]]
            $box GetPolyData -result $boxpd
            repos_setLabel -obj $boxpd -key opacity -value 0.25
            repos_setLabel -obj $boxpd -key color -value yellow
            gdscGeneralView $ren $boxpd
    }
        }
      }

  }

  # update gRen3d display
  bcGUIupdate3dWindow

  # display baseline regions
  global bcGUIbaselineAddPolys
  if {$bcGUIbaselineAddPolys != 0} {
     bcGUIbaselineRedisplayAllRegions
  }
}


# Procedure: bcGUIclearAllSegs
proc bcGUIclearAllSegs {} {

  # find min/max frame number
  global symbolicName
  global gBC
  set minFrame 1
  set maxFrame $gBC(numSlices)

  for {set i $minFrame} {$i <= $maxFrame} {incr i} {
    catch {repos_delete -obj /gBC/pcdata/mag/$i/ls}
    catch {repos_delete -obj /gBC/pcdata/mag/$i/thr}
    catch {repos_delete -obj /gBC/pcdata/mag/$i/thr/selected}
