# Procedure: guiPPchooserUpdateIncrement
proc guiPPchooserUpdateIncrement {} {
   global symbolicName
   global guiPPchooserIncrement
   global gImageVol
   global guiPPchooserXvar
   global guiPPchooserYvar
   global guiPPchooserZvar
   global gOptions
   if {$gOptions(orientImgVolToRAS) == 0} {
     # old xyz system
     set org_x [lindex $gImageVol(vtk_org_xyz) 0]
     set org_y [lindex $gImageVol(vtk_org_xyz) 1]
     set org_z [lindex $gImageVol(vtk_org_xyz) 2]
     set ext_i $gImageVol(ext_i)
     set ext_j $gImageVol(ext_j)
     set ext_k $gImageVol(ext_k)
     set vdims_x $gImageVol(vdims_x)
     set vdims_y $gImageVol(vdims_y)
     set vdims_z $gImageVol(vdims_z)
   } else {
     # properly oriented volume in ras space
     set org_x [lindex $gImageVol(vtk_org_ras) 0]
     set org_y [lindex $gImageVol(vtk_org_ras) 1]
     set org_z [lindex $gImageVol(vtk_org_ras) 2]
     set ext_i [lindex $gImageVol(ext_ras) 0]
     set ext_j [lindex $gImageVol(ext_ras) 1]
     set ext_k [lindex $gImageVol(ext_ras) 2]
     set vdims_x [lindex $gImageVol(vdims_ras) 0]
     set vdims_y [lindex $gImageVol(vdims_ras) 1]
     set vdims_z [lindex $gImageVol(vdims_ras) 2]
   }
   set axes_x $gImageVol(axes-x)
   set axes_y $gImageVol(axes-y)
   set axes_z $gImageVol(axes-z)
   set increment $guiPPchooserIncrement

   set scalex $symbolicName(guiPPchooserXscale)
   set scaley $symbolicName(guiPPchooserYscale)
   set scalez $symbolicName(guiPPchooserZscale)

   set curx $guiPPchooserXvar
   set cury $guiPPchooserYvar
   set curz $guiPPchooserZvar

   $scalex configure -to [expr int($ext_i/$increment)]
   $scaley configure -to [expr int($ext_j/$increment)]
   $scalez configure -to [expr int($ext_k/$increment)]

   # adding the vdims/4 just to avoid rounding issues
   set gImageVol(axes-x) [expr int(($curx-$org_x+$increment*$vdims_x/4.0)/($increment*$vdims_x))]
   set gImageVol(axes-y) [expr int(($cury-$org_y+$increment*$vdims_y/4.0)/($increment*$vdims_y))]
   set gImageVol(axes-z) [expr int(($curz-$org_z+$increment*$vdims_z/4.0)/($increment*$vdims_z))]
   $scalex set $gImageVol(axes-x)
   $scaley set $gImageVol(axes-y)
   $scalez set $gImageVol(axes-z)

   # currently can only do guess good point on whole pixels
   global symbolicName
   if {$increment == "1.0" || $increment == "1"} {
     catch {$symbolicName(guiPPchooserGuessGoodPointButton2) configure -state normal}
   } else {
     catch {$symbolicName(guiPPchooserGuessGoodPointButton2) configure -state disabled}
   }
}


# Procedure: guiPPchooserUpdateLBforPathID
proc guiPPchooserUpdateLBforPathID {} {
  global symbolicName
  set box $symbolicName(guiPPchooserPointListBox)
  set id [$symbolicName(guiPPchooserPathID) get]
  set nameEntry $symbolicName(guiPPchooserPathName)
  set interpEntry $symbolicName(guiPPchooserNumIntPoints)

  set name [$nameEntry get]

  global gPathPoints
  # update listbox
  $box delete 0 end
  $nameEntry delete 0 end
  $interpEntry delete 0 end
  set pointIds {}
  foreach i [array names gPathPoints] {
    set ab [split $i ,]
    set a [lindex $ab 0]
    set b [lindex $ab 1]
    #puts "a $a b $b"
    if {$a == $id} {
  if {$b == "name"} {
          $nameEntry insert end $gPathPoints($i)
        } elseif {$b == "numSplinePts"} {
          $interpEntry insert end $gPathPoints($i)
  } elseif {$b == "splinePts"} {
          # skip splined path
        } else {
          lappend pointIds $b
  }
    }
  }
  foreach pt [lsort -integer $pointIds] {
     $box insert end $gPathPoints($id,$pt)
  }
  guiPPchooserShowPath
  guiPPchooserShowSpline
}


