# Procedure: gui3Dfast
proc gui3Dfast { operation} {

  global symbolicName
  global gui3Dvars
 
  if {$operation == "sigmod"} {
    set min   $gui3Dvars(fast_sigmod_min_value)
    set max   $gui3Dvars(fast_sigmod_max_value)
    set alpha $gui3Dvars(fast_sigmod_alpha)
    set beta  $gui3Dvars(fast_sigmod_beta)
    set img   $gui3Dvars(ls_volume)

    set fimg /gui3D/fast/volume_float

    set caster tmp-caster
    catch {$caster Delete}

    vtkImageCast $caster
    $caster SetInputDataObject [repos_exportToVtk -src $img]
    $caster SetOutputScalarTypeToFloat
    $caster Update
    catch {repos_delete -obj $fimg}
    repos_importVtkImg -src [$caster GetOutput] -dst $fimg
    $caster Delete

    set gui3Dvars(fast_sigmod_map_obj) [itk_sigMap -image $fimg -min $min -max $max -alpha $alpha -beta $beta]
  } elseif {$operation == "fmm_segment"} {
    # create initialization PD
    set seedPts {}
    set tbox $symbolicName(gui3DseedsTB)
    set numlines [lindex [split [$tbox index end] .] 0]
    set all_spheres {}
    for {set i 1} {$i < $numlines} {incr i} {
      set line [$tbox get $i.0 $i.end]
      set line [string trim $line]
      if {[string length $line] == 0} {
        continue
      }
      if {[string index $line 0] == "\#"} {
        continue
      }
      if {[lindex $line 0] == "sphere"} {
  lappend seedPts [lrange $line 2 4]
      } else {
        puts "WARNING: line ignored ($line)"
      }
    }

    if {[llength $seedPts] == 0} {
       return -code error "ERROR: no seeds!"
    }
    puts "seeds: $seedPts"
    set img   $gui3Dvars(fast_sigmod_map_obj)
    set starttime $gui3Dvars(fast_fmm_start_time)
    set endtime   $gui3Dvars(fast_fmm_stop_time)
    set gui3Dvars(fast_ls_obj) "/tmp/gui3D/fast/fmm"
    set lsimg $gui3Dvars(fast_ls_obj)
    set gui3Dvars(fast_fmm_obj) [itk_fastMarch -image $img -output $img -seedPts $seedPts -start $starttime -stop $endtime]
    set gui3Dvars(fast_ls_obj) $gui3Dvars(fast_fmm_obj)
  } elseif {$operation == "ls_segment"} {
    set img $gui3Dvars(fast_ls_obj)
    set l $gui3Dvars(fast_ls_lower_threshold)
    set u $gui3Dvars(fast_ls_upper_threshold)
    set p $gui3Dvars(fast_ls_propagation_scaling)
    set c $gui3Dvars(fast_ls_curvature_scaling)
    set n $gui3Dvars(fast_ls_num_iterations)
    set gui3Dvars(fast_ls_obj) [itk_TLSF -image $gui3Dvars(fast_sigmod_map_obj) -levelset $img -upperThreshold $u -lowerThreshold $l -propagationScaling $p -curvatureScaling $c -numberOfIterations $n]
  } else {
    return -error "ERROR: invalid operation ($operation)"
  }
}


