proc guiPHASTAcreateNumstartFile {} {
  global gFilenames
  puts "Creating numstart.dat file."

  set initialdirectory [file dirname $gFilenames(numstart_file)]
  set initialfilename [file tail $gFilenames(numstart_file)]
  set fn [tk_getSaveFile -filetypes { {"Numstart File" .dat}} -defaultextension .dat -title "Save Numstart" -initialfile $initialfilename -initialdir $initialdirectory ]

  if {$fn == ""} { 
     puts "User Cancelled."
     return
  }

  set fp [open $fn w]
  fconfigure $fp -translation lf
  puts $fp "0"
  close $fp
}


