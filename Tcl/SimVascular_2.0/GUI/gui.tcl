# Procedure: lsGUIfourierSmoothSeg
proc lsGUIfourierSmoothSeg { type {batch "0"}} {

  global symbolicName
  global lsGUIcurrentPathNumber
  set pathId $lsGUIcurrentPathNumber
  global gPathPoints
  set path $gPathPoints($pathId,splinePts)

  global lsGUIcurrentPositionNumber
  set posId $lsGUIcurrentPositionNumber

  # need to translate hand/analytic to proper type
  if {$type == "analytic"} {
    global lsGUIanalContourType
    set type $lsGUIanalContourType
  } elseif {$type == "hand"} {
    global lsGUIhandContourType
    set type $lsGUIhandContourType
  }

  if {$type == "levelset"} {
    set seg /lsGUI/$pathId/$posId/ls
    set segoriented $seg/oriented
  } elseif {$type == "itklevelset"} {
    set seg /lsGUI/$pathId/$posId/ls
    set segoriented $seg/oriented
  } elseif {$type == "threshold"} {
    set baseName /lsGUI/$pathId/$posId
    set seg $baseName/thr/selected
    set segoriented $baseName/thr/oriented
  } else {
    return -code error "Invalid segmentation type ($type)."
  }

  if {[repos_exists -obj $seg] == 0} {
    return
  }

  # keep the tags on the segmentation
  foreach key [repos_getLabelKeys -obj $seg] {
    set seg_keys($key) [repos_getLabel -obj $seg -key $key]
  }
  foreach key [repos_getLabelKeys -obj $segoriented] {
    set segoriented_keys($key) [repos_getLabel -obj $segoriented -key $key]
  }

  set segpts ""
  catch {set segpts [geom_getOrderedPts -obj $seg]}


  if {$segpts == ""} {
    if {$batch > 0 } {
    puts "Could not smooth $seg, could not order points."
    return 1
    } else {
      tk_messageBox -title "Could not smooth contour!"  -type ok -message "We could not smooth this contour. Ensure that it is closed and try again."
    }
    return 1
  }

  catch {repos_delete -obj $seg}
  catch {repos_delete -obj $segoriented}

if {$type == "itklevelset"} {
    global itklsGUIfourierNumModes
    set numModes $itklsGUIfourierNumModes
  } else {
    global lsGUIfourierNumModes
    set numModes $lsGUIfourierNumModes
  }

  if {[expr [llength $segpts] * 2] < $numModes} {
    set numInterpPts [expr 3*$numModes]
  } else {
    set numInterpPts [llength $segpts]
  }

  set newpts [math_smoothCurve -pts $segpts -closed 1 -numModes $numModes -numInterpPts $numInterpPts]

  geom_mkLinesFromPts $newpts $seg 1

  array set items [lindex $path $posId]
  set pos [TupleToList $items(p)]
  set nrm [TupleToList $items(t)]
  set xhat [TupleToList $items(tx)]
  geom_orientProfile -src $seg -path_pos $pos -path_tan $nrm -path_xhat $xhat -dst $segoriented

  # reattach tags
  foreach key [array names seg_keys] {
    repos_setLabel -obj $seg -key $key -value $seg_keys($key)
  }
  foreach key [array names segoriented_keys] {
    repos_setLabel -obj $segoriented -key $key -value $segoriented_keys($key)
  }
  # update seg
  if {[info exists seg_keys(num_smooth_modes)] == 1} {
    repos_clearLabel -obj $seg -key num_smooth_modes
  }
  repos_setLabel -obj $seg -key num_smooth_modes -value $numModes

  # update segoriented
  if {[info exists segoriented_keys(num_smooth_modes)] == 1} {
    repos_clearLabel -obj $segoriented -key num_smooth_modes
  }
  repos_setLabel -obj $segoriented -key num_smooth_modes -value $numModes

  # update display
  lsGUIchangeFrame 0
  return 0
}


# Procedure: lsGUIguessParams
proc lsGUIguessParams {} {
  # this code is a copy of the code found in lsGUIlssSlice
  # related to guessing parameters.  A change in one location
  # should be mirrored in other!

  global lsGUIcurrentPositionNumber
  set posId $lsGUIcurrentPositionNumber
  global lsGUIcurrentPathNumber
  set pathId $lsGUIcurrentPathNumber

  global lsGUIthrValue
  if {$lsGUIthrValue == 0} {
      return -code error "ERROR:  To guess parameters you must set a non-zero threshold value!"
  }

  set mag   /tmp/lsGUI/mag
  set pot   /tmp/lsGUI/pot

  set lsres /lsGUI/$pathId/$posId/ls
  set lsres_unclean /lsGUI/$pathId/$posId/ls/unclean
  set lsoriented /lsGUI/$pathId/$posId/ls/oriented

  global lsGUIctrX
  global lsGUIctrY
  global lsGUIradius
  global lsGUIlsParams
  global lsGUInumSamplePoints
  set numSamplePoints $lsGUInumSamplePoints

  # need header to convert voxel number into coord
  global gImageFoo
  set voxel_dims $gImageFoo(voxelDims)

  set radius  $lsGUIradius
  set x [expr double([lindex $voxel_dims 0])*double($lsGUIctrX)]
  set y [expr double([lindex $voxel_dims 1])*double($lsGUIctrY)]
  set z 0
  set kt $lsGUIlsParams(Kthr)
  set klow $lsGUIlsParams(Klow)
  set kupp $lsGUIlsParams(Kupp)
  set magGradFactor $lsGUIlsParams(MagFactor)
  set gridType SparseGrid
  set maxTS $lsGUIlsParams(MaxSteps)
  set gridFactor $lsGUIlsParams(GridFactor)
  set isotropicFlag 1
  set goodnessStop $lsGUIlsParams(GoodnessStop)

    puts "Guessing parameters for position $posId."
    puts "  Reseting ctrX and ctrY to zero!"
    set lsGUIctrX 0
    set lsGUIctrY 0
    # threshold slice with current threshold setting
    lsGUIthrSlice
    set baseName /lsGUI/$pathId/$posId
    set picked $baseName/thr/selected
    if {[repos_exists -obj $picked] == 0} {
        puts "  No selected contour found.  Skipping $posId."
        return
    }
    # need the center of the selected contour and the min radius
    # from it to the nearest point on the threshold section
    set center [geom_avgPt -obj $picked]
    # can't use closestPt since there are points in picked
    # not on the boundary!
    #set closestpt [geom_closestPt $picked $center]
    #set minr [math_distance $center $closestpt]
    set minr 1000000
    set closestPt {}
    foreach i [geom_getOrderedPts -obj $picked] {
      set tmpdist [math_distance $i $center]
      if  {$tmpdist < $minr} {
        set closestPt $i
  set minr $tmpdist
      }
    }
    if {$closestPt == ""} {
      return -code error "ERROR:  Could not find minimum radius."
    }

    puts "  center: $center closestPt: $closestPt minr: $minr"

    # reset the parameters if it makes sense.  All sorts of stuff
    # hard coded below.  As Ken would say, the weak of heart should
    # avert their eyes...

    if {$minr > 4.0} {
       set gridFactor 1.0
       set maxTS 500
    } elseif {$minr > 2.0} {
       set gridFactor 0.5
       set maxTS 1000
    } elseif {$minr > 0.5} {
       set gridFactor 0.25
       set maxTS 2000
    } else {
       puts "  Minimum radius to small.  Skipping $posId."
       return
    }

    set lsGUIlsParams(gridFactor) $gridFactor
    set radius [expr 0.8*$minr]
    set lsGUIradius $radius
    set x [lindex $center 0]
    set y [lindex $center 1]
    set lsGUIctrX [expr double($x)/double([lindex $voxel_dims 0])]
    set lsGUIctrY [expr double($y)/double([lindex $voxel_dims 1])]
    set kt [expr 1.0/(0.9*$radius)]
    set klow 0.0
    set kupp [expr 1.2*$kt]
    set lsGUIlsParams(Klow) $klow
    set lsGUIlsParams(Kthr) $kt
    set lsGUIlsParams(Kupp) $kupp
    set lsGUIlsParams(MaxSteps) $maxTS
    set lsGUIlsParams(GridFactor) $gridFactor

    #puts "  minr: $minr  radius: $radius  gridFactor: $gridFactor  x: $x  y: $y"
    #puts "  lsGUIctrX: $lsGUIctrX  lsGUIctrY:  $lsGUIctrY"
    #puts "  kthr: $kt  klow: $klow  kupp: $kupp"

  # update display
  lsGUIchangeFrame 0

  return 0
}


# Procedure: lsGUIinterpCircle
proc lsGUIinterpCircle { type} {
  global lsGUIcurrentPositionNumber
  set posId $lsGUIcurrentPositionNumber
  global lsGUIcurrentPathNumber
  set pathId $lsGUIcurrentPathNumber
  global gPathPoints
  set path $gPathPoints($pathId,splinePts)

  set tmpclean /lsGUI/interpCircle/clean
  set tmppd /lsGUI/interpCircle/clean/pd
  set tmpcir /lsGUI/interpCircle/cir

  catch {repos_delete -obj $tmpclean}
  catch {repos_delete -obj $tmppd}
  catch {repos_delete -obj $tmpcir}

  if {$type == "levelset"} {
    set seg /lsGUI/$pathId/$posId/ls
    set segoriented $seg/oriented
  } elseif {$type == "threshold"} {
    set baseName /lsGUI/$pathId/$posId
    set seg $baseName/thr/selected
    set segoriented $baseName/thr/oriented
  } else {
    return -code error "Invalid segmentation type ($type)."
  }

  if {[repos_exists -obj $seg] == 0} {
    puts "WARNING:  No segmentation found!"
    return -code error "ERROR:  No oriented segmentation found!"
  }

  # clean up point list
  geom_mergePts -src $seg -dst $tmpclean

  # check that points define single region
  if {[geom_numClosedLineRegions -obj $tmpclean] != 1} {
       return -code error "Need just 1 closed region in $tmpclean"
  }

  set points [geom_getOrderedPts -obj $tmpclean]
  geom_polygonFromPts $points $tmppd
  set area [geom_surfArea -src $tmppd]

  set r [expr sqrt(double($area)/[math_pi])]
  set center [geom_avgPt -obj $seg]

  # keep the tags on the segmentation
  foreach key [repos_getLabelKeys -obj $seg] {
    set seg_keys($key) [repos_getLabel -obj $seg -key $key]
  }
  foreach key [repos_getLabelKeys -obj $segoriented] {
    set segoriented_keys($key) [repos_getLabel -obj $segoriented -key $key]
  }

  catch {repos_delete -obj $seg}
  catch {repos_delete -obj $segoriented}

  # create the unoriented circle
  geom_circle $r 0 0 $tmpcir
  geom_translate -src $tmpcir -vec $center -dst $seg

  # orient profile
  array set items [lindex $path $posId]
  set pos [TupleToList $items(p)]
  set nrm [TupleToList $items(t)]
  set xhat [TupleToList $items(tx)]
  geom_orientProfile -src $seg -path_pos $pos -path_tan $nrm -path_xhat $xhat -dst $segoriented

  # reattach tags
  foreach key [array names seg_keys] {
    repos_setLabel -obj $seg -key $key -value $seg_keys($key)
  }
  foreach key [array names segoriented_keys] {
    repos_setLabel -obj $segoriented -key $key -value $segoriented_keys($key)
  }
  if {[info exists seg_keys(replaced_with_circle)] == 0} {
    repos_setLabel -obj $seg         -key replaced_with_circle -value 1
  }
  if {[info exists segoriented_keys(replaced_with_circle)] == 0} {
    repos_setLabel -obj $segoriented -key replaced_with_circle -value 1
  }

  # clean up
  catch {repos_delete -obj $tmpclean}
  catch {repos_delete -obj $tmppd}
  catch {repos_delete -obj $tmpcir}

  # update display
  lsGUIchangeFrame 0
}


