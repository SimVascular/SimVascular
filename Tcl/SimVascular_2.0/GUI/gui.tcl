  }
}


# Procedure: guiVISupdateCutVol
proc guiVISupdateCutVol {} {

   global guiVISvars
   set theMesh $guiVISvars(mesh_vtk_obj)
   set pressurePD /guiVIS/cutvol/pressurePD
   set velocityPD /guiVIS/cutvol/velocityPD
   set allsurfacePD /guiVIS/cutvol/allsurface
   set surfacePD /guiVIS/cutvol/mesh
   set surfacePDslice /guiVIS/slice/mesh
   set allSurfMesh /guiVIS/vol/surface_mesh

   # update the display for volume
   global guiVISvars
   global guiVISvarsPrev

   set ren $guiVISvars(ren)


   global gRen3dFreeze
   if {$gRen3dFreeze > 0} {
      set alreadyfrozen 1
   } else {
      set alreadyfrozen 0
      set gRen3dFreeze 1
   }

   # although the mesh slice topology is technically independent
   # of the time step, since it is regenerated by the vtkCutter
   # we will assume here that it is not constant over time

   # the vol pressure and velocity depend on the step number,
   # if it appears the step number has changed since the last
   # display, we turn off all vis and let it update.  Note
   # that this code does not update the prev step number, that
   # should be done outside of this routine.  The graphics
   # also depend on the slice location, so if its changed
   # we also need to update.  Finally, if the user switches
   # between using the cut above / below the plane, we need
   # to also make big changes.
   if {($guiVISvarsPrev(currentStep) != $guiVISvars(currentStep)) ||  ($guiVISvarsPrev(p1) != $guiVISvars(p1)) ||  ($guiVISvarsPrev(p2) != $guiVISvars(p2)) ||  ($guiVISvarsPrev(p3) != $guiVISvars(p3)) ||  ($guiVISvars(cutvol_planeLocChanged) == 1) ||  ($guiVISvarsPrev(cutvol_side) != $guiVISvars(cutvol_side))} {
     set guiVISvarsPrev(cutvol_side) $guiVISvars(cutvol_side)
     if {$guiVISvarsPrev(cutvol_displayPressure) != "off"} {
       puts "step change: cutvol:  remove old pressure display"
       vis_pRm $ren $pressurePD
       repos_delete -obj $pressurePD
       set guiVISvarsPrev(cutvol_displayPressure) "off"
     } else {
       puts "step change: cutvol:  pressure do nothing"
     }
     if {$guiVISvarsPrev(cutvol_displayVelocity) == "off"} {
          # dont need to do anything
          puts "step change: cutvol: velocity off, off -- do nothing"
     } elseif {$guiVISvarsPrev(cutvol_displayVelocity) == "as vectors"} {
          puts "step change: cutvol: turn off vectors"
          vis_vRm $ren $velocityPD
          set guiVISvarsPrev(cutvol_displayVelocity) "off"
          repos_delete -obj $velocityPD
     } else {
          puts "step change: cutvol: turn off velocity scalars"
          vis_pRm $ren $velocityPD
          set guiVISvarsPrev(cutvol_displayVelocity) "off"
          repos_delete -obj $velocityPD
     }
     if {$guiVISvarsPrev(cutvol_showSurfMesh) == 1} {
  if {$guiVISvars(cutvol_preserve_property) == 1} {
          # keep a copy of the property if desired by the user
    catch {guiVIS-cutvol-kept-property Delete}
          vtkOpenGLProperty guiVIS-cutvol-kept-property
          guiVIS-cutvol-kept-property DeepCopy [[vis_pGetActor $ren $surfacePD] GetProperty]
  }
        vis_pRm $ren $surfacePD
        set guiVISvarsPrev(cutvol_showSurfMesh) 0
     }

     # create the slice
     # we shouldn't have to do this here, but we will
     # since the gui will get confused if the user first
     # turns on the cutvol then the slice vis.  Note the
     # resulting slice is not really used by cutvol code (just
     # the implicit plane).
     # create the slice
     if {$guiVISvars(cutvol_planeLocChanged) == 0} {
       catch {repos_delete -obj $surfacePDslice}
       guiVIScreateCutPlane $theMesh $surfacePDslice
     }
     set guiVISvars(cutvol_planeLocChanged) 0

     set topRight $guiVISvars(p1)
     set botRight $guiVISvars(p2)
     set topLeft  $guiVISvars(p3)

     if {[llength $topRight] != 3 ||
        [llength $botRight] != 3 ||
        [llength $topLeft] != 3} {
      return
      #return -code error "ERROR:  incorrect length of corner pts."
     }

     # create the explicit cut plane
     set plane tmp-guiVISupdateCutVol-plane
     catch {$plane Delete}
     vtkPlaneSource $plane
     $plane SetOrigin [lindex $topRight 0] [lindex $topRight 1] [lindex $topRight 2]
     $plane SetPoint1 [lindex $topLeft 0] [lindex $topLeft 1] [lindex $topLeft 2]
     $plane SetPoint2 [lindex $botRight 0] [lindex $botRight 1] [lindex $botRight 2]
     $plane Update
     set nrm [$plane GetNormal]

     # create the implicit cut plane
     set impPlane tmp-guiVISupdateCutVol-imp
     catch {$impPlane Delete}
     vtkPlane $impPlane
     $impPlane SetOrigin [lindex $topRight 0] [lindex $topRight 1] [lindex $topRight 2]
     $impPlane SetNormal [lindex $nrm 0] [lindex $nrm 1] [lindex $nrm 2]
     set guiVISvars(cutvol_plane) $impPlane

   }

   # hack that needs fixed
   if {$guiVISvars(cutvol_plane) == ""} {
     return
   }

   set clipper tmp-guiVISupdateCutVol-clipper
   catch {$clipper Delete}
   vtkClipPolyData $clipper
   $clipper SetClipFunction $guiVISvars(cutvol_plane)
   $clipper GenerateClippedOutputOn
   $clipper SetValue $guiVISvars(slice_offset)
   # toggle display of exterior mesh
   # note: the exterior volume mesh is independent of the
   #       actual step number
   if {$guiVISvarsPrev(cutvol_showSurfMesh) == 1 &&  $guiVISvars(cutvol_showSurfMesh) == 0} {
      if {$guiVISvars(cutvol_preserve_property) == 1} {
         # keep a copy of the property if the user requests it
   catch {guiVIS-cutvol-kept-property Delete}
         vtkOpenGLProperty guiVIS-cutvol-kept-property
         guiVIS-cutvol-kept-property DeepCopy [[vis_pGetActor $ren $surfacePD] GetProperty]
      }
      vis_pRm $ren $surfacePD
      set guiVISvarsPrev(cutvol_showSurfMesh) 0
   } elseif {$guiVISvarsPrev(cutvol_showSurfMesh) == 0 &&  $guiVISvars(cutvol_showSurfMesh) == 1} {
      # show the opposite part of the mesh from the cut
      $clipper SetInputDataObject [repos_exportToVtk -src $allSurfMesh]
      $clipper Update
      catch {repos_delete -obj $surfacePD}
      if {$guiVISvars(cutvol_side) == "above"} {
          repos_importVtkPd -src [$clipper GetClippedOutput] -dst $surfacePD
      } else {
          repos_importVtkPd -src [$clipper GetOutput] -dst $surfacePD
      }
      set actor [vis_pRepos $ren $surfacePD]
      if {($guiVISvars(cutvol_preserve_property) == 1) &&  ([cmdExists guiVIS-cutvol-kept-property] == 1)} {
        [[vis_pGetActor $ren $surfacePD] GetProperty] DeepCopy guiVIS-cutvol-kept-property
      } else {
        set myprop [$actor GetProperty]
        $myprop SetRepresentationToWireframe
        $myprop SetInterpolationToFlat
        $myprop SetColor 0.8900 0.8100 0.3400
        $myprop SetSpecularColor 1 1 1
        $myprop SetSpecular 0.3
        $myprop SetSpecularPower 20
        $myprop SetAmbient 0.2
        $myprop SetDiffuse 0.8
      }
      vis_render $ren
      # set the actor props here
      set guiVISvarsPrev(cutvol_showSurfMesh) 1
   }


   # toggle display of pressure
   if {$guiVISvarsPrev(cutvol_displayPressure) == "off" &&  $guiVISvars(cutvol_displayPressure) != "off"} {
       puts "cutvol: new display"
       catch {repos_delete -obj $pressurePD}
       set resultObj [repos_exportToVtk -src $guiVISvars(current_step)]
       set displayObj [repos_exportToVtk -src $allSurfMesh]
       [$displayObj GetPointData] SetScalars [[$resultObj GetPointData] GetScalars]
       $clipper SetInputDataObject [repos_exportToVtk -src $allSurfMesh]
       $clipper Update
       if {$guiVISvars(cutvol_side) == "below"} {
          repos_importVtkPd -src [$clipper GetClippedOutput] -dst $pressurePD
       } else {
          repos_importVtkPd -src [$clipper GetOutput] -dst $pressurePD
       }
       set guiVISvarsPrev(cutvol_minPressure) $guiVISvars(cutvol_minPressure)
       set guiVISvarsPrev(cutvol_maxPressure) $guiVISvars(cutvol_maxPressure)
       set guiVISvarsPrev(cutvol_displayPressure) $guiVISvars(cutvol_displayPressure)
       set actor [vis_pRepos $ren $pressurePD]
       [$actor GetMapper] ScalarVisibilityOn
       [$actor GetMapper] SetLookupTable gBlueToRedLUT
       [$actor GetMapper] SetScalarRange $guiVISvars(cutvol_minPressure) $guiVISvars(cutvol_maxPressure)
       vis_render $ren
   }  elseif {($guiVISvarsPrev(cutvol_displayPressure) != "off") &&  ($guiVISvars(cutvol_displayPressure) == "off")} {
       puts "cutvol: remove it!"
       vis_pRm $ren $pressurePD
       repos_delete -obj $pressurePD
       set guiVISvarsPrev(cutvol_displayPressure) $guiVISvars(cutvol_displayPressure)
   } elseif {($guiVISvarsPrev(cutvol_minPressure) != $guiVISvars(cutvol_minPressure)) ||  ($guiVISvarsPrev(cutvol_maxPressure) != $guiVISvars(cutvol_maxPressure))} {
       puts "cutvol: update"
       set guiVISvarsPrev(cutvol_minPressure) $guiVISvars(cutvol_minPressure)
       set guiVISvarsPrev(cutvol_maxPressure) $guiVISvars(cutvol_maxPressure)
       set guiVISvarsPrev(cutvol_displayPressure) $guiVISvars(cutvol_displayPressure)
       set actor [vis_pGetActor $ren $pressurePD]
       [$actor GetMapper] ScalarVisibilityOn
       [$actor GetMapper] SetLookupTable gBlueToRedLUT
       [$actor GetMapper] SetScalarRange $guiVISvars(cutvol_minPressure) $guiVISvars(cutvol_maxPressure)
       vis_render $ren
   }


   # toggle display of velocity
   # two cases, scalars and vectors
   if {$guiVISvars(cutvol_displayVelocity) == "off"} {

       if {$guiVISvarsPrev(cutvol_displayVelocity) == "off"} {
          # dont need to do anything
          puts "cutvol: off, off -- do nothing"
       } elseif {$guiVISvarsPrev(cutvol_displayVelocity) == "as vectors"} {
          puts "cutvol: turn off vectors"
          vis_vRm $ren $velocityPD
          set guiVISvarsPrev(cutvol_displayVelocity) $guiVISvars(cutvol_displayVelocity)
          repos_delete -obj $velocityPD
       } else {
          puts "cutvol: turn off scalars"
          vis_pRm $ren $velocityPD
          set guiVISvarsPrev(cutvol_displayVelocity) $guiVISvars(cutvol_displayVelocity)
          repos_delete -obj $velocityPD
       }

   } else {

      if {($guiVISvarsPrev(cutvol_displayVelocity) == "as vectors") &&  ($guiVISvars(cutvol_displayVelocity) != "as vectors") } {
               puts "cutvol: remove vectors from display"
               vis_vRm $ren $velocityPD
               set guiVISvarsPrev(cutvol_displayVelocity) "off"
      }  elseif {($guiVISvarsPrev(cutvol_displayVelocity) != "off") &&  ($guiVISvarsPrev(cutvol_displayVelocity) != "as vectors") &&  ($guiVISvars(cutvol_displayVelocity) == "as vectors") } {
                puts "cutvol: remove scalars from display"
               vis_pRm $ren $velocityPD
               set guiVISvarsPrev(cutvol_displayVelocity) "off"
      }

      # two cases
      if {$guiVISvarsPrev(cutvol_displayVelocity) == "off" &&  $guiVISvars(cutvol_displayVelocity) == "as vectors"} {
          puts "cutvol: new display as vectors"
          catch {repos_delete -obj $velocityPD}
          set vectnorm tmp-guiVISupdateCutVol-vn
          catch {$vectnorm Delete}
          vtkVectorNorm $vectnorm
          set resultObj [repos_exportToVtk -src $guiVISvars(current_step)]
          set displayObj [repos_exportToVtk -src $allSurfMesh]
          [$displayObj GetPointData] SetVectors [[$resultObj GetPointData] GetVectors]
          $clipper SetInputDataObject [repos_exportToVtk -src $allSurfMesh]
          $clipper Update
          if {$guiVISvars(cutvol_side) == "below"} {
            $vectnorm SetInputDataObject [$clipper GetClippedOutput]
          } else {
            $vectnorm SetInputDataObject [$clipper GetOutput]
          }
          $vectnorm Update
          repos_importVtkPd -src [$vectnorm GetOutput] -dst $velocityPD
          $vectnorm Delete
          set actor [vis_vRepos $ren $velocityPD]
          [$actor GetMapper] ScalarVisibilityOn
          [$actor GetMapper] SetLookupTable gBlueToRedLUT
          [$actor GetMapper] SetScalarRange $guiVISvars(cutvol_minVelMag) $guiVISvars(cutvol_maxVelMag)
          vis_render $ren
          vis_vSetScale $ren $velocityPD $guiVISvars(cutvol_velocityScale)
          set guiVISvarsPrev(cutvol_velocityScale) $guiVISvars(cutvol_velocityScale)
          set guiVISvarsPrev(cutvol_displayVelocity) $guiVISvars(cutvol_displayVelocity)
          repos_setLabel -obj $velocityPD -key showK -value [list $guiVISvars(cutvol_minVelMag) $guiVISvars(cutvol_maxVelMag)]
          set guiVISvarsPrev(cutvol_minVelMag) $guiVISvars(cutvol_minVelMag)
          set guiVISvarsPrev(cutvol_maxVelMag) $guiVISvars(cutvol_maxVelMag)
      } elseif {$guiVISvarsPrev(cutvol_displayVelocity) == "off" &&  $guiVISvars(cutvol_displayVelocity) != "as vectors"} {
          puts "cutvol: new display as scalars"
          catch {repos_delete -obj $velocityPD}
          set vectnorm tmp-guiVISupdateCutVol-vn
          catch {$vectnorm Delete}
          vtkVectorNorm $vectnorm
          set resultObj [repos_exportToVtk -src $guiVISvars(current_step)]
          set displayObj [repos_exportToVtk -src $allSurfMesh]
          [$displayObj GetPointData] SetVectors [[$resultObj GetPointData] GetVectors]
          $clipper SetInputDataObject [repos_exportToVtk -src $allSurfMesh]
          $clipper Update
          if {$guiVISvars(cutvol_side) == "below"} {
            $vectnorm SetInputDataObject [$clipper GetClippedOutput]
          } else {
            $vectnorm SetInputDataObject [$clipper GetOutput]
          }
          $vectnorm Update
          repos_importVtkPd -src [$vectnorm GetOutput] -dst $velocityPD
          $vectnorm Delete
          repos_setLabel -obj $velocityPD -key showK -value [list $guiVISvars(cutvol_minVelMag) $guiVISvars(cutvol_maxVelMag)]
          set guiVISvarsPrev(cutvol_minVelMag) $guiVISvars(cutvol_minVelMag)
          set guiVISvarsPrev(cutvol_maxVelMag) $guiVISvars(cutvol_maxVelMag)
          set guiVISvarsPrev(cutvol_displayVelocity) $guiVISvars(cutvol_displayVelocity)
          set actor [vis_pRepos $ren $velocityPD]
          [$actor GetMapper] ScalarVisibilityOn
          [$actor GetMapper] SetLookupTable gBlueToRedLUT
          [$actor GetMapper] SetScalarRange $guiVISvars(cutvol_minVelMag) $guiVISvars(cutvol_maxVelMag)
          vis_render $ren
      } elseif { ($guiVISvars(cutvol_displayVelocity) == "as vectors") } {

          if {$guiVISvarsPrev(cutvol_velocityScale) == $guiVISvars(cutvol_velocityScale)} {
             # do nothing
             puts "cutvol: as vectors , as vectors -- do nothing"
          } else {
             puts "cutvol: as vectors , as vectors -- scale"
             vis_vSetScale $ren $velocityPD $guiVISvars(cutvol_velocityScale)
             set guiVISvarsPrev(cutvol_velocityScale) $guiVISvars(cutvol_velocityScale)
    }

          if {($guiVISvarsPrev(cutvol_minVelMag) != $guiVISvars(cutvol_minVelMag)) ||  ($guiVISvarsPrev(cutvol_maxVelMag) != $guiVISvars(cutvol_maxVelMag))} {
            puts "cutvol: update scalars for vectors"
            set guiVISvarsPrev(cutvol_minVelMag) $guiVISvars(cutvol_minVelMag)
            set guiVISvarsPrev(cutvol_maxVelMag) $guiVISvars(cutvol_maxVelMag)
            set guiVISvarsPrev(cutvol_displayVelocity) $guiVISvars(cutvol_displayVelocity)
            set actor [vis_vGetActor $ren $velocityPD]
            [$actor GetMapper] ScalarVisibilityOn
            [$actor GetMapper] SetLookupTable gBlueToRedLUT
            [$actor GetMapper] SetScalarRange $guiVISvars(cutvol_minVelMag) $guiVISvars(cutvol_maxVelMag)
            vis_render $ren
      }

      } elseif { ($guiVISvarsPrev(cutvol_displayVelocity) != "as vectors") &&  ($guiVISvars(cutvol_displayVelocity) != "as vectors") } {

        if {($guiVISvarsPrev(cutvol_minVelMag) != $guiVISvars(cutvol_minVelMag)) ||  ($guiVISvarsPrev(cutvol_maxVelMag) != $guiVISvars(cutvol_maxVelMag))} {
            puts "cutvol: scalars update"
            set guiVISvarsPrev(cutvol_minVelMag) $guiVISvars(cutvol_minVelMag)
            set guiVISvarsPrev(cutvol_maxVelMag) $guiVISvars(cutvol_maxVelMag)
            set guiVISvarsPrev(cutvol_displayVelocity) $guiVISvars(cutvol_displayVelocity)
            set actor [vis_pGetActor $ren $velocityPD]
            [$actor GetMapper] ScalarVisibilityOn
            [$actor GetMapper] SetLookupTable gBlueToRedLUT
            [$actor GetMapper] SetScalarRange $guiVISvars(cutvol_minVelMag) $guiVISvars(cutvol_maxVelMag)
            vis_render $ren
  } else {
            puts "cutvol: not vectors, not vectors -- do nothing"
  }
      }
  }

  if {$alreadyfrozen == 0} {
    set gRen3dFreeze 0
    vis_render $ren
  }
}


# Procedure: guiVISupdateFromSolid
proc guiVISupdateFromSolid {} {
;#fixed

    global guiVISvars

    #if {$guiVISvars(mesh_loaded) == 0} {
    #   return GDSC_OK
    #}

    set model $guiVISvars(solidModelName)

    # check to see if a repository object exists
    if {[repos_exists -obj $model] == 0} {
       puts "ERROR:  Model \"$model\" does not exist."
       return -code error "Model does not exist."
    }
    # check to see if its a solid model
    if {[repos_type -obj $model] != "SolidModel"} {
       puts "ERROR:  Model \"$model\" is not of type SolidModel."
       return -code error "Model is not of type SolidModel."
    }

    set faceIds [$model GetFaceIds]

    if {[llength $faceIds] == 0} {
       puts "ERROR:  No Face Ids."
       return -code error "No Face Ids."
    }

    set foundIt 0
    foreach face $faceIds {
       if {[$model GetFaceAttr -attr gdscName -faceId $face] == $guiVISvars(faceName)} {
          set foundIt 1
          break
       }
    }
    if {$foundIt == "0"} {
       return -code error "ERROR:  Face name not found!"
    }

    # get a polydata for the requested face, and grab the first 3 points corresponding
    # to the first triangle.
    set pd /guiVIS/updateFromSolid/pd
    catch {repos_delete -obj $pd}
    $model GetFacePolyData -face $face -result $pd
    set points {}
    gdscGetPointsFromNthPoly 0 [repos_exportToVtk -src $pd] points

    set guiVISvars(p1) [lindex $points 0]
    set guiVISvars(p2) [lindex $points 1]
    set guiVISvars(p3) [lindex $points 2]

    set nrm [$model GetFaceNormal -face $face -u 0 -v 0]
    set ctr [geom_avgPt -obj $pd]

    # cross pollenation with the gui to calc vol flow
    set guiVISvars(volFlowSlice_sphereCenter) $ctr
    set guiVISvars(volFlowSlice_surfaceNormal) $nrm

    catch {repos_delete -obj $pd}

    return GDSC_OK
}


