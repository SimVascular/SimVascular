# Procedure: lsGUI_hand_UpdateQueryInteraction
proc lsGUI_hand_UpdateQueryInteraction { widget x y} {

  global lsGUImagWindow
  global lsGUIpotWindow

  set x [expr [winfo pointerx $widget] - [winfo rootx $widget]] 
  set y [expr [winfo pointery $widget] - [winfo rooty $widget]] 

  global symbolicName
  if {[file rootname $widget] == $symbolicName(lsRenWinMag)} {
    set ren $lsGUImagWindow(ren)
  } else {
    set ren $lsGUIpotWindow(ren)
  }

  # y is flipped upside down
  set height [lindex [$widget configure -height] 4]
  set y [expr $height - $y]

  set coord tmp-add_point_cord
  catch {$coord Delete}
  vtkCoordinate $coord
  $coord SetCoordinateSystemToDisplay
  $coord SetValue $x $y
  set worldValue [$coord GetComputedWorldValue $ren]
  $coord Delete
  puts "x: $x  y: $y  worldValue: $worldValue"

  set mag [repos_exportToVtk -src /tmp/lsGUI/mag]
  set org [$mag GetOrigin]
  set ext [$mag GetExtent]
  set spacing [$mag GetSpacing]

  set i [expr int(floor([expr [lindex $worldValue 0] - [lindex $org 0]]/[lindex $spacing 0]))]
  set j [expr int(floor([expr [lindex $worldValue 1] - [lindex $org 1]]/[lindex $spacing 1]))]

  if {$i < [lindex $ext 0] || $i > [lindex $ext 1]  ||  $j < [lindex $ext 2] || $j > [lindex $ext 3]} {
      puts "$i $j out of range!"
      return
   }

  set pixel [expr ($i - [lindex $ext 0]) + ($j - [lindex $ext 2])*([lindex $ext 1] -[lindex $ext 0]+1)]

  set magv [[[[repos_exportToVtk -src /tmp/lsGUI/mag] GetPointData] GetScalars] GetTuple1 $pixel]
  set potv [[[[repos_exportToVtk -src /tmp/lsGUI/pot] GetPointData] GetScalars] GetTuple1 $pixel]

  vis_textSet $ren "$i $j intensity: $magv   mag. of grad: $potv"
}


