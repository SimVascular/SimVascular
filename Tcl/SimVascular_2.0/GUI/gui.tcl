# Procedure: guiPPquantSaveCurvToFile
proc guiPPquantSaveCurvToFile { pathNo} {
  global guiSVvars
  set startId $guiSVvars(path_quantification_start_id_$pathNo)
  set stopId  $guiSVvars(path_quantification_stop_id_$pathNo)
  set posId   $guiSVvars(path_quantification_current_position_$pathNo)
  set name    $guiSVvars(path_quantification_current_path_name_$pathNo)
  set pathId  $guiSVvars(path_quantification_current_pathId_$pathNo)
  set pts     $guiSVvars(path_quantification_spline_$pathNo)
  set windowSize $guiSVvars(path_quantification_window_size_$pathNo)
  set startId $guiSVvars(path_quantification_start_id_$pathNo)
  set stopId  $guiSVvars(path_quantification_stop_id_$pathNo)
  set distances $guiSVvars(path_quantification_distances_$pathNo)

  set fp [open "curvature_$pathId\_$windowSize\_created_on_[clock seconds]" w]
  puts $fp "\#  simvascular curvature calcuations"
  puts $fp "\#  pathId: $pathId"
  puts $fp "\#  pathName: $name"
  puts $fp "\#  windowSize: $windowSize"
  puts $fp "\#  startId: $startId"
  puts $fp "\#  stopId: $stopId"
  puts $fp "\#  curved length: [expr [lindex $distances $stopId] - [lindex $distances $startId]]"
  puts $fp "\#"
  puts $fp "\#  pathId  posId  rel_distance  curvature abs_distance  x  y  z"
  puts $fp "\#"

  set meanCurvature 0.0
  set numPts 0
  set maxCurvature -1.0
  set minCurvature 9999999.0

  for {set i $startId} {$i <= $stopId} {incr i} {
    set guiSVvars(path_quantification_current_position_$pathNo) $i
    guiPPquantCalcCurvature $pathNo
    set pt [lindex $pts $i]
    set curvature $guiSVvars(path_quantification_curvature_$pathNo)
    puts $fp "$pathId\t $i\t [expr [lindex $distances $i] - [lindex $distances $startId]]\t $guiSVvars(path_quantification_curvature_$pathNo)\t [lindex $distances $i]\t [lindex $pt 0]\t [lindex $pt 1]\t [lindex $pt 2]"
    set meanCurvature [expr $meanCurvature + $curvature]
    if {$curvature >= $maxCurvature} {
      set maxCurvature $curvature
      set maxCurvPos $i
    }
    if {$curvature <= $minCurvature} {
      set minCurvature $curvature
      set minCurvPos $i
    }
    incr numPts
  }
  set meanCurvature [expr $meanCurvature/(1.0*$numPts)]

  puts $fp "\#  min curvature: $minCurvature   (position id: $minCurvPos)"
  puts $fp "\#  mean curvature: $meanCurvature"
  puts $fp "\#  max curvature: $maxCurvature  (position id: $maxCurvPos)"

  close $fp
}


# Procedure: guiPPquantSelectPath
proc guiPPquantSelectPath { pathNo} {

  global symbolicName
  global gPathPoints
  global guiSVvars

  set paths [guiSV_path_get_tree_current_paths_selected]
  if {$paths == ""} {
    puts "ERROR:  Must specify a Path ID before quantification!"
    return -code error "ERROR:  Must specify a Path ID before quantification!"
  }
  if {[llength $paths] > 1} {
    return -code error "Cannot quantify more than one path!"
  }
  set pathId [lindex $paths 0]
  global symbolicName
  set tv $symbolicName(guiSV_path_tree)
  set maxnum [llength [$tv children .paths.all.$pathId]]

  # clear current display
  guiPPquantUpdateDisplay $pathNo clear

  # update basic path information
  set guiSVvars(path_quantification_angle_of_separation) 0

  set guiSVvars(path_quantification_current_pathId_$pathNo) $pathId
  set guiSVvars(path_quantification_current_path_name_$pathNo) $gPathPoints($pathId,name)
  set guiSVvars(path_quantification_curvature_$pathNo) 0.0
  set guiSVvars(path_quantification_current_position_$pathNo) 0
  set numSplinePts $guiSVvars(path_quantification_num_spline_pts_$pathNo)
  set guiSVvars(path_quantification_path_point_$pathNo) {}
  set guiSVvars(path_quantification_start_id_$pathNo) 0
  set guiSVvars(path_quantification_stop_id_$pathNo) [expr $numSplinePts - 1]

  set minHandPts 0
  set maxHandPts [expr $maxnum - 1]

  set min 0
  set max [expr $guiSVvars(path_quantification_num_spline_pts_$pathNo) - 1]

  if {$pathNo != "twistpath"} {
    # need to update the scale limits
    set startScale $symbolicName(guiPPquantStartIDScale_$pathNo)
    set stopScale $symbolicName(guiPPquantStopIDScale_$pathNo)
    set posScale  $symbolicName(guiPPquantPositionScale_$pathNo)
 
    $startScale configure -from $min -to $max
    $stopScale  configure -from $min -to $max
    $posScale   configure -from $min -to $max
 
    if {$guiSVvars(path_quantification_current_position_$pathNo) > $max} {
      set guiSVvars(path_quantification_current_position_$pathNo) $max
    }

    $startScale set $min
    $stopScale  set $max
    $posScale   set [expr ($min+$max)/2]
  }

  # can only create a spline if we have more than 2 points
  #if {[llength $ptIndex] < 2} {
  #  set gPathPoints($id,splinePts) {}
  #  return -code error "ERROR: not enough points in path!"
  #}

  # create an ordered list of points
  set pts {}
  for {set i $minHandPts} {$i <= $maxHandPts} {incr i} {
    lappend pts $gPathPoints($pathId,$i)
  }

  set ptpd /tmp/guiPPquant/ptpd/$pathNo

  geom_mkPtsPd $pts $ptpd

  set flag 0

  # spline points
  set guiSVvars(path_quantification_path_$pathNo) [geom_splinePtsToPathPlan -src $ptpd  -numOutputPts $numSplinePts  -flag $flag]
  catch {repos_delete -obj $ptpd}

    set pts {}
    set tanpts {}
    for {set i 0} {$i < $numSplinePts} {incr i} {
      array set splinePt [lindex $guiSVvars(path_quantification_path_$pathNo) $i]
      lappend pts [TupleToList $splinePt(p)]
      lappend tanpts [TupleToList $splinePt(t)]
      if {$i == 0} {
         lappend distances 0.0
      } else {
         lappend distances [math_curveLength -pts $pts -closed 0]
      }
    }

    set guiSVvars(path_quantification_spline_$pathNo) $pts
    set guiSVvars(path_quantification_tangents_$pathNo) $tanpts
    set guiSVvars(path_quantification_distances_$pathNo) $distances

    set guiSVvars(path_quantification_startPt_$pathNo) [lindex $pts 0]
    set guiSVvars(path_quantification_stopPt_$pathNo) [lindex $pts end]
    set guiSVvars(path_quantification_path_point_$pathNo) [lindex $pts $guiSVvars(path_quantification_current_position_$pathNo)]
    set guiSVvars(path_quantification_curved_length_$pathNo) [math_curveLength -pts $pts -closed 0]
    set guiSVvars(path_quantification_straight_length_$pathNo) [math_curveLength -pts [list [lindex $pts 0] [lindex $pts end]] -closed 0]

  guiPPquantUpdateDisplay $pathNo all

  return
}


# Procedure: guiPPquantUpdateDisplay
proc guiPPquantUpdateDisplay { pathNo pressed} {

  global gRen3d
  global guiSVvars
  
  set splinePd /guiPPquant/path/$pathNo
  set splinePts $guiSVvars(path_quantification_spline_$pathNo)
  if {$pressed == "path" || $pressed == "all"} {
    if {$guiSVvars(path_quantification_display_path_$pathNo) != 0 && $splinePts != ""} {
      catch {repos_delete -obj $splinePd}
      geom_mkLinesFromPts $splinePts $splinePd 0
      catch {vis_pRm $gRen3d $splinePd}
      vis_pRepos $gRen3d $splinePd
      set myact [vis_pGetActor $gRen3d $splinePd]
      set myprop [$myact GetProperty]
      $myprop SetLineWidth 2
      $myprop SetInterpolationToFlat
      if {$pathNo == "path1"} {
        eval $myprop SetColor $guiSVvars(path_quantification_color_path1)
      } elseif {$pathNo == "path2"} {
        eval $myprop SetColor $guiSVvars(path_quantification_color_path2) 
      } elseif {$pathNo == "twistpath"} {
        eval $myprop SetColor $guiSVvars(path_quantification_color_twistpath)
      } elseif {$pathNo == "refpath"} {
        eval $myprop SetColor $guiSVvars(path_quantification_color_refpath)
      } else {
        $myprop SetColor 1 1 1
      }
      vis_render $gRen3d
    } else {
  catch {vis_pRm $gRen3d $splinePd}
    }
  }
  global gOptions
  set sphereSize $gOptions(pathDisplayPointSize)
  if {$pressed == "start" || $pressed == "all"} {
    set pt $guiSVvars(path_quantification_startPt_$pathNo)
    set startPd /guiPPquant/startPd/$pathNo
    if {$guiSVvars(path_quantification_display_start_sphere_$pathNo) != 0 && $pt != ""} {
        catch {repos_delete -obj $startPd}
  geom_mkSphere [lindex $pt 0] [lindex $pt 1] [lindex $pt 2] $sphereSize $startPd
        catch {vis_pRm $gRen3d $startPd}
        vis_pRepos $gRen3d $startPd
        set myact [vis_pGetActor $gRen3d $startPd]
        set myprop [$myact GetProperty]
        eval $myprop SetColor $guiSVvars(path_quantification_color_start)
        vis_render $gRen3d
    } else {
  catch {vis_pRm $gRen3d $startPd}
    }
  }
  if {$pressed == "stop" || $pressed == "all"} {
    set pt $guiSVvars(path_quantification_stopPt_$pathNo)
    set stopPd /guiPPquant/stopPd/$pathNo
    if {$guiSVvars(path_quantification_display_stop_sphere_$pathNo) != 0 && $pt != ""} {
        catch {repos_delete -obj $stopPd}
  geom_mkSphere [lindex $pt 0] [lindex $pt 1] [lindex $pt 2] $sphereSize $stopPd
        catch {vis_pRm $gRen3d $stopPd}
        vis_pRepos $gRen3d $stopPd
        set myact [vis_pGetActor $gRen3d $stopPd]
        set myprop [$myact GetProperty]
        eval $myprop SetColor $guiSVvars(path_quantification_color_stop)
        vis_render $gRen3d
    } else {
  catch {vis_pRm $gRen3d $stopPd}
    }
  }
  if {$pressed == "position" || $pressed == "all"} {
    set pt $guiSVvars(path_quantification_path_point_$pathNo)
    set curPd /guiPPquant/curPd/$pathNo
    if {$guiSVvars(path_quantification_display_position_sphere_$pathNo) != 0 && $pt != ""} {
        catch {repos_delete -obj $curPd}
  geom_mkSphere [lindex $pt 0] [lindex $pt 1] [lindex $pt 2] $sphereSize $curPd
        catch {vis_pRm $gRen3d $curPd}
        vis_pRepos $gRen3d $curPd
        set myact [vis_pGetActor $gRen3d $curPd]
        set myprop [$myact GetProperty]
        eval $myprop SetColor $guiSVvars(path_quantification_color_position)
        vis_render $gRen3d
    } else {
  catch {vis_pRm $gRen3d $curPd}
    }
  }
  set torus /guiPPquant/torus/$pathNo
  set torusPD /guiPPquant/torus/$pathNo/PD
  if {$pressed == "torus" || $pressed == "all"} {
    if {$guiSVvars(path_quantification_display_radius_torus_$pathNo) != 0} {
      catch {repos_delete -obj $torus}
      catch {repos_delete -obj $torusPD}
      catch {vis_pRm $gRen3d $torusPD}
      set radius $guiSVvars(path_quantification_torus_radius_$pathNo)
      if {$radius != ""} {
        set ctr $guiSVvars(path_quantification_torus_center_$pathNo)
        set nrm $guiSVvars(path_quantification_torus_normal_$pathNo)
        set thick $guiSVvars(path_quantification_torus_thickness_$pathNo)
        solid_torus -result $torus -rmaj [expr $radius - $thick] -rmin $thick -ctr $ctr -axis $nrm
        $torus GetPolyData -result $torusPD
        vis_pRepos $gRen3d $torusPD
        set myact [vis_pGetActor $gRen3d $torusPD]
        set myprop [$myact GetProperty]
        #$myprop SetLineWidth 2
        #$myprop SetInterpolationToFlat
        eval $myprop SetColor $guiSVvars(path_quantification_color_torus_$pathNo)
      }
      vis_render $gRen3d
    } else {
      catch {vis_pRm $gRen3d $torusPD}
    }
  }
}


