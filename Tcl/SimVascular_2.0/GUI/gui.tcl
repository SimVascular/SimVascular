# Procedure: guiVISchangeStep
proc guiVISchangeStep { value} {
;#fixed

  global guiVISvars
  global guiVISvarsPrev

  if {$guiVISvars(mesh_loaded) == 0} {
    return GDSC_OK
  }

  set theMesh $guiVISvars(mesh_vtk_obj)

  if {$value != "fakeValue"} {
    set guiVISvars(currentStep) $value
  } else {
    set value $guiVISvars(currentStep)
  }

  # get the step data
  set stepData [guiVISgetStepData $value]
  set guiVISvars(current_step) $stepData
  # associate it with the mesh
  set vtkpd [repos_exportToVtk -src $stepData]

  [$theMesh GetPointData] SetScalars [[$vtkpd GetPointData] GetScalars]
  [$theMesh GetPointData] SetVectors [[$vtkpd GetPointData] GetVectors]
  $theMesh Update

  # update vis
  global gRen3d
  global gRen3dFreeze
  set gRen3dFreeze 1
  guiVISupdateVol
  guiVISupdateCutPlane
  guiVISupdateCutVol
  set gRen3dFreeze 0
  vis_render $gRen3d
  set guiVISvarsPrev(currentStep) $guiVISvars(currentStep)

  return
}


# Procedure: guiVIScreateCutPlane
proc guiVIScreateCutPlane { theMesh surfacePD} {

  # we actually don't need to recreate the
  # plane & implicit plane everytime, but
  # this should be fast so we'll do it anyway

  set cutter guiVIScreateCutPlane-Cutter
  set plane guiVIScreateCutPlane-Plane
  set impPlane guiVIScreateCutPlane-Imp

  set reposPlane /guiVIS/slice/plane

  catch {$cutter Delete}
  catch {$plane Delete}
  catch {$impPlane Delete}

  catch {repos_delete -obj $surfacePD}
  catch {repos_delete -obj $reposPlane}

  global guiVISvars
  global guiVISvarsPrev

  if {$guiVISvars(mesh_loaded) == 0} {
    return -code error "ERROR: mesh not loaded."
  }

  set topRight $guiVISvars(p1)
  set botRight $guiVISvars(p2)
  set topLeft  $guiVISvars(p3)

  if {[llength $topRight] != 3 ||
      [llength $botRight] != 3 ||
      [llength $topLeft] != 3} {
    return
    #return -code error "ERROR:  incorrect length of corner pts."
  }

  # create the explicit cut plane

  vtkPlaneSource $plane
  $plane SetOrigin [lindex $topRight 0] [lindex $topRight 1] [lindex $topRight 2]
  $plane SetPoint1 [lindex $topLeft 0] [lindex $topLeft 1] [lindex $topLeft 2]
  $plane SetPoint2 [lindex $botRight 0] [lindex $botRight 1] [lindex $botRight 2]
  $plane Update
  set nrm [$plane GetNormal]
  repos_importVtkPd -src [$plane GetOutput] -dst $reposPlane

  # create the implicit cut plane
  vtkPlane $impPlane
  $impPlane SetOrigin [lindex $topRight 0] [lindex $topRight 1] [lindex $topRight 2]
  $impPlane SetNormal [lindex $nrm 0] [lindex $nrm 1] [lindex $nrm 2]

  set guiVISvars(cut_plane) $impPlane

  # do the cutting
  #puts "Using cutter..."
  catch {$cutter Delete}
  vtkCutter $cutter
  $cutter SetInputDataObject $theMesh
  $cutter SetCutFunction $guiVISvars(cut_plane)
  $cutter SetValue 0 $guiVISvars(slice_offset)
  $cutter Update

  # bring the results into Geodesic
  catch {repos_delete -obj $surfacePD}
  repos_importVtkPd -src [$cutter GetOutput] -dst $surfacePD

  $cutter Delete
  $plane Delete

  set guiVISvarsPrev(p1) $guiVISvars(p1)
  set guiVISvarsPrev(p2) $guiVISvars(p2)
  set guiVISvarsPrev(p3) $guiVISvars(p3)

  # needed so that cutvol knows the plane location has changed!
  set guiVISvars(cutvol_planeLocChanged) 1

  return GDSC_OK
}


# Procedure: guiVISdeleteStepData
