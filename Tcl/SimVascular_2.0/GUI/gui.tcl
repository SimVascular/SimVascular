# Procedure: bcGUImapToMeshBatch
proc bcGUImapToMeshBatch {} {

  global bcGUIcurrentFrameNumber
  global gBC
  global bcGUIbatchEntry

  set allframes [string_parse $bcGUIbatchEntry 1 $gBC(numSlices)]
  if {[llength $allframes] == 0} {
     puts "ERROR:  No frames specified."
     return -code error "ERROR:  No frames specified."
  }
  foreach frame $allframes {
    set bcGUIcurrentFrameNumber $frame
    bcGUImapToMeshCur
  }
}


# Procedure: bcGUImapToMeshCur
proc bcGUImapToMeshCur {} {

  global bcGUIcurrentFrameNumber
  global gFilenames
  global gObjects

  set frame $bcGUIcurrentFrameNumber

  set warpVel /gBC/pcdata/warpVel/$frame

  set inletMeshFace /gBC/pcdata/inletMeshFace/$frame
  catch {repos_delete -obj $inletMeshFace}

  # check that face exists
  set face $gObjects(inflow_mesh_face)
  set flatFace $gObjects(inflow_mesh_face_flat)

  if {[repos_exists -obj $face] == 0} {
     puts "ERROR:  No inflow mesh face loaded!"
     return -code error "NO MESH FACE LOADED!"
  }

  # now rotate it to be flat
  set mymatrix tmp-bcGUImapToMeshCur-matrix
  catch {$mymatrix Delete}
  catch {repos_delete -obj $flatFace}
  img_calcTransformMatrixToRAS $gFilenames(phase_contrast_geometric) $mymatrix
  $mymatrix Invert
  geom_applyTransformMatrix $face $mymatrix $flatFace
  catch {$mymatrix Delete}

  if {[repos_exists -obj /gBC/pcdata/warpVel/$frame] == 1} {
       geom_mapVectors $warpVel $flatFace $inletMeshFace
       repos_setLabel -obj $inletMeshFace -key showK -value 1
  } else {
      puts "ERROR:  No velocity map for frame $frame."
  }

  # update display
  bcGUIchangeFrame $frame

  return GDSC_OK
}


# Procedure: bcGUIparallelProjection
proc bcGUIparallelProjection {} {

  global bcGUImagWindow
  global bcGUIpotWindow

  set renMag $bcGUImagWindow(ren)
  set renPot $bcGUIpotWindow(ren)

  global bcGUIparallelProjectionFlag
  set flag $bcGUIparallelProjectionFlag

  if {$flag == 0} {

    foreach renWin {bcRenWinMag bcRenWinPot} {

      global symbolicName
      set widget $symbolicName($renWin).r1

      if {[cmdExists $widget] == 0} {
        continue
      }

      vis_renPerspectiveProjection $renMag
      vis_renPerspectiveProjection $renPot
 
      set selectedPoints bcGUItmp-handdraw-pts1
      set selectedLines bcGUItmp-handdraw-lines1
      set selectedActor bcGUItmp-handdraw-actor

      set cursorPoints bcGUItmp-handdraw-crsr-pts1
      set cursorLines bcGUItmp-handdraw-crsr-lines1
      set cursorPd bcGUItmp-handdraw-crsr-polydata-1
      set cursorActor bcGUItmp-handdraw-crsr-actor

      # remove the actor from the scene
      $renMag RemoveActor2D $selectedActor
      $renPot RemoveActor2D $selectedActor

      # remove the actor from the scene
      $renMag RemoveActor2D $cursorActor
      $renPot RemoveActor2D $cursorActor

      global symbolicName
      set widget $symbolicName($renWin).r1
      bind $widget <B1-Motion> {}
      bind $widget <KeyPress-a> {}
      bind $widget <KeyPress-s> {}
      bind $widget <Motion> {}
      bind $widget <KeyPress-c> {}
      bind $widget <KeyPress-x> {}
      bind $widget <KeyRelease-x> {}
      bind $widget <KeyPress-y> {}
      bind $widget <KeyRelease-y> {}
      bind $widget <KeyPress-z> {}
      bind $widget <KeyRelease-z> {}

      # Get the value
      bind $widget <ButtonPress-1> {}
      bind $widget <B1-Motion> {}
      bind $widget <ButtonRelease-1> {}

      # now reattach original bindings
      BindTkRenderWidget $symbolicName($renWin).r1
    }

    catch {vis_textRm $renMag}
    catch {vis_textRm $renPot}

    # clear the selection polygon
    $selectedLines Reset
    $selectedPoints Reset

    # clear the cursor if necessary
    $cursorPoints Reset
    $widget Render

    return
  }

  # a little sloppy, but easiest to remove the window
  # from the display and then redisplay it before we
  # turn on parallel projection (in case the user has
  # screwed up the window orientation somehow).

  # remove the window if it's displayed
  global bcGUImagWindow
  set bcGUImagWindow(showWindow) 0
  bcGUIupdateMagWindow

  # now show the window
  set bcGUImagWindow(showWindow) 1
  bcGUIupdateMagWindow

  # remove the window if it's displayed
  global bcGUIpotWindow
  set bcGUIpotWindow(showWindow) 0
  bcGUIupdatePotWindow

  # now show the window
  set bcGUIpotWindow(showWindow) 1
  bcGUIupdatePotWindow

  set renMag $bcGUImagWindow(ren)
  set renPot $bcGUIpotWindow(ren)

  # turn on parallel projection
  vis_renParallelProjection $renMag
  vis_renParallelProjection $renPot

  # bind key events needed for drawing by hand
    set selectedPoints bcGUItmp-handdraw-pts1
    set selectedLines bcGUItmp-handdraw-lines1
    set selectedPd bcGUItmp-handdraw-polydata-1
    set selectedMap bcGUItmp-handdraw-mapper
    set selectedProp bcGUItmp-handdraw-prop
    set selectedActor bcGUItmp-handdraw-actor

    set cursorPoints bcGUItmp-handdraw-crsr-pts1
    set cursorLines bcGUItmp-handdraw-crsr-lines1
    set cursorPd bcGUItmp-handdraw-crsr-polydata-1
    set cursorMap bcGUItmp-handdraw-crsr-mapper
    set cursorProp bcGUItmp-handdraw-crsr-prop
    set cursorActor bcGUItmp-handdraw-crsr-actor

    set crdtrans bcGUItm-handdraw-crdsystem

    foreach i [list $selectedPoints $selectedLines $selectedPd $selectedMap  $selectedProp $selectedActor $cursorPoints $cursorLines  $cursorPd $cursorMap $cursorProp $cursorActor $crdtrans] {
      catch {$i Delete}
    }

    # create a coordinate system transform for the 2d actors
    vtkCoordinate $crdtrans
    $crdtrans SetCoordinateSystemToWorld

    # create a 3-d polydata object representing the
    # selected points (and line segments)

    vtkPoints $selectedPoints
    $selectedPoints Allocate 1 1

    vtkCellArray $selectedLines
    $selectedLines InitTraversal

    vtkPolyData $selectedPd
    $selectedPd Allocate 1 1
    $selectedPd SetPoints $selectedPoints
    $selectedPd SetLines $selectedLines

    # create a 2D mapper
    vtkPolyDataMapper2D $selectedMap
    $selectedMap SetInputDataObject $selectedPd
    $selectedMap SetTransformCoordinate $crdtrans

    # create a 2D property
    vtkProperty2D $selectedProp
    $selectedProp SetColor 1 0 0
    $selectedProp SetLineWidth 1

    # create a 2D actor
    vtkActor2D $selectedActor
    $selectedActor SetMapper $selectedMap
    $selectedActor SetProperty $selectedProp

    # add the actor to the scene
    $renMag AddActor2D $selectedActor
    $renPot AddActor2D $selectedActor

    # create a 3-d polydata object representing
    # a line connecting the final selected point
    # with the current mouse pointer location

    vtkPoints $cursorPoints
    $cursorPoints Allocate 1 1

    vtkCellArray $cursorLines
    $cursorLines InitTraversal

    vtkPolyData $cursorPd
    $cursorPd Allocate 1 1
    $cursorPd SetPoints $cursorPoints
    $cursorPd SetLines $cursorLines
    $cursorLines InsertNextCell 2
    $cursorLines InsertCellPoint 0
    $cursorLines InsertCellPoint 1

    # create a 2D mapper
    vtkPolyDataMapper2D $cursorMap
    $cursorMap SetInputDataObject $cursorPd
    $cursorMap SetTransformCoordinate $crdtrans

    # create a 2D property
    vtkProperty2D $cursorProp
    $cursorProp SetColor 1 0 0
    $cursorProp SetLineWidth 1

    # create a 2D actor
    vtkActor2D $cursorActor
    $cursorActor SetMapper $cursorMap
    $cursorActor SetProperty $cursorProp

    # add the actor to the scene
    $renMag AddActor2D $cursorActor
    $renPot AddActor2D $cursorActor

    global bcGUIhanddrawShowCursorFlag
    set bcGUIhanddrawShowCursorFlag  1

    foreach renWin {bcRenWinMag bcRenWinPot} {
      global symbolicName
      set widget $symbolicName($renWin).r1

      bind $widget <B1-Motion> {}
      bind $widget <KeyPress-a> {bcGUI_hand_add_point %W %x %y}
      bind $widget <KeyPress-A> {bcGUI_hand_remove_last_point %W %x %y}
      bind $widget <KeyPress-s> {bcGUI_hand_toggle_show_cursor %W %x %y}
      bind $widget <Motion> {bcGUI_hand_show_cursor %W %x %y}
      bind $widget <KeyPress-c> {bcGUI_hand_make_poly %W %x %y}
      bind $widget <KeyPress-x> {bcGUI_hand_xyplot_velocity %W %x %y 0}
      bind $widget <KeyRelease-x> {bcGUI_hand_xyplot_velocity_release %W %x %y}
      bind $widget <KeyPress-y> {bcGUI_hand_xyplot_velocity %W %x %y 1}
      bind $widget <KeyRelease-y> {bcGUI_hand_xyplot_velocity_release %W %x %y}
      bind $widget <KeyPress-z> {bcGUI_hand_xyplot_velocity %W %x %y 2}
      bind $widget <KeyRelease-z> {bcGUI_hand_xyplot_velocity_release %W %x %y}
      bind $widget <KeyPress-t> {bcGUI_hand_tag_voxel %W %x %y}
      bind $widget <KeyPress-T> {bcGUI_hand_untag_voxel %W %x %y}

      # Get the value
      bind $widget <ButtonPress-1> {bcGUI_hand_StartQueryInteraction %W %x %y}
      bind $widget <B1-Motion> {bcGUI_hand_UpdateQueryInteraction %W %x %y}
      bind $widget <ButtonRelease-1> {bcGUI_hand_EndQueryInteraction %W}
    }
    catch {vis_textRm $renMag}
    catch {vis_textRm $renPot}

    vis_textSetup $renMag
    vis_textSetup $renPot
}


# Procedure: bcGUIpasteSeg
proc bcGUIpasteSeg { type} {

  set copyobj /bcGUI/copied

  if {[repos_exists -obj $copyobj] == 0} {
       puts "ERROR:  No copy slice found!"
       return -code error "ERROR:  No copy slice found!"
  }

  global bcGUIcurrentFrameNumber
  set frame $bcGUIcurrentFrameNumber

  if {$type == "levelset"} {
    set seg /gBC/pcdata/mag/$frame/ls
  } elseif {$type == "threshold"} {
    set seg /gBC/pcdata/mag/$frame/thr/selected
  } else {
    return -code error "Invalid segmentation type ($type)."
  }

  catch {repos_delete -obj $seg}

  # copy the unoriented slice
  geom_copy -src $copyobj -dst $seg

  # copy tags
  foreach myobj [list $seg] {
    foreach key [repos_getLabelKeys -obj $copyobj] {
      set value [repos_getLabel -obj $copyobj -key $key]
      repos_setLabel -obj $myobj -key $key -value $value
    }
  }
  # update tags
  catch {repos_clearLabel -obj $seg -key paste_frame}
  repos_setLabel -obj $seg -key paste_frame -value $frame

  # update display
  bcGUIchangeFrame $frame
}


