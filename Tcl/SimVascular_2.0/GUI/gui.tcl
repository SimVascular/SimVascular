# Procedure: guiRESULTSupdateLabelImage
proc guiRESULTSupdateLabelImage { side resizeOnly} {

  global symbolicName
  global guiRESULTSvars

  set myframe $symbolicName(results_tab_pic_$side\_frame)
  set height [winfo height $myframe]
  set width [winfo width $myframe]

  if {$resizeOnly == 1} {
    if {($guiRESULTSvars($side\_pic_frame_height) == $height) && ($guiRESULTSvars($side\_pic_frame_width) == $width)} {
       return
    }
    set guiRESULTSvars($side\_pic_frame_height) $height
    set guiRESULTSvars($side\_pic_frame_width) $width
  }

  set myphoto $guiRESULTSvars($side\_plot_photo)
  set mylabel $symbolicName(results_tab_pic_$side)

  if {$myphoto == "None"} {
      $mylabel configure -image {}
      return
