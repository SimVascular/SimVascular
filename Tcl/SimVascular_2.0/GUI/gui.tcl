# Procedure: guiPPchooserShowPath
proc guiPPchooserShowPath {} {
  global gPathPoints
  global symbolicName
  global gRen3d
  global guiPPchooserShowPathFlag
  global guiPPchooserShowPointsFlag

  set ids [guiSV_path_get_tree_current_paths_selected]
  if {$ids == ""} return
  set id [lindex $ids 0]

  # create a list of points if we're going to display something
  if {$guiPPchooserShowPathFlag == 1 || $guiPPchooserShowPointsFlag == 1} {
    foreach path $id {
      set indexes [array names gPathPoints $path,*]
      set ptIndex {}
      set numSplinePts {}
      foreach index $indexes {
        set pieces [split [string trim $index] ,]
        set tok1 [lindex $pieces 0]
        set tok2 [lindex $pieces 1]
        if {$path == $tok1} {
    if {$tok2 == "name"} {
            # skip
    } elseif {$tok2 == "numSplinePts"} {
            # skip
          } elseif {$tok2 == "splinePts"} {
            # skip
    } else {
            lappend ptIndex $tok2
    }
        }
      }
    }

    if {$ptIndex == ""} {
      puts "Warning:  pathId $path contains no points!"
      return
    }

    set ptIndex [lsort -integer $ptIndex]

    set pts {}

    foreach ptId $ptIndex {
      set pt $gPathPoints($path,$ptId)
      lappend pts $pt
    }
    #puts "pts: $pts"
  }

  # do show path stuff
  global gRen3dFreeze
  set gRen3dFreeze 1

  global gOptions
  set color $gOptions(color_for_path)
  set thick $gOptions(line_width_for_path)

  if {$guiPPchooserShowPathFlag == "0"} {
    set obj  /tmp/pathplan/current_path
    if {[repos_exists -obj $obj] == "1"} {
      repos_delete -obj $obj
      vis_pRm $gRen3d $obj
    }
  } else {
    if {[llength $ptIndex] > 1} {
      set obj  /tmp/pathplan/current_path
      if {[repos_exists -obj $obj] == "1"} {
        repos_delete -obj $obj
        vis_pRm $gRen3d $obj
      }
      geom_openLinesFromPts $pts $obj
      repos_setLabel -obj $obj -key color -value $color
      gdscGeneralView $gRen3d $obj
      set myact [vis_pGetActor $gRen3d $obj]
      set myprop [$myact GetProperty]
      $myprop SetLineWidth $thick
      $myprop SetInterpolationToFlat
    }
  }

  # show points stuff
  foreach i [repos_subList /tmp/pathplan/box*] {
    repos_delete -obj $i
    vis_pRm $gRen3d $i
  }
  if {$guiPPchooserShowPointsFlag == 1} {
    solid_setKernel -name PolyData
    for {set i 0} {$i < [llength $pts]} {incr i} {
      set pt [lindex $pts $i]
      set pti [lindex $ptIndex $i]
      set obj /tmp/pathplan/box
      set objpd /tmp/pathplan/box/pd/$path/$pti
      catch {repos_delete -obj $obj}
      catch {repos_delete -obj $objpd}
      global gOptions
      set bdims $gOptions(pathDisplayPointSize)
      solid_box3d -dims [list $bdims $bdims $bdims] -ctr $pt -result $obj
      $obj GetPolyData -result $objpd
      repos_setLabel -obj $objpd -key color -value purple
      gdscGeneralView $gRen3d $objpd
    }
  }
  set gRen3dFreeze 0
  vis_render $gRen3d
  return GDSC_OK
}


