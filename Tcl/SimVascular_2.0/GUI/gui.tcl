# Procedure: guiSV_group_delete_selected_groups
proc guiSV_group_delete_selected_groups {} {
  global gRen3d
  global symbolicName
  set tv $symbolicName(guiSV_group_tree)
  set selected [guiSV_group_get_tree_current_groups_selected]
  if {$selected != ""} {
    set numgrps [llength $selected]
    if { $numgrps == 1 } {
        set g [lindex $selected 0]
        set msg "Are you sure you like to delete group $g? This action cannot be undone"
      } else {
        set msg "Are you sure you like to delete $numgrps groups? This action cannot be undone"
      }

    set selected_delete 1
    set selected_delete [tk_dialog .askthem "Delete Groups?" "$msg" question 0 " No " " Yes "]

    if {!$selected_delete} return
    foreach grp $selected {
      foreach member [group_get $grp] {
        vis_pRm $gRen3d $member
      }
      vis_pRm $gRen3d /guiGROUPS/polydatasurface/$grp
      if [catch {group_delete $grp} msg] {
        puts "error deleting group ($grp): $msg"
      }
    }
  }

  set selected [guiSV_group_get_tree_current_surfaces_selected]
  if {$selected != ""} {
    set numgrps [llength $selected]
    if { $numgrps == 1 } {
        set g [lindex $selected 0]
        set msg "Are you sure you like to delete segmentation $g? This action cannot be undone"
      } else {
        set msg "Are you sure you like to delete $numgrps segmentations? This action cannot be undone"
      }

    set selected_delete 1
    set selected_delete [tk_dialog .askthem "Delete Groups?" "$msg" question 0 " No " " Yes "]

    if {!$selected_delete} return
    foreach grp $selected {
      global gSeg3D
      vis_pRm $gRen3d $gSeg3D($grp)
      if [catch {seg3d_delete $grp} msg] {
        puts "error deleting group ($grp): $msg"
      }
    }
  }
  guiSV_group_update_tree
}


