# Procedure: guiMMwriteInflowFace
proc guiMMwriteInflowFace {} {

  global gFilenames
  global gObjects
  global gOptions

  set solidkernel $gOptions(meshing_solid_kernel)

  if {$solidkernel == "Parasolid" || $solidkernel == "Discrete"} {
    set solid $gObjects(atdb_solid)
  } elseif {$solidkernel == "PolyData"} {
    set solid $gObjects(polydata_solid)
  } else {
    return -code error "ERROR: Invalid solid kernel"
  }

  set faceid -1

  set mesh $gObjects(mesh_object)
  set outfile $gFilenames(inflow_mesh_face_file)
  set facePD $gObjects(inflow_mesh_face)

  if {[repos_exists -obj $facePD]} {
      puts "inflow_mesh_face exists and is being replaced."
      repos_delete -obj $facePD
  }

  foreach face [$solid GetFaceIds] {
    set facename {}
    catch {set facename [$solid GetFaceAttr -attr gdscName -faceId $face]}
    if {$facename == "inflow"} {
      set faceid $face
      break
    }
  }
  if {$faceid < 0} {
    puts "ERROR:  Could not find \"inflow\" face!"
    return -code error "ERROR:  Could not find \"inflow\" face!"
  }

  puts "Getting Inflow Mesh Face PolyData (face $faceid)."
  $mesh GetFacePolyData -result $facePD -face $faceid
  puts "Writing file $outfile."
  repos_writeVtkPolyData -file $outfile -type ascii -obj $facePD
}


