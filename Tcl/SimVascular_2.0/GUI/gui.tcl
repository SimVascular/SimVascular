# Procedure: lsGUIparallelProjection
proc lsGUIparallelProjection {} {


  global lsGUImagWindow
  global lsGUIpotWindow

  set renMag $lsGUImagWindow(ren)
  set renPot $lsGUIpotWindow(ren)

  global lsGUIparallelProjectionFlag
  set flag $lsGUIparallelProjectionFlag
  set shown $lsGUImagWindow(showWindow)

  if {$flag == 0} {

    foreach renWin {lsRenWinMag lsRenWinPot} {

      global symbolicName
      set widget $symbolicName($renWin).r1

      if {[cmdExists $widget] == 0} {
        continue
      }

      vis_renPerspectiveProjection $renMag
      vis_renPerspectiveProjection $renPot

      set selectedPoints lsGUItmp-handdraw-pts1
      set selectedLines lsGUItmp-handdraw-lines1
      set selectedActor lsGUItmp-handdraw-actor

      set cursorPoints lsGUItmp-handdraw-crsr-pts1
      set cursorLines lsGUItmp-handdraw-crsr-lines1
      set cursorPd lsGUItmp-handdraw-crsr-polydata-1
      set cursorActor lsGUItmp-handdraw-crsr-actor

      # remove the actor from the scene
      $renMag RemoveActor2D $selectedActor
      $renPot RemoveActor2D $selectedActor

      # remove the actor from the scene
      $renMag RemoveActor2D $cursorActor
      $renPot RemoveActor2D $cursorActor

      global symbolicName
      set widget $symbolicName($renWin).r1
      bind $widget <B1-Motion> {}
      bind $widget <KeyPress-a> {}
      bind $widget <KeyPress-s> {}
      bind $widget <Motion> {}
      bind $widget <KeyPress-c> {}
      bind $widget <KeyPress-x> {}
      bind $widget <KeyRelease-x> {}
      bind $widget <KeyPress-y> {}
      bind $widget <KeyRelease-y> {}
      bind $widget <KeyPress-z> {}
      bind $widget <KeyRelease-z> {}

      # Get the value
      bind $widget <ButtonPress-1> {}
      bind $widget <B1-Motion> {}
      bind $widget <ButtonRelease-1> {}

      # now reattach original bindings
      global symbolicName
      BindTkRenderWidget $symbolicName($renWin).r1
    }

    catch {vis_textRm $renMag}
    catch {vis_textRm $renPot}

    # clear the selection polygon
    $selectedLines Reset
    $selectedPoints Reset

    # clear the cursor if necessary
    $cursorPoints Reset
    $widget Render

    return
  }

  # a little sloppy, but easiest to remove the window
  # from the display and then redisplay it before we
  # turn on parallel projection (in case the user has
  # screwed up the window orientation somehow).

  # remove the window if it's displayed
  global lsGUImagWindow
  set lsGUImagWindow(showWindow) 0
  lsGUIupdateMagWindow

  # now show the window
  set lsGUImagWindow(showWindow) 1
  lsGUIupdateMagWindow

  # remove the window if it's displayed
  global lsGUIpotWindow
  set lsGUIpotWindow(showWindow) 0
  lsGUIupdatePotWindow

  # now show the window
  set lsGUIpotWindow(showWindow) 1
  lsGUIupdatePotWindow

  set renMag $lsGUImagWindow(ren)
  set renPot $lsGUIpotWindow(ren)

  # turn on parallel projection
  vis_renParallelProjection $renMag
  vis_renParallelProjection $renPot

  # bind key events needed for drawing by hand
    set selectedPoints lsGUItmp-handdraw-pts1
    set selectedLines lsGUItmp-handdraw-lines1
    set selectedPd lsGUItmp-handdraw-polydata-1
    set selectedMap lsGUItmp-handdraw-mapper
    set selectedProp lsGUItmp-handdraw-prop
    set selectedActor lsGUItmp-handdraw-actor

    set cursorPoints lsGUItmp-handdraw-crsr-pts1
    set cursorLines lsGUItmp-handdraw-crsr-lines1
    set cursorPd lsGUItmp-handdraw-crsr-polydata-1
    set cursorMap lsGUItmp-handdraw-crsr-mapper
    set cursorProp lsGUItmp-handdraw-crsr-prop
    set cursorActor lsGUItmp-handdraw-crsr-actor

    set crdtrans lsGUItm-handdraw-crdsystem

    foreach i [list $selectedPoints $selectedLines $selectedPd $selectedMap  $selectedProp $selectedActor $cursorPoints $cursorLines  $cursorPd $cursorMap $cursorProp $cursorActor $crdtrans] {
      catch {$i Delete}
    }

    # create a coordinate system transform for the 2d actors
    vtkCoordinate $crdtrans
    $crdtrans SetCoordinateSystemToWorld

    # create a 3-d polydata object representing the
    # selected points (and line segments)

    vtkPoints $selectedPoints
    $selectedPoints Allocate 1 1

    vtkCellArray $selectedLines
    $selectedLines InitTraversal

    vtkPolyData $selectedPd
    $selectedPd Allocate 1 1
    $selectedPd SetPoints $selectedPoints
    $selectedPd SetLines $selectedLines

    # create a 2D mapper
    vtkPolyDataMapper2D $selectedMap
    $selectedMap SetInputDataObject $selectedPd
    $selectedMap SetTransformCoordinate $crdtrans

    # create a 2D property
    vtkProperty2D $selectedProp
    $selectedProp SetColor 1 0 0
    $selectedProp SetLineWidth 1

    # create a 2D actor
    vtkActor2D $selectedActor
    $selectedActor SetMapper $selectedMap
    $selectedActor SetProperty $selectedProp

    # add the actor to the scene
    $renMag AddActor2D $selectedActor
    $renPot AddActor2D $selectedActor

    # create a 3-d polydata object representing
    # a line connecting the final selected point
    # with the current mouse pointer location

    vtkPoints $cursorPoints
    $cursorPoints Allocate 1 1

    vtkCellArray $cursorLines
    $cursorLines InitTraversal

    vtkPolyData $cursorPd
    $cursorPd Allocate 1 1
    $cursorPd SetPoints $cursorPoints
    $cursorPd SetLines $cursorLines
    $cursorLines InsertNextCell 2
    $cursorLines InsertCellPoint 0
    $cursorLines InsertCellPoint 1

    # create a 2D mapper
    vtkPolyDataMapper2D $cursorMap
    $cursorMap SetInputDataObject $cursorPd
    $cursorMap SetTransformCoordinate $crdtrans

    # create a 2D property
    vtkProperty2D $cursorProp
    $cursorProp SetColor 1 0 0
    $cursorProp SetLineWidth 1

    # create a 2D actor
    vtkActor2D $cursorActor
    $cursorActor SetMapper $cursorMap
    $cursorActor SetProperty $cursorProp

    # add the actor to the scene
    $renMag AddActor2D $cursorActor
    $renPot AddActor2D $cursorActor

    global lsGUIhanddrawShowCursorFlag
    set lsGUIhanddrawShowCursorFlag  1

    foreach renWin {lsRenWinMag lsRenWinPot} {
      global symbolicName
      set widget $symbolicName($renWin).r1
      bind $widget <B1-Motion> {}
      bind $widget <KeyPress-a> {lsGUI_hand_add_point %W %x %y}
      bind $widget <KeyPress-A> {lsGUI_hand_remove_last_point %W %x %y}
      bind $widget <KeyPress-s> {lsGUI_hand_toggle_show_cursor %W %x %y}
      bind $widget <Motion> {lsGUI_hand_show_cursor %W %x %y}
      bind $widget <KeyPress-c> {lsGUI_hand_make_poly %W %x %y}
      # Get the value
      bind $widget <ButtonPress-1> {lsGUI_hand_StartQueryInteraction %W %x %y}
      bind $widget <B1-Motion> {lsGUI_hand_UpdateQueryInteraction %W %x %y}
      bind $widget <ButtonRelease-1> {lsGUI_hand_EndQueryInteraction %W}
    }

    catch {vis_textRm $renMag}
    catch {vis_textRm $renPot}

    vis_textSetup $renMag
    vis_textSetup $renPot
}


# Procedure: lsGUIpasteSeg
proc lsGUIpasteSeg { type} {

  set copyobj /lsGUI/copied

  if {[repos_exists -obj $copyobj] == 0} {
       puts "ERROR:  No copy slice found!"
       return -code error "ERROR:  No copy slice found!"
  }

  global symbolicName
  global lsGUIcurrentPathNumber
  set pathId $lsGUIcurrentPathNumber
  global gPathPoints
  set path $gPathPoints($pathId,splinePts)

  global lsGUIcurrentPositionNumber
  set posId $lsGUIcurrentPositionNumber

  # paste profile as type specified in seg current with
    if {$type == "levelset"} {
    set seg /lsGUI/$pathId/$posId/ls
    set segoriented $seg/oriented
  } elseif {$type == "threshold"} {
    set baseName /lsGUI/$pathId/$posId
    set seg $baseName/thr/selected
    set segoriented $baseName/thr/oriented
  } else {
    return -code error "Invalid segmentation type ($type)."
  }

  catch {repos_delete -obj $seg}
  catch {repos_delete -obj $segoriented}

  # copy the unoriented slice
  geom_copy -src $copyobj -dst $seg

  array set items [lindex $path $posId]
  set pos [TupleToList $items(p)]
  set nrm [TupleToList $items(t)]
  set xhat [TupleToList $items(tx)]
  geom_orientProfile -src $seg -path_pos $pos -path_tan $nrm -path_xhat $xhat -dst $segoriented

  # copy tags
  foreach myobj [list $seg $segoriented] {
    foreach key [repos_getLabelKeys -obj $copyobj] {
      set value [repos_getLabel -obj $copyobj -key $key]
      repos_setLabel -obj $myobj -key $key -value $value
    }
  }
  # update tags
  catch {repos_clearLabel -obj $seg -key paste_pos}
  catch {repos_clearLabel -obj $seg -key paste_nrm}
  catch {repos_clearLabel -obj $seg -key paste_xhat}
  catch {repos_clearLabel -obj $seg -key paste_pathId}
  catch {repos_clearLabel -obj $seg -key paste_posId}
  catch {repos_clearLabel -obj $segoriented -key paste_pos}
  catch {repos_clearLabel -obj $segoriented -key paste_nrm}
  catch {repos_clearLabel -obj $segoriented -key paste_xhat}
  catch {repos_clearLabel -obj $segoriented -key paste_pathId}
  catch {repos_clearLabel -obj $segoriented -key paste_posId}
  repos_setLabel -obj $seg         -key paste_pos    -value $pos
  repos_setLabel -obj $seg         -key paste_nrm    -value $nrm
  repos_setLabel -obj $seg         -key paste_xhat   -value $xhat
  repos_setLabel -obj $seg         -key paste_pathId -value $pathId
  repos_setLabel -obj $seg         -key paste_posId  -value $posId
  repos_setLabel -obj $segoriented -key paste_pos    -value $pos
  repos_setLabel -obj $segoriented -key paste_nrm    -value $nrm
  repos_setLabel -obj $segoriented -key paste_xhat   -value $xhat
  repos_setLabel -obj $segoriented -key paste_pathId -value $pathId
  repos_setLabel -obj $segoriented -key paste_posId  -value $posId

  # update display
  lsGUIchangeFrame 0
}


# Procedure: lsGUIscaleSeg
proc lsGUIscaleSeg { type} {

  global symbolicName
  global lsGUIcurrentPathNumber
  set pathId $lsGUIcurrentPathNumber
  global gPathPoints
  set path $gPathPoints($pathId,splinePts)

  global lsGUIcurrentPositionNumber
  set posId $lsGUIcurrentPositionNumber

  # need to translate hand/analytic to proper type
  if {$type == "analytic"} {
    global lsGUIanalContourType
    set type $lsGUIanalContourType
  } elseif {$type == "hand"} {
    global lsGUIhandContourType
    set type $lsGUIhandContourType
  }

  if {$type == "levelset"} {
    set seg /lsGUI/$pathId/$posId/ls
    set segoriented $seg/oriented
  } elseif {$type == "threshold"} {
    set baseName /lsGUI/$pathId/$posId
    set seg $baseName/thr/selected
    set segoriented $baseName/thr/oriented
  } else {
    return -code error "Invalid segmentation type ($type)."
  }

  if {[repos_exists -obj $seg] == 0} {
    return
  }

  # keep the tags on the segmentation
  foreach key [repos_getLabelKeys -obj $seg] {
    set seg_keys($key) [repos_getLabel -obj $seg -key $key]
  }
  foreach key [repos_getLabelKeys -obj $segoriented] {
    set segoriented_keys($key) [repos_getLabel -obj $segoriented -key $key]
  }

  global lsGUIscaleFactor

  set cntrseg   /lsGUI/scaled/centered
  set scaledseg /lsGUI/scaled/scaled

  catch {repos_delete -obj $cntrseg}
  catch {repos_delete -obj $scaledseg}

  # move center to origin
  set center [geom_avgPt -obj $seg]
  geom_translate -src $seg -dst $cntrseg -vec [math_scaleVec $center -1.0]
  # scale segmentation
  geom_scaleAvg -src $cntrseg -dst $scaledseg -factor $lsGUIscaleFactor
  catch {repos_delete -obj $seg}
  catch {repos_delete -obj $segoriented}
  # move back to center
  geom_translate -src $scaledseg -dst $seg -vec $center

  array set items [lindex $path $posId]
  set pos [TupleToList $items(p)]
  set nrm [TupleToList $items(t)]
  set xhat [TupleToList $items(tx)]
  geom_orientProfile -src $seg -path_pos $pos -path_tan $nrm -path_xhat $xhat -dst $segoriented

  # reattach tags
  foreach key [array names seg_keys] {
    repos_setLabel -obj $seg -key $key -value $seg_keys($key)
  }
  foreach key [array names segoriented_keys] {
    repos_setLabel -obj $segoriented -key $key -value $segoriented_keys($key)
  }
  # update seg
  set scale_factor 1.0
  if {[info exists seg_keys(scale_factor)] == 1} {
    repos_clearLabel -obj $seg -key scale_factor
    set scale_factor $seg_keys(scale_factor)
  }
  repos_setLabel -obj $seg -key scale_factor -value [expr double($scale_factor) * $lsGUIscaleFactor]
  # update segoriented
  set scale_factor 1.0
  if {[info exists segoriented_keys(scale_factor)] == 1} {
    repos_clearLabel -obj $segoriented -key scale_factor
    set scale_factor $segoriented_keys(scale_factor)
  }
  repos_setLabel -obj $segoriented -key scale_factor -value [expr double($scale_factor) * $lsGUIscaleFactor]

  # update display
  lsGUIchangeFrame 0
}


# Procedure: lsGUIsegBatch
proc lsGUIsegBatch { choice} {

  global lsGUIbatchEntry

  set entry $lsGUIbatchEntry

  # find min/max frame number
  global symbolicName
  set minFrame [expr int([$symbolicName(lsGUIpathPositionScale) cget -from])]
  set maxFrame [expr int([$symbolicName(lsGUIpathPositionScale) cget -to])]

  set allframes [string_parse $lsGUIbatchEntry $minFrame $maxFrame]
  if {[llength $allframes] == 0} {
     puts "ERROR:  No frames specified."
     return -code error "ERROR:  No frames specified."
  }

  # check that the user hasn't accidently selected this button!
  set yesno [tk_messageBox -message "Do you want to attempt [llength $allframes] segmentations?" -default no -icon question -type yesno]
  if {$yesno == "no"} {
        return -code error "User halted."
  }

  if {[llength $allframes] >= 5} {
     set yesno [tk_messageBox -message "Are you really, really sure?  You will be attempting [llength $allframes] segmentations." -default no -icon question -type yesno]
     if {$yesno == "no"} {
        return -code error "User halted."
     }
  }

  set starttime [clock seconds]
  foreach frame $allframes {
    $symbolicName(lsGUIpathPositionScale) set $frame
    lsGUIupdatePositionScale 0
    if {$choice == "threshold"} {
      lsGUIthrSlice
    } elseif {$choice == "levelset"} {
      lsGUIlsSlice
    } else {
      return -code error "ERROR:  $type not valid."
    }
  }
  puts "time for batch run: [expr [clock seconds] - $starttime] seconds."
}


