}


# Procedure: guiWSScreateCutBox
proc guiWSScreateCutBox { side offset bbox} {

  global guiWSSvars
  global gRen3d

  set wsspd /guiWSS/wssPD
  set cutpd /guiWSS/cutPD

  set clipper tmp-guiWSS-clipper
  set impBox guiWSScreateCutBox-ImpBox

  catch {$clipper Delete}
  catch {$impBox Delete}

  # create the implicit cut box
  vtkBox $impBox
  eval $impBox SetBounds $bbox

  # do the clipping
  #puts "Using clipper..."
  catch {$clipper Delete}
  vtkClipPolyData $clipper
  $clipper SetInputDataObject [repos_exportToVtk -src $cutpd]
  $clipper GenerateClippedOutputOn
  $clipper SetClipFunction $impBox
  $clipper SetValue $offset
  $clipper Update

  global gRen3d

  set ren $gRen3d

  global gRen3dFreeze
  if {$gRen3dFreeze > 0} {
    set alreadyfrozen 1
  } else {
    set alreadyfrozen 0
    set gRen3dFreeze 1
  }

  if {$guiWSSvars(vis_cut_surface) != 0} {
    vis_pRm $gRen3d $cutpd
  }

  # bring the results in aspire2
  catch {repos_delete -obj $cutpd}
  if {$side == "inside"} {
    repos_importVtkPd -src [$clipper GetOutput] -dst $cutpd
  } else {
    repos_importVtkPd -src [$clipper GetClippedOutput] -dst $cutpd
  }

  guiWSSvis vis_cut_surface

  $clipper Delete
  $impBox Delete

  if {$alreadyfrozen == 0} {
    set gRen3dFreeze 0
    vis_render $ren
  }
}


# Procedure: guiWSScreateCutPlane
proc guiWSScreateCutPlane { side} {

  global guiWSSvars
  global gRen3d

  set wsspd /guiWSS/wssPD
  set cutpd /guiWSS/cutPD
#  set tmpcutpd /guiWSS/tmpcutpd

  set clipper tmp-guiWSS-clipper

  set plane guiWSScreateCutPlane-Plane
  set impPlane guiWSScreateCutPlane-Imp

  set reposPlane /guiWSS/slice_plane

  catch {$clipper Delete}
  catch {$plane Delete}
  catch {$impPlane Delete}

  catch {repos_delete -obj $tmpcutpd}
  catch {repos_delete -obj $reposPlane}

  set topRight $guiWSSvars(plane_p1)
  set botRight $guiWSSvars(plane_p2)
  set topLeft  $guiWSSvars(plane_p3)

  if {[llength $topRight] != 3 ||
      [llength $botRight] != 3 ||
      [llength $topLeft] != 3} {
    return
    #return -code error "ERROR:  incorrect length of corner pts."
  }

  # create the explicit cut plane

  vtkPlaneSource $plane
  $plane SetOrigin [lindex $topRight 0] [lindex $topRight 1] [lindex $topRight 2]
  $plane SetPoint1 [lindex $topLeft 0] [lindex $topLeft 1] [lindex $topLeft 2]
  $plane SetPoint2 [lindex $botRight 0] [lindex $botRight 1] [lindex $botRight 2]
  $plane Update
  set nrm [$plane GetNormal]
  repos_importVtkPd -src [$plane GetOutput] -dst $reposPlane

  # create the implicit cut plane
  vtkPlane $impPlane
  $impPlane SetOrigin [lindex $topRight 0] [lindex $topRight 1] [lindex $topRight 2]
  $impPlane SetNormal [lindex $nrm 0] [lindex $nrm 1] [lindex $nrm 2]

  # do the clipping
  #puts "Using clipper..."
  catch {$clipper Delete}
  vtkClipPolyData $clipper
  $clipper SetInputDataObject [repos_exportToVtk -src $cutpd]
  $clipper GenerateClippedOutputOn
  $clipper SetClipFunction $impPlane
  $clipper SetValue $guiWSSvars(plane_offset)
  $clipper Update

  global gRen3d

  set ren $gRen3d

  global gRen3dFreeze
  if {$gRen3dFreeze > 0} {
    set alreadyfrozen 1
  } else {
    set alreadyfrozen 0
    set gRen3dFreeze 1
  }

  if {$guiWSSvars(vis_cut_surface) != 0} {
    vis_pRm $gRen3d $cutpd
  }

  # bring the results in aspire2
  catch {repos_delete -obj $cutpd}
  if {$side == "above"} {
    repos_importVtkPd -src [$clipper GetOutput] -dst $cutpd
  } else {
    repos_importVtkPd -src [$clipper GetClippedOutput] -dst $cutpd
  }

  guiWSSvis vis_cut_surface

  $clipper Delete
  $plane Delete

  if {$alreadyfrozen == 0} {
    set gRen3dFreeze 0
    vis_render $ren
  }
}


# Procedure: guiWSScutRefresh
proc guiWSScutRefresh {} {
  # replace object with original and then
  # process the current cut file
  set wsspd /guiWSS/wssPD
  set cutpd /guiWSS/cutPD

  guiWSSselectFaces refresh

  global symbolicName
  global guiWSSvars
  set mytext {}
  set tbox $symbolicName(guiWSScutFileTB)
  set numlines [lindex [split [$tbox index end] .] 0]
  for {set i 1} {$i < $numlines} {incr i} {
    set line [$tbox get $i.0 $i.end]
    set line [string trim $line]
    if {[string length $line] == 0} {
      continue
    }
    if {[string index $line 0] == "\#"} {
      continue
    }
    if {[lindex $line 0] == "plane"} {
      set side [lindex $line 1]
      set offset [lindex $line 2]
      set topRight [lrange $line 3 5]
      set botRight [lrange $line 6 8]
      set topLeft  [lrange $line 9 11]
      set guiWSSvars(plane_p1) $topRight
      set guiWSSvars(plane_p2) $botRight
      set guiWSSvars(plane_p3) $topLeft
      set guiWSSvars(plane_offset) $offset
      guiWSScreateCutPlane $side
    } elseif {[lindex $line 0] == "box"} {
      set side [lindex $line 1]
      set offset [lindex $line 2]
      set bbox [lrange $line 3 8]
      guiWSScreateCutBox $side $offset $bbox
    }
  }
}


