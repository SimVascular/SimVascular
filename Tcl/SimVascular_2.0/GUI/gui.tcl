# Procedure: guiMMselectDir
proc guiMMselectDir { dir} {

   if {$dir == "error_input_dir"} {
     set otherdir error_output_dir
   } else {
     set otherdir error_input_dir
   }

   set tryme [tk_chooseDirectory -initialdir [pwd]]
   if {$tryme != ""} {
      global guiMMvars
      set guiMMvars($dir) $tryme
      if {$guiMMvars($dir) != $guiMMvars($otherdir)} {
        set yesno [tk_messageBox -message "You must set the input and output directories to be the same."  -title "Input Dir .ne. Output Dir"  -type ok]
        set yesno yes
        switch -- $yesno {
          yes {
            set guiMMvars($otherdir) $tryme
          }
          no {
            # do nothing
          }
         }
      }
   }
}


# Procedure: guiMMviewMeshVtk
proc guiMMviewMeshVtk {} {

  global gObjects
  set meshobj $gObjects(mesh_object)
  set meshpd $gObjects(mesh_object_pd)
  global gRen3d
  global gObjects
  global gOptions
  global guiMMvars

  if {$guiMMvars(viewMeshVtkFlag) ==  0} {
      catch {vis_pRm $gRen3d $meshpd}
      return
  }

  # check to make sure we've read in the mesh
  if {[repos_exists -obj $meshobj] == 0} {
     set guiMMvars(viewMeshVtkFlag) 0
     puts "ERROR:  mesh apparently not loaded."
     return -code error "ERROR:  mesh apparently not loaded."
  }

  if {[repos_exists -obj $meshpd] == 0} {
    $meshobj GetPolyData -result $meshpd
  }

  set gRen3d [vis_gRenWin_3D]
  vis_pRepos $gRen3d $meshpd
  set myact [vis_pGetActor $gRen3d $meshpd]
  set myprop [$myact GetProperty]
  $myprop SetRepresentationToWireframe
  $myprop SetInterpolationToFlat
  $myprop SetColor 0.8900 0.8100 0.3400
  $myprop SetSpecularColor 1 1 1
  $myprop SetSpecular 0.3
  $myprop SetSpecularPower 20
  $myprop SetAmbient 0.2
  $myprop SetDiffuse 0.8
  [$gRen3d GetRenderWindow] Render
}


