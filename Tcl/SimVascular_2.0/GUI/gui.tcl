# Procedure: bcGUIbaselineRedisplayAllRegions
proc bcGUIbaselineRedisplayAllRegions {} {

  global symbolicName
  global bcGUIbaselineRegions
  global bcGUImagWindow
  set lbox $symbolicName(bcGUIbaselineRegionListbox)
  set ren $bcGUImagWindow(ren)
  set regions $bcGUIbaselineRegions

  $lbox delete 0 end

  foreach region $regions {
    set id [file tail [lindex $region 0]]

    set nextRegion /bcGUI/region/$id
    set visobj /bcGUI/region/vis/$id
    catch {repos_delete -obj $visobj}

    geom_triangulate $nextRegion $visobj
    catch {vis_pRm $ren $visobj}
    set actor [vis_pRepos $ren $visobj]
    [$actor GetProperty] SetColor 0 1 0
    [$actor GetProperty] SetOpacity 0.33

    $lbox insert end [list "region_$id" [lindex $region 1]]
  }

  # update display
  [$ren GetRenderWindow] Render
}


# Procedure: bcGUIbaselineSaveRegions
proc bcGUIbaselineSaveRegions {} {
  global gFilenames
  set mydir $gFilenames(baseline_regions_dir)
  if {[file exists $mydir] == 0} {
     file mkdir $mydir
  } elseif {[file isdirectory $mydir] == 0} {
     return -code error "ERROR:  $mydir is not a directory."
  }
  global bcGUIbaselineRegions
  foreach region $bcGUIbaselineRegions {
    set id [file tail [lindex $region 0]]
    repos_writeVtkPolyData -obj [lindex $region 0] -type ascii -file [file join $mydir region_$id.vtk]
  }

  # create a file to track parameters and load back in regions
  set fp [open [file join $mydir baseline_regions.tcl] "w"]
  puts $fp "# geodesic_baseline_regions 1.0"
  puts $fp "global bcGUIbaselineRegions"
  puts $fp "set bcGUIbaselineRegions \{\}"
  puts $fp "repos_deleteList \[repos_subList /bcGUI/region/*\]"
  foreach region $bcGUIbaselineRegions {
      set id [file tail [lindex $region 0]]
      puts $fp "lappend bcGUIbaselineRegions \{$region\}"
      puts $fp "repos_readVtkPolyData -file [file join $mydir region_$id.vtk]  -obj [lindex $region 0]"
  }

  # save the baseline equations
  global gBC
  puts $fp "global gBC"
  puts $fp "set gBC(baselineCorrectionX) \{$gBC(baselineCorrectionX)\}"
  puts $fp "set gBC(baselineCorrectionY) \{$gBC(baselineCorrectionY)\}"
  puts $fp "set gBC(baselineCorrectionZ) \{$gBC(baselineCorrectionZ)\}"

  close $fp
}


