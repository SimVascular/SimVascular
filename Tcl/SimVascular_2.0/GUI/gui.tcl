# Procedure: bcGUIwriteBCTDAT
proc bcGUIwriteBCTDAT {} {

  global symbolicName

  global gImageVol
  #img_guessRASmin $gImageVol(filename)

  global gFilenames
  set mymatrix tmp-bcGUIwriteBCTDAT-matrix
  catch {$mymatrix Delete}
  img_calcTransformMatrixToRAS $gFilenames(phase_contrast_geometric) $mymatrix

  set outFileName $gFilenames(bct_dat_file)

  # parameters needed to create bct & do fft
  global gBC
  global bcGUIbctdatNumFourierTerms
  global bcGUIbctdatNumPoints
  set period $gBC(period)
  set npts $bcGUIbctdatNumPoints
  set nterms $bcGUIbctdatNumFourierTerms

  global bcGUIbctdatFrames

  catch {repos_deleteList [repos_subList /tmp/bcGUIwriteBCTDAT/*/ras]}

  global gBC
  set allframes [string_parse $bcGUIbctdatFrames 1 $gBC(numSlices)]
  foreach frame $allframes {

    set inletMeshFace /gBC/pcdata/inletMeshFace/$frame
    if {[repos_exists -obj $inletMeshFace] == 0} {
      puts "ERROR:  No usable velocity map found for frame $frame."
      continue
    }

    # do the transformation
    catch {repos_delete -obj /tmp/bcGUIwriteBCTDAT/$frame/ras}
    geom_applyTransformMatrix $inletMeshFace $mymatrix /tmp/bcGUIwriteBCTDAT/$frame/ras
  }

  set fp [open $outFileName "w"]

  foreach frame $allframes {
    set object /tmp/bcGUIwriteBCTDAT/$frame/ras
    set pointer($frame) [repos_exportToVtk -src $object]
    set pointData($frame) [$pointer($frame) GetPointData]
    set vectors($frame) [$pointData($frame) GetVectors]
    set points($frame) [$pointer($frame) GetPoints]
  }

  set numPoints [$points([lindex $allframes 0]) GetNumberOfPoints]
  puts "number of points: $numPoints"

  set numTimeSteps [llength $allframes]
  puts "numTimeSteps: $numTimeSteps"

  set timeStep [expr double($period)/double($numTimeSteps)]
  puts "Time Step: $timeStep"

  incr numTimeSteps

  #
  #  this code does not fft the nodal velocities
  #

  if {$nterms <= 0} {
    # first line of bct.dat
    puts $fp "$numPoints $numTimeSteps"
    for {set ptId 0} {$ptId < $numPoints} {incr ptId} {
      set pt [$points([lindex $allframes 0]) GetPoint $ptId]
      puts $fp [format "%e %e %e %i" [lindex $pt 0] [lindex $pt 1] [lindex $pt 2] $numTimeSteps]
      set objId 0
      foreach frame $allframes {
        set time [expr $objId*$timeStep]
        set v [$vectors($frame) GetTuple3 $ptId]
        puts $fp [format "%e %e %e %e" [lindex $v 0] [lindex $v 1] [lindex $v 2] $time]
        incr objId
      }
      # output the first point again
      set v [$vectors([lindex $allframes 0]) GetTuple3 $ptId]
      puts $fp [format "%e %e %e %e" [lindex $v 0] [lindex $v 1] [lindex $v 2] $period]
    }
  }

  #
  #  this code does FFT the nodal velocities
  #

  if {$nterms >= 1} {
    set dt [expr double($period)/double([expr $npts - 1])]
    set omega [expr 2.0*[math_pi]/$period]
    puts "period: $period"
    puts "dt: $dt"
    puts "omega: $omega"
    puts "num fourier terms: $nterms"
    puts "Time Step: $timeStep"
    puts "numTimeSteps: $numTimeSteps"

    # first line of bct.dat
    puts $fp "$numPoints $npts"
    for {set ptId 0} {$ptId < $numPoints} {incr ptId} {
      set pt [$points([lindex $allframes 0]) GetPoint $ptId]
      puts $fp [format "%e %e %e %i" [lindex $pt 0] [lindex $pt 1] [lindex $pt 2] $npts]
      set objId 0
      set vx {}
      set vy {}
      set vz {}
      foreach frame $allframes {
        set time [expr $objId*$timeStep]
        set v [$vectors($frame) GetTuple3 $ptId]
        lappend vx [list $time [lindex $v 0]]
        lappend vy [list $time [lindex $v 1]]
        lappend vz [list $time [lindex $v 2]]
        incr objId
      }
      # output the first point again
      set v [$vectors([lindex $allframes 0]) GetTuple3 $ptId]
      lappend vx [list $period [lindex $v 0]]
      lappend vy [list $period [lindex $v 1]]
      lappend vz [list $period [lindex $v 2]]
      # now do the fft in the x dir
      set terms [math_FFT -pts $vx -numInterpPts 64 -nterms $nterms]
      set vx [math_inverseFFT -terms $terms -t0 0  -dt $dt -omega $omega -numPts $npts]
      # now do the fft in the y dir
      set terms [math_FFT -pts $vy -numInterpPts 64 -nterms $nterms]
      set vy [math_inverseFFT -terms $terms -t0 0  -dt $dt -omega $omega -numPts $npts]
      # now do the fft in the z dir
      set terms [math_FFT -pts $vz -numInterpPts 64 -nterms $nterms]
      set vz [math_inverseFFT -terms $terms -t0 0  -dt $dt -omega $omega -numPts $npts]
      for {set i 0} {$i < $npts} {incr i} {
        puts $fp [format "%e %e %e %e" [lindex [lindex $vx $i] 1]  [lindex [lindex $vy $i] 1]  [lindex [lindex $vz $i] 1] [lindex [lindex $vx $i] 0]]
      }
    }
  }

  close $fp
  puts "File $outFileName written."

  catch {repos_deleteList [repos_subList /tmp/bcGUIwriteBCTDAT/*/ras]}
  catch {$mymatrix Delete}
}


