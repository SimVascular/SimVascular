# Procedure: guiPPchooserAutoPathNew
proc guiPPchooserAutoPathNew {} {

  global symbolicName
  global gPathPoints

  set paths [guiSV_path_get_tree_current_paths_selected]
  if {$paths == ""} {
    puts "ERROR:  Must specify a Path ID before automatically finding path!"
    return -code error "ERROR:  Must specify a Path ID before automatically finding path!"
  }
  if {[llength $paths] > 1} {
    return -code error "Cannot automatically find more than one path!"
  }
  set pathId [lindex $paths 0]
  global symbolicName
  set tv $symbolicName(guiSV_path_tree)
  set maxnum [llength [$tv children .paths.all.$pathId]]

  global guiPPsmoothVolumeChoice
  set choice $guiPPsmoothVolumeChoice

  if {$choice == "volume"} {
    global guiVIB
    set imgobj $guiVIB(repos_image_obj)
    set distobj $guiVIB(repos_distancemap_obj)
    if {[repos_exists -obj $distobj] == 0} {
      return -code error "ERROR:  DistanceMap object does not exist."
    }
  } elseif {$choice == "resample"} {
    global guiRIB
    set imgobj $guiRIB(repos_image_obj)
    set distobj $guiRIB(repos_distancemap_obj)
    if {[repos_exists -obj $distobj] == 0} {
      return -code error "ERROR:  DistanceMap object does not exist."
    }
    if {[repos_exists -obj $imgobj] == 0} {
      return -code error "ERROR:  Resampled object does not exist."
    }
  } elseif {$choice == "seed_resample"} {
    global guiRIB
    set imgobj $guiRIB(repos_seed_thr_obj)
    set distobj $guiRIB(repos_distancemap_obj)
    if {[repos_exists -obj $distobj] == 0} {
      return -code error "ERROR:  DistanceMap object does not exist."
    }
    if {[repos_exists -obj $imgobj] == 0} {
      return -code error "ERROR:  Seeded threshold object does not exist."
    }
  } elseif {$choice == "seed_volume"} {
    global guiVIB
    set imgobj $guiVIB(repos_seed_thr_obj)
    set distobj $guiVIB(repos_distancemap_obj)
    if {[repos_exists -obj $distobj] == 0} {
      return -code error "ERROR:  DistanceMap object does not exist."
    }
    if {[repos_exists -obj $imgobj] == 0} {
      return -code error "ERROR:  Seeded threshold object does not exist."
    }
  } else {
    return -code error "ERROR:  invalid choice ($choice)."
  }

  if {[string trim $pathId] == ""} {
    puts "ERROR:  Must specify a Path ID before selecting points!"
    return -code error "ERROR:  Must specify a Path ID before selecting points!"
  }

  global guiPPchooserGuessThrValue
  set thr $guiPPchooserGuessThrValue

  if {$thr <= 0 && $choice != "seed_volume" && $choice != "seed_resample"} {
    return -code error "ERROR:  Threshold ($thr) must be > 0."
  }

  # we allow 3 cases:
  #  1.  just 2 points, obviously autopath between them
  #  2.  > 2 points, no current selection, autopath between
  #      last two points
  #  3.  if there is a current selection, autopath between the two
  #      points (NOT IMPLEMENTED YET!!!)

  # check that only two points have been specified in the listbox
  if {$maxnum < 2} {
    puts "ERROR:  You must select at least a beginning and ending point."
    return -code error "ERROR:  You must select at least a beginning and ending point."
  } elseif {$maxnum == 2} {
    set startPos $gPathPoints($pathId,0)
    set insertAtPoint 0
    set stopPos $gPathPoints($pathId,1)
  } else {
    set insertAtPoint $gPathPoints($pathId,[expr $maxnum - 3])
    set startPos $gPathPoints($pathId,[expr $maxnum - 2])
    set stopPos $gPathPoints($pathId,[expr $maxnum - 1])
  }

  # let vtk find the starting and ending voxel
  set startIJK {}
  set stopIJK {}
  set startIJK [img_computeStructuredCoord -image $imgobj -pt $startPos]
  set stopIJK [img_computeStructuredCoord -image $imgobj -pt $stopPos]

  if {$startIJK == ""} {
    return -code error "ERROR: starting point ($startPos) not inside of volume!"
  }
  if {$stopIJK == ""} {
      return -code error "ERROR: stopping point ($stopPos) not inside of volume!"
  }

  # check pixel value here

  set start_pixel_value [lindex $startIJK 2]
  set stop_pixel_value [lindex $stopIJK 2]
  set startIJK [lindex $startIJK 0]
  set stopIJK [lindex $stopIJK 0]

  puts "start: $startIJK  pixel_value: $start_pixel_value"
  puts "stop: $stopIJK  pixel_value: $stop_pixel_value"

  if {$choice == "seed_volume" || $choice == "seed_resample"} {
    if {([expr $start_pixel_value - 0.5] < 0) || ([expr $stop_pixel_value - 0.5] < 0)} {
      return -code error "ERROR:  start or stop point not in segmentation!"
    }
  } else {
    if {($start_pixel_value < $thr) || ($stop_pixel_value < $thr)} {
      return -code error "ERROR:  start or stop point not in segmentation!"
    }
  }

  # check distance map values here

  if {$choice == "volume" || $choice ==  "seed_volume"} {
     if {![repos_exists -obj $guiVIB(repos_distancemap_obj)]} {
        return -code error "ERROR: DistanceMap does not exist for volume!"
     }
     set distanceval_start [lindex [img_computeStructuredCoord -image $guiVIB(repos_distancemap_obj) -pt $startPos] 2]
     set distanceval_stop [lindex [img_computeStructuredCoord -image $guiVIB(repos_distancemap_obj) -pt $stopPos] 2]
    if {$distanceval_start == "" || $distanceval_stop == ""} {
      return -code error "ERROR: could not get distance value from DistanceMap!"
    }
  }

  if {$choice == "resample" || $choice ==  "seed_resample"} {
     if {![repos_exists -obj $guiRIB(repos_distancemap_obj)]} {
        return -code error "ERROR: DistanceMap does not exist for resampled volume!"
     }
     set distanceval_start [lindex [img_computeStructuredCoord -image $guiRIB(repos_distancemap_obj) -pt $startPos] 2]
     set distanceval_stop [lindex [img_computeStructuredCoord -image $guiRIB(repos_distancemap_obj) -pt $stopPos] 2]
    if {$distanceval_start == "" || $distanceval_stop == ""} {
      return -code error "ERROR: could not get distance value from DistanceMap!"
    }
  }

  # allow user to fake it to get the initial starting point of the
  # distance map as the first point in the path
  if {$startIJK == $stopIJK} {
     set minqstop 0
  } else {
     if {$distanceval_start >= $distanceval_stop} {
       return -code error "ERROR:  start distance ($distanceval_start) cannot be larger than stop ($distanceval_stop)"
     }
      set minqstop [expr int($distanceval_start)]
  }

  set tmppath /tmp/guiPPchooserAutoPathNew/path
  catch {repos_delete -obj $tmppath}

  # find the medial axis path
  global guiPPchooserNumThinningPasses
  set thinpasses $guiPPchooserNumThinningPasses
  if {[string trim $thinpasses] == "" || $thinpasses == "0"} {
    img_findPath -src $distobj -stop $stopIJK -dst $tmppath -min_dist $minqstop
  } else {
    img_findPath -src $distobj -stop $stopIJK -dst $tmppath -min_dist $minqstop -thin_passes $thinpasses
  }

  geom_getPts $tmppath pts
  catch {repos_delete -obj $tmppath}

  # update gPathPoints
  for {set i 0} {$i < [llength $pts]} {incr i} {
     set gPathPoints($pathId,[expr $i + $insertAtPoint]) [lindex $pts $i]
  }
  global guiSVvars
  set gPathPoints($pathId,name) $guiSVvars(path_entry_path_name)
  set gPathPoints($pathId,numSplinePts) $guiSVvars(path_entry_number_spline_pts)

  # create a new spline for the path
  set gPathPoints($pathId,splinePts) {}
  guiPPchooserSplinePts

  guiSV_path_update_tree

  set tv $symbolicName(guiSV_path_tree)
  $tv see .paths.all.$pathId
  $tv selection set .paths.all.$pathId

  # update displayed graphics
  guiPPchooserShowPath
  guiPPchooserShowSpline
}


# Procedure: guiPPchooserDeletePoint
proc guiPPchooserDeletePoint {} {
   global symbolicName
   global gPathPoints
   set tv $symbolicName(guiSV_path_tree)
  set selectedPoints [guiSV_path_get_tree_current_points_selected]
  if {[llength $selectedPoints] == 0} {
    return -code error "Must select points in a path first!"
  }

  # first unset the points
  foreach spt $selectedPoints {
    set pathId [lindex $spt 0]
    set posId  [lindex $spt 1]
    set updateMePaths($pathId) [llength [$tv children .paths.all.$pathId]]
    unset gPathPoints($pathId,$posId)
  }

  # now loop over paths with deleted points and condense
  foreach p [array names updateMePaths] {
    set max $updateMePaths($p)
    set newId 0
    set numdel 0
    for {set i 0} {$i < $max} {incr i} {
  if [info exists gPathPoints($p,$i)] {
      set gPathPoints($p,$newId) $gPathPoints($p,$i)
      incr newId
  } else {
            incr numdel
  }
    }
    for {set i [expr $max - $numdel]} {$i < $max} {incr i} {
  catch {unset gPathPoints($p,$i)}
    }
    set gPathPoints($p,splinePts) {}
    guiPPchooserSplinePts $p
  }

  guiSV_path_update_tree
 
  guiPPchooserShowPath
  guiPPchooserShowSpline
}


# Procedure: guiPPchooserShowAllPaths
proc guiPPchooserShowAllPaths {} {
  global gPathPoints
  global symbolicName
  global gRen3d

  # find the current list of paths to choose from
  set names [lsort -dictionary [array names gPathPoints *,name]]
  set allPathIds {}
  foreach n $names {
    set busted [split [string trim $n] ,]
    lappend allPathIds [lindex $busted 0]
  }

  # do show path stuff
  global gRen3dFreeze
  set gRen3dFreeze 1

  global gOptions
  set color $gOptions(color_for_all_paths)
  set thick $gOptions(line_width_for_all_paths)

  foreach id $allPathIds {

    foreach path $id {
      set indexes [array names gPathPoints $path,*]
      set ptIndex {}
      set numSplinePts {}
      foreach index $indexes {
        set pieces [split [string trim $index] ,]
        set tok1 [lindex $pieces 0]
        set tok2 [lindex $pieces 1]
        if {$path == $tok1} {
    if {$tok2 == "name"} {
            # skip
    } elseif {$tok2 == "numSplinePts"} {
            # skip
          } elseif {$tok2 == "splinePts"} {
            # skip
    } else {
            lappend ptIndex $tok2
    }
        }
      }
    }

    if {$ptIndex == ""} {
      puts "Warning:  pathId $path contains no points!"
      return
    }

    set ptIndex [lsort -integer $ptIndex]

    set pts {}

    foreach ptId $ptIndex {
      set pt $gPathPoints($path,$ptId)
      lappend pts $pt
    }

    set obj  /tmp/pathplan/allpaths/$id

    if {[llength $ptIndex] > 1} {
      if {[repos_exists -obj $obj] == "1"} {
        repos_delete -obj $obj
        vis_pRm $gRen3d $obj
      }
      geom_openLinesFromPts $pts $obj
      repos_setLabel -obj $obj -key color -value $color
      gdscGeneralView $gRen3d $obj
      set myact [vis_pGetActor $gRen3d $obj]
      set myprop [$myact GetProperty]
      $myprop SetLineWidth $thick
      $myprop SetInterpolationToFlat
    }
  }

  set gRen3dFreeze 0
  vis_render $gRen3d
  return GDSC_OK
}


