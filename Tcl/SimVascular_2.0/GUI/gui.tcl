# Procedure: guiPPchooserSplineInteractorUpdatePath
proc guiPPchooserSplineInteractorUpdatePath {} {
  global gRen3d
  global gPathPoints
  global symbolicName

  global guiPPchooserSplineInteractorFlag
  set flag $guiPPchooserSplineInteractorFlag
  global guiSVvars

  set paths [guiSV_path_get_tree_current_paths_selected]
  if {$paths == ""} {
    puts "ERROR:  Must specify a Path ID setting with spline!"
    return -code error "ERROR:  Must specify a Path ID before selecting points!"
  }
  if {[llength $paths] > 1} {
    return -code error "Cannot replace more than one path!"
  }
  set pathId [lindex $paths 0]
  set tv $symbolicName(guiSV_path_tree)
  set maxnum [llength [$tv children .paths.all.$pathId]]

  set name $guiSVvars(path_entry_path_name)
  set numSplinePts $guiSVvars(path_entry_number_spline_pts)

  if {$flag == 1} {
    set pts [vis_splineWidgetGetPts $gRen3d guiPP]
  } else {
    set pts [vis_splineImageWidgetGetPts $gRen3d guiPP]
  }
  set targetID $pathId

  # update target gPathPoints
  foreach i [array names gPathPoints $targetID,*] {
    unset gPathPoints($i)
  }
  for {set i 0} {$i < [llength $pts]} {incr i} {
     set gPathPoints($targetID,$i) [lindex $pts $i]
  }
  # preserve name and number of spline points if it exists
  set gPathPoints($targetID,name) $name
  set gPathPoints($targetID,numSplinePts) $numSplinePts

  set gPathPoints($targetID,splinePts) {}
  guiPPchooserSplinePts

  guiSV_path_update_tree
  set tv $symbolicName(guiSV_path_tree)
  $tv see .paths.all.$targetID
  $tv selection set .paths.all.$targetID
}


# Procedure: guiPPchooserSplinePts
proc guiPPchooserSplinePts { args} {

  set ptpd /tmp/guiPPchooserSplinePts
  catch {repos_delete -obj $ptpd}

  global symbolicName
  if {$args == ""} {
    set paths [guiSV_path_get_tree_current_paths_selected]
  } else {
    set paths $args
  }

  if {$paths == ""} {
    puts "ERROR:  Must specify a Path ID before creating spline!"
    return -code error "ERROR:  Must specify a Path ID before creating spline!"
  }
  if {[llength $paths] > 1} {
    return -code error "Cannot spline more than one path!"
  }

  global symbolicName
  global gPathPoints

  set id [lindex $paths 0]

  set indexes [array names gPathPoints $id,*]
  set ptIndex {}
  set numSplinePts {}
  foreach index $indexes {
    set pieces [split [string trim $index] ,]
    set tok1 [lindex $pieces 0]
    set tok2 [lindex $pieces 1]
    if {$tok2 == "name"} {
      # skip
    } elseif {$tok2 == "numSplinePts"} {
      set numSplinePts $gPathPoints($id,numSplinePts)
    } elseif {$tok2 == "splinePts"} {
      # skip
    } else {
      lappend ptIndex $tok2
    }
  }

  # can only create a spline if we have more than 2 points
  if {[llength $ptIndex] < 2} {
    set gPathPoints($id,splinePts) {}
    return
  }

  set ptIndex [lsort -integer $ptIndex]

  # create an ordered list of points
  set pts {}
  foreach i $ptIndex {
    lappend pts $gPathPoints($id,$i)
  }

  geom_mkPtsPd $pts $ptpd

  if {$numSplinePts == ""} {
      puts "Warning:  Number of requested spline points for path $id"
      puts "          not specified.  Defaulting to 100."
      set numSplinePts 100
      set gPathPoints($id,numSplinePts) $numSplinePts
  }

  set flag 0
  # spline points
  set gPathPoints($id,splinePts) [geom_splinePtsToPathPlan -src $ptpd  -numOutputPts $numSplinePts  -flag $flag]

  catch {repos_delete -obj $ptpd}
}


