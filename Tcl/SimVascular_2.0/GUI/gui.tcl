       if {$guiVISvarsPrev(cutplane_displayVelocity) == "off"} {
          # dont need to do anything
          puts "off, off -- do nothing"
       } elseif {$guiVISvarsPrev(cutplane_displayVelocity) == "as vectors"} {
          puts "turn off vectors"
          vis_vRm $ren $velocityPD
          set guiVISvarsPrev(cutplane_displayVelocity) $guiVISvars(cutplane_displayVelocity)
