# Procedure: lsGUItranslateSeg
proc lsGUItranslateSeg { type} {

  global symbolicName
  global lsGUIcurrentPathNumber
  set pathId $lsGUIcurrentPathNumber
  global gPathPoints
  set path $gPathPoints($pathId,splinePts)

  global lsGUIcurrentPositionNumber
  set posId $lsGUIcurrentPositionNumber

  # need to translate hand/analytic to proper type
  if {$type == "analytic"} {
    global lsGUIanalContourType
    set type $lsGUIanalContourType
  } elseif {$type == "hand"} {
    global lsGUIhandContourType
    set type $lsGUIhandContourType
  }

  if {$type == "levelset"} {
    set seg /lsGUI/$pathId/$posId/ls
    set segoriented $seg/oriented
  } elseif {$type == "threshold"} {
    set baseName /lsGUI/$pathId/$posId
    set seg $baseName/thr/selected
    set segoriented $baseName/thr/oriented
  } else {
    return -code error "Invalid segmentation type ($type)."
  }

  if {[repos_exists -obj $seg] == 0} {
    return
  }

  # keep the tags on the segmentation
  foreach key [repos_getLabelKeys -obj $seg] {
    set seg_keys($key) [repos_getLabel -obj $seg -key $key]
  }
  foreach key [repos_getLabelKeys -obj $segoriented] {
    set segoriented_keys($key) [repos_getLabel -obj $segoriented -key $key]
  }

  global lsGUItransX
  global lsGUItransY

  set beforetrans   /lsGUI/before-translate

  catch {repos_delete -obj $beforetrans}
  geom_copy -src $seg -dst $beforetrans
  catch {repos_delete -obj $seg}
  catch {repos_delete -obj $segoriented}
  geom_translate -src $beforetrans -dst $seg -vec [list $lsGUItransX $lsGUItransY 0.0]

  array set items [lindex $path $posId]
  set pos [TupleToList $items(p)]
  set nrm [TupleToList $items(t)]
  set xhat [TupleToList $items(tx)]
  geom_orientProfile -src $seg -path_pos $pos -path_tan $nrm -path_xhat $xhat -dst $segoriented

  # reattach tags
  foreach key [array names seg_keys] {
    repos_setLabel -obj $seg -key $key -value $seg_keys($key)
  }
  foreach key [array names segoriented_keys] {
    repos_setLabel -obj $segoriented -key $key -value $segoriented_keys($key)
  }
  # update seg
  set translation_in_x 0.0
  set translation_in_y 0.0
  if {[info exists seg_keys(translation_in_x)] == 1} {
    repos_clearLabel -obj $seg -key translation_in_x
    set translation_in_x $seg_keys(translation_in_x)
  }
  repos_setLabel -obj $seg -key translation_in_x -value [expr $translation_in_x + $lsGUItransX]
  if {[info exists seg_keys(translation_in_y)] == 1} {
    repos_clearLabel -obj $seg -key translation_in_y
    set translation_in_y $seg_keys(translation_in_y)
  }
  repos_setLabel -obj $seg -key translation_in_y -value [expr $translation_in_y + $lsGUItransY]
  # update segoriented
  set translation_in_x 0.0
  set translation_in_y 0.0
  if {[info exists segoriented_keys(translation_in_x)] == 1} {
    repos_clearLabel -obj $segoriented -key translation_in_x
    set translation_in_x $segoriented_keys(translation_in_x)
  }
  repos_setLabel -obj $segoriented -key translation_in_x -value [expr $translation_in_x + $lsGUItransX]
  if {[info exists segoriented_keys(translation_in_y)] == 1} {
    repos_clearLabel -obj $segoriented -key translation_in_y
    set translation_in_y $segoriented_keys(translation_in_y)
  }
  repos_setLabel -obj $segoriented -key translation_in_y -value [expr $translation_in_y + $lsGUItransY]

  # update display
  lsGUIchangeFrame 0
}


