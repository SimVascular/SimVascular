}


# Procedure: guiVISppEnableInteraction
proc guiVISppEnableInteraction {} {
  global gInteractorInteracting
  set gInteractorInteracting 0

  set surfacePD /guiVIS/vol/surface_mesh
  set bbox [geom_bbox -obj $surfacePD]

  global guiVISvars
  global gRen3d
  if {$guiVISvars(p1) == "" || $guiVISvars(p2) == "" || $guiVISvars(p3) == ""} {
    set cX [expr double([lindex $bbox 1] + [lindex $bbox 0])/2.0]
    set cY [expr double([lindex $bbox 3] + [lindex $bbox 2])/2.0]
    set cZ [expr double([lindex $bbox 5] + [lindex $bbox 4])/2.0]
    vis_planeWidgetSetCenter $gRen3d guiVIS $cX $cY $cZ
    vis_planeWidgetSetNormal $gRen3d guiVIS 0 0 1
    set guiVISvars(p1) [vis_planeWidgetGetOrigin $gRen3d guiVIS]
    set guiVISvars(p2) [vis_planeWidgetGetPoint1 $gRen3d guiVIS]
    set guiVISvars(p3) [vis_planeWidgetGetPoint2 $gRen3d guiVIS]
  }
}


# Procedure: guiVISppEndInteraction
proc guiVISppEndInteraction {} {
  global gInteractorInteracting
