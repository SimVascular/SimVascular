  return -code error "Should not reach here!"
}


# Procedure: guiVOLRENupdate
proc guiVOLRENupdate { type pt args} {

  puts "update transfer functions($type $pt)!"

  global guiVOLRENvars

  if {$type == "color"} {
    set  color [tk_chooseColor]
    if {$color == ""} return
    set r [expr (0x[string range $color 1 2])/255.0]
    set g [expr (0x[string range $color 3 4])/255.0]
    set b [expr (0x[string range $color 5 6])/255.0]
    set guiVOLRENvars(color_pt_$pt) [list $r $g $b]
  }

  # only for entire volume for now
  set repos_image volume_image
  set image [repos_exportToVtk -src $repos_image]
  set rng [$image GetScalarRange]
  set low [lindex $rng 0]
  set hi [lindex $rng 1]
  set range [expr $hi - $low]

  for {set i 1} {$i <= 4} {incr i} {
      lappend opacityFn [list [expr $low + $guiVOLRENvars(intensity_pt_$i) * $range] $guiVOLRENvars(opacity_pt_$i)]
  }
 
  for {set i 1} {$i <= 4} {incr i} {
     set r [lindex $guiVOLRENvars(color_pt_$i) 0]
     set g [lindex $guiVOLRENvars(color_pt_$i) 1]
     set b [lindex $guiVOLRENvars(color_pt_$i) 2]
     lappend colorFn [list [expr $low + $guiVOLRENvars(intensity_pt_$i) * $range] $r $g $b] 
  }

  global guiVIB
  set guiVIB(opacityFunction) $opacityFn
  set guiVIB(colorFunction) $colorFn

  if {$type == "init"} return

  if {$type == "intensity"} {
    set type opacity
  }

  volGUIupdateVolRender guiVIB $type
}


