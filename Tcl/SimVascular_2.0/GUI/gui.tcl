# Procedure: guiPP3dSplWidData
proc guiPP3dSplWidData {} {
  global gRen3d
  global guiPPchooserNumHandles
  global gInteractorRen
  global guiPPchooserSplineInteractorFlag
  set flag $guiPPchooserSplineInteractorFlag
  if {$flag == 1} {
    set pts [vis_splineWidgetGetPts $gInteractorRen guiPP]
  } else {
    set pts [vis_splineImageWidgetGetPts $gInteractorRen guiPP]
  }
  global guiPPorgSplinePts
  global guiPPchooserFixAxisX
  global guiPPchooserFixAxisY
  global guiPPchooserFixAxisZ
  for {set i 0} {$i < [llength $pts]} {incr i} {
    set orgpt [lindex $guiPPorgSplinePts $i]
    set newpt [lindex $pts $i]
    if {$guiPPchooserFixAxisX != 0} {
       set x [lindex $orgpt 0]
    } else {
       set x [lindex $newpt 0]
    }
    if {$guiPPchooserFixAxisY != 0} {
       set y [lindex $orgpt 1]
    } else {
       set y [lindex $newpt 1]
    }
    if {$guiPPchooserFixAxisZ != 0} {
       set z [lindex $orgpt 2]
    } else {
       set z [lindex $newpt 2]
    }
    lappend fixedpts [list $x $y $z]
  }

  if {$flag == 1} {
    vis_splineWidgetUpdatePts $gRen3d guiPP $fixedpts
  } else {
    global guiPPchooserFixAxisPlane
      if {$guiPPchooserFixAxisPlane == 0} {
        vis_splineImageWidgetUpdatePts $gRen3d guiPP $fixedpts
      } else {
        vis_splineImageWidgetUpdatePtsPlaneOnly $gRen3d guiPP $fixedpts
      }
  }
  vis_render $gRen3d
}


