# Procedure: guiRUNSOLVERlaunchSolver
proc guiRUNSOLVERlaunchSolver { system} {

  global guiRUNSOLVERvars
  global tcl_platform

  set user $guiRUNSOLVERvars(user)

  set num_procs 0
  set num_hosts 0

  if {$system == "no_mpi"} {

    set num_procs 1
    set num_hosts 1

  } elseif {$system == "localhost"} {

    #
    #  localhost
    #

    set num_procs $guiRUNSOLVERvars(num_procs_localhost)
    if {$num_procs == 0} {
      return -code error "ERROR:  No procs ($num_procs)"
    }

    if {$tcl_platform(platform) == "windows"} {
      set hosts {}
    } else {
	set hosts "-localonly"
    }

  } elseif {$system == "osmsc"} {

    #
    # osmsc setup
    #

    set n2 $guiRUNSOLVERvars(num_procs_osmsc02)
    set n4 $guiRUNSOLVERvars(num_procs_osmsc04)
  
    set num_procs [expr $n2 + $n4 ]

    if {$n2 > 0} {
      incr num_hosts
    }
    if {$n4 > 0} {
      incr num_hosts
    }
 
    if {$num_procs == 0} {
      return -code error "ERROR:  No procs ($num_procs)"
    }

    set hosts "-hosts $num_hosts"

    if {$n2 > 0} {
     set hosts "$hosts 192.168.1.242 $n2"
    }

    if {$n4 > 0} {
     set hosts "$hosts 192.168.1.241 $n4"
    }

  } elseif {$system == "msu"} {

    set host_ip_pairs [list  [list cvtm01 35.9.139.56]  [list cvtm02 35.9.139.57]  [list cvtm03 35.9.139.62]  [list cvtm04 35.9.139.67]  [list cvtm05 35.9.139.73]]

    foreach i $host_ip_pairs {
      set h  [lindex $i 0]
      set ip [lindex $i 1]
      set n $guiRUNSOLVERvars(num_procs_$h)
      if {$n != 0} {
        set num_procs [expr $num_procs + $n]
        set useip($ip) $n
        incr num_hosts
      }
    }

    if {$num_hosts == 0 || $num_procs == 0} {
      return -code error "ERROR:  No hosts or procs ($num_hosts,$num_procs)"
    }

    set hosts "-hosts $num_hosts"

    if {$user == "zambran2"} {
      set personal_ip 35.9.139.57
    } elseif {$user == "nwilson"} {
      set personal_ip 35.9.139.57
    } elseif {$user == "gharahih"} {
      set personal_ip 35.9.139.57
    } elseif {$user == "labuser"} {
      set personal_ip 35.9.139.57
    } else {
      return -code error "INVALID user ($user)!"
    }

    # make sure master runs on users machine if possible
    if [info exists useip($personal_ip)] {
      set hosts "$hosts $personal_ip $useip($personal_ip)"
      unset useip($personal_ip)
    }

    # order ip addresses lowest to highest
    foreach i [lsort -dictionary [array names useip]] {
      set hosts "$hosts $i $useip($i)"
    }
 
  } else {

    return -code error "ERROR: unknown system ($system)."

  }

  set rundir $guiRUNSOLVERvars(init_dir)
  if {![file isdirectory $rundir]} {
   return -code error "ERROR:  Run directory does not exist ($rundir)!\n"
  }

  set fullrundir $rundir

  if {$guiRUNSOLVERvars(rundir_add_date)} {
     set fullrundir [file join $rundir [clock format [clock seconds] -format "%m-%d-%Y-%H%M%S"]]
     file mkdir $fullrundir
      file copy [file join $rundir restart.0.1] $fullrundir
      file copy [file join $rundir geombc.dat.1] $fullrundir
      foreach fn [glob [file join $rundir *.inp]] {
        file copy $fn $fullrundir
      }
      foreach fn [glob [file join $rundir *.dat]] {
        file copy $fn $fullrundir
      }
  
      #file copy [file join $rundir numstart.dat] $fullrundir

      #if {[file exists [file join $rundir bct.dat]] || [file exists [file join $rundir bct1.dat]]} {
      #  foreach fn [glob [file join $rundir bct*.dat]] {
      #    file copy $fn $fullrundir
      #  }
      #}

      if {[file exists [file join $rundir bct.vtp]] || [file exists [file join $rundir bct1.vtp]]} {
        foreach fn [glob [file join $rundir bct*.vtp]] {
          file copy $fn $fullrundir
        }
      }

  }

  if {$num_procs == 1} {
    set fullsimdir $fullrundir
  } else {
    set fullsimdir [file join $fullrundir $num_procs-procs_case]
  }

  set guiRUNSOLVERvars(fullrundir) $fullrundir
  set guiRUNSOLVERvars(fullsimdir) $fullsimdir

  #
  #  more files needed by solver
  #

  set fp [open [file join $fullrundir numstart.dat] w]
  fconfigure $fp -translation lf
  puts $fp "$guiRUNSOLVERvars(startnum)"
  close $fp

  # need to copy numstart.dat directly into sim directory if we are restarting
  # simulation
  if {$num_procs != 1 && [file isdirectory $fullsimdir]} {
    catch {file delete [file join $fullsimdir numstart.dat]}
    file copy [file join $fullrundir numstart.dat] [file join $fullsimdir numstart.dat]
  }

  # hardcode the file type
  set file_format binary

  #
  #  Run solver.
  #

  puts "Run Solver."

  global gExternalPrograms
  set PRESOLVER     $gExternalPrograms(cvpresolver)
  set POSTSOLVER    $gExternalPrograms(cvpostsolver)
  set MPIEXEC       $gExternalPrograms(mpiexec)
  set SOLVER        $gExternalPrograms(cvflowsolver)
  set FLOWSOLVER_CONFIG [file dirname $gExternalPrograms(cvflowsolver)] 
  set ADAPTOR       $gExternalPrograms(cvadaptor)

  set stdout_file $guiRUNSOLVERvars(log_dir)

  #
  # use a default if no log dir specified
  #

  if {$stdout_file == ""} {
    set stdout_file "$fullrundir"
  }

  set stdout_file "$stdout_file/$user-stdout_stderr-[clock seconds]"

  # in case log directory doesn't exist
  file mkdir [file dirname $stdout_file]

  set solver_log_file [file join [file dirname $stdout_file] $user-[clock seconds].solver.log.txt]
  set guiRUNSOLVERvars(solver_log_file) $solver_log_file
  set guiRUNSOLVERvars(stdout_file_prefix) $stdout_file
  set 00000_log_file "$guiRUNSOLVERvars(stdout_file_prefix).00000.txt"
  set guiRUNSOLVERvars(00000_log_file) $00000_log_file
  set guiRUNSOLVERvars(histor_dat_file) [file join $fullsimdir histor.dat]

  set fp [open $solver_log_file  w]

  fconfigure $fp -translation lf
  puts $fp "Start running solver...\n\n"

 if {$system == "no_mpi"} {

    set savedpwd [pwd]
    cd $fullrundir
    puts $fp "exec \"$SOLVER\" solver.inp >>& [file join . solver.log] &\n\n"
    set pid [eval exec \"$SOLVER\" solver.inp >>& [file join . solver.log] &]
    cd $savedpwd

  } else {

     set mapcmd ""
   # set map_dir $guiRUNSOLVERvars(map_dir_$system)
   # if {[string trim $map_dir] != ""} {
   #   set mapcmd "-map \"[string trim $map_dir]\""
   # }

    global tcl_platform
    if {$tcl_platform(platform) == "windows"} {
      set npflag "-np"
    } else {
      set npflag "-np"
    }

    puts $fp "exec \"$MPIEXEC\" -wdir \"$fullrundir\" $npflag $num_procs \"$SOLVER\" \"$fullrundir/solver.inp\" \"$stdout_file\" >>& $solver_log_file &\n\n"
    set pid [eval exec \"$MPIEXEC\" -wdir \"$fullrundir\" $npflag $num_procs \"$SOLVER\" \"$fullrundir/solver.inp\" \"$stdout_file\" >>& $solver_log_file &]
 
  }

  puts $fp "Solver pid ($pid)."
  close $fp
}


