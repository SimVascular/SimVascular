# Procedure: guiMMwriteSUPRE
proc guiMMwriteSUPRE {} {
  global gFilenames
  global gObjects
  set mesh $gObjects(mesh_object)
  set prefix $gFilenames(mesh_out_prefix)

  #
  #  Create script file for supre
  #
  foreach i [$mesh Print] {
    set [lindex $i 0] [lindex $i 1]
