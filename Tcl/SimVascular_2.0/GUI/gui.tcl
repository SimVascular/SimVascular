proc lsGUIshowSurface {} {
  global gRen3d
  global gRen3dFreeze
  global lsGUIsurfaceOpacity
  global lsGUIshowSurfaceFlag

  set seg /lsGUI/segment
  set gRen3dFreeze 1

  foreach i [repos_subList $seg/*] {
    catch {vis_pRm $gRen3d $i}
  }

