# Procedure: guiMMreadMesh
proc guiMMreadMesh {} {

  global symbolicName
  global gFilenames
  global gObjects
  global gOptions

  set yesno [tk_messageBox -message "Read the script file from disk?" -default yes -icon question -type yesno]
  if {$yesno == "yes" } {
    guiMMreadScriptFile
  } else {
   set fn [tk_getOpenFile -title "Select mesh filename"]
   if {$fn != ""} {
     set gFilenames(mesh_file) $fn
   }
  }

  # create mesh object if requested

  puts "Read mesh."

  set yesno [tk_messageBox -message "Reading a mesh can take a long time.  Continue anyway?" -default no -icon question -type yesno]

  if  {$yesno == "no"} {
     return
  }

  set meshobj $gObjects(mesh_object)
  set meshpd $gObjects(mesh_object_pd)

  catch {repos_delete -obj $meshobj}
  catch {repos_delete -obj $meshpd}

  mesh_setKernel -name $gOptions(meshing_kernel)
  mesh_newObject -result $meshobj
  global gOptions
  $meshobj SetSolidKernel -name $gOptions(meshing_solid_kernel)
  $meshobj LoadModel -file $gFilenames(atdb_solid_file)
  $meshobj NewMesh  
  $meshobj LoadMesh -file $gFilenames(mesh_file)
  #mesh_newObject -result $meshobj -solidfile $gFilenames(atdb_solid_file)  -meshfile $gFilenames(mesh_file)
