# Procedure: createPREOPloadsaveLoadPCMRI
proc createPREOPloadsaveLoadPCMRI {} {

  global gFilenames
  global gBC
  global gImageVol

  # for pc data
  if {[info exists gFilenames(phase_contrast_geometric)] == 1} {
    if {[string length [string trim $gFilenames(phase_contrast_geometric)]] != 0} {
      set fn $gFilenames(phase_contrast_geometric)
      set roi [list $gBC(voi_x0) $gBC(voi_x1) $gBC(voi_y0) $gBC(voi_y1)]
      catch {repos_deleteList [repos_subList /gBC/pcdata/mag/*]}
      catch {repos_deleteList [repos_subList /gBC/pcdata/pot/*]}
      catch {repos_deleteList [repos_subList /gBC/pcdata/vel/*]}
      catch {repos_deleteList [repos_subList /gBC/pcdata/phase/*]}
      catch {repos_deleteList [repos_subList /gBC/pcdata/warpVel/*]}
      catch {unset gFlow}
      puts "Loading phase contrast data."

      # autodect if we have 1 or 3 component velocity data
      set speed {}
      set collapse {}
      set r {}
      set a {}
      set s {}
      set mag {}

      foreach i [lsort -dictionary [glob [file join [file dirname $fn] I.*]]] {
        if [catch {set header [img_readHeader -file $i]}] continue
        foreach piece $header {
          set [lindex $piece 0] [lindex $piece 1]
        }
        if {$vas_collapse == 0 || $vas_collapse == 7 ||  $vas_collapse == 12} {
           #puts "$i: $vas_collapse is speed"
           lappend speed $i
        } elseif {$vas_collapse == 1 || $vas_collapse == 13} {
           #puts "$i: $vas_collapse is collapse"
           lappend collapse $i
        } elseif {$vas_collapse == 2 || $vas_collapse == 8} {
           #puts  "$i: $vas_collapse is mag"
           lappend mag $i
        } elseif {$vas_collapse == 3 || $vas_collapse == 9} {
           #puts "$i: $vas_collapse is R/L"
           lappend r $i
        } elseif {$vas_collapse == 4 || $vas_collapse == 10} {
           #puts  "$i: $vas_collapse is A/P"
           lappend a $i
        } elseif {$vas_collapse == 5 || $vas_collapse == 11} {
           #puts  "$i: $vas_collapse is S/I"
           lappend s $i
        }

      }

      if {[llength $mag] == 0} {
        puts "ERROR:  No magnitude image slices!"
        return -code error "ERROR:  No magnitude image slices!"
      }

      # we basically are ignoring the user set value of numSlices
      global gBC
      set numSlices $gBC(numSlices)

      # load the magnitude data
      for {set i 1} {$i <= $numSlices} {incr i} {
        set magslice "/gBC/pcdata/mag/$i"
        set imgname [lindex $mag [expr $i - 1]]
        img2_readSliceROI $imgname $magslice $roi
        repos_importVtkImg -src [img_magGrad $magslice] -dst /gBC/pcdata/pot/$i
      }

      if {[llength $r] == $numSlices && [llength $a] == $numSlices && [llength $s] == $numSlices} {
         puts "Found 3-component data."
         set gBC(numVelComp) 3
         # the orientation should be the same for all slices, so just
         # arbitrarily pick a file
   set xyz {}
   set directions {}
         img_findPCMRIorientation [lindex $r 0] xyz directions
         set gBC(xyz) $xyz
         set gBC(directions) $directions
         puts "xyz: $xyz  directions: $directions"

         #  process the files

         foreach fileSet {r a s} {

           eval set filenames \$$fileSet

     for {set i 1} {$i <= $numSlices} {incr i} {

             # read phase information
             set imgname [lindex $filenames [expr $i - 1]]
             set phaseslice "/gBC/pcdata/phase/$i/$fileSet"
             puts "$fileSet: $imgname $i $phaseslice"
             img2_readSliceROI $imgname $phaseslice $roi

             # get userN and vencscale from header
             set imageInfo [img_readHeader -file $imgname]
             foreach piece $imageInfo {
               set [lindex $piece 0] [lindex $piece 1]
             }

             # we have two venc's in this case, so lets rename from the header
             set in_plane_venc $venc

             # Determine venc to use for this image based on sequence used
             # and slice direction.

             # is 2dpcspgr the same as 2dphcon??

             if {$psdname == "~wedding/2d2vpc"} {
           if {[lindex $xyz 2] == $fileSet} {
              set venc [expr double($venc) * double($user2)]
              puts "    through plane venc: $venc  ($psdname --> venc * user2)"
           } else {
              set venc $in_plane_venc
                    puts "    in plane venc:  $venc ($psdname -->  venc)"
               }
       } elseif {$psdname == "mVINNIE"} {
           if {$fileSet == "r"} {
              set venc [expr 10.0 * double($user5)]
              puts "    r/l venc:  $venc ($psdname --> user5)"
           } elseif {$fileSet == "a"} {
              set venc [expr 10.0 * double($user6)]
              puts "    a/p venc:  $venc ($psdname --> user6)"
           } elseif {$fileSet == "s"} {
              set venc [expr 10.0 * double($user7)]
                    puts "    s/i venc:  $venc ($psdname --> user7)"
             } else {
              return -code error "ERROR:  unknown file type  $fileSet"
           }
       } elseif {$psdname == "mvinnie"} {
           if {$fileSet == "r"} {
              set venc [expr 10.0 * double($user5)]
              puts "    r/l venc:  $venc ($psdname --> user5)"
           } elseif {$fileSet == "a"} {
              set venc [expr 10.0 * double($user6)]
              puts "    a/p venc:  $venc ($psdname --> user6)"
           } elseif {$fileSet == "s"} {
              set venc [expr 10.0 * double($user7)]
                    puts "    s/i venc:  $venc ($psdname --> user7)"
             } else {
              return -code error "ERROR:  unknown file type  $fileSet"
           }
       } elseif {$psdname == "calvpc"} {
           if {$fileSet == "r"} {
              set venc [expr 10.0 * double($user12)]
              puts "    r/l venc:  $venc ($psdname --> user12)"
           } elseif {$fileSet == "a"} {
              set venc [expr 10.0 * double($user13)]
              puts "    a/p venc:  $venc ($psdname --> user13)"
           } elseif {$fileSet == "s"} {
              set venc [expr 10.0 * double($user14)]
                    puts "    s/i venc:  $venc ($psdname --> user14)"
             } else {
              return -code error "ERROR:  unknown file type  $fileSet"
           }
       } elseif {$psdname == "2dpcspgr"} {
           if {[lindex $xyz 2] == $fileSet} {
              set venc $user2
              puts "    through plane venc:  $venc  ($psdname --> user2)"
           } else {
              set venc $in_plane_venc
                    puts "    in plane venc:  $venc ($psdname -->  venc)"
               }
             } elseif {$psdname == "2dphcon"} {
           set venc $in_plane_venc
           puts "     venc:  $venc ($psdname --> venc)"
             } elseif {$psdname == "fgre"} {
           set venc $in_plane_venc
           puts "     venc:  $venc ($psdname --> venc)"
       } else {
                 puts " $psdname NOT SUPPORTED!!"
                 return -code error "ERROR:  $psdname not supported!"
       }

             # decode velocity info
             #puts "decode slice $i"
             set magslice "/gBC/pcdata/mag/$i"
             # we need to be careful and look at the value of _MR_vasflags to see
             # if the phase angle has been multipled by the magnitude.
             if {[expr $mag_weight_flag & 2] == 0} {
               img_decode -phaseImage $phaseslice -venc $venc  -vencscale $vencscale -result /gBC/pcdata/vel/$i/$fileSet
         } else {
               img_decode -magImage $magslice -phaseImage $phaseslice -venc $venc  -vencscale $vencscale -result /gBC/pcdata/vel/$i/$fileSet
         }
             repos_setLabel -obj /gBC/pcdata/vel/$i/$fileSet -key originalImageFile -value $imgname
           }

         }

      } else {

         puts "Assuming through-plane data."
         set gBC(numVelComp) 1
         # 1-comp data usually appear as just r slices, but
         # just in case
         set velImages [lsort -dictionary [concat $r $a $s]]
         if {[llength $velImages] != $numSlices} {
           puts "ERROR:  Not enough phase image slices!"
           return -code error "ERROR:  Not enough phase image slices!"
         }

         # the orientation should be the same for all slices, so just
         # arbitrarily pick a file
   set xyz {}
   set directions {}
         img_findPCMRIorientation [lindex $velImages 0] xyz directions
         # xyz is ignored for single component data
         set gBC(xyz) {}
         set gBC(directions) $directions
         puts "xyz: $xyz  directions: $directions"

        for {set i 1} {$i <= $numSlices} {incr i} {
          # read phase information
          set imgname [lindex $velImages [expr $i - 1]]
          set phaseslice "/gBC/pcdata/phase/$i"
          img2_readSliceROI $imgname $phaseslice $roi
          # get venc and vencscale from header
          set imageInfo [img_readHeader -file $imgname]
          foreach piece $imageInfo {
            set [lindex $piece 0] [lindex $piece 1]
          }
          # now the claim is that venc is used for 1-D calvpc
          # venc is ignored in calvpc sequence
    #if {$psdname == "calvpc"} {
    #   set venc [expr 10.0 * double($user14)]
    #}
          # decode velocity info
          #puts "decode slice $i"
          set magslice "/gBC/pcdata/mag/$i"
          if {[expr $mag_weight_flag & 2] == 0} {
            img_decode -phaseImage $phaseslice -venc $venc  -vencscale $vencscale -result /gBC/pcdata/vel/$i
    } else {
            img_decode -magImage $magslice -phaseImage $phaseslice -venc $venc  -vencscale $vencscale -result /gBC/pcdata/vel/$i
    }
          repos_setLabel -obj /gBC/pcdata/vel/$i -key originalImageFile -value $imgname
        }
      }

      # set the initial frame number
      global bcGUIcurrentFrameNumber
      set bcGUIcurrentFrameNumber 1
      # set the initial center
      global bcGUIctrX
      global bcGUIctrY
      global gBC
      set bcGUIctrX [expr $gBC(ext_i)/2]
      set bcGUIctrY [expr $gBC(ext_j)/2]
      puts "Done loading phase contrast data."
    }
   }

  bcGUIsetupCtrlWindow
  return GDSC_OK
}


# Procedure: createPREOPloadsaveLoadVol
proc createPREOPloadsaveLoadVol {} {

  global gOptions
  global gFilenames
  global gImageVol

  set change_to_cm_flag 0

  # want to load VTI, DICOM if no vti specified
  if {[string length $gImageVol(xml_filename)] > 0} {
    mc_LoadImageXML
    } elseif {[string length $gImageVol(data_dirname)] > 0} {
      set yesno [tk_messageBox -default yes  -message "Would you like to convert this volume from mm to cm?"  -title "Convert from mm to cm"  -type yesno]
      switch -- $yesno {
       yes {
        set change_to_cm_flag 1
      }
      no {
        set change_to_cm_flag 0
      }
    }

    if {$gOptions(image_data_type) == "GE_MR_5X"} {
      mc_LoadImage
      } else { 
        mc_LoadImageDICOM $change_to_cm_flag
      }

      set yesno [tk_messageBox -default yes  -message "Would you like to save this volume as a VTI file?"  -title "Save Volume as VTI"  -type yesno]
      switch -- $yesno {
       yes {
        set filename [tk_getSaveFile -filetypes {{VTI *.vti} {"All Files" *.*}} -title "Choose File Name for Image File In VTI Format"]
        if {$filename != ""} {
         if {[file extension $filename] == ""} {
          set filename "$filename.vti"
        }
        img_writeXML volume_image $filename
      }
    }
    no {
    }
  }
  } elseif {[string length $gImageVol(mha_filename)] > 0} {
    seg_LoadImageMha
    } else {
      return -code error "ERROR:  No image data specified!"
    }

  global gImageVol
  global symbolicName
  global gOptions
  global guiPPchooserXlabel
  global guiPPchooserYlabel
  global guiPPchooserZlabel
  global guiPPsmoothVoxelSize

  global guiPPchooserExists
  set guiPPchooserExists 1

  if {$gOptions(orientImgVolToRAS) == 0} {
     # old xyz system
     set voi_x0 $gImageVol(voi_x0)
     set voi_x1 $gImageVol(voi_x1)
     set voi_y0 $gImageVol(voi_y0)
     set voi_y1 $gImageVol(voi_y1)
     set voi_z0 $gImageVol(voi_z0)
     set voi_z1 $gImageVol(voi_z1)
     set guiPPchooserXlabel { X }
     set guiPPchooserYlabel { Y }
     set guiPPchooserZlabel { Z }
     set guiPPsmoothVoxelSize [math_minVec  [list $gImageVol(vdims_x) $gImageVol(vdims_y) $gImageVol(vdims_z)]]
  } else {
     # properly oriented volume in ras space
     set voi_x0 [lindex $gImageVol(voi_ras) 0]
     set voi_x1 [lindex $gImageVol(voi_ras) 1]
     set voi_y0 [lindex $gImageVol(voi_ras) 2]
     set voi_y1 [lindex $gImageVol(voi_ras) 3]
     set voi_z0 [lindex $gImageVol(voi_ras) 4]
     set voi_z1 [lindex $gImageVol(voi_ras) 5]
     set guiPPchooserXlabel { L }
     set guiPPchooserYlabel { P }
     set guiPPchooserZlabel { I }
     set guiPPsmoothVoxelSize [math_minVec $gImageVol(vdims_ras)]
  }
  $symbolicName(guiPPchooserXscale) configure -from $voi_x0
  $symbolicName(guiPPchooserXscale) configure -to $voi_x1
  $symbolicName(guiPPchooserYscale) configure -from $voi_y0
  $symbolicName(guiPPchooserYscale) configure -to $voi_y1
  $symbolicName(guiPPchooserZscale) configure -from $voi_z0
  $symbolicName(guiPPchooserZscale) configure -to $voi_z1

  guiVOLRENupdate init 0

  return GDSC_OK
}


