# Procedure: guiPPchooserAddPointAboveSelection
proc guiPPchooserAddPointAboveSelection { args} {

  global guiPPchooserXvar
  global guiPPchooserYvar
  global guiPPchooserZvar
  global symbolicName

  if {$args == ""} {
    set x $guiPPchooserXvar
    set y $guiPPchooserYvar
    set z $guiPPchooserZvar
    set point [list $x $y $z]
  } else {
    eval set point "$args"
  }

  set selectedPoints [guiSV_path_get_tree_current_points_selected]
  if {[llength $selectedPoints] == 0 || [llength $selectedPoints] > 1 } {
    return -code error "Must select a single point in a path first!"
  }

  set pathId [lindex [lindex $selectedPoints 0] 0]
  set posId  [lindex [lindex $selectedPoints 0] 1]

  global symbolicName
  set tv $symbolicName(guiSV_path_tree)
  set maxnum [llength [$tv children .paths.all.$pathId]]

  # update global data structure
  global gPathPoints

  for {set i $maxnum} {$i >= 1} {incr i -1} {
    set prevId [expr $i - 1]
    set gPathPoints($pathId,$i) $gPathPoints($pathId,$prevId)
    if {$prevId == $posId} {
      set gPathPoints($pathId,$posId) $point
      break
    }
  }

  guiSV_path_update_tree

  global symbolicName
  set tv $symbolicName(guiSV_path_tree)
  $tv see .paths.all.$pathId.$posId
  $tv selection set .paths.all.$pathId.$posId

  set gPathPoints($pathId,splinePts) {}
  guiPPchooserSplinePts

  guiPPchooserShowPath
  guiPPchooserShowSpline
}


