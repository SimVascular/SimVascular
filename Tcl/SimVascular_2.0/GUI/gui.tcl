# Procedure: guiMMcreateMeshMacro
proc guiMMcreateMeshMacro { type} {

  global guiMMvars
  global symbolicName
  set addstr {}

  global symbolicName
  set facename ""
  set facename [$symbolicName(smasherAttNameLabel) get]
  if {$facename == ""} {
     return -code error "ERROR: facename cannot be blank!"
  }

  set guiMMvars(mca_id) $facename
  set guiMMvars(mca_bl_id) $facename

  if {$type == "global_size"} {
    set flag $guiMMvars(mca_global_size_type)
    if {$flag == "Global Max Edge Size"} {
      set addstr "gsize $guiMMvars(mca_global_type) $guiMMvars(mca_global_size)\n"
    } elseif {$flag == "Global Max Curvature Size"} {
      set addstr "gcurv $guiMMvars(mca_global_type) $guiMMvars(mca_global_size)\n"
    } elseif {$flag == "Global Min Curvature Size"} {
      set addstr "gmincurv $guiMMvars(mca_global_type) $guiMMvars(mca_global_size)\n"
    } else {
      return -code error "ERROR: invalid flag ($flag)."
    }
  } elseif {$type == "size"} {
    set flag $guiMMvars(mca_size_type)
    if {$flag == "Local Max Edge Size"} {
      set addstr "size $guiMMvars(mca_id) $guiMMvars(mca_type) $guiMMvars(mca_size)\n"
    } elseif {$flag == "Local Max Curvature Size"} {
      set addstr "curv $guiMMvars(mca_id) $guiMMvars(mca_type) $guiMMvars(mca_size)\n"
    } elseif {$flag == "Local Min Curvature Size"} {
      set addstr "mincurv $guiMMvars(mca_id) $guiMMvars(mca_type) $guiMMvars(mca_size)\n"
    } else {
      return -code error "ERROR: invalid flag ($flag)."
    }
  } elseif {$type == "boundaryLayer"} {
    puts "boundaryLayer"
    set flag $guiMMvars(mca_bl_type)
    if {$flag == "Geometric Growth of Layer Thickness (type 1)"} {
      set opt 1
    } elseif {$flag == "Relative Height Gradation (type 2)"} {
      set opt 2
    } elseif {$flag == "User Specified Heights (type 3)"} {
      set opt 3
    } elseif {$flag == "Relative Height Gradation (type 4)"} {
      set opt 4
    } else {
      return -code error "ERROR: invalid flag ($flag)."
    }
    set addstr "boundaryLayer $guiMMvars(mca_bl_id) $opt $guiMMvars(mca_bl_dir) $guiMMvars(mca_bl_numLayers) $guiMMvars(mca_bl_params)\n"
  } elseif {$type == "sphereRefinement"} {
    set addstr "sphereRefinement $guiMMvars(mca_sphere_size) $guiMMvars(mca_sphere_radius) $guiMMvars(mca_sphere_center)\n"
  } elseif {$type == "cylinderRefinement"} {
    set addstr "cylinderRefinement $guiMMvars(mca_cylinder_size) $guiMMvars(mca_cylinder_radius) $guiMMvars(mca_cylinder_length) $guiMMvars(mca_cylinder_center)  $guiMMvars(mca_cylinder_normal)\n"
  } else {
    return -code error "ERROR: invalid code ($type)."
  }

  $symbolicName(guiMMmeshControlAttributesTextBox) insert end $addstr
}


# Procedure: guiMMcreateMeshSimScriptFile
proc guiMMcreateMeshSimScriptFile {} {

  global gFilenames
  global guiMMvars
  global gOptions

  set mesh_kernel $gOptions(meshing_kernel)

  if {$mesh_kernel == "MeshSim"} {
    set scriptfile [file rootname $gFilenames(mesh_script_file)].mss
  } else {
    return -code error "ERROR: Invalid Mesh Script File"
  }

  set gFilenames(mesh_script_file) $scriptfile

  set basename [file rootname [file tail $gFilenames(mesh_file)]]

  puts "Writing attribute file $scriptfile."

  set fp [open $scriptfile "w"]

  if {$mesh_kernel == "MeshSim"} {
    puts $fp "# geodesic_meshSim_script 1.0"
  } elseif {$mesh_kernel == "TetGen"} {
    puts $fp "# geodesic_tetGen_script 1.0"
  } else {
    return -code error "ERROR: Invalide Mesh Script File"
  }

  puts $fp "#"
  puts $fp "msinit"
  # only initialize if we are going to create a boundary layer mesh
  if {$guiMMvars(meshGenerateBoundaryLayerMesh) == "1"} {
    puts $fp "msinitbl"
  }
  puts $fp "# create log"
  puts $fp "logon $basename.logfile"
  puts $fp "# load solid model"
  puts $fp "loadModel $gFilenames(atdb_solid_file)"
  puts $fp "# create a new mesh object"
  puts $fp "newMesh"
  puts $fp "# control options"
  if {$guiMMvars(meshConstrainSurface) == 1} {
    puts $fp "constrainSurface"
  }
  puts $fp "option surface optimization $guiMMvars(meshOptimizeSurfaceMesh)"
  puts $fp "option surface smoothing $guiMMvars(meshNumSmoothingPasses)"
  puts $fp "option volume optimization $guiMMvars(meshOptimizeVolumeMesh)"
  puts $fp "option volume smoothing $guiMMvars(meshSmoothVolumeMesh)"
  puts $fp "option surface $guiMMvars(meshGenerateSurfaceMesh)"
  puts $fp "option volume $guiMMvars(meshGenerateVolumeMesh)"

  puts $fp "gsize $guiMMvars(gsize_type) $guiMMvars(gsize)"

  puts $fp "# start_of_user_meshing_control_parameters"

  # strip out blank lines
  set broke [split $guiMMvars(meshControlAttributes) "\n"]
  for {set i 0} {$i < [llength $broke]} {incr i} {
    set trimed [string trim [lindex $broke $i]]
    if {$trimed != ""} {
      puts $fp $trimed
    }
  }

  puts $fp "# end_of_user_meshing_control_parameters"

  if {$guiMMvars(meshGenerateSurfaceMesh) == 1 && $guiMMvars(meshGenerateVolumeMesh) == 1} {
     puts $fp "generateMesh"
  } elseif {$guiMMvars(meshGenerateSurfaceMesh) == 1} {
     puts $fp "generateSurfaceMesh"
  } elseif {$guiMMvars(meshGenerateVolumeMesh) == 1} {
     puts $fp "generateVolumeMesh"
  }

  puts $fp "# write out mesh"
  puts $fp "writeMesh $gFilenames(mesh_file) sms 0"

  if {$guiMMvars(meshWriteMeshStats) == 1} {
     puts $fp "writeStats $basename.sts"
  }

  puts $fp "# clean up and shut down"
  puts $fp "deleteMesh"
  puts $fp "deleteModel"
  puts $fp "logoff"

  close $fp
}


