# Procedure: guiCVshowHideMenu
proc guiCVshowHideMenu { mainbox text} {

  global symbolicName
  global guiCVvars

  set fbox $symbolicName($mainbox)
  
  set varname show_tab_[string range $mainbox 15 end]\_$text
  set varname [string tolower [regsub -all " " $varname _]]
  
  eval set flag \$guiCVvars($varname)

  #puts "tab is $varname"
  set num_tabs [$fbox index end]
  #puts "found $num_tabs tabs!"
  for {set i 0} {$i < $num_tabs} {incr i} {
    set opvals [$fbox tab $i]
    array set vals $opvals
    # hide 
    #puts "-text $vals(-text)"

    if {[string tolower $vals(-text)] == [string tolower $text]} {

      if {$flag} {
        puts "adding tab $i \($vals(-text)\)"
        set slavewindow [lindex [$fbox tabs] $i]
        $fbox add $slavewindow
        break
        } else {
          puts "hiding tab $i \($vals(-text)\)"
          $fbox hide $i
        }
        break
      }
    }
}


