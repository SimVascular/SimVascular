# Procedure: lsGUIshowInteractiveContour
proc lsGUIshowInteractiveContour {} {

  global gRen3d
  global lsGUIshowInteractiveContourFlag
  set flag $lsGUIshowInteractiveContourFlag

  catch {vis_splineClosedWidgetRm $gRen3d lsGUI}

  if {$flag == 0} {
    return
  }

  vis_splineClosedWidgetAdd $gRen3d  [repos_exportToVtk -src volume_image]  lsGUI  lsGUIsplineEnableEvent  lsGUIsplineStartInteractionEvent  lsGUIsplineInteractionEvent  lsGUIsplineEndInteractionEvent

  global gPlusMinus
  global lsGUIcurrentGroup
  global lsGUIspline_group
  set lsGUIspline_group $lsGUIcurrentGroup

  set groups [group_get $lsGUIcurrentGroup]
  if {[llength $groups] == 0} {
     return
  }

  global symbolicName
  global lsGUIcurrentPathNumber
  set pathId $lsGUIcurrentPathNumber

  set seg /lsGUI/segment/$pathId
  global lsGUIspline_id
  global lsGUIspline_pathId
  global lsGUIspline_posId

  global lsGUIspline_id
  global lsGUIseg_before_id
  global lsGUIseg_after_id

  set idloc $lsGUIspline_id
  set lsGUIspline_pathId $pathId

  set idloclength [expr [llength $groups] - 1]

  if {$idloc == 0} {
    set lsGUIseg_before_id ""
    set lsGUIseg_after_id  $seg/0
  } elseif {$idloc == $idloclength } {
    set lsGUIseg_before_id $seg/[expr $idloclength - 1]
    set lsGUIseg_after_id  ""
  } else {
    set lsGUIseg_before_id $seg/[expr $idloc - 1]
    set lsGUIseg_after_id $seg/$idloc
  }
  global gRen3d
  global lsGUIsurfaceOpacity
  if {[repos_exists -obj $lsGUIseg_before_id/copy] == 1} {
     repos_delete -obj $lsGUIseg_before_id
     geom_copy -dst $lsGUIseg_before_id -src $lsGUIseg_before_id/copy
     repos_delete -obj $lsGUIseg_before_id/copy
     catch {vis_pRm $gRen3d $lsGUIseg_before_id}
     set a [vis_pRepos $gRen3d $lsGUIseg_before_id]
     [$a GetProperty] SetOpacity $lsGUIsurfaceOpacity
  }
  if {[repos_exists -obj $lsGUIseg_after_id/copy] == 1} {
     repos_delete -obj $lsGUIseg_after_id
     geom_copy -src $lsGUIseg_after_id/copy -dst $lsGUIseg_after_id
     repos_delete -obj $lsGUIseg_after_id/copy
     catch {vis_pRm $gRen3d $lsGUIseg_after_id}
     set a [vis_pRepos $gRen3d $lsGUIseg_after_id]
     [$a GetProperty] SetOpacity $lsGUIsurfaceOpacity
  }

  incr idloc $gPlusMinus

  if {$idloc < 0} {
     set idloc 0
  } elseif {$idloc >= [llength $groups]} {
     set idloc [expr [llength $groups] - 1]
  }

  global lsGUIspline_id
  global lsGUIseg_before_id
  global lsGUIseg_after_id

  set lsGUIspline_id $idloc
  global lsGUIspline_creation_type
  set lsGUIspline_creation_type [repos_getLabel -obj [lindex $groups $idloc] -key creation_type]
  set lsGUIspline_posId [group_itemid $lsGUIcurrentGroup [lindex $groups $idloc]]

  if {$idloc == 0} {
    set lsGUIseg_before_id ""
    set lsGUIseg_after_id  $seg/0
    catch {repos_delete -obj $lsGUIseg_after_id/copy}
    geom_copy -src $lsGUIseg_after_id -dst $lsGUIseg_after_id/copy
  } elseif {$idloc == $idloclength } {
    set lsGUIseg_before_id $seg/[expr $idloclength - 1]
    set lsGUIseg_after_id  ""
    catch {repos_delete -obj $lsGUIseg_before_id/copy}
    geom_copy -src $lsGUIseg_before_id -dst $lsGUIseg_before_id/copy

  } else {
    set lsGUIseg_before_id $seg/[expr $idloc - 1]
    set lsGUIseg_after_id $seg/$idloc
    catch {repos_delete -obj $lsGUIseg_after_id/copy}
    catch {repos_delete -obj $lsGUIseg_before_id/copy}
    geom_copy -src $lsGUIseg_after_id -dst $lsGUIseg_after_id/copy
    geom_copy -src $lsGUIseg_before_id -dst $lsGUIseg_before_id/copy
  }
  global gPathBrowser
  set numHandles $gPathBrowser(solid_sample)

  set seg [lindex $groups $gPlusMinus]

  vis_splineClosedWidgetSetPts $gRen3d lsGUI [lindex $groups $idloc] $numHandles

  vis_splineClosedWidgetOn $gRen3d lsGUI
  # since it appears the beginInteraction gets called when you turn on the widget,
  # we need to explicitly call the endInteraction the first time
  lsGUIsplineEndInteractionEvent

  # vis stuff
  global lsGUIcurrentPositionNumber
  set lsGUIcurrentPositionNumber $lsGUIspline_posId
  lsGUIupdatePositionScale 0
#  lsGUIshowSurface
}


