# Procedure: guiPOSTv2pv
proc guiPOSTv2pv {} {

  set mymeshpara   /tmp/guiPOSTcreatev2pv/mymeshpara
  set myrespara    /tmp/guiPOSTcreatev2pv/myrespara
  set mytractpara  /tmp/guiPOSTcreatev2pv/mytractpara
  set mydisppara   /tmp/guiPOSTcreatev2pv/mydisppara
  set mydispinit   /tmp/guiPOSTcreatev2pv/mydispinit
  set mywsspara    /tmp/guiPOSTcreatev2pv/mywsspara
  set myexterior   /tmp/guiPOSTcreatev2pv/myexterior
  set mytranspara  /tmp/guiPOSTcreatev2pv/mytranspara
  set warped       /tmp/guiPOSTcreatev2pv-warped

  set my8bitGrayscaleLUT  guiPOSTcreatev2pv-my8bitGrayscaleLUT
  set myBlueToRedLUT      guiPOSTcreatev2pv-myBlueToRedLUT
  set xwrite              guiPOSTcreatev2pv-xwrite
  set walldisp            guiPOSTcreatev2pv-walldisp
  set walldispsub         guiPOSTcreatev2pv-walldispsub
  set wallvel             guiPOSTcreatev2pv-wallvel
  set myextcopy           guiPOSTcreatev2pv-myextcopy
  set mywarpit            guiPOSTcreatev2pv-mywarpit
  set dummyscalars        guiPOSTcreatev2pv-dummyscalars
  set myw                 guiPOSTcreatev2pv-myw

  catch {repos_delete -obj $mymeshpara}
  catch {repos_delete -obj $myrespara}
  catch {repos_delete -obj $mytractpara}
  catch {repos_delete -obj $mydisppara}
  catch {repos_delete -obj $mydispinit}
  catch {repos_delete -obj $mywsspara}
  catch {repos_delete -obj $myexterior}
  catch {repos_delete -obj $mytranspara}
  catch {repos_delete -obj $warped}

  catch {$my8bitGrayscaleLUT Delete}
  catch {$myBlueToRedLUT Delete}
  catch {$xwrite Delete}
  catch {$walldisp Delete}
  catch {$walldispsub Delete}
  catch {$wallvel Delete}

  global guiPOSTvars
  set displacement_flag $guiPOSTvars(displacement_flag)
  set in_prefix $guiPOSTvars(in_prefix)
  set increment $guiPOSTvars(increment)
  set input_dir $guiPOSTvars(input_dir)
  set open_inventor_flag $guiPOSTvars(open_inventor_flag)
  set out_prefix $guiPOSTvars(out_prefix)
  set output_dir $guiPOSTvars(output_dir)
  set phasta_format $guiPOSTvars(phasta_format)
  set polydata_flag $guiPOSTvars(polydata_flag)
  set pressure_flag $guiPOSTvars(pressure_flag)
  set scale_factor $guiPOSTvars(scale_factor)
  set start $guiPOSTvars(start)
  set stop $guiPOSTvars(stop)
  set subtract_step_number $guiPOSTvars(subtract_step_number)
  set traction_flag $guiPOSTvars(traction_flag)
  set transport_flag $guiPOSTvars(transport_flag)
  set velocity_flag $guiPOSTvars(velocity_flag)
  set vis_format $guiPOSTvars(vis_format)
  set wall_displacement_flag $guiPOSTvars(wall_displacement_flag)
  set wall_displacement_real_flag $guiPOSTvars(wall_displacement_real_flag)
  set wall_nbc_file $guiPOSTvars(wall_nbc_file)
  set wall_velocity_flag $guiPOSTvars(wall_velocity_flag)
  set wss_flag $guiPOSTvars(wss_flag)

  # create color maps if needed for open inventor files
  if {$open_inventor_flag == 1} {
    # 8-bit grayscale lookup table
    # ----------------------------
    vtkLookupTable $my8bitGrayscaleLUT
    $my8bitGrayscaleLUT SetHueRange 0.0 0.0
    $my8bitGrayscaleLUT SetSaturationRange 0.0 0.0
    $my8bitGrayscaleLUT SetValueRange 0.0 1.0
    $my8bitGrayscaleLUT SetNumberOfColors 16384
    $my8bitGrayscaleLUT Build

    # Red-to-blue colormap
    # --------------------
    vtkLookupTable $myBlueToRedLUT
    $myBlueToRedLUT SetHueRange 0.6667 0.0
    $myBlueToRedLUT SetSaturationRange 1.0 1.0
    $myBlueToRedLUT SetValueRange 1.0 1.0
    $myBlueToRedLUT SetAlphaRange 1.0 1.0
    $myBlueToRedLUT SetNumberOfColors 16384
    $myBlueToRedLUT Build
  }

  # read in the nodes from the wall.nbc
  # if needed  (assumes file is not gzipped)

  if {$wall_displacement_flag != 0 ||  $wall_displacement_real_flag != 0 ||  $wall_velocity_flag != 0} {

    set dispfp [open $guiPOSTvars(wall_nbc_file) r]
    set displacement_nodes {}
    while {[gets $dispfp line] > 0} {
      lappend displacement_nodes $line
    }
    close $dispfp
    set displacement_nodes [lsort -integer -unique $displacement_nodes]

  }

  # read mesh
  post_readVisMesh -file [file join $input_dir $in_prefix\_mesh.vis$vis_format] -obj $mymeshpara

  if {$wall_displacement_flag != 0 ||  $wall_displacement_real_flag != 0 ||  $wall_velocity_flag != 0} {
    post_getSurfMesh [repos_exportToVtk -src $mymeshpara] $myexterior
  }

  if {$wall_displacement_real_flag != 0} {
    post_readVisRes -file [file join $input_dir $in_prefix\_res$subtract_step_number.vis$vis_format]  -grid $mymeshpara -displacement $mydispinit
    set initdisplacement [[[repos_exportToVtk -src $mydispinit] GetPointData] GetVectors]
  }

  for {set i $start} {$i <= $stop} {incr i $increment} {

    set execstr [list post_readVisRes -file [file join $input_dir $in_prefix\_res$i.vis$vis_format]  -grid $mymeshpara -result $myrespara]

    if {$displacement_flag == 1} {
      set execstr "$execstr -displacement $mydisppara"
    } else {
    }

    if {$traction_flag == 1} {
      set execstr "$execstr -traction $mytractpara"
    } else {
    }

    if {$wss_flag == 1} {
      set execstr "$execstr -wss $mywsspara"
    } else {
    }

    if {$transport_flag == 1} {
      set execstr "$execstr -transport $mytranspara"
    } else {
    }

    puts $execstr
    eval $execstr

    set theMesh [repos_exportToVtk -src $mymeshpara]

    if {$pressure_flag == 1} {
      set pressure [[[repos_exportToVtk -src $myrespara] GetPointData] GetScalars]
    }
    if {$velocity_flag == 1} {
      set velocity [[[repos_exportToVtk -src $myrespara] GetPointData] GetVectors]
    }
    if {$traction_flag == 1} {
      set traction [[[repos_exportToVtk -src $mytractpara] GetPointData] GetVectors]
    }
    if {$transport_flag == 1} {
      set transport [[[repos_exportToVtk -src $mytranspara] GetPointData] GetScalars]
    }
    if {$wss_flag == 1} {
      set wss [[[repos_exportToVtk -src $mywsspara] GetPointData] GetVectors]
    }
    if {$displacement_flag == 1} {
      set displacement [[[repos_exportToVtk -src $mydisppara] GetPointData] GetVectors]
    }

    if {$wall_displacement_flag == 1} {
       vtkFloatArray $walldisp
       $walldisp SetNumberOfComponents 3
       $walldisp SetNumberOfTuples [$displacement GetNumberOfTuples]
       $walldisp FillComponent 0 0
       $walldisp FillComponent 1 0
       $walldisp FillComponent 2 0
    }

    if {$wall_displacement_real_flag == 1} {
       vtkFloatArray $walldispsub
       $walldispsub SetNumberOfComponents 3
       $walldispsub SetNumberOfTuples [$displacement GetNumberOfTuples]
       $walldispsub FillComponent 0 0
       $walldispsub FillComponent 1 0
       $walldispsub FillComponent 2 0
    }

    if {$wall_velocity_flag == 1} {
      vtkFloatArray $wallvel
      $wallvel SetNumberOfComponents 3
      $wallvel SetNumberOfTuples [$velocity GetNumberOfTuples]
      $wallvel FillComponent 0 0
      $wallvel FillComponent 1 0
      $wallvel FillComponent 2 0
    }

    if {$wall_displacement_flag != 0 ||  $wall_displacement_real_flag != 0 ||  $wall_velocity_flag != 0} {
      foreach dn $displacement_nodes {
  if {$wall_displacement_flag == 1} {
          set d [$displacement GetTuple3 [expr $dn-1]]
          $walldisp SetTuple3 [expr $dn-1] [lindex $d 0] [lindex $d 1] [lindex $d 2]
  }
        if {$wall_displacement_real_flag == 1} {
          set dinit [$initdisplacement GetTuple3 [expr $dn-1]]
          set drel [math_subVectors $d $dinit]
          $walldispsub SetTuple3 [expr $dn-1] [lindex $drel 0] [lindex $drel 1] [lindex $drel 2]
  }
        if {$wall_velocity_flag == 1} {
           set v [$velocity GetTuple3 [expr $dn-1]]
           $wallvel SetTuple3 [expr $dn-1] [lindex $v 0] [lindex $v 1] [lindex $v 2]
  }
      }
    }

    if {$pressure_flag == 1} {
      $pressure SetName pressure
      [$theMesh GetPointData] SetScalars $pressure
    }
    if {$velocity_flag == 1} {
      $velocity SetName velocity
      [$theMesh GetPointData] SetVectors $velocity
    }

    if {$transport_flag == 1} {
      $transport SetName transport
      [$theMesh GetPointData] AddArray $transport
    }

    if {$traction_flag == 1} {
      $traction SetName traction
      [$theMesh GetPointData] AddArray $traction
    }

    if {$displacement_flag == 1} {
      $displacement SetName displacement
      [$theMesh GetPointData] AddArray $displacement
    }

    if {$wss_flag == 1} {
      $wss SetName wss
      [$theMesh GetPointData] AddArray $wss
    }

    if {$wall_displacement_flag == 1} {
      $walldisp SetName wall_displacement
      [$theMesh GetPointData] AddArray $walldisp
    }

    if {$wall_displacement_real_flag == 1} {
      $walldispsub SetName wall_displacement_real
      [$theMesh GetPointData] AddArray $walldispsub
    }

    if {$wall_velocity_flag == 1} {
      $wallvel  SetName wall_velocity
      [$theMesh GetPointData] AddArray $wallvel
    }

    $theMesh Update

    vtkXMLDataSetWriter $xwrite
    $xwrite SetInputDataObject $theMesh
    $xwrite SetFileName [file join $output_dir $out_prefix-[format "%04i" $i].vtu]
    $xwrite Write

    if {$polydata_flag == 1} {
      # create vtk polydata file
      catch {repos_delete -obj $myextcopy}
      geom_copy -src $myexterior -dst $myextcopy
      [[repos_exportToVtk -src $myextcopy] GetPointData] SetVectors $walldispsub
      catch {$mywarpit Delete}
      vtkWarpVector $mywarpit
      $mywarpit SetInputDataObject [repos_exportToVtk -src $myextcopy]
      $mywarpit SetScaleFactor $scale_factor
      $mywarpit Update
      catch {repos_delete -obj $warped}
      repos_importVtkPd -src [$mywarpit GetOutput] -dst $warped
      repos_writeVtkPolyData -file [file join $output_dir $out_prefix-[format "%04i" $i].vtk] -obj $warped -type ascii
    }

    # create open inventor file
    if {$open_inventor_flag == 1} {
      catch {$dummyscalars Delete}
      vtkFloatArray $dummyscalars
      $dummyscalars SetNumberOfComponents 3
      $dummyscalars SetNumberOfTuples [$displacement GetNumberOfTuples]
      $dummyscalars FillComponent 0 0
      $dummyscalars FillComponent 1 0
      $dummyscalars FillComponent 2 0

      [[repos_exportToVtk -src $warped] GetPointData] SetScalars $dummyscalars
      [[[repos_exportToVtk -src $warped] GetPointData] GetScalars] SetLookupTable $myBlueToRedLUT

      $myBlueToRedLUT SetTableRange 0 1
      catch {$myw Delete}
      vtkIVWriter $myw
      $myw SetFileTypeToASCII
      $myw SetInputDataObject [repos_exportToVtk -src $warped]
      $myw SetFileName [file join $output_dir $out_prefix-[format "%04i" $i].iv]
      $myw Write
      catch {$myx Delete}
    }

    catch {repos_delete -obj $myrespara}
    catch {repos_delete -obj $mytractpara}
    catch {repos_delete -obj $mydisppara}
    catch {repos_delete -obj $mywsspara}
    catch {repos_delete -obj $mytranspara}

    catch {$xwrite Delete}
    catch {$walldisp Delete}
    catch {$walldispsub Delete}
    catch {$wallvel Delete}

  }
  catch {repos_delete -obj $myexterior}
  catch {repos_delete -obj $mydispinit}
  catch {repos_delete -obj $mymeshpara}

  catch {$my8bitGrayscaleLUT Delete}
  catch {$myBlueToRedLUT Delete}
}


