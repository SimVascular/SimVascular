# Procedure: vis_2DRenWin
proc vis_2DRenWin { renWin} {

  global symbolicName
  set newWinPath $symbolicName($renWin)
  set renId 1
  set ren [format "%s_ren%d" $renWin $renId]
  if {[cmdExists $renWin] == 0} {
    vtkRenderWindow $renWin
    vtkRenderer $ren
    $renWin AddRenderer $ren
    vtkTkRenderWidget $newWinPath.r1 -width 320 -height 320 -rw $renWin
    BindTkRenderWidget $newWinPath.r1
    pack $newWinPath.r1 -side left -padx 3 -pady 3 -fill both -expand t
    set renWin1 [$newWinPath.r1 GetRenderWindow]
    $ren SetBackground 0 0 0

    # add generic interactors
    catch {genericInteractor_$ren Delete}
    catch {genericInteractorStyle_$ren Delete}
    vtkGenericRenderWindowInteractor genericInteractor_$ren
    vtkInteractorStyleSwitch genericInteractorStyle_$ren
    genericInteractorStyle_$ren SetCurrentStyleToTrackballCamera
    genericInteractor_$ren SetInteractorStyle genericInteractorStyle_$ren
    set iren genericInteractor_$ren
    $iren SetRenderWindow $renWin1

    # note: parallel projection flag not always being handled properly
    return $ren
  }
  return $ren
}


# Procedure: vis_2DRenWin_delete
proc vis_2DRenWin_delete { renWin} {
  global symbolicName
  set newWinPath $symbolicName($renWin)
  set renId 1
  set ren [format "%s_ren%d" $renWin $renId]
  if {[cmdExists $renWin] == 1} {
    genericInteractor_$ren Delete
    genericInteractorStyle_$ren Delete
    $renWin Delete
    destroy $newWinPath.r1
    $ren Delete
    return
  }
  return
}


# Procedure: vis_gRenWin_3D
proc vis_gRenWin_3D {} {
  set renWin gRenWin_3D
  set renId 1
  set ren [format "%s_ren%d" $renWin $renId]
  global gRen3d
  global gRen3dCopies
  global gRen3dFreeze
  set gRen3dFreeze 0
  if {[cmdExists $ren] == 0} {
    if {$gRen3dCopies == 0 || $gRen3dCopies == 1 } {
      set panes .gRenWin_3D.pane0.pane1.f3
      set panes .guiCV.tframe3.tpanedwindow4.tframe5.tpanedwindow45.tframe36.tpanedwindow0.tframe1.pane0.pane1.f3
      set rens gRenWin_3D
    } elseif {$gRen3dCopies == 4} {
      set panes [list .gRenWin_3D.pane0.pane0.f0 .gRenWin_3D.pane0.pane0.f1  .gRenWin_3D.pane0.pane1.f2 .gRenWin_3D.pane0.pane1.f3]
      set rens  {gRenWin_3D4 gRenWin_3D3 gRenWin_3D2 gRenWin_3D}
    } else {
      return -code error "ERROR: Invalid gRen3dCopies ($gRen3dCopies)!"
    }

    set i 0
    set gRen3dCopies {}

    foreach renWin $rens {
      set renId 1
      set ren [format "%s_ren%d" $renWin $renId]
      set pane [lindex $panes $i]
      incr i
      vtkRenderWindow $renWin
      vtkRenderer $ren
      $renWin AddRenderer $ren
      set ::vis::frozen($ren) 0
      #puts "vtkTkRenderWidget $pane.r1 -width 320 -height 320 -rw $renWin"
      #puts "BindTkRenderWidget $pane.r1"
      vtkTkRenderWidget $pane.r1 -width 320 -height 320 -rw $renWin
      BindTkRenderWidget $pane.r1
      pack $pane.r1 -side left -padx 0 -pady 0 -fill both -expand t
      set renWin1 [$pane.r1 GetRenderWindow]
      $ren SetBackground 0 0 0
      # add generic interactors
      catch {genericInteractor_$ren Delete}
      catch {genericInteractorStyle_$ren Delete}
      vtkGenericRenderWindowInteractor genericInteractor_$ren
      vtkInteractorStyleSwitch genericInteractorStyle_$ren
      genericInteractorStyle_$ren SetCurrentStyleToTrackballCamera
      genericInteractor_$ren SetInteractorStyle genericInteractorStyle_$ren
      set iren genericInteractor_$ren
      $iren SetRenderWindow $renWin1
      lappend gRen3dCopies $ren
    }
    set gRen3dCopies [lrange $gRen3dCopies 0 end-1]
    set gRen3d $ren
    # just in case, set default interactor renwin to gRen3d
    global gInteractorRen
    set gInteractorRen $gRen3d
    return $ren
  }

  return $ren
}


# Procedure: vis_gRenWin_3D_delete
proc vis_gRenWin_3D_delete {} {
  set renWin gRenWin_3D
  set renId 1
  set ren [format "%s_ren%d" $renWin $renId]
  global xfShowWindow.$renWin
  global gRen3d
  global gRen3dCopies
  global gRen3dFreeze
  set gRen3dFreeze 0
  eval set flag \$\{xfShowWindow.$renWin\}
  if {[cmdExists .$renWin] == 0} {
    return
  }

  if {[llength $gRen3dCopies] == 0 } {
      set panes .gRenWin_3D.pane0.pane1.f3
      set rens gRenWin_3D
  } elseif {[llength $gRen3dCopies] == 3} {
      set panes [list .gRenWin_3D.pane0.pane0.f0 .gRenWin_3D.pane0.pane0.f1  .gRenWin_3D.pane0.pane1.f2 .gRenWin_3D.pane0.pane1.f3]
      set rens  {gRenWin_3D4 gRenWin_3D3 gRenWin_3D2 gRenWin_3D}
  } else {
      return -code error "ERROR: Invalid gRen3dCopies ($gRen3dCopies)!"
  }

  set i 0
  foreach renWin $rens {
      set renId 1
      set ren [format "%s_ren%d" $renWin $renId]
      set pane [lindex $panes $i]
      incr i
      genericInteractor_$ren Delete
      genericInteractorStyle_$ren Delete
      $renWin Delete
      #$ren Delete
  }
  destroy .gRenWin_3D
}


# Procedure: volGUIsetup
proc volGUIsetup { context} {

  upvar #0 $context guiVars

  global symbolicName

  if { [info exists guiVars(use_alt_image)] && $guiVars(use_alt_image) } {
  set repos_image $guiVars(repos_image_obj_alt)  
  } else {
  set repos_image $guiVars(repos_image_obj)   
  }
 
  set image [repos_exportToVtk -src $repos_image]

  set ldims [$image GetDimensions]
  set vdims [$image GetSpacing]
  set rng [$image GetScalarRange]

  # set ranges
  $symbolicName(volGUI_sliderx) configure -from 0
  $symbolicName(volGUI_sliderx) configure -to [expr [lindex $ldims 0] - 1]
  $symbolicName(volGUI_slidery) configure -from 0
  $symbolicName(volGUI_slidery) configure -to [expr [lindex $ldims 1] - 1]
  $symbolicName(volGUI_sliderz) configure -from 0
  $symbolicName(volGUI_sliderz) configure -to [expr [lindex $ldims 2] - 1]

  # these are for 3-D seeds
  $symbolicName(gui3DchooserXscale) configure -from 0
  $symbolicName(gui3DchooserXscale) configure -to [expr [lindex $ldims 0] - 1]
  $symbolicName(gui3DchooserYscale) configure -from 0
  $symbolicName(gui3DchooserYscale) configure -to [expr [lindex $ldims 1] - 1]
  $symbolicName(gui3DchooserZscale) configure -from 0
  $symbolicName(gui3DchooserZscale) configure -to [expr [lindex $ldims 2] - 1]

  volGUIupdateColorMaps guiVIB

  # update text showing range
  set guiVars(nativeRange) "native \[$rng\]"

  # set the min/max thr values
  set guiVars(min_thr_value) 128
  set guiVars(max_thr_value) [lindex $rng 1]

  # use of guiRIB is intentional
  global guiRIB
  # set the min/max thr values
  set guiRIB(min_thr_value) 128
  set guiRIB(max_thr_value) [lindex $rng 1]

  # intentional use of guiRIB
  global guiRIB
  set guiRIB(resample_voxel_size) [math_minVec $vdims]
}


# Procedure: volGUItoggleDisplaySlices
proc volGUItoggleDisplaySlices { context sliceDir} {

    upvar #0 $context guiVars

    if {$guiVars(show_image) == 0} {return}

    global gRen3d
    set ren $gRen3d

    set basename $guiVars(vis_basename)

    if {$sliceDir == "RL"} {
      set act    $basename\_actorx
      set flag  $guiVars(show_RL)
    } elseif {$sliceDir == "AP"} {
      set act    $basename\_actory
      set flag  $guiVars(show_AP)
    } elseif {$sliceDir == "SI"} {
      set act    $basename\_actorz
      set flag $guiVars(show_SI)
    }

    if {($flag) && (!([[$ren GetActors] IsItemPresent $act]))} {
  vis_renAddActor $ren $act
  vis_render $ren
  return
    }
    if {(!($flag)) && ([[$ren GetActors] IsItemPresent $act])} {
  vis_renRmActor $ren $act
  vis_render $ren
  return
    }
}


