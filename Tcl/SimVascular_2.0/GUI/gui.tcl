  .guiCV.tframe2.tmenubutton2.m add separator
  .guiCV.tframe2.tmenubutton2.m add checkbutton  -variable {guiCVvars(show_tabs_legacy_level_set)}  -command {guiCVshowHideMenu_SV_level_set}  -label {Legacy Level Set}
  .guiCV.tframe2.tmenubutton2.m add checkbutton  -variable {guiCVvars(show_tabs_level_set)}  -command {guiCVshowHideMenu_itk_level_set}  -label {Level Set}
  .guiCV.tframe2.tmenubutton2.m add separator
  .guiCV.tframe2.tmenubutton2.m add cascade  -menu {.guiCV.tframe2.tmenubutton2.m.n}  -label {Displays Subtabs}  -state {active}

  # build widget .guiCV.tframe2.tmenubutton2.m.n
  menu .guiCV.tframe2.tmenubutton2.m.n  -activeborderwidth {1}  -borderwidth {1}  -font {Helvetica 10}  -tearoff {0}
  .guiCV.tframe2.tmenubutton2.m.n add checkbutton  -variable {guiCVvars(show_tab_main_displays)}  -command {guiCVshowHideMenu guiCV_notebook_main "Displays"}  -label {Displays Toplevel}
  .guiCV.tframe2.tmenubutton2.m.n add checkbutton  -variable {guiCVvars(show_tab_displays_3-d_window)}  -command {guiCVshowHideMenu guiCV_notebook_displays "3-D Window"}  -label {3-D Window}
  .guiCV.tframe2.tmenubutton2.m.n add checkbutton  -variable {guiCVvars(show_tab_displays_3-d_pcmri)}  -command {guiCVshowHideMenu guiCV_notebook_displays "3-D PCMRI"}  -label {3-D PCMRI}
  .guiCV.tframe2.tmenubutton2.m.n add checkbutton  -variable {guiCVvars(show_tab_displays_2-d_intensity)}  -command {guiCVshowHideMenu guiCV_notebook_displays "2-D Intensity"}  -label {2-D Intensity}
  .guiCV.tframe2.tmenubutton2.m.n add checkbutton  -variable {guiCVvars(show_tab_displays_2-d_potential)}  -command {guiCVshowHideMenu guiCV_notebook_displays "2-D Potential"}  -label {2-D Potential}
  .guiCV.tframe2.tmenubutton2.m.n add checkbutton  -variable {guiCVvars(show_tab_displays_pcmri_intensity)}  -command {guiCVshowHideMenu guiCV_notebook_displays "PCMRI Intensity"}  -label {PCMRI Intensity}
  .guiCV.tframe2.tmenubutton2.m.n add checkbutton  -variable {guiCVvars(show_tab_displays_pcmri_potential)}  -command {guiCVshowHideMenu guiCV_notebook_displays "PCMRI Potential"}  -label {PCMRI Potential}
  .guiCV.tframe2.tmenubutton2.m.n add checkbutton  -variable {guiCVvars(show_tab_displays_general)}  -command {guiCVshowHideMenu guiCV_notebook_displays "General"}  -label {General}
  .guiCV.tframe2.tmenubutton2.m.n add checkbutton  -variable {guiCVvars(show_tab_displays_actor_properties)}  -command {guiCVshowHideMenu guiCV_notebook_displays "Actor Properties"}  -label {Actor Properties}

