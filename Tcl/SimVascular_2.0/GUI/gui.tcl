# Procedure: guiMMloadPolyData
proc guiMMloadPolyData { object} {
  #Author: Adam Updegrove
  #e-mail: updega2@gmail.com
  #UC Berkeley
  #shaddenlab.berkeley.edu
  #
  #This is a procedure to load the solid model as a full PolyData
  #The boundaries can then be extracted later with the GetBoundaryFaces 
  #method

  global gRenWin_3D_vars
  global gFilenames
  global gObjects
  global gOptions
  global gRen3d
  
  set kernel $gOptions(meshing_solid_kernel)
  set useLOD $gRenWin_3D_vars(solid_use_LOD)
  set pd /gRenWin_3D/meshing_solid/pd

  if {$kernel == "PolyData"} {
    if {[repos_exists -obj $object] == 1} {
       catch {repos_delete -obj $pd}
       if {$gOptions(facet_max_edge_size) == ""} {
         $object GetPolyData -result $pd
       } else {
         $object GetPolyData -result $pd  -max_edge_size $gOptions(facet_max_edge_size)
       }
       if {$useLOD == 1} {
         vis_lodRepos $gRen3d $pd
       } else {
         vis_pRepos $gRen3d $pd
       }
    }
  } else {
    return -code error "Kernel must be PolyData"
  }
}


# Procedure: guiMMmeshControlAttributesShow
proc guiMMmeshControlAttributesShow {} {

  global symbolicName
  global guiMMvars
  $symbolicName(guiMMmeshControlAttributesTextBox) delete 0.0 end
  $symbolicName(guiMMmeshControlAttributesTextBox) insert end $guiMMvars(meshControlAttributes)
  global gObjects
  set solid $gObjects(atdb_solid)
  set names {}
  global gOptions
  if {$gOptions(meshing_solid_kernel) == "Parasolid"} {
    foreach i [$solid GetFaceIds] {
      set name ""
      catch {set name [$solid GetFaceAttr -attr gdscName -faceId $i]}
      if {$name == ""} {
         set name [$solid GetFaceAttr -attr identifier -faceId $i]
      }
      lappend names $name
    }
  } elseif {$gOptions(meshing_solid_kernel) == "Discrete"} {
    global gDiscreteModelFaceNames
    foreach id [array names gDiscreteModelFaceNames] {
       lappend names $gDiscreteModelFaceNames($id) 
    }
  } else {
    return -code error "ERROR: invalid solid kernel ($kernel)"
  }

  set names [lsort -dictionary $names]

  # insert the face names into menu buttons
  $symbolicName(guiMMsizeFaceNamesMenu) delete 0 end
  foreach i $names {
    $symbolicName(guiMMsizeFaceNamesMenu) add radiobutton -value $i -label $i -variable guiMMvars(mca_id)
  }
  $symbolicName(guiMMblFaceNamesMenu) delete 0 end
  foreach i $names {
    $symbolicName(guiMMblFaceNamesMenu) add radiobutton -value $i -label $i -variable guiMMvars(mca_bl_id)
  }
  global guiMMvars
  set guiMMvars(mca_id) [lindex $names 0]
  set guiMMvars(mca_bl_id) [lindex $names 0]
}


