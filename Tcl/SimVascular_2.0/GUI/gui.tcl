# Procedure: guiPOSTcalcFlowsFromVTK
proc guiPOSTcalcFlowsFromVTK {} {
  global guiPOSTvars
  global gFilenames
  global gOptions
  set vtkFileName $guiPOSTvars(vtkFlowFile)
  if {$guiPOSTvars(look_for_vtp)} {
    set vtpfn [file rootname $vtkFileName].vtp
    if [file exists $vtpfn] {
       set guiPOSTvars(vtkFlowFile) $vtpfn
       set vtkFileName $vtpfn
    }
  }

  if {![file exists $vtkFileName]} {
     return -code error "ERROR: must specify existing filename ($vtkFileName)."
  }
  if [file isdirectory $vtkFileName] {
     return -code error "ERROR: must specify file not directory ($vtkFileName)."
  }
  set mesh_surfaces_dir $gFilenames(complete_mesh_dir)

  if {![file isdirectory $mesh_surfaces_dir]} {
     return -code error "ERROR: must be directory ($mesh_surfaces_dir)."
  }
  if [file isdirectory [file join $mesh_surfaces_dir mesh-surfaces]] {
    set mesh_surfaces_dir [file join $mesh_surfaces_dir mesh-surfaces]
  }

  # first look for vtp's, then fall back to vtk's
  set mesh_surface_vtk_filenames {}
  if [catch {set mesh_surface_vtk_filenames [glob [file join $mesh_surfaces_dir *.vtp]]}] {
    if [catch {set mesh_surface_vtk_filenames [glob [file join $mesh_surfaces_dir *.vtk]]}] {
      return -code error "ERROR:  does not appear to be a mesh-surfaces dir ($mesh_surfaces_dir)"
    }
  }

  set skipWalls $gOptions(exclude_walls_from_flows)
  set sim_units $guiPOSTvars(sim_units)

  set outFlowFileName [file rootname $vtkFileName]-flows.txt
  set outPressureFileName [file rootname $vtkFileName]-pressures.txt
  set outAveragesFileName [file rootname $vtkFileName]-averages.txt
  set outAveragesUnitsFileName [file rootname $vtkFileName]-averages-from_$sim_units-to-mmHg-L_per_min.txt
  vtx_TemporalFlowThruFaces $vtkFileName $mesh_surface_vtk_filenames $skipWalls $sim_units $outFlowFileName $outPressureFileName $outAveragesFileName $outAveragesUnitsFileName
}


# Procedure: guiPOSTconvertFilesToVTK
proc guiPOSTconvertFilesToVTK {} {

  global guiPOSTvars

  set indir $guiPOSTvars(restartFilesDir)
  if {[string trim $indir] == ""} {
      set indir [pwd]
  }
  if {![file isdirectory $indir]} {
    set indir [file dirname $indir]
  }

  set outdir $guiPOSTvars(vtkFlowFile)
  if {[string trim $outdir] == ""} {
      set outdir [pwd]
  }
  if {![file isdirectory $outdir]} {
    set outdir [file dirname $outdir]
  }

  set vtkcombo {}
  if {$guiPOSTvars(combine_results)} {
     set vtkcombo "-vtkcombo"
  }

  set laststep {}
  if {$guiPOSTvars(reduce_last_step)} {
    set laststep "-ph -laststep"
  }

  set sim_units $guiPOSTvars(sim_units)

  if {$sim_units == "mm"} {
     set simunitsflag "-sim_units_mm"
  } elseif {$sim_units == "cm"} {
     set simunitsflag "-sim_units_cm"
  } else {
     return -code error "ERROR: illegal sim_units ($sim_units)!"
  }

  set makevtu {}
  set vtufn {}
  if {$guiPOSTvars(write_vtu_results)} {
    set makevtu "-vtu"
    if {$guiPOSTvars(combine_results)} {
      set vtufn [file rootname [file tail $guiPOSTvars(vtkFlowFile)]].vtu
    } else {
      set vtufn [file rootname [file tail $guiPOSTvars(vtkFlowFile)]]
    }
    set vtufn "\"$vtufn\""
  }

  set makevtp {}
  set vtpfn {}
  if {$guiPOSTvars(write_vtp_results)} {
    set makevtp "-vtp"
    if {$guiPOSTvars(combine_results)} {
      set vtpfn [file rootname [file tail $guiPOSTvars(vtkFlowFile)]].vtp
    } else {
      set vtpfn [file rootname [file tail $guiPOSTvars(vtkFlowFile)]]
    }
    set vtpfn "\"$vtpfn\""
  }

  set start $guiPOSTvars(start)
  set stop $guiPOSTvars(stop)
  set increment $guiPOSTvars(increment)

  set wallflag {}
  set wallfile {}
  set calcws {}
  set muflag {}
  set mu {}
  set applywd {}

  if {$guiPOSTvars(wall_filter)} {
    set wallfile [string trim $guiPOSTvars(meshFaceFile)]
    if {$wallfile == ""} {
      tk_messageBox -title "Wall File Not Provided"  -type ok -message "Please specify a wall file for wall data output!"
      return       
    } else {
      if {[file exists $wallfile]==0} {
        tk_messageBox -title "Wall File Not Found"  -type ok -message "Please provide a correct name!"
        return    
      }
      set wallflag "-wfilter"
      set wallfile "\"$wallfile\""
    }
  
    if {$guiPOSTvars(recalc_wall_stress)} {

      set mu [string trim $guiPOSTvars(mu)]
      if {$mu != ""} {
        set muflag "-mu"
      } else { 
        set yesno [tk_messageBox -default yes -message "You have not provided fluid viscosity. Make sure geombc file has the value. Continue?" -title "Missing Viscosity Value" -type yesno]   
        switch -- $yesno {
          yes {
          }
          no {
            return
          }
        }
      }
      set calcws "-calcws"
      
      if {$guiPOSTvars(apply_wall_deformation)} {
        set applywd "-applywd"
      }
    }
  }
  set cmdstr "-all -indir \"$indir\" -outdir \"$outdir\" $vtkcombo $simunitsflag -start $start -stop $stop -incr $increment $makevtu $vtufn $makevtp $vtpfn $laststep $wallflag $wallfile $calcws $applywd $muflag $mu"
  
  puts "executing cvpostsolver with cmdstr: $cmdstr"

  global gExternalPrograms
  catch {eval exec \"$gExternalPrograms(cvpostsolver)\" $cmdstr} msg
  guiSVMSGshow $msg
}


