       } else {
          puts "turn off scalars"
          vis_pRm $ren $velocityPD
          set guiVISvarsPrev(cutplane_displayVelocity) $guiVISvars(cutplane_displayVelocity)
          repos_delete -obj $velocityPD
       }

   } else {

      if {($guiVISvarsPrev(cutplane_displayVelocity) == "as vectors") &&  ($guiVISvars(cutplane_displayVelocity) != "as vectors") } {
               puts "remove vectors from display"
               vis_vRm $ren $velocityPD
               set guiVISvarsPrev(cutplane_displayVelocity) "off"
      }  elseif {($guiVISvarsPrev(cutplane_displayVelocity) != "off") } {
                puts "remove scalars/warp from display"
               vis_pRm $ren $velocityPD
               set guiVISvarsPrev(cutplane_displayVelocity) "off"
      }  else {
               puts "nothing done here."
      }

      # three cases
      if {$guiVISvarsPrev(cutplane_displayVelocity) == "off" &&  $guiVISvars(cutplane_displayVelocity) == "as vectors"} {
          puts "cutplane: new display as vectors"
          catch {repos_delete -obj $velocityPD}
          set vectnorm tmp-guiVISupdateCutPlane-vn
          catch {$vectnorm Delete}
          vtkVectorNorm $vectnorm
          $vectnorm SetInputDataObject [repos_exportToVtk -src $surfacePD]
          $vectnorm Update
          repos_importVtkPd -src [$vectnorm GetOutput] -dst $velocityPD
          $vectnorm Delete
          set actor [vis_vRepos $ren $velocityPD]
          [$actor GetMapper] ScalarVisibilityOn
          [$actor GetMapper] SetLookupTable gBlueToRedLUT
          [$actor GetMapper] SetScalarRange $guiVISvars(cutplane_minVelMag) $guiVISvars(cutplane_maxVelMag)
          vis_render $ren
          vis_vSetScale $ren $velocityPD $guiVISvars(cutplane_velocityScale)
          set guiVISvarsPrev(cutplane_minVelMag) $guiVISvars(cutplane_minVelMag)
          set guiVISvarsPrev(cutplane_maxVelMag) $guiVISvars(cutplane_maxVelMag)
          set guiVISvarsPrev(cutplane_velocityScale) $guiVISvars(cutplane_velocityScale)
          set guiVISvarsPrev(cutplane_displayVelocity) $guiVISvars(cutplane_displayVelocity)
      } elseif {$guiVISvarsPrev(cutplane_displayVelocity) == "off" &&  $guiVISvars(cutplane_displayVelocity) == "warp vectors"} {
          puts "cutplane: new display as warp vectors"
          catch {repos_delete -obj $velocityPD}
          set vectnorm tmp-guiVISupdateCutPlane-vn
          catch {$vectnorm Delete}
          vtkPolyDataNormals $vectnorm
          $vectnorm SetInputDataObject [repos_exportToVtk -src $surfacePD]
          $vectnorm Update
          set velocityPDtmp /tmp/guiVIS/slice/velocityPD
          catch {repos_delete -obj $velocityPDtmp}
          repos_importVtkPd -src [$vectnorm GetOutput] -dst $velocityPDtmp
          geom_warp3dPts -src $velocityPDtmp -dst $velocityPD -scale $guiVISvars(cutplane_velocityScale)
          repos_delete -obj $velocityPDtmp
          $vectnorm Delete
          set actor [vis_pRepos $ren $velocityPD]
          [$actor GetMapper] ScalarVisibilityOn
          [$actor GetMapper] SetLookupTable gBlueToRedLUT
          [$actor GetMapper] SetScalarRange $guiVISvars(cutplane_minVelMag) $guiVISvars(cutplane_maxVelMag)
          vis_render $ren
          set guiVISvarsPrev(cutplane_minVelMag) $guiVISvars(cutplane_minVelMag)
          set guiVISvarsPrev(cutplane_maxVelMag) $guiVISvars(cutplane_maxVelMag)
          set guiVISvarsPrev(cutplane_velocityScale) $guiVISvars(cutplane_velocityScale)
          set guiVISvarsPrev(cutplane_displayVelocity) $guiVISvars(cutplane_displayVelocity)
      } elseif {$guiVISvarsPrev(cutplane_displayVelocity) == "off"} {
          puts "cutplane: new display as scalars"
          catch {repos_delete -obj $velocityPD}
          set vectnorm tmp-guiVISupdateCutPlane-vn
          catch {$vectnorm Delete}
          vtkVectorNorm $vectnorm
          $vectnorm SetInputDataObject [repos_exportToVtk -src $surfacePD]
          $vectnorm Update
          repos_importVtkPd -src [$vectnorm GetOutput] -dst $velocityPD
          $vectnorm Delete
          repos_setLabel -obj $velocityPD -key showK -value [list $guiVISvars(cutplane_minVelMag) $guiVISvars(cutplane_maxVelMag)]
          set guiVISvarsPrev(cutplane_minVelMag) $guiVISvars(cutplane_minVelMag)
          set guiVISvarsPrev(cutplane_maxVelMag) $guiVISvars(cutplane_maxVelMag)
          set guiVISvarsPrev(cutplane_displayVelocity) $guiVISvars(cutplane_displayVelocity)
          set actor [vis_pRepos $ren $velocityPD]
          [$actor GetMapper] ScalarVisibilityOn
          [$actor GetMapper] SetLookupTable gBlueToRedLUT
          [$actor GetMapper] SetScalarRange $guiVISvars(cutplane_minVelMag) $guiVISvars(cutplane_maxVelMag)
          vis_render $ren
      } elseif { ($guiVISvars(cutplane_displayVelocity) == "as vectors") } {

          if {$guiVISvarsPrev(cutplane_velocityScale) == $guiVISvars(cutplane_velocityScale)} {
             # do nothing
             puts "cutplane: as vectors , as vectors -- do nothing"
          } else {
             puts "cutplane: as vectors , as vectors -- scale"
             vis_vSetScale $ren $velocityPD $guiVISvars(cutplane_velocityScale)
             set guiVISvarsPrev(cutplane_velocityScale) $guiVISvars(cutplane_velocityScale)
    }
          if {($guiVISvarsPrev(cutplane_minVelMag) != $guiVISvars(cutplane_minVelMag)) ||  ($guiVISvarsPrev(cutplane_maxVelMag) != $guiVISvars(cutplane_maxVelMag))} {
            puts "cutplane: update scalars for vectors"
            set guiVISvarsPrev(cutplane_minVelMag) $guiVISvars(cutplane_minVelMag)
            set guiVISvarsPrev(cutplane_maxVelMag) $guiVISvars(cutplane_maxVelMag)
            set guiVISvarsPrev(cutplane_displayVelocity) $guiVISvars(cutplane_displayVelocity)
            set actor [vis_vGetActor $ren $velocityPD]
            [$actor GetMapper] ScalarVisibilityOn
            [$actor GetMapper] SetLookupTable gBlueToRedLUT
            [$actor GetMapper] SetScalarRange $guiVISvars(cutplane_minVelMag) $guiVISvars(cutplane_maxVelMag)
            vis_render $ren
  }
      } elseif { $guiVISvars(cutplane_displayVelocity) == "warp vectors"} {
          catch {vis_pRm $ren $velocityPD}
          puts "cutplane: new display as warp vectors"
          catch {repos_delete -obj $velocityPD}
          set vectnorm tmp-guiVISupdateCutPlane-vn
          catch {$vectnorm Delete}
          vtkPolyDataNormals $vectnorm
          $vectnorm SetInputDataObject [repos_exportToVtk -src $surfacePD]
          $vectnorm Update
          set velocityPDtmp /tmp/guiVIS/slice/velocityPD
          catch {repos_delete -obj $velocityPDtmp}
          repos_importVtkPd -src [$vectnorm GetOutput] -dst $velocityPDtmp
          geom_warp3dPts -src $velocityPDtmp -dst $velocityPD -scale $guiVISvars(cutplane_velocityScale)
          repos_delete -obj $velocityPDtmp
          $vectnorm Delete
          set actor [vis_pRepos $ren $velocityPD]
          [$actor GetMapper] ScalarVisibilityOn
          [$actor GetMapper] SetLookupTable gBlueToRedLUT
          [$actor GetMapper] SetScalarRange $guiVISvars(cutplane_minVelMag) $guiVISvars(cutplane_maxVelMag)
          vis_render $ren
          set guiVISvarsPrev(cutplane_minVelMag) $guiVISvars(cutplane_minVelMag)
          set guiVISvarsPrev(cutplane_maxVelMag) $guiVISvars(cutplane_maxVelMag)
          set guiVISvarsPrev(cutplane_velocityScale) $guiVISvars(cutplane_velocityScale)
          set guiVISvarsPrev(cutplane_displayVelocity) $guiVISvars(cutplane_displayVelocity)
      } else {

        if {($guiVISvarsPrev(cutplane_minVelMag) != $guiVISvars(cutplane_minVelMag)) ||  ($guiVISvarsPrev(cutplane_maxVelMag) != $guiVISvars(cutplane_maxVelMag))} {
