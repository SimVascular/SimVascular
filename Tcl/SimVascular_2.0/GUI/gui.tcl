# Procedure: guiPPsmoothSmoothPath
proc guiPPsmoothSmoothPath {} {

  global symbolicName
  global gImageVol
  global gOptions
  global guiPPsmoothTargetPathID
  global guiPPsmoothSubSampleRate
  global guiPPsmoothUseThresholding
  global guiPPsmoothThreshKeepPointAnyway
  global gPathPoints
  global guiSVvars

  set paths [guiSV_path_get_tree_current_paths_selected]
  if {$paths == ""} {
    tk_messageBox -title "Must specify a Path ID"  -type ok -message "Must specify a Path ID before selecting points!"
   return
  }
  if {[llength $paths] > 1} {
    tk_messageBox -title "Too Many Paths"  -type ok -message "Cannot add point to more than one path!"
   return
  }
  set pathId [lindex $paths 0]
  set tv $symbolicName(guiSV_path_tree)
  set maxnum [llength [$tv children .paths.all.$pathId]]

  set name $guiSVvars(path_entry_path_name)
  set numSplinePts $guiSVvars(path_entry_number_spline_pts)
  set targetID [string trim $guiPPsmoothTargetPathID]
  set sampleRate [string trim $guiPPsmoothSubSampleRate]
  set useThr $guiPPsmoothUseThresholding
  set keepPoints $guiPPsmoothThreshKeepPointAnyway

  if {[string trim $pathId] == ""} {
   tk_messageBox -title "Must specify a Path ID"  -type ok -message "Must specify a Path ID before selecting points!"
   return
  }

  if { [string trim $targetID] == "" } {
    set answer [tk_messageBox -title "No New ID Specified"  -type yesno -message "You did not specify a new Path ID. Do you wish to overwrite the Path ID: $pathId? This cannot be undone."]
  }

  switch -- $answer {
    no return
    yes {set targetID $pathId}
  }


  # create a list of points from the current path
  set pts {}
  for {set i 0} {$i < $maxnum} {incr i} {
     lappend pts $gPathPoints($pathId,$i)
  }

  # check for auto-sample rate here
  if {$sampleRate == "" || $sampleRate == "0"} {
    # hard code for now
    if {[llength $pts] > 250} {
       set sampleRate 10
    } elseif {[llength $pts] > 100} {
       set sampleRate 5
    } else {
       set sampleRate 3
    }
  }

  if {$useThr == 1} {

    # get threshold value
    global guiPPchooserGuessThrValue
    set thresholdValue $guiPPchooserGuessThrValue
    if {$thresholdValue <= 0} {
      return -code error "ERROR:  Threshold ($thresholdValue) must be > 0."
    }

    return -code "ERROR:  Using threshold to smooth path currently disabled.  Contact support."

    # hard coded number of spline points
    set numTmpSplinePts [expr int([llength $pts]/1.5)]

    # create a vtk object containing the original points
    set reppd /tmp/fix_path/pd
    catch {repos_delete -obj $reppd}
    geom_mkPtsPd $pts $reppd

    set flag 0

    # remove tmp files
    catch {file delete tmp-fix_path.path}
    catch {file delete tmp-again-fix_path.path}

    # spline points and create a tmp pathplan file
    geom_splinePtsToPathPlan -src $reppd -file tmp-fix_path.path  -numOutputPts $numTmpSplinePts  -flag $flag

    # get around bug in spline code
    exec sed "s/#IND//g" tmp-fix_path.path > tmp-again-fix_path.path

    # read in the pathplan file
    set fp [open "tmp-again-fix_path.path" "r"]
    set viewcnt [path_JumpToViewcounts $fp]
    set path [path_ReadOnePath $fp $viewcnt]
    close $fp

    # returns an un-oriented threshold
    global gOptions
    set ext $gOptions(resliceDims)

    # the volume data set
    #global gImageFoo
    #set src $gImageFoo(conv)
    set src volume_image

    # get the image slice
    set baseName /tmp/fix_path
    set rtnImg $baseName/mag
    set rtnPot $baseName/pot
    set thr $baseName/thr
    set thr_unclean $baseName/thr/unclean
    set picked $baseName/thr/selected
    set oriented $baseName/thr/oriented

    #  find new points
    set pts {}

    # set first point
    set posId 0
    array set items [lindex $path $posId]
    set pos [TupleToList $items(p)]
    set nrm [TupleToList $items(t)]
    set xhat [TupleToList $items(tx)]
    lappend pts $pos

    for {set posId $sampleRate} {$posId < $numTmpSplinePts} {incr posId $sampleRate} {
      catch {repos_delete -obj $rtnImg}
      catch {repos_delete -obj $rtnPot}
      catch {repos_delete -obj $thr}
      catch {repos_delete -obj $thr_unclean}
      catch {repos_delete -obj $picked}
      catch {repos_delete -obj $oriented}

      array set items [lindex $path $posId]
      set pos [TupleToList $items(p)]
      set nrm [TupleToList $items(t)]
      set xhat [TupleToList $items(tx)]

      img_getSliceAtPathPoint $src $path $posId $ext $rtnImg $rtnPot

      set img [repos_exportToVtk -src $rtnImg]
      repos_importVtkPd -src [img_contour $img $thresholdValue]  -dst $thr_unclean
      geom_mergePts -src $thr_unclean -dst $thr -tol 0.001
      if [catch {geom_pick -result $picked -obj $thr -pos {0 0 0}} msg] {
  if {$keepPoints == 0} {
          puts "Did not find closed boundary for pt ($pos).  ($msg)"
  } else {
          lappend pts $pos
  }
      } else {
        geom_orientProfile -src $picked -path_pos $pos -path_tan $nrm -path_xhat $xhat -dst $oriented
        lappend pts [geom_avgPt -obj $oriented]
      }
    }

    # add last point
    set posId end
    array set items [lindex $path $posId]
    set pos [TupleToList $items(p)]
    set nrm [TupleToList $items(t)]
    set xhat [TupleToList $items(tx)]
    lappend pts $pos

  } else {

    # only subsample

    set spts {}

    # set first point
    lappend spts [lindex $pts 0]

    for {set posId $sampleRate} {$posId < [llength $pts]} {incr posId $sampleRate} {
          lappend spts [lindex $pts $posId]
    }

    # add last point
    lappend spts [lindex $pts end]

    set pts $spts

  }

  # do fourier smoothing of the path if requested
  global guiPPsmoothUseFourierSmooth
  global guiPPsmoothNumFourierModes
  set numModes $guiPPsmoothNumFourierModes
  if { $numModes < 2} {
    tk_messageBox -title "Not Enough Modes"  -type ok -message "Number of fourier mode must be greater than 1."
   return 
  }



  if {$guiPPsmoothUseFourierSmooth == 1} {
     puts "smooth path (using FFT) keeping $numModes modes."
     # create a "periodic" function from the path pts
     set biglist $pts
     for {set i [expr [llength $pts] - 1]} {$i >= 0} {incr i -1} {
       lappend biglist [lindex $pts $i]
     }
     set newpts [math_smoothCurve -pts $biglist -closed 0 -numModes $numModes -numInterpPts [expr 2*[llength $pts]]]
     set pts [lrange $newpts 0 [expr [llength $pts]-1]]
  }


  # update target gPathPoints
  foreach i [array names gPathPoints $targetID,*] {
    unset gPathPoints($i)
  }
  for {set i 0} {$i < [llength $pts]} {incr i} {
     set gPathPoints($targetID,$i) [lindex $pts $i]
  }
  
  # preserve name and number of spline points if it exists
  set gPathPoints($targetID,name) $name
  set gPathPoints($targetID,numSplinePts) $numSplinePts
  set gPathPoints($targetID,splinePts) {}
  guiPPchooserSplinePts

  guiSV_path_update_tree
  set tv $symbolicName(guiSV_path_tree)
  $tv see .paths.all.$targetID
  $tv selection set .paths.all.$targetID
}


# Procedure: guiPPsmoothUpdateBinaryDisplay
proc guiPPsmoothUpdateBinaryDisplay {} {

  global guiPPsmoothShowBox
  global guiPPsmoothCenterBoxBy
  global guiPPsmoothVoxelSize
  global guiPPsmoothResampleVolume
  global guiPPsmoothShowContour
  global gRen3d
  global guiPPchooserMinGuessX
  global guiPPchooserMaxGuessX
  global guiPPchooserMinGuessY
  global guiPPchooserMaxGuessY
  global guiPPchooserMinGuessZ
  global guiPPchooserMaxGuessZ
  global guiPPchooserXvar
  global guiPPchooserYvar
  global guiPPchooserZvar
  global guiPPsmoothBoxCenter
  global guiPPsmoothVolRenderFlag
  global guiPPsmoothShowPointCloud

  set resample $guiPPsmoothResampleVolume
  set centerBy $guiPPsmoothCenterBoxBy
  set showBox $guiPPsmoothShowBox
  set size $guiPPsmoothVoxelSize
  set showContour $guiPPsmoothShowContour
  set showVolRender $guiPPsmoothVolRenderFlag
  set showCloud $guiPPsmoothShowPointCloud

  set box /tmp/guiPPsmooth/box
  set boxpd /tmp/guiPPsmooth/box/pd
  set contourpd /tmp/guiPPsmooth/contour/pd
  set cloud /tmp/guiPPsmooth/cloud/pd

  if {$resample == 0} {
     catch {repos_delete -obj $box}
     catch {repos_delete -obj $boxpd}
     catch {vis_pRm $gRen3d $boxpd}
     return
  }


  # show the box if desired
  if {$showBox == 1} {
     catch {repos_delete -obj $box}
     catch {repos_delete -obj $boxpd}
     catch {vis_pRm $gRen3d $boxpd}
     if {$centerBy == "cursor"} {
       set orgctr [list $guiPPchooserXvar $guiPPchooserYvar $guiPPchooserZvar]
     } elseif {$centerBy == "bbox"} {
       if {[info exists guiPPsmoothBoxCenter] == 0} {
          guiPPsmoothAutoSizeBox
          # ugly, but autosizebox calls this proc
          return
       }
       set orgctr $guiPPsmoothBoxCenter
     } else {
       return -code error "ERROR:  Illegal centerBy code."
     }
     # the user specified center does not correspond to the real center
     # of the box if the user has not specified a box centered around 0,0
     set llx [expr [lindex $orgctr 0] + $guiPPchooserMinGuessX * $size]
     set lly [expr [lindex $orgctr 1] + $guiPPchooserMinGuessY * $size]
     set llz [expr [lindex $orgctr 2] + $guiPPchooserMinGuessZ * $size]
     set urx [expr [lindex $orgctr 0] + $guiPPchooserMaxGuessX * $size]
     set ury [expr [lindex $orgctr 1] + $guiPPchooserMaxGuessY * $size]
     set urz [expr [lindex $orgctr 2] + $guiPPchooserMaxGuessZ * $size]

     set ctr [list [expr ($llx+$urx)/2.0]  [expr ($lly+$ury)/2.0]  [expr ($llz+$urz)/2.0]]

     set dx [expr $size*abs($guiPPchooserMaxGuessX-$guiPPchooserMinGuessX)]
     set dy [expr $size*abs($guiPPchooserMaxGuessY-$guiPPchooserMinGuessY)]
     set dz [expr $size*abs($guiPPchooserMaxGuessZ-$guiPPchooserMinGuessZ)]
     #puts "$dx $dy $dz"

     solid_box3d -result $box -dims [list $dx $dy $dz] -ctr $ctr
     $box GetPolyData -result $boxpd
     repos_setLabel -obj $boxpd -key opacity -value 0.33
     repos_setLabel -obj $boxpd -key color -value yellow
     gdscGeneralView $gRen3d $boxpd
  } else {
     catch {repos_delete -obj $box}
     catch {repos_delete -obj $boxpd}
     catch {vis_pRm $gRen3d $boxpd}
  }

  # show the contour if desired
  # Create the sampled image if its needed.  In an ideal
  # world we would only regenerate the sampled image
  # if required, but in typical brute force fashion
  # I'll do it every time.

  if {$showContour == 1 || $showVolRender == 1 || $showCloud == 1} {

    set imgobj resampled_image

    if {$centerBy == "cursor"} {
       set orgctr [list $guiPPchooserXvar $guiPPchooserYvar $guiPPchooserZvar]
    } elseif {$centerBy == "bbox"} {
       if {[info exists guiPPsmoothBoxCenter] == 0} {
          guiPPsmoothAutoSizeBox
          # ugly but autosizebox calls this proc
          return
       }
       set orgctr $guiPPsmoothBoxCenter
    } else {
       return -code error "ERROR:  Illegal centerBy code."
    }
    # the user specified center does not correspond to the real center
    # of the box if the user has not specified a box centered around 0,0
    set llx [expr [lindex $orgctr 0] + $guiPPchooserMinGuessX * $size]
    set lly [expr [lindex $orgctr 1] + $guiPPchooserMinGuessY * $size]
    set llz [expr [lindex $orgctr 2] + $guiPPchooserMinGuessZ * $size]
    set urx [expr [lindex $orgctr 0] + $guiPPchooserMaxGuessX * $size]
    set ury [expr [lindex $orgctr 1] + $guiPPchooserMaxGuessY * $size]
    set urz [expr [lindex $orgctr 2] + $guiPPchooserMaxGuessZ * $size]

    set ctr [list [expr ($llx+$urx)/2.0]  [expr ($lly+$ury)/2.0]  [expr ($llz+$urz)/2.0]]

    set dx [expr abs($guiPPchooserMaxGuessX-$guiPPchooserMinGuessX)]
    set dy [expr abs($guiPPchooserMaxGuessY-$guiPPchooserMinGuessY)]
    set dz [expr abs($guiPPchooserMaxGuessZ-$guiPPchooserMinGuessZ)]

    # our resampled image has a new origin
    set org_x $llx
    set org_y $lly
    set org_z $llz
    set vdims_x $size
    set vdims_y $size
    set vdims_z $size
    set voi_x0 0
    set voi_y0 0
    set voi_z0 0
    puts "extent $dx $dy $dz"

    puts "create resampled image"
    set rs tmp-guiPPchooserAutoPath
    catch {$rs Delete}
    vtkImageReslice $rs
    $rs SetInputDataObject [repos_exportToVtk -src volume_image]
    $rs SetInterpolationModeToLinear
    $rs SetOutputOrigin $org_x $org_y $org_z
    $rs SetOutputExtent 0 $dx 0 $dy 0 $dz
    $rs SetOutputSpacing $vdims_x $vdims_y $vdims_z
    $rs Update

    catch {repos_delete -obj $imgobj}
    repos_importVtkImg -src [$rs GetOutput] -dst $imgobj
    $rs Delete

  }

  # show the contour if desired
  if {$showContour == 1} {
    catch {repos_delete -obj $contourpd}
    catch {vis_lodRm $gRen3d $contourpd}
    global guiPPchooserGuessThrValue
    set thresholdValue $guiPPchooserGuessThrValue
    if {$thresholdValue <= 0} {
      return -code error "ERROR:  Threshold value must be > 0!"
    }
    set c [img_contour [repos_exportToVtk -src $imgobj] $thresholdValue]
    repos_importVtkPd -src $c -dst $contourpd
    vis_lodRepos $gRen3d $contourpd
    vis_lodSetColor $gRen3d $contourpd 0 1 0
    vis_lodSetOpacity $gRen3d $contourpd 0.66
  } else {
    catch {repos_delete -obj $contourpd}
    catch {vis_lodRm $gRen3d $contourpd}
  }

  # show threshold if desired
  if {$showCloud == 1} {
    catch {repos_delete -obj $cloud}
    catch {vis_pRm $gRen3d $cloud}
    global guiPPchooserGuessThrValue
    set thresholdValue $guiPPchooserGuessThrValue
    if {$thresholdValue <= 0} {
      return -code error "ERROR:  Threshold value must be > 0!"
    }
    global guiPPchooserGuessThrValueMax
    set thrMax $guiPPchooserGuessThrValueMax
    img_threshold -result $cloud -image $imgobj  -min_value $thresholdValue -max_value $thrMax  -max_num_pts 2000000
    set numthrpts [geom_numPts -obj $cloud]
    if {$numthrpts > 2000000} {
       tk_messageBox -message "Your requested point cloud is to large to be properly displayed.  Please change your threshold values." -icon error
    }
    vis_pRepos $gRen3d $cloud
  } else {
    catch {repos_delete -obj $cloud}
    catch {vis_pRm $gRen3d $cloud}
  }

  # do some volume rendering if desired
  set imgobj resampled_image
  if {$showVolRender == 1} {
    catch {vis_volRm $gRen3d $imgobj}
    vis_volRepos $gRen3d $imgobj
    global guiPPsmoothOpacityFunction
    global guiPPsmoothColorFunction
    global guiPPsmoothGradientFunction
    vis_volSetOpacityTransferFunction $gRen3d $imgobj $guiPPsmoothOpacityFunction
    vis_volSetColorTransferFunction $gRen3d $imgobj $guiPPsmoothColorFunction
    vis_volSetGradientTransferFunction $gRen3d $imgobj $guiPPsmoothGradientFunction
  } else {
    catch {vis_volRm $gRen3d $imgobj}
  }
}


# Procedure: guiPPtracePickedAssembly
proc guiPPtracePickedAssembly { name1 name2 op} {

  global PickedAssembly

  # if not a point do nothing
  if {[regexp "/tmp/pathplan/box/pd" $PickedAssembly] == 0} {
    return
  }

  set pieces [split $PickedAssembly /]
  global symbolicName
  set tv $symbolicName(guiSV_path_tree)
  $tv see .paths.all.[lindex $pieces end-1].[lindex $pieces end]
  $tv selection set .paths.all.[lindex $pieces end-1].[lindex $pieces end]
}


# Procedure: guiPPtracePlusMinus
proc guiPPtracePlusMinus { name1 name2 op} {

  global gPlusMinus

  if {$gPlusMinus == 0} {
    return
  }

  # if image slices arent displayed, do nothing
  global guiPPchooserSplineInteractorFlag
  set flag $guiPPchooserSplineInteractorFlag
  if {$flag != 2} {
    set gPlusMinus 0
    return
  }

  global symbolicName
  global guiPPimageSplineWidgetDisplayedSlice
  global guiPPchooserNumHandles
  global gInteractorRen
  global gRen3d

  if {$gPlusMinus > 0} {
    incr guiPPimageSplineWidgetDisplayedSlice 1
  } else {
    incr guiPPimageSplineWidgetDisplayedSlice -1
  }
  if {$guiPPimageSplineWidgetDisplayedSlice < 0} {
    set guiPPimageSplineWidgetDisplayedSlice 0
    return
  }
  if {$guiPPimageSplineWidgetDisplayedSlice == $guiPPchooserNumHandles} {
    set guiPPimageSplineWidgetDisplayedSlice [expr $guiPPchooserNumHandles  - 1]
    return
  }


  global guiPPchooserSplineInteractorFlag
  set flag $guiPPchooserSplineInteractorFlag
  if {$flag == 1} {
    set pts [vis_splineWidgetGetPts $gInteractorRen guiPP]
  } else {
    set pts [vis_splineImageWidgetGetPts $gInteractorRen guiPP]
  }

  vis_splineImageWidgetSetShowIds $gRen3d guiPP $guiPPimageSplineWidgetDisplayedSlice
  vis_splineImageWidgetCalcSlices $gRen3d guiPP $pts 1

  set gPlusMinus 0
}


