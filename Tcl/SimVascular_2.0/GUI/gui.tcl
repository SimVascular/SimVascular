    puts $fp "  \}"

    puts $fp "\} elseif \{\$batch_status == \"read\"\} \{"

    puts $fp "  if \{\[file exists \{$fullPathBSN-seg.vtk\}\] == 1\} \{"
    puts $fp "     repos_readVtkPolyData -obj \$lsres_unclean -file \{$fullPathBSN-seg.vtk\}"
    puts $fp "     # subsample the loop"
    puts $fp "     geom_mergePts -src \$lsres_unclean -dst \$lsres"
    puts $fp "     catch \{repos_delete -obj \$lsres_unclean\}"
    puts $fp "     set pts \[geom_getOrderedPts -obj \$lsres\]"
    puts $fp "     set curveLength \[math_curveLength -pts \$pts -closed 1\]"
    puts $fp "     set numSamplePoints \[expr int(ceil(\$curveLength/[lindex $voxel_dims 0]))\]"
    puts $fp "     set newpts \[math_linearInterpCurve -pts \$pts -closed 1 -numInterpPts \$numSamplePoints\]"
    puts $fp "     repos_delete -obj \$lsres"
    puts $fp "     geom_mkLinesFromPts \$newpts \$lsres 1"
    puts $fp ""
    puts $fp "     foreach tagobj \[list \$lsres\] \{"
    puts $fp "       repos_setLabel -obj \$tagobj -key radius                 -value \{$radius\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key center_x               -value \{$x\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key center_y               -value \{$y\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key center_z               -value \{$z\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key kt                     -value \{$kt\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key klow                   -value \{$klow\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key kupp                   -value \{$kupp\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key mag_grad_factor        -value \{$magGradFactor\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key grid_type              -value \{$gridType\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key max_num_time_steps     -value \{$maxTS\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key grid_factor            -value \{$gridFactor\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key isotropic_flag         -value \{$isotropicFlag\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key goodness_stop_criteria -value \{$goodnessStop\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key center_x_in_pixels     -value \{$bcGUIctrX\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key center_y_in_pixels     -value \{$bcGUIctrY\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key frame                  -value \{$frame\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key creation_method        -value \{levelset\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key creation_type          -value \{levelset\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key run_in_parallel        -value 1"
    puts $fp "       repos_setLabel -obj \$tagobj -key stop_velocity          -value \{$stopV\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key rebuild_phi_interval   -value \{$rebuildPhiFreq\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key area_check_interval    -value \{$acInterval\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key calc_decay_const_mtd   -value \{$calcDecayConstBy\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key explicit_pot_for_decay -value \{$potentialValueDecay\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key calc_pot_const_mtd     -value \{$calcPotConstBy\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key pFactor                -value \{$pFactor\}"
    puts $fp "       repos_setLabel -obj \$tagobj -key explicit_pot_for_pot   -value \{$potentialValuePotential\}"
    puts $fp "     \}"
    puts $fp ""
    puts $fp "  \}"
    puts $fp ""
    puts $fp "\}"
    puts $fp ""
    puts $fp "if \{\$exit_when_done == 1\} \{"
    puts $fp "  exit 0"
    puts $fp "\}"

    close $fp

    set params [list [file join [pwd] $gFilenames(tmp_dir) pcmri]   $fullPathBSN.tcl  $fullPathBSN-seg.vtk]

    dde eval geodesic_batch_server batch_addToQueue \{$params\}

  }
}


# Procedure: bcGUImakeCircle
proc bcGUImakeCircle {} {

  global bcGUIcurrentFrameNumber
  set frame $bcGUIcurrentFrameNumber

  global bcGUIanalContourType
  set type $bcGUIanalContourType

  global gBC
  set imageInfo $gBC(phase_contrast_params)
  foreach i $imageInfo {
    set [lindex $i 0] [lindex $i 1]
  }

  global bcGUIctrX
  global bcGUIctrY
  set x [expr double([lindex $voxel_dims 0])*double($bcGUIctrX)]
  set y [expr double([lindex $voxel_dims 1])*double($bcGUIctrY)]

  if {$type == "levelset"} {
    set seg /gBC/pcdata/mag/$frame/ls
  } elseif {$type == "threshold"} {
    set seg /gBC/pcdata/mag/$frame/thr/selected
  } else {
    return -code error "Invalid segmentation type ($type)."
  }

  catch {repos_delete -obj $seg}

  # create the unoriented circle
  global bcGUIradius
  set r $bcGUIradius
  geom_circle $r $x $y $seg

  # track creation parameters via labels
  foreach obj [list $seg] {
       repos_setLabel -obj $obj -key center_x_in_pixels -value $bcGUIctrX
       repos_setLabel -obj $obj -key center_y_in_pixels -value $bcGUIctrY
       repos_setLabel -obj $obj -key center_x           -value $x
       repos_setLabel -obj $obj -key center_y           -value $y
       repos_setLabel -obj $obj -key radius             -value $r
       repos_setLabel -obj $obj -key creation_method    -value "make_circle"
       repos_setLabel -obj $obj -key creation_type      -value $type
       repos_setLabel -obj $obj -key frame              -value $frame
  }

  # update display
  bcGUIchangeFrame $frame
}


