  set guiVISvars(volFlowSlice_sphereCenter)  [vis_planeWidgetGetCenter $gInteractorRen guiVIS]
  set guiVISvars(volFlowSlice_surfaceNormal) [vis_planeWidgetGetNormal $gInteractorRen guiVIS]
}


# Procedure: guiVISsaveImages
proc guiVISsaveImages { args} {

   # allow the user defined parameters to be ignored so
   # this proc can be called by the code to make movies

   # args 0: type
   # args 1: format string (either jpeg or pnm)
   # args 2: format string for pnm images

   global guiVISvars
   global guiVISvarsPrev

   set type $guiVISvars(images_type)
   set formatstring $guiVISvars(images_filenameFormat)
   set pnmalso 0
   if {[llength $args] >= 2} {
     set type [lindex $args 0]
     set formatstring [lindex $args 1]
   }
   if {[llength $args] == 3} {
     set pnmalso 1
     set formatstr2 [lindex $args 2]
   }

   set step_start 0
   set step_stop [expr [llength $guiVISvars(all_results_names)]-1]
   set steps [string_parse $guiVISvars(images_selectedSteps) $step_start $step_stop]
   if {[llength $steps] == 0} {
     return -code error "ERROR:  no steps specified."
   }

   puts "create images for steps: $steps"

   set w2i tmp-guiVISsaveImages-w2i
   set writer tmp-guiVISsaveImages-writer
   set pwriter tmp-guiVISsaveimages-also
   catch {$writer Delete}
   catch {$w2i Delete}

   if {$type == "jpeg"} {
     vtkJPEGWriter $writer
     $writer ProgressiveOff
     $writer SetQuality 90
   } elseif {$type =="pnm"} {
     vtkPNMWriter $writer
   }

   foreach step $steps {

     guiVISchangeStep $step

     # write out tmp jpeg file
     set ren $guiVISvars(ren)
     vis_render $ren

     catch {$w2i Delete}
     vtkWindowToImageFilter $w2i
     $w2i SetInputDataObject [$guiVISvars(ren) GetRenderWindow]
     $w2i Update

     $writer SetInputDataObject [$w2i GetOutput]
     $writer SetFileName [format $formatstring $step]
     $writer Write
     $writer SetInputDataObject [$w2i GetOutput]
     $writer SetFileName [format $formatstring $step]
     $writer Write

     if {$pnmalso == 1} {
       catch {$pwriter Delete}
       vtkPNMWriter $pwriter
       $pwriter SetInputDataObject [$w2i GetOutput]
       $pwriter SetFileName [format $formatstr2 $step]
       $pwriter Write
     }

    }

    catch {$w2i Delete}
    catch {$writer Delete}
    catch {$pwriter Delete}
}


