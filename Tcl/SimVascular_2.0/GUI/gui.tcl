# Procedure: guiSVIMG_vol_ren_with_canvas_update_histo
proc guiSVIMG_vol_ren_with_canvas_update_histo {} {

  global guiCVIMGvars
  global symbolicName

  set Cblue $symbolicName(guiSVIMG_blue_canvas)
  set Cgreen $symbolicName(guiSVIMG_green_canvas)
  set Copacity $symbolicName(guiSVIMG_opacity_canvas)
  set Cred $symbolicName(guiSVIMG_red_canvas)

  # save the values to restore
  set oLine [$Copacity coords line0]
  set bLine [$Cblue coords line0]
  set gLine [$Cgreen coords line0]
  set rLine [$Cred coords line0]

  set bounds {50 350 10 110}
  set tol 10

  set histopts [guiSVIMG_canvas_calc_histogram $bounds]

  foreach mypair [list [list $Cgreen green $gLine]  [list $Cred red $rLine]  [list $Cblue blue $bLine]  [list $Copacity orange $oLine]] {

    set w [lindex $mypair 0]
    set color [lindex $mypair 1]
    set coords [lindex $mypair 2]

    # clean first
    $w delete all

    # create bounding box
    $w create line [lindex $bounds 0] [lindex $bounds 3]  [lindex $bounds 1] [lindex $bounds 3]  -fill black -tag botbdr
    $w create line [lindex $bounds 1] [lindex $bounds 3]  [lindex $bounds 1] [lindex $bounds 2]  -fill black -tag rightbdr
    $w create line [lindex $bounds 1] [lindex $bounds 2]  [lindex $bounds 0] [lindex $bounds 2]  -fill black -tag topbdr
    $w create line [lindex $bounds 0] [lindex $bounds 2]  [lindex $bounds 0] [lindex $bounds 3]  -fill black -tag leftbdr
    foreach pt $histopts { 
      eval $w create line $pt -fill gray
    }
    eval $w create line $coords  -fill $color -tag line0
    guiSVIMG_canvas_markNodes $w line0
  }
}


