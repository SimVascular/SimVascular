# Procedure: createPREOPloadsavePCgetParams
proc createPREOPloadsavePCgetParams {} {

  global gFilenames
  set filename $gFilenames(phase_contrast_geometric)

  if {[file isdirectory $filename] == 0} {
     set filename [file dirname $filename]
     set gFilenames(phase_contrast_geometric) $filename
  }

  # check if specified name is a directory
  if {[file isdirectory $filename] == 0} {
    puts "ERROR:  Invalid directory ($filename) for phase contrast data."
    set gFilenames(phase_contrast_geometric) {}
    return -code error "ERROR: Invalid directory name ($filename) for phase contrast directory!"
  }

  # autodect if we have 1 or 3 component velocity data
  set speed {}
  set collapse {}
  set r {}
  set a {}
  set s {}
  set mag {}

  if [catch {glob [file join $filename I.*]}] {
    puts "ERROR: No image slices found in directory $filename."
    return -code error "ERROR: No image slices found in directory $filename."
  }
  global gBC
  foreach i [lsort -dictionary [glob [file join $filename I.*]]] {
        if [catch {set header [img_readHeader -file $i]}] continue
        foreach piece $header {
          set [lindex $piece 0]  [lindex $piece 1]
        }
        if {$vas_collapse == 0 || $vas_collapse == 7 ||  $vas_collapse == 12} {
           #puts "$i: $vas_collapse is speed"
           lappend speed $i
        } elseif {$vas_collapse == 1 || $vas_collapse == 13} {
           #puts "$i: $vas_collapse is collapse"
           lappend collapse $i
        } elseif {$vas_collapse == 2 || $vas_collapse == 8} {
           #puts  "$i: $vas_collapse is mag"
           lappend mag $i
        } elseif {$vas_collapse == 3 || $vas_collapse == 9} {
           #puts "$i: $vas_collapse is R/L"
           lappend r $i
        } elseif {$vas_collapse == 4 || $vas_collapse == 10} {
           #puts  "$i: $vas_collapse is A/P"
           lappend a $i
        } elseif {$vas_collapse == 5 || $vas_collapse == 11} {
           #puts  "$i: $vas_collapse is S/I"
           lappend s $i
        }

  }

  if {[llength $mag] == 0} {
        puts "ERROR:  Not enough magnitude image slices!"
        set gFilenames(phase_contrast_geometric) {}
        set gBC(numSlices) {}
        return -code error "ERROR:  No magnitude image slices!"
  }
  set gBC(numSlices) [llength $mag]

  set filename [lindex [lsort -dictionary $mag] 0]
  set gFilenames(phase_contrast_geometric) $filename
  set gBC(phase_contrast_params) [img_readHeader -file $filename]
  foreach i $gBC(phase_contrast_params) {
    set option [lindex $i 0]
    set value [lindex $i 1]
    if {[lindex $i 0] == "extent"} {
      set gBC(ext_i) [lindex $value 0]
      set gBC(ext_j) [lindex $value 1]
      set gBC(voi_x0) 0
      set gBC(voi_y0) 0
      set gBC(voi_x1) [expr $gBC(ext_i) - 1]
      set gBC(voi_y1) [expr $gBC(ext_j) - 1]
    } elseif {[lindex $i 0] == "top_left_corner"} {
      set gBC(top_left_corner) $value
    } elseif {[lindex $i 0] == "top_right_corner"} {
      set gBC(top_right_corner) $value
    } elseif {[lindex $i 0] == "bottom_right_corner"} {
      set gBC(bottom_right_corner) $value
    } elseif {[lindex $i 0] == "heart_rate_bpm"} {
      set gBC(heart_rate_bpm) $value
      set gBC(period) 0
      catch {set gBC(period) [expr 60.0/double($value)]}
    }
  }
  set gBC(size_of_trim_box) -400

  return GDSC_OK
}


# Procedure: createPREOPloadsaveSave
proc createPREOPloadsaveSave {} {

  global gFilenames
  global gOptions

