# Procedure: wormGUIwriteMultipleFaces
proc wormGUIwriteMultipleFaces {} {

  # we get the normal from the user defined face name
  global gObjects
  global guiABC
  global gOptions

  set solidkernel $gOptions(meshing_solid_kernel)

  if {$solidkernel == "Parasolid" || $solidkernel == "Discrete"} {
    set solid $gObjects(atdb_solid)
  } elseif {$solidkernel == "PolyData"} {
    set solid $gObjects(polydata_solid)
  } else {
    return -code error "ERROR: Invalid solid kernel"
  }

  if {[repos_exists -obj $solid] == 0} {
    return -code error "ERROR: Need to load solid first!"
  }
  if {[repos_type -obj $solid] != "SolidModel"} {
    return -code error "ERROR: Object not of expected SolidModel type."
  }

  set org_mesh_face_file $guiABC(mesh_face_file)
  set org_face_name $guiABC(face_name)
  set org_flow_rate_file $guiABC(flow_rate_file)
  set org_spectrum_x_file $guiABC(spectrum_x_file)
  set org_spectrum_y_file $guiABC(spectrum_y_file)
  set org_spectrum_z_file $guiABC(spectrum_z_file)
  set org_bct_dat_file $guiABC(bct_dat_file)
  set org_bct_vtp_file $guiABC(bct_vtp_file)
  # get all the named faces, then check for existence of flow file

  global gDiscreteModelFaceNames
  global gPolyDataFaceNames
	 
  foreach id [$solid GetFaceIds] {
    set guiABC(mesh_face_file) $org_mesh_face_file
    set guiABC(face_name) $org_face_name
    set guiABC(flow_rate_file) $org_flow_rate_file
    set guiABC(spectrum_x_file) $org_spectrum_x_file
    set guiABC(spectrum_y_file) $org_spectrum_y_file
    set guiABC(spectrum_z_file) $org_spectrum_z_file
    set guiABC(bct_dat_file) $org_bct_dat_file
    set guiABC(bct_vtp_file) $org_bct_vtp_file
    set facename "missing"
    if {$solidkernel == "Parasolid"} {
       set facename [$solid GetFaceAttr -attr gdscName -faceId $id]
    } elseif {$solidkernel == "Discrete"} {
      global gDiscreteModelFaceNames
      set facename $gDiscreteModelFaceNames($id)
    } elseif {$solidkernel == "PolyData"} {
      global gPolyDataFaceNames
	  set facename $gPolyDataFaceNames($id)
    } else {
      return -code error "ERROR: invalid solid kernel ($kernel)"
    }

    if {$facename != ""} {
      puts "  working on: $facename"
      eval set guiABC(face_name) $guiABC(face_name)
      # read in vtk mesh face file
      eval set guiABC(mesh_face_file) $guiABC(mesh_face_file)
      if {[file exists $guiABC(mesh_face_file)] == 0} {
        continue
      }
      eval set guiABC(flow_rate_file) $guiABC(flow_rate_file)
      if {[file exists $guiABC(flow_rate_file)] == 0} {
        continue
      }
      eval set guiABC(spectrum_x_file) $guiABC(spectrum_x_file)
      eval set guiABC(spectrum_y_file) $guiABC(spectrum_y_file)
      eval set guiABC(spectrum_z_file) $guiABC(spectrum_z_file)
      eval set guiABC(bct_dat_file) $guiABC(bct_dat_file)
      eval set guiABC(bct_vtp_file) $guiABC(bct_vtp_file)
      puts "   do face $facename"
      wormGUIwritePHASTA 0
      wormGUIwriteSpectrum
    }
  }

  # reset the original values
  set guiABC(mesh_face_file) $org_mesh_face_file
  set guiABC(face_name) $org_face_name
  set guiABC(flow_rate_file) $org_flow_rate_file
  set guiABC(spectrum_x_file) $org_spectrum_x_file
  set guiABC(spectrum_y_file) $org_spectrum_y_file
  set guiABC(spectrum_z_file) $org_spectrum_z_file
  set guiABC(bct_dat_file) $org_bct_dat_file
  set guiABC(bct_vtp_file) $org_bct_vtp_file
}


# Procedure: wormGUIwritePHASTA
proc wormGUIwritePHASTA { prompt_user_for_dir} {

  global gFilenames
  global guiABC
  global gObjects
  global gBC

  set face /tmp/wormGUI/writePHASTA/pd
  set radiusMap /tmp/wormGUI/writePHASTA/radiusMap
  set flat_face /tmp/wormGUI/writePHASTA/flat_face
  set flat_0 /tmp/wormGUI/writePHASTA/flat_0
  set mapped_flat /tmp/wormGUI/writePHASTA/mfat
  set mapped_flat_0 /tmp/wormGUI/writePHASTA/mfat0

  catch {repos_delete -obj $face}
  catch {repos_delete -obj $flat_face}
  catch {repos_delete -obj $flat_0}
  catch {repos_delete -obj $radiusMap}
  catch {repos_delete -obj $mapped_flat}
  catch {repos_delete -obj $mapped_flat_0}

  #set file directory to save
  if {$prompt_user_for_dir} {
    #set initialdirectory [file dirname $gFilenames(bct_dat_file)]
    set initialdirectory [file dirname $guiABC(bct_dat_file)]

    set fdir [tk_chooseDirectory -mustexist 1 -title "Select Save Location for BCT Files" -initialdir $initialdirectory ]

    if {$fdir == ""} { 
       puts "User Cancelled."
       return
    }
    set guiABC(bct_dat_file) [file join $fdir $gFilenames(bct_dat_file)]
    set guiABC(bct_vtp_file) [file join $fdir $gFilenames(bct_vtp_file)]
  }

  # read in vtk mesh face file
  set face_file $guiABC(mesh_face_file)
  repos_readXMLPolyData $face_file $face

  global gOptions

  set face_name $guiABC(face_name)

  set normal [geom_avgNormal $face]
  # usually need to invert normal
  if $guiABC(invert_face_normal) {
    set normal [list [expr -1.0*[lindex $normal 0]]  [expr -1.0*[lindex $normal 1]]  [expr -1.0*[lindex $normal 2]]]
  }

  puts "outward nrm: $normal"

  set boolean 1
  set rotated_norm {}
  geom_flatten $normal $boolean $face rotated_norm $flat_face

  # set the idealized unit outward normal for the flattened face
  if {[lindex $rotated_norm 2] > 0} {
      set outwardUnitNormal {0 0 1}
  } else {
      set outwardUnitNormal {0 0 -1}
  }

  set bbox [geom_bbox -obj $flat_face]

  geom_translate -src $flat_face  -vec [list 0 0 [expr -1.0*[lindex $bbox 4]]]  -dst $flat_0
  set area [geom_surfArea -src $flat_0]
  puts "area: $area"

  set radmax [expr sqrt(double($area)/[math_pi])]
  puts "effective radius: $radmax"

  # read flow rate from file
  set fp [open $guiABC(flow_rate_file) r]

  set pts {}
  while {[gets $fp line] >= 0} {
      set line [string trim $line]
      if {[string index $line 0] == "#" || $line == ""} {
        continue
      }
      if {[scan $line "%lf %lf" time flow] != 2} {
         close $fp
         return -code error "Incorrect format of line in flow rate file ($line)."
      }
      lappend pts [list $time $flow]
  }
  close $fp

  puts "found [llength $pts] points in file."

  set num_f_terms $guiABC(num_output_modes)
  set period $gBC(period)
  set viscosity [lindex $guiABC(viscosity) 0]
  set density [lindex $guiABC(density) 0]
  # we subtract 1 from the number of points to get the number
  # of intervals (which is what the variable should be called in
  # the code below).
  set numPtsInPeriod [expr $guiABC(num_output_pts_in_period) -1]

  # for now in PHASTA code we only write out 1 period.
  #set numPeriods $guiABC(num_output_periods)
  set numPeriods 1

  set terms [math_FFT -pts $pts -numInterpPts 256 -nterms $num_f_terms]

  set omega [expr 2.0*[math_pi]/$period]

  set plotme [math_inverseFFT -terms $terms -t0 0  -dt [expr $period/double($numPtsInPeriod)]  -omega $omega -numPts $numPtsInPeriod]

  geom_createRatioMap $flat_0 $radmax $radiusMap

  # create velocity maps
  repos_deleteList [repos_subList /tmp/wormGUI/writePHASTA/mapped/*]
  catch {unset mapArray}
  for {set j 0} {$j < $numPtsInPeriod} {incr j} {
      set mapped /tmp/wormGUI/writePHASTA/mapped/$j
      catch {repos_delete -obj $mapped}
      catch {repos_delete -obj $mapped_flat}
      catch {repos_delete -obj $mapped_flat_0}
      set flow_rate [lindex [lindex $plotme $j] 1]
      set time [lindex [lindex $plotme $j] 0]
      geom_mapWomersleyMap $terms $time $viscosity $omega  $density $radmax   $flow_rate $flat_0  $radiusMap $outwardUnitNormal  $mapped_flat_0

      # undo move to z=0
      geom_translate -src $mapped_flat_0  -vec [list 0 0 [expr 1.0*[lindex $bbox 4]]]  -dst $mapped_flat

      # rotate back
      set boolean 0
      set back_to_norm $rotated_norm
      geom_flatten $normal $boolean $mapped_flat back_to_norm $mapped

      # check normal
      #puts "\noriginal nrm : $normal"
      #puts "rotated  nrm : $rotated_norm"
      #puts "back to nrm  : $back_to_norm"
      #puts "outward unit : $outwardUnitNormal"

      set mapArray($j) [[[repos_exportToVtk -src $mapped] GetPointData] GetVectors]

  }


  # scale factor in case we want to scale the velocities being written out for
  # some reason (e.g. unit coversion).
  set factor  1.0

  # output the velocities to the files
  set fp [open $guiABC(bct_dat_file) "w"]
  fconfigure $fp -translation lf

  set numNodes [geom_numPts -obj $face]
  set nodeids [[[repos_exportToVtk -src $face] GetPointData] GetScalars]

  # write out header
  puts $fp "$numNodes [expr $numPtsInPeriod + 1]"

  for {set nodeLoop 0} {$nodeLoop < $numNodes} {incr nodeLoop} {
    if {![expr $nodeLoop % 10]} {
      puts "working on node $nodeLoop of [expr $numNodes - 1]."
    }
    set node [$nodeids GetTuple1 $nodeLoop]
    for {set i 0} {$i < $numPeriods} {incr i} {
      set pt [[repos_exportToVtk -src $face] GetPoint $nodeLoop]
      puts $fp [format "%e %e %e %i %i" [lindex $pt 0] [lindex $pt 1] [lindex $pt 2] [expr $numPtsInPeriod+1] [expr int($node)]]
      for {set j 0} {$j < $numPtsInPeriod} {incr j} {
        set v [$mapArray($j) GetTuple3 $nodeLoop]
        set time [expr double($i)*$period + $j*$period/double($numPtsInPeriod)]
        set time [format "%e" $time]
        puts $fp [format "%e %e %e %e" [expr $factor*[lindex $v 0]]  [expr $factor*[lindex $v 1]]  [expr $factor*[lindex $v 2]]  $time]
      }
    }
    # explicitly write out the last point of the final period
    set v [$mapArray(0) GetTuple3 $nodeLoop]
    set node [$nodeids GetTuple1 $nodeLoop]
    set time [expr double($numPeriods)*$period]
    set time [format "%e" $time]
    puts $fp [format "%e %e %e %e" [expr $factor*[lindex $v 0]]  [expr $factor*[lindex $v 1]]  [expr $factor*[lindex $v 2]]  $time]
  }
  close $fp

  repos_deleteList [repos_subList /tmp/wormGUI/writePHASTA/mapped/*]
  catch {unset mapArray}

  catch {repos_delete -obj $face}
  catch {repos_delete -obj $flat_face}
  catch {repos_delete -obj $flat_0}
  catch {repos_delete -obj $radiusMap}
  catch {repos_delete -obj $mapped_flat}
  catch {repos_delete -obj $mapped_flat_0}

  if {$guiABC(create_vtp_file)} {
    bctdat_to_vtp $guiABC(bct_dat_file) $guiABC(mesh_face_file) {} {} {} $guiABC(bct_vtp_file) $guiABC(bct_vtp_file).flow
  }
}


