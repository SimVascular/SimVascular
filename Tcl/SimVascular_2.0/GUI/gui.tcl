# Procedure: guiPPquant_calc_discrete_curvature
proc guiPPquant_calc_discrete_curvature { p1 p2 p3} {

  # mid point
  set mid_p1_p2 [math_scaleVec [math_addVectors $p1 $p2] 0.5]
  set x1 [lindex $mid_p1_p2 0]
  set y1 [lindex $mid_p1_p2 1]
  set z1 [lindex $mid_p1_p2 2]

  set mid_p2_p3 [math_scaleVec [math_addVectors $p2 $p3] 0.5]
  set x2 [lindex $mid_p2_p3 0]
  set y2 [lindex $mid_p2_p3 1]
  set z2 [lindex $mid_p2_p3 2]


  # normal vector of plane
  set vec_p2_p1 [math_subVectors $p1 $p2]
  set vec_p2_p3 [math_subVectors $p3 $p2]

  set a1 [lindex $vec_p2_p1 0]
  set b1 [lindex $vec_p2_p1 1]
  set c1 [lindex $vec_p2_p1 2]
 
  set a2 [lindex $vec_p2_p3 0]
  set b2 [lindex $vec_p2_p3 1]
  set c2 [lindex $vec_p2_p3 2]

  # origin (x,y,z)
  # x = alpha1 z + beta1
  # y = alpha2 z + beta2

  set k1 [expr $a1*$x1+$b1*$y1+$c1*$z1]
  set k2 [expr $a2*$x2+$b2*$y2+$c2*$z2]

  set alpha1 [expr $b1/$a1/[expr $b1/$a1-$b2/$a2]*[expr $c1/$a1-$c2/$a2]-$c1/$a1]
  set beta1 [expr $k1/$a1-$b1/$a1/[expr $b1/$a1-$b2/$a2]*[expr $k1/$a1-$k2/$a2]]

  set alpha2 [expr -1.0/[expr $b1/$a1-$b2/$a2]*[expr $c1/$a1-$c2/$a2]]
  set beta2 [expr 1.0/[expr $b1/$a1-$b2/$a2]*[expr $k1/$a1-$k2/$a2]]

  # norm vector of curvature plane
  set norm_vec_plane [math_cross $vec_p2_p1 $vec_p2_p3]

  set a3 [lindex $norm_vec_plane 0]
  set b3 [lindex $norm_vec_plane 1]
  set c3 [lindex $norm_vec_plane 2]

  set t_coeff [expr 1.0/[expr $a3*$alpha1+$b3*$alpha2+$c3]*[expr $a3*$x1+$b3*$y1+$c3*$z1-$a3*$beta1-$b3*$beta2]]

  # curvature origin
  set x3 [expr $alpha1*$t_coeff+$beta1]
  set y3 [expr $alpha2*$t_coeff+$beta2]
  set z3 [expr $t_coeff]

  set origin_curvature "$x3 $y3 $z3"

  # radius of curvature
  set radius [math_magnitude [math_subVectors $p1 $origin_curvature]]

  # curvature
  set curvature [expr 1.0/$radius]

  return [list $curvature $radius [math_normalize $norm_vec_plane] $origin_curvature]
}


# Procedure: guiPPquant_calc_index_projection
proc guiPPquant_calc_index_projection { coord_main_vessel coord_branch} {
  

  #-------------------------------
  # calculate coorrdinate of the projection
  # coord_main_vessel: 3D coordinates of the main vessel, where a point is being projected
  # coord_branch: 3D coordinate of a point to be projected on the main vessel
  #-------------------------------

  set numSplinePts [llength $coord_main_vessel]
  set min_distance 10000
  for {set j 0} {$j < $numSplinePts} {incr j} {
    set coord_main_vessel_j [lindex $coord_main_vessel $j]
    set distance_to_branch [math_distance $coord_main_vessel_j $coord_branch]

    if {$distance_to_branch < $min_distance} {
      set index_min_distance $j 
      set min_distance $distance_to_branch
    }
  }

  return $index_min_distance
}


# Procedure: guiPPquant_calc_parallel_ref
proc guiPPquant_calc_parallel_ref {} {

  global guiSVvars
  
  set main_path $guiSVvars(path_quantification_spline_twistpath)
  set main_tangents $guiSVvars(path_quantification_tangents_twistpath)
  if {$main_path == "" || $main_tangents == ""} {
    return -code error "No main path or tangents selected!"
  }
  set coord_ostia_A $guiSVvars(path_quantification_path_point_path1)
  set coord_ostia_B $guiSVvars(path_quantification_path_point_path2)
  if {$coord_ostia_A == "" || $coord_ostia_B == ""} {
    return -code error "missing ostia first or second point!"
  }

  set coord_main_vessel_spline $main_path
  set coord_main_vessel_spline_tan $main_tangents

  set index_start [guiPPquant_calc_index_projection $coord_main_vessel_spline $coord_ostia_A]
  set index_end   [guiPPquant_calc_index_projection $coord_main_vessel_spline $coord_ostia_B]

  set coord_ostia_A_prj [lindex $coord_main_vessel_spline $index_start]
  set coord_ostia_B_prj [lindex $coord_main_vessel_spline $index_end]

  set vec_start [math_subVectors $coord_ostia_A $coord_ostia_A_prj]
  set mag_vec_start [math_magnitude $vec_start]

  set parallel_ref {}
  set parallel_ref_for_plot {}

  lappend parallel_ref $coord_ostia_A
  lappend parallel_ref_for_plot $coord_ostia_A_prj
  lappend parallel_ref_for_plot $coord_ostia_A

  for {set i $index_start} {$i < $index_end} {incr i} {

    set next_i [expr $i+1]
    set coord_main_vessel_spline_next_i [lindex $coord_main_vessel_spline $next_i]
    set coord_main_vessel_spline_tan_i [lindex $coord_main_vessel_spline_tan $i]
    set coord_main_vessel_spline_tan_next_i [lindex $coord_main_vessel_spline_tan $next_i]
    set vec_ref_i [math_cross $coord_main_vessel_spline_tan_next_i $coord_main_vessel_spline_tan_i]

    set mag_invariant_axis [math_magnitude $vec_ref_i]   
    set vec_ref_i_normalized [math_normalize $vec_ref_i]
    set vec_ref_i_scaled [math_scaleVec $vec_ref_i_normalized $mag_vec_start]
    
    set ang_wrt_ref_deg [math_radToDeg [math_angleBtw3DVectors $vec_ref_i $vec_start]]

    # find sign to determine rotation orientation
    set cross_ref_start [math_cross $vec_ref_i $vec_start ]  
    set dot_tan_vec_i_cross_ref_start [math_dot $coord_main_vessel_spline_tan_i $cross_ref_start]
    set sign_criteria $dot_tan_vec_i_cross_ref_start

    if { $sign_criteria < 0} {
      set sign_axis -1
    } else {
      set sign_axis 1
    }

    set rot_axis_with_sign [math_scaleVec $coord_main_vessel_spline_tan_next_i $sign_axis]
    set n_taylor_expansion 11
    set rot_mat [calc_rotation_matrix $rot_axis_with_sign $ang_wrt_ref_deg $n_taylor_expansion]

    set vec_start [math_matrix_mult_3x1 $rot_mat $vec_ref_i_scaled]
    set parallel_ref_j [math_addVectors $vec_start $coord_main_vessel_spline_next_i]
    lappend parallel_ref $parallel_ref_j
    lappend parallel_ref_for_plot $parallel_ref_j 
  }

  lappend parallel_ref_for_plot $coord_ostia_B_prj
  lappend parallel_ref_for_plot $coord_ostia_B

  set n_parallel_ref [llength $parallel_ref]
  set parallel_ref_last_point [lindex $parallel_ref [expr $n_parallel_ref-1]]

  set vec_branch_next_i [math_subVectors $coord_ostia_B $coord_ostia_B_prj]
  set vec_ref_next_i [math_subVectors $parallel_ref_last_point $coord_ostia_B_prj]
  set angle_of_separation_deg [math_radToDeg [math_angleBtw3DVectors $vec_branch_next_i $vec_ref_next_i]]

  set guiSVvars(path_quantification_spline_refpath) $parallel_ref_for_plot
  set guiSVvars(path_quantification_angle_of_separation) $angle_of_separation_deg

  puts "Angle of separation (deg): $angle_of_separation_deg"
}


# Procedure: guiPPsavePaths
proc guiPPsavePaths {} {

  # check for missing splines and ask to create
  global gPathPoints

  set tmppaths [array names gPathPoints *,name]
  set paths {}
  foreach i $tmppaths {
    # if the user forgets to specify a pathId, ignore path
    if {[llength [split [string trim $i] ,]] != 2} {
      puts "ERROR:  invalid blank pathId being ignored."
      continue
    }
    set pathidnum [string trim [lindex [split $i ,] 0]]
    # ignore non-integer pathids
    if [math_isInt $pathidnum] {
      lappend paths $pathidnum
    } else {
      puts "ERROR: pathId ($pathidnum) is not an integer and is being ignored."
    }
  }
  set paths [lsort -integer $paths]
  set splines [lsort [array names gPathPoints *,splinePts]]
  # eliminate blanks from spline list
  set keepsplines {}
  foreach i $splines {
    if {[llength $gPathPoints($i)] > 0} {
      # if the user forgets to specify a pathId, ignore path
      if {[llength [split [string trim $i] ,]] != 2} {
        puts "ERROR:  invalid blank pathId being ignored."
        continue
      }
      set pathidnum [string trim [lindex [split $i ,] 0]]
      # ignore non-integer pathids
      if [math_isInt $pathidnum] {
        lappend keepsplines [string trim [lindex [split $i ,] 0]]
      } else {
        puts "ERROR: pathId ($pathidnum) is not an integer and is being ignored."
      }
    }
  }
  set splines $keepsplines

  #puts "path: $paths"
  #puts "splinePts: $splines"

  if {[llength $paths] != [llength $splines]} {
    set yesno [tk_messageBox -default yes  -message "Generate missing splines?"  -title "Missing Splines Detected"  -type yesno]
    switch -- $yesno {
       yes {
          global symbolicName
          set ewidget $symbolicName(guiPPchooserPathID)
          foreach p $paths {
      if {[lsearch -exact $splines $p] < 0} {
              $ewidget delete 0 end
              $ewidget insert end $p
              guiPPchooserSplinePts
              #puts "splining $p"
      }
    }
       }
       no {
       }
     }
  }

  global gFilenames
  set initialdirectory [file dirname $gFilenames(path_file)]
  set initialfilename [file tail $gFilenames(path_file)]
  set fn [tk_getSaveFile -filetypes {{"SimVascular Path File" {.paths .PATHS}} {"All Files" *}}  -title "Save SimVascular Path File"  -initialfile $initialfilename -initialdir $initialdirectory ]

  if {$fn == ""} {
    return
  }

  set gFilenames(path_file) $fn
  if { [string tolower [file extension gFilenames(path_file)]] != ".paths"} {
    set gFilenames(path_file) $gFilenames(path_file).paths 
    puts $gFilenames(path_file)
  }




  # write stuff here
  global gFilenames
  set fp [open $gFilenames(path_file) "w"]

  global gPathPoints
  puts $fp "#  Geodesic path file version 2.0 (from guiPP)"
  puts $fp "global gPathPoints"
  foreach i [array names gPathPoints] {

    # once again, check to make sure there are no
    # invalid path ids
    set pathId [string trim [lindex [split $i ,] 0]]
    if {![math_isInt $pathId]} {
       puts "Warning: gPathPoints($i) ignored."
       continue
    }
    if {[lindex [split $i ,] 1] == "splinePts"} {

      set splinePts $gPathPoints($i)
      if {$splinePts == ""} {
  puts $fp "set gPathPoints($i) \{$gPathPoints($i)\}"
      } else {
        puts $fp "set gPathPoints($i) \[list \\"
    for {set j 0} {$j < [llength $splinePts]} {incr j} {
        puts $fp "  \{[lindex $splinePts $j]\} \\"
    }
        puts $fp "  \]"
      }

    } else {

      puts $fp "set gPathPoints($i) \{$gPathPoints($i)\}"

    }
  }
  close $fp
}


