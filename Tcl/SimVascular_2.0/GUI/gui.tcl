# Procedure: guiWSScalcMean
proc guiWSScalcMean {} {
  set cutpd /guiWSS/cutPD
  set total [geom_integrateScalarSurf -src $cutpd]
  set area [geom_surfArea -src $cutpd]
  set mean [expr double($total)/double($area)]

  global guiWSSvars
  set thrarea [geom_integrateScalarThresh -src  $cutpd -thr $guiWSSvars(area_max_value)]
