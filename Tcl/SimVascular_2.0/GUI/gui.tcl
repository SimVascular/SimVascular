# Procedure: guiPOSTselectRestartDir
proc guiPOSTselectRestartDir {} {
  
  global guiPOSTvars

  set fn [tk_getOpenFile -defaultextension restart.*.1 -initialdir [pwd] -multiple 1]
  if {$fn == ""} return

  if {[llength $fn] == 1} {
     set increment 1
     set start [string range [file extension [file rootname [file tail $fn]]] 1 end]
     set stop $start
  }
  set stepnos {}
  foreach i $fn {
      puts "[file extension [file extension [file tail $fn]]]"
      set stepno [string range [file extension [file rootname [file tail $i]]] 1 end]
      #puts "$stepno"
      lappend stepnos $stepno
  }
  set stepnos [lsort -integer -unique $stepnos]
  if {[llength $stepnos] == 1} {
     set start [lindex $stepnos 0]
     set stop $start
     set increment 1
  } else {
    set start [lindex $stepnos 0]
    set stop [lindex $stepnos end]
    set increment [expr [lindex $stepnos 1] - [lindex $stepnos 0]]
  }

  set guiPOSTvars(restartFilesDir) [file dirname [lindex $fn 0]]
  set guiPOSTvars(start) $start
  set guiPOSTvars(stop) $stop
  set guiPOSTvars(increment) $increment
}


