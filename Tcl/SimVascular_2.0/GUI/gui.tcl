# Procedure: guiPPloadPaths
proc guiPPloadPaths {} {

  global gFilenames

  set fn [tk_getOpenFile -filetypes {{"SimVascular Path File" {.paths .PATHS}} {"All Files" *}}  -title "Read SimVascular Path File"  -initialfile $gFilenames(path_file)]

  if {$fn != ""} {
     set gFilenames(path_file) $fn
     guiFNMloadHandPaths
  }

  # check for missing splines and ask to create
  global gPathPoints
  set tmppaths [array names gPathPoints *,name]
  set paths {}
  foreach i $tmppaths {
    lappend paths [string trim [lindex [split $i ,] 0]]
  }
  set paths [lsort -integer $paths]
  set splines [lsort [array names gPathPoints *,splinePts]]
  # eliminate blanks from spline list
  set keepsplines {}
  foreach i $splines {
    if {[llength $gPathPoints($i)] > 0} {
      lappend keepsplines [string trim [lindex [split $i ,] 0]]
    }
  }
  set splines $keepsplines

  #puts "path: $paths"
  #puts "splinePts: $splines"

  if {[llength $paths] != [llength $splines]} {
    set yesno [tk_messageBox -default yes  -message "Generate missing splines?"  -title "Missing Splines Detected"  -type yesno]
    switch -- $yesno {
       yes {
          return -code error "ERROR:  Currently must load without generating missings splines."
          global symbolicName
          set ewidget $symbolicName(guiPPchooserPathID)
          foreach p $paths {
      if {[lsearch -exact $splines $p] < 0} {
              $ewidget delete 0 end
              $ewidget insert end $p
              guiPPchooserSplinePts
              #puts "splining $p"
      }
    }
       }
       no {
       }
     }
  }
  return
}


# Procedure: guiPPprobeData
proc guiPPprobeData {} {

  global gImageVol
  global guiPPchooserIncrement
  global symbolicName
  global gOptions
  if {$gOptions(orientImgVolToRAS) == 0} {
    # old xyz system
    set org_x [lindex $gImageVol(vtk_org_xyz) 0]
    set org_y [lindex $gImageVol(vtk_org_xyz) 1]
    set org_z [lindex $gImageVol(vtk_org_xyz) 2]
    set ext_i $gImageVol(ext_i)
    set ext_j $gImageVol(ext_j)
    set ext_k $gImageVol(ext_k)
    set vdims_x $gImageVol(vdims_x)
    set vdims_y $gImageVol(vdims_y)
    set vdims_z $gImageVol(vdims_z)
    set voi_x0 $gImageVol(voi_x0)
    set voi_x1 $gImageVol(voi_x1)
    set voi_y0 $gImageVol(voi_y0)
    set voi_y1 $gImageVol(voi_y1)
    set voi_z0 $gImageVol(voi_z0)
    set voi_z1 $gImageVol(voi_z1)
  } else {
    # properly oriented volume in ras space
    set org_x [lindex $gImageVol(vtk_org_ras) 0]
    set org_y [lindex $gImageVol(vtk_org_ras) 1]
    set org_z [lindex $gImageVol(vtk_org_ras) 2]
    set ext_i [lindex $gImageVol(ext_ras) 0]
    set ext_j [lindex $gImageVol(ext_ras) 1]
    set ext_k [lindex $gImageVol(ext_ras) 2]
    set vdims_x [lindex $gImageVol(vdims_ras) 0]
    set vdims_y [lindex $gImageVol(vdims_ras) 1]
    set vdims_z [lindex $gImageVol(vdims_ras) 2]
    set voi_x0 [lindex $gImageVol(voi_ras) 0]
    set voi_x1 [lindex $gImageVol(voi_ras) 1]
    set voi_y0 [lindex $gImageVol(voi_ras) 2]
    set voi_y1 [lindex $gImageVol(voi_ras) 3]
    set voi_z0 [lindex $gImageVol(voi_ras) 4]
    set voi_z1 [lindex $gImageVol(voi_ras) 5]
  }

  set increment $guiPPchooserIncrement
  global gInteractorRen
  set curpos [vis_pointWidgetGetPosition $gInteractorRen guiPP]

  global guiPPchooserCurPos
  set orgcurpos $guiPPchooserCurPos
  global guiPPchooserFixAxisX
  global guiPPchooserFixAxisY
  global guiPPchooserFixAxisZ

  if {$guiPPchooserFixAxisX != 0} {
    set x [lindex $orgcurpos 0]
  } else {
    set x [lindex $curpos 0]
  }
  if {$guiPPchooserFixAxisY != 0} {
    set y [lindex $orgcurpos 1]
  } else {
    set y [lindex $curpos 1]
  }
  if {$guiPPchooserFixAxisZ != 0} {
    set z [lindex $orgcurpos 2]
  } else {
    set z [lindex $curpos 2]
  }
  set curpos [list $x $y $z]

  set axes_x $gImageVol(axes-x)
  set axes_y $gImageVol(axes-y)
  set axes_z $gImageVol(axes-z)

  # need to do the inverse of the code from sliders, that
  # is calculate axes location give an interactor point

  set axes_x [expr int(([lindex $curpos 0] - $org_x)/($increment*$vdims_x))]
  set axes_y [expr int(([lindex $curpos 1] - $org_y)/($increment*$vdims_y))]
  set axes_z [expr int(([lindex $curpos 2] - $org_z)/($increment*$vdims_z))]

  set gImageVol(axes-x) $axes_x
  set gImageVol(axes-y) $axes_y
  set gImageVol(axes-z) $axes_z

  global guiPPchooserXscale
  global guiPPchooserYscale
  global guiPPchooserZscale
  set guiPPchooserXscale $axes_x
  set guiPPchooserYscale $axes_y
  set guiPPchooserZscale $axes_z

  guiPPchooserUpdateXYZ
}


# Procedure: guiPPquantCalcCurvature
proc guiPPquantCalcCurvature { pathNo} {
  global guiSVvars
  set startId $guiSVvars(path_quantification_start_id_$pathNo)
  set stopId  $guiSVvars(path_quantification_stop_id_$pathNo)
  set posId  $guiSVvars(path_quantification_current_position_$pathNo)
  set windowSize $guiSVvars(path_quantification_window_size_$pathNo)
  set distances $guiSVvars(path_quantification_distances_$pathNo)
  set pts $guiSVvars(path_quantification_spline_$pathNo)

  set clampEnds $guiSVvars(path_quantification_clamp_ends_$pathNo)

  set curPosL [lindex $distances $posId]

  # need to find the starting point for curvature calc
  if {$clampEnds} {
    set minId $startId
    set maxId $stopId
  } else {
    set minId 0
    set maxId [expr $guiSVvars(path_quantification_num_spline_pts_$pathNo) - 1]
  }

  set ptOneId -1
  set ptTwoId $posId
  set ptThreeId -1

  # find point one (proximal point of curve)
  for {set i $posId} {$i >= $minId} {incr i -1} {
    if {[expr $curPosL - [lindex $distances $i]] >= $windowSize} {
       set ptOneId $i
       #puts "$ptOneId (distance: [expr $curPosL - [lindex $distances $i]])"
       #puts "$ptOneId +1 (distance: [expr $curPosL - [lindex $distances [expr $i + 1]]])"
       break
    }
  }
  # find point three (distal point on curve)
  for {set i $posId} {$i <= $maxId} {incr i 1} {
      if {[expr [lindex $distances $i] - $curPosL] >= $windowSize} {
       set ptThreeId $i
       #puts "$ptThreeId (distance: [expr [lindex $distances $i] - $curPosL])"
       break
    }
  }

  puts "ptIds for curve calc: $ptOneId $ptTwoId $ptThreeId"
  if {$ptOneId < 0 || $ptThreeId < 0} {
    set guiSVvars(path_quantification_curvature_$pathNo) "N/A"
    set guiSVvars(path_quantification_torus_radius_$pathNo) {}
    set guiSVvars(path_quantification_torus_center_$pathNo) {}
    set guiSVvars(path_quantification_torus_normal_$pathNo) {}
    guiPPquantUpdateDisplay $pathNo torus
  } else {
    set res [guiPPquant_calc_discrete_curvature [lindex $pts $ptOneId] [lindex $pts $ptTwoId] [lindex $pts $ptThreeId]]
    set guiSVvars(path_quantification_curvature_$pathNo) [lindex $res 0]
    set guiSVvars(path_quantification_torus_radius_$pathNo) [lindex $res 1]
    set guiSVvars(path_quantification_torus_center_$pathNo) [lindex $res 3]
    set guiSVvars(path_quantification_torus_normal_$pathNo) [lindex $res 2]
    guiPPquantUpdateDisplay $pathNo torus
  }

  #set guiSVvars(path_quantification_curvature) "$ptOneId $ptTwoId $ptThreeId"
}


