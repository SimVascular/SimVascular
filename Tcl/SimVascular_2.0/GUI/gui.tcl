# Procedure: lsGUIaddToGroup
proc lsGUIaddToGroup { type} {

  global lsGUIcurrentPositionNumber
  set posId $lsGUIcurrentPositionNumber
  global lsGUIcurrentPathNumber
  set pathId $lsGUIcurrentPathNumber
  global lsGUIcurrentGroup
  set group $lsGUIcurrentGroup
  global guiGROUPSvars
  global symbolicName
  global gRen3d
  global gOptions

  # check that the group exists, if it doesn't offer to
  # create
  if {$group == ""} {
    return -code error "Group name cannot be blank!"
  }

  if {[group_exists $group] == 0} {
    set yesno [tk_messageBox -default yes  -message "Current group ($group) does not exist.  It must exist to be able to add segmentations.  Create it now?"  -title "Current Group Does Not Exist"  -type yesno]
    switch -- $yesno {
       yes {
          group_create $group
       }
       no {
          return -code error "ERROR: group does not exist."
       }
    }
  }

  # need to translate hand/analytic to proper type
  if {$type == "analytic"} {
    global lsGUIanalContourType
    set type $lsGUIanalContourType
  } elseif {$type == "hand"} {
    global lsGUIhandContourType
    set type $lsGUIhandContourType
  }


  set groupItem "/group/$group/$posId"

  if {$type == "levelset"} {
    set seg /lsGUI/$pathId/$posId/ls/oriented
  } elseif {$type == "threshold"} {
    set baseName /lsGUI/$pathId/$posId
    set seg $baseName/thr/oriented
  } else {
    return -code error "Invalid segmentation type ($type)."
  }

  if {[repos_exists -obj $seg] == 0} {
    puts "WARNING:  No oriented segmentation found!"
    return
    #return -code error "ERROR:  No oriented segmentation found!"
  }
  if {[group_idtaken $group $posId] == 1} {
     puts "WARNING:  A slice existed at $posId and is being replaced with current selection!"
     group_remove $group $groupItem
  }

    catch {repos_delete -obj tmp/pd}
     if { [catch { geom_sampleLoop -src $seg -num 20 -dst tmp/pd} fid] } {
        puts "Cannot loft:\n$fid"
        catch {repos_delete -obj tmp/pd}
        tk_messageBox -title "Could not loft contour!"  -type ok -message "We could not loft this contour. Ensure that it is closed and try again."
        return
        }
    catch {repos_delete -obj tmp/pd}

  catch {repos_delete -obj $groupItem}
  geom_copy -src $seg -dst $groupItem
  foreach key [repos_getLabelKeys -obj $seg] {
    set value [repos_getLabel -obj $seg -key $key]
    repos_setLabel -obj $groupItem -key $key -value $value
  }
  group_add $group $groupItem $posId

  set tv $symbolicName(guiSV_group_tree)


  # tree stuff
  guiSV_group_update_tree
  $tv see .groups.all.$group.$posId
  $tv selection set .groups.all.$group.$posId

  set groupShow [lindex [$tv item .groups.all.$group -values] 2]
  set solidShow [lindex [$tv item .groups.all.$group -values] 1] 

  if {$solidShow == "X"} {
    lsGUIloftGroup $group
  }
  puts "groupShow $groupShow"
  if {$groupShow == "X"} { 
    guiSV_group_display_selected_groups 1
  }
}


# Procedure: lsGUIaddToGroupBatch
proc lsGUIaddToGroupBatch { type} {

  global lsGUIbatchEntry
  set entry $lsGUIbatchEntry

  # find min/max frame number
  global symbolicName
  set minFrame [expr int([$symbolicName(lsGUIpathPositionScale) cget -from])]
  set maxFrame [expr int([$symbolicName(lsGUIpathPositionScale) cget -to])]

  set allframes [string_parse $lsGUIbatchEntry $minFrame $maxFrame]
  if {[llength $allframes] == 0} {
       puts "ERROR:  No frames specified."
       return -code error "ERROR:  No frames specified."
  }
  foreach frame $allframes {
      puts "frame: $frame"
      global lsGUIcurrentPositionNumber
      set lsGUIcurrentPositionNumber $frame
      lsGUIaddToGroup $type
  }
}


# Procedure: lsGUIalignAllProfiles
proc lsGUIalignAllProfiles {} {

    global gPathBrowser

    global lsGUIcurrentGroup
    set grp $lsGUIcurrentGroup

    global lsGUIcurrentPathNumber
    set pathId $lsGUIcurrentPathNumber

    set seg /lsGUI/segment/$pathId
    foreach i [repos_subList $seg/*] {
      repos_delete -obj $i
    }

    switch -exact $gPathBrowser(align_mtd_radio) {
  dist {
      set vecFlag false
  }
  vec {
      set vecFlag true
  }
  default {
      return -code error "ERR: Unexpected profile alignment  option \[$gPathBrowser(align_mtd_radio)\]"
  }
    }

    puts ">>> Vector-based alignment flag:  \[$vecFlag\]"

    if {($grp == "") || (![group_exists $grp])} {
  return -code error "Current group $grp not valid."
    }
    set sortedList [group_get $grp]
    if {[llength $sortedList] < 2} {
       return
    }

    #
    # supersample profiles
    #

    # sample all to the same resolution as the maximal resolution
    set numSuperPts 0
    foreach profile $sortedList {
      set numpts [geom_numPts -obj $profile]
      if {$numpts > $numSuperPts} {
        set numSuperPts $numpts
      }
    }

    global lsGUInumFacetsForSurface
    set sample $lsGUInumFacetsForSurface
    #set sample $gPathBrowser(solid_sample)
    puts ">>> Num fit pts per curve:  \[$sample\]"
    if {$sample > $numSuperPts} {
       set numSuperPts $sample
    }

    if {$numSuperPts == 0} {
      return -code error "ERROR: numSuperPts cant be zero!"
    }

    puts "Sampling all contours to a resolution of $numSuperPts points."

    foreach profile $sortedList {
      catch {repos_delete -obj $profile/supersample}
      geom_sampleLoop -src $profile -num $numSuperPts -dst $profile/supersample
    }


    #
    #  align profiles
    #

    set prof [lindex $sortedList 0]
    catch {repos_delete -obj $prof/aligned}
    geom_copy -src $prof/supersample -dst $prof/aligned

    for {set i 1} {$i < [llength $sortedList]} {incr i} {

  set p [lindex $sortedList [expr $i - 1]]
  set q [lindex $sortedList $i]

        catch {repos_delete -obj $q/aligned}

  geom_alignProfile  -ref $p/aligned  -src $q/supersample -dst $q/aligned  -vecMtd $vecFlag

    }

    #
    #  sample profiles
    #

    #set sample $gPathBrowser(solid_sample)
    puts ">>> Num fit pts per curve:  \[$sample\]"

    foreach profile $sortedList {
      catch {repos_delete -obj $profile/sample}
      geom_sampleLoop -src $profile/aligned -num $sample -dst $profile/sample
    }

    set numSegs [expr [llength $sortedList] - 1]
    for {set i 0} {$i < $numSegs} {incr i} {
      set seg /lsGUI/segment/$pathId/$i
      catch {repos_delete -obj $seg}
      geom_getPts [lindex $sortedList $i]/sample pts1
      geom_getPts [lindex $sortedList [expr $i+1]]/sample pts2
      geomConnectTwoProfiles $pts1 $pts2 $seg
    }
}


