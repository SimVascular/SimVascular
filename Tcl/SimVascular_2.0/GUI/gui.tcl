# Procedure: gui3DchooserShowAllSeeds
proc gui3DchooserShowAllSeeds { clicked args} {

  global gui3Dvars
  global symbolicName
  global gRen3d

  set flag $gui3Dvars(chooserShowAllSeedsFlag)
  set state $::vis::frozen($gRen3d)
  set ::vis::frozen($gRen3d) 1

  if {$clicked == "clicked"} {
    if {$flag == 0 } {
      set spheres $gui3Dvars(displayed_spheres)
      set pds     $gui3Dvars(displayed_pds)
      set len [llength $pds]
      for {set i 0} {$i < $len} {incr i} {
        set ss  [lindex $spheres $i]
        set pd  [lindex $pds $i]
        ::vis::polyRm $gRen3d $pd
        $ss Delete
      }
      set gui3Dvars(displayed_spheres) ""
      set gui3Dvars(displayed_pds) ""
    } else {
      set mytext {}
      set tbox $symbolicName(gui3DseedsTB)
      set numlines [lindex [split [$tbox index end] .] 0]
      for {set i 1} {$i < $numlines} {incr i} {
        set line [$tbox get $i.0 $i.end]
        set line [string trim $line]
        if {[string length $line] == 0} { continue }
        if {[string index $line 0] == "\#"} { continue }
        if {[lindex $line 0] == "sphere"} {
          set r [lindex $line 1]
          set x [lindex $line 2]
          set y [lindex $line 3]
          set z [lindex $line 4]
          #jtm seed color
          set cr [lindex $gui3Dvars(vis_seed_color) 0]
          set cg [lindex $gui3Dvars(vis_seed_color) 1]
          set cb [lindex $gui3Dvars(vis_seed_color) 2]
          puts "rgb: $cr $cg $cb"
          set ss [tmpobj]
          vtkSphereSource $ss
          $ss SetRadius $r
          $ss SetCenter $x $y $z
          $ss Update
          set spd [$ss GetOutput]
          lappend gui3Dvars(displayed_spheres) $ss
          lappend gui3Dvars(displayed_pds) $spd
          set actor [::vis::poly $gRen3d $spd]
          [$actor GetProperty] SetOpacity $gui3Dvars(sphere_opacity)
          [$actor GetProperty] SetColor $cr $cg $cb
        } else { 
          puts "WARNING: line ignored ($line)" 
        }
      }
    }
    # end clicked
  } else {
    if {$flag != 0} {
      set r [lindex $args 0]
      set x [lindex $args 1]
      set y [lindex $args 2]
      set z [lindex $args 3]
      set ss [tmpobj]
      vtkSphereSource $ss
      $ss SetRadius $r
      $ss SetCenter $x $y $z
      $ss Update
      set spd [$ss GetOutput]
      lappend gui3Dvars(displayed_spheres) $ss
      lappend gui3Dvars(displayed_pds) $spd
      set actor [::vis::poly $gRen3d $spd]
      [$actor GetProperty] SetOpacity $gui3Dvars(sphere_opacity)
      set cr [lindex $gui3Dvars(vis_seed_color) 0]
      set cg [lindex $gui3Dvars(vis_seed_color) 1]
      set cb [lindex $gui3Dvars(vis_seed_color) 2]
      [$actor GetProperty] SetColor $cr $cg $cb
    }
  }
  set ::vis::frozen($gRen3d) $state
  vis_render $gRen3d
}


