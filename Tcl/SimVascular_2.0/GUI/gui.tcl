package require tile


# procedure to show window ShowWindow.guiCV
proc ShowWindow.guiCV { args} {
# xf ignore me 7

  # build widget .guiCV
  if {"[info procs XFEdit]" != ""} {
    catch "XFDestroy .guiCV"
  } {
    catch "destroy .guiCV"
  }
  toplevel .guiCV   -background {white}

  # Window manager configurations
  wm positionfrom .guiCV program
  wm sizefrom .guiCV program
  wm maxsize .guiCV 2560 1600
  #wm minsize .guiCV 1890 1155
  wm minsize .guiCV 124 95
  wm protocol .guiCV WM_DELETE_WINDOW {mainGUIexit}
  wm title .guiCV {SimVascular}


  # build widget .guiCV.tframe2
  ttk::frame .guiCV.tframe2  -borderwidth {0}  -relief {flat}  -width {30}  -height {30}

  # build widget .guiCV.tframe2.tmenubutton0
  ttk::menubutton .guiCV.tframe2.tmenubutton0  -menu {.guiCV.tframe2.tmenubutton0.m}  -text {File}

  # build widget .guiCV.tframe2.tmenubutton0.m
  menu .guiCV.tframe2.tmenubutton0.m  -activeborderwidth {1}  -borderwidth {1}  -font {Helvetica 10}  -tearoff {0}
  .guiCV.tframe2.tmenubutton0.m add command  -command {guiCV_select_project_directory}  -label {Select Project Directory}
  .guiCV.tframe2.tmenubutton0.m add separator
  .guiCV.tframe2.tmenubutton0.m add command  -command {guiCVloadVTI}  -label {Load Image (VTI)}
  .guiCV.tframe2.tmenubutton0.m add command  -command {guiCVloadMha}  -label {Load Image (MetaImage)}
  .guiCV.tframe2.tmenubutton0.m add command  -command {ShowWindow.guiDICOM}  -label {Load Images (DICOM)}
  .guiCV.tframe2.tmenubutton0.m add separator
  .guiCV.tframe2.tmenubutton0.m add command  -command {guiPPloadPaths}  -label {Load Paths}
  .guiCV.tframe2.tmenubutton0.m add command  -command {guiPPsavePaths}  -label {Save Paths}
  .guiCV.tframe2.tmenubutton0.m add separator
  .guiCV.tframe2.tmenubutton0.m add command  -command {guiSV_group_load_groups}  -label {Load Surfaces}
  .guiCV.tframe2.tmenubutton0.m add command  -command {guiSV_group_save_groups}  -label {Save Surfaces}
  .guiCV.tframe2.tmenubutton0.m add separator
  .guiCV.tframe2.tmenubutton0.m add command  -command {guiCV_select_cpm_directory}  -label {Load CPM}

  # build widget .guiCV.tframe2.tmenubutton1
  ttk::menubutton .guiCV.tframe2.tmenubutton1  -menu {.guiCV.tframe2.tmenubutton1.m}  -text {Image Menus}

  # build widget .guiCV.tframe2.tmenubutton1.m
  menu .guiCV.tframe2.tmenubutton1.m  -activeborderwidth {1}  -borderwidth {1}  -font {Helvetica 10}  -tearoff {0}
  .guiCV.tframe2.tmenubutton1.m add checkbutton  -variable {guiCVvars(show_tab_image_volume_properties)}  -command {guiCVshowHideMenu guiCV_notebook_image "Volume Properties"}  -label {Volume Properties}
  .guiCV.tframe2.tmenubutton1.m add checkbutton  -variable {guiCVvars(show_tab_image_volume_rendering)}  -command {guiCVshowHideMenu guiCV_notebook_image "Volume Rendering"}  -label {Volume Rendering}
  .guiCV.tframe2.tmenubutton1.m add checkbutton  -variable {guiCVvars(show_tab_image_subvolume_properties)}  -command {guiCVshowHideMenu guiCV_notebook_image "Subvolume Properties"}  -label {Subvolume Properties}
  .guiCV.tframe2.tmenubutton1.m add checkbutton  -variable {guiCVvars(show_tab_image_subvolume_rendering)}  -command {guiCVshowHideMenu guiCV_notebook_image "Subvolume Rendering"}  -label {Subvolume Rendering}
  .guiCV.tframe2.tmenubutton1.m add checkbutton  -variable {guiCVvars(show_tab_image_mip)}  -command {guiCVshowHideMenu guiCV_notebook_image "MIP"}  -label {MIP}
  .guiCV.tframe2.tmenubutton1.m add checkbutton  -variable {guiCVvars(show_tab_image_filters)}  -command {guiCVshowHideMenu guiCV_notebook_image "Filters"}  -label {Filters}
  .guiCV.tframe2.tmenubutton1.m add checkbutton  -variable {guiCVvars(show_tab_image_info)}  -command {guiCVshowHideMenu guiCV_notebook_image "Info"}  -label {Info}

  # build widget .guiCV.tframe2.tmenubutton2
  ttk::menubutton .guiCV.tframe2.tmenubutton2  -menu {.guiCV.tframe2.tmenubutton2.m}  -text {Work Menus}

  # build widget .guiCV.tframe2.tmenubutton2.m
  menu .guiCV.tframe2.tmenubutton2.m  -activeborderwidth {1}  -borderwidth {1}  -font {Helvetica 10}  -tearoff {0}
  .guiCV.tframe2.tmenubutton2.m add checkbutton  -variable {guiCVvars(show_tab_main_console)}  -command {guiCVshowHideMenu guiCV_notebook_main "Console"}  -label {Console}
  .guiCV.tframe2.tmenubutton2.m add checkbutton  -variable {guiCVvars(show_tab_main_dicom)}  -command {guiCVshowHideMenu guiCV_notebook_main "DICOM"}  -label {DICOM}
  .guiCV.tframe2.tmenubutton2.m add checkbutton  -variable {guiCVvars(show_tab_main_paths)}  -command {guiCVshowHideMenu guiCV_notebook_main "Paths"}  -label {Paths}
  .guiCV.tframe2.tmenubutton2.m add checkbutton  -variable {guiCVvars(show_tab_main_2d_segmentation)}  -command {guiCVshowHideMenu guiCV_notebook_main "2D Segmentation"}  -label {2D Segmentation}
  .guiCV.tframe2.tmenubutton2.m add checkbutton  -variable {guiCVvars(show_tab_main_3d_segmentation)}  -command {guiCVshowHideMenu guiCV_notebook_main "3D Segmentation"}  -label {3D Segmentation}
  .guiCV.tframe2.tmenubutton2.m add checkbutton  -variable {guiCVvars(show_tab_main_model)}  -command {guiCVshowHideMenu guiCV_notebook_main "Model"}  -label {Model}
  .guiCV.tframe2.tmenubutton2.m add checkbutton  -variable {guiCVvars(show_tab_main_meshing)}  -command {guiCVshowHideMenu guiCV_notebook_main "Meshing"}  -label {Meshing}
  .guiCV.tframe2.tmenubutton2.m add checkbutton  -variable {guiCVvars(show_tab_main_pcmri)}  -command {guiCVshowHideMenu guiCV_notebook_main "PCMRI"}  -label {PCMRI}
  .guiCV.tframe2.tmenubutton2.m add checkbutton  -variable {guiCVvars(show_tab_main_simulations)}  -command {guiCVshowHideMenu guiCV_notebook_main "Simulations"}  -label {Simulations}
  .guiCV.tframe2.tmenubutton2.m add checkbutton  -variable {guiCVvars(show_tab_main_displays)}  -command {guiCVshowHideMenu guiCV_notebook_main "Displays"}  -label {Displays}
  .guiCV.tframe2.tmenubutton2.m add checkbutton  -variable {guiCVvars(show_tab_main_cpm_results)}  -command {guiCVshowHideMenu guiCV_notebook_main "CPM Results"}  -label {CPM Results}
  .guiCV.tframe2.tmenubutton2.m add separator
  .guiCV.tframe2.tmenubutton2.m add checkbutton  -variable {guiCVvars(show_tab_model_parasolid)}  -command {guiCVshowHideMenu guiCV_notebook_model "Parasolid"}  -label {Parasolid}
