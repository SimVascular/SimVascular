# Procedure: guiPOSTp2vis
proc guiPOSTp2vis {} {

  global guiPOSTvars

  set startnum $guiPOSTvars(start)
  set endnum   $guiPOSTvars(stop)
  set incrnum  $guiPOSTvars(increment)
  set prefix   $guiPOSTvars(out_prefix)
  set ascflag  $guiPOSTvars(phasta_format)
  set gzipped  $guiPOSTvars(vis_format)
  set outdir   $guiPOSTvars(output_dir)
  set indir    $guiPOSTvars(input_dir)

  if {$guiPOSTvars(displacement_flag) != 0} {
    set dispflag "-disp"
  } else {
    set dispflag ""
  }
  if {$guiPOSTvars(traction_flag) != 0} {
    set tractflag "-bflux"
  } else {
    set tractflag ""
  }
  if {$guiPOSTvars(wss_flag) != 0} {
    set wssflag "-wss"
  } else {
    set wssflag ""
  }

  global gExternalPrograms
  set cvpost $gExternalPrograms(cvpostsolver)

  for {set i $startnum} {$i <= $endnum} {incr i $incrnum} {
    puts "creating file $prefix\_res$i.vis"
    exec $cvpost -sn $i -indir $indir -outdir $indir $ascflag $dispflag $tractflag $wssflag -vis [file join $outdir $prefix\_res$i.vis]
  }

  puts "creating file $prefix\_mesh.vis"
  exec $cvpost -sn $startnum -indir $indir -outdir $indir  $ascflag -vismesh [file join $outdir $prefix\_mesh.vis]
}


# Procedure: guiPOSTselectDir
proc guiPOSTselectDir { dir} {

   if {$dir == "input_dir"} {
     set otherdir output_dir
   } else {
     set otherdir input_dir
   }

   set tryme [tk_chooseDirectory -initialdir [pwd]]
   if {$tryme != ""} {
      global guiPOSTvars
      set guiPOSTvars($dir) $tryme
      if {$guiPOSTvars($dir) != $guiPOSTvars($otherdir)} {
        set yesno [tk_messageBox -default yes  -message "Usually you want the input and output directories to be the same.  Set them equal now?"  -title "Input Dir .ne. Output Dir"  -type yesno]
        switch -- $yesno {
          yes {
            set guiPOSTvars($otherdir) $tryme
          }
          no {
            # do nothing
          }
         }
      }
   }
}


