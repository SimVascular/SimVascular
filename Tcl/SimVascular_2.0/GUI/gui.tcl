  ttk::panedwindow .guiCV.tframe3.tpanedwindow4.tframe5.tpanedwindow45.tframe36.tpanedwindow0

  # build widget .guiCV.tframe3.tpanedwindow4.tframe5.tpanedwindow45.tframe36.tpanedwindow0.tframe1
  ttk::frame .guiCV.tframe3.tpanedwindow4.tframe5.tpanedwindow45.tframe36.tpanedwindow0.tframe1  -borderwidth {0}  -relief {flat}  -width {30}  -height {30}

  # build widget .guiCV.tframe3.tpanedwindow4.tframe5.tpanedwindow45.tframe36.tpanedwindow0.tframe1.pane0
  if {[catch "panedwindow .guiCV.tframe3.tpanedwindow4.tframe5.tpanedwindow45.tframe36.tpanedwindow0.tframe1.pane0"]} {
    if {"[info procs XFEdit]" != ""} {
      XFProcError "Unknown widget type: panedwindow"
      return
    } {
      puts stderr "Unknown widget type: panedwindow"
    }
  }
  .guiCV.tframe3.tpanedwindow4.tframe5.tpanedwindow45.tframe36.tpanedwindow0.tframe1.pane0 config  -handlesize {0}  -orient {vertical}  -sashwidth {0}

  # build widget .guiCV.tframe3.tpanedwindow4.tframe5.tpanedwindow45.tframe36.tpanedwindow0.tframe1.pane0.pane0
  if {[catch "panedwindow .guiCV.tframe3.tpanedwindow4.tframe5.tpanedwindow45.tframe36.tpanedwindow0.tframe1.pane0.pane0"]} {
