# Procedure: guiCV_squeezer_labelframe
proc guiCV_squeezer_labelframe { frame widget} {

  global guiCVvars

  if {![info exists guiCVvars($widget$frame)]} {
   set guiCVvars($widget$frame) 1
   pack configure $widget$frame -side bottom -fill x -expand false
   return
  }

  # add in some simple defaults if pack params haven't been saved
  if {![info exists guiCVvars($widget$frame\_pack_info)]} {
     set guiCVvars($widget$frame\_pack_info) "-side top -fill x -expand false"
  }

  if {!$guiCVvars($widget$frame)} {
    #puts "$widget$frame"
    array set opts $guiCVvars($widget$frame\_pack_info)
    #foreach i [array names opts] {
      #puts "$i: $opts($i)"
      #if {$i != "-in" } {
      #  
      #}
    #}
    pack configure $widget$frame -side $opts(-side) -fill $opts(-fill) -expand $opts(-expand) -anchor $opts(-anchor)
    set guiCVvars($widget$frame) 1
    set orgtext [$widget cget -text]
    if {[string range $orgtext end-2 end] == {[+]}} {
      set orgtext [string range $orgtext 0 end-4]
    } 
    $widget configure -text "$orgtext \[\-\]" 
  } else {
    set guiCVvars($widget$frame\_pack_info) [pack info $widget$frame]
    set guiCVvars($widget$frame) 0
    set reqwidth [winfo reqwidth $widget$frame]
    pack forget $widget$frame
    $widget configure -height 25 -width $reqwidth
    set orgtext [$widget cget -text]
    if {[string range $orgtext end-2 end] == {[-]}} {
      set orgtext [string range $orgtext 0 end-4]
    } 
    $widget configure -text "$orgtext \[\+\]" 
  }
}


