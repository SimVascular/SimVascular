  set x [expr [winfo pointerx $widget] - [winfo rootx $widget]] 
  set y [expr [winfo pointery $widget] - [winfo rooty $widget]] 

  $cursorPoints Reset

  set desiredNumPts [expr [$selectedPoints GetNumberOfPoints] - 1]
  if {$desiredNumPts <= 0} {
     $selectedLines Reset
     $selectedPoints  Reset
     foreach renWin {lsRenWinMag lsRenWinPot} {
       global symbolicName
       set widget $symbolicName($renWin).r1
       if {[info commands $widget] != ""} {
           $widget Render
       }
     }
     return
  }

  $selectedLines Reset

  foreach renWin {lsRenWinMag lsRenWinPot} {
    global symbolicName
    set widget $symbolicName($renWin).r1
    if {[info commands $widget] != ""} {
       $widget Render
    }
  }

  $selectedPoints SetNumberOfPoints $desiredNumPts
  $selectedPoints Squeeze

  if {$desiredNumPts > 1} {

    # not sure how to remove just the last cell, lets rebuild from scratch
    for {set i 0} {$i < [expr $desiredNumPts - 1]} {incr i} {
      puts "$i: [expr 2*$i] [expr 2*$i+1]"
      $selectedLines InsertNextCell 2
      $selectedLines InsertCellPoint $i
      $selectedLines InsertCellPoint [expr $i+1]
    }
  }

  foreach renWin {lsRenWinMag lsRenWinPot} {
      global symbolicName
      set widget $symbolicName($renWin).r1
      if {[info commands $widget] != ""} {
           $widget Render
      }
  }

  lsGUI_hand_show_cursor $widget $x $y
}


# Procedure: lsGUI_hand_show_cursor
proc lsGUI_hand_show_cursor { widget x y} {


  set selectedPoints lsGUItmp-handdraw-pts1

  set cursorPoints lsGUItmp-handdraw-crsr-pts1

  global lsGUImagWindow
  global lsGUIpotWindow

  set x [expr [winfo pointerx $widget] - [winfo rootx $widget]] 
  set y [expr [winfo pointery $widget] - [winfo rooty $widget]] 

  global symbolicName
  if {[file rootname $widget] == $symbolicName(lsRenWinMag)} {
    set ren $lsGUImagWindow(ren)
  } else {
    set ren $lsGUIpotWindow(ren)
  }

  global lsGUIhanddrawShowCursorFlag
  set showit $lsGUIhanddrawShowCursorFlag

  if {!$showit} {
    return
  }

  if {[$selectedPoints GetNumberOfPoints] == 0} {return}

  # y is flipped upside down
  set height [lindex [$widget configure -height] 4]
  set y [expr $height - $y]

  set p1 [$selectedPoints GetPoint [expr [$selectedPoints GetNumberOfPoints] - 1]]

  set coord tmp-add_point_cord
  catch {$coord Delete}
  vtkCoordinate $coord
  $coord SetCoordinateSystemToDisplay
  $coord SetValue $x $y

  set worldValue [$coord GetComputedWorldValue $ren]
  $coord Delete

  set p2 [list [lindex $worldValue 0] [lindex $worldValue 1] 0]

  $cursorPoints Reset
  $cursorPoints InsertNextPoint [lindex $p1 0] [lindex $p1 1] [lindex $p1 2]
  $cursorPoints InsertNextPoint [lindex $p2 0] [lindex $p2 1] [lindex $p2 2]

  foreach renWin {lsRenWinMag lsRenWinPot} {
      global symbolicName
      set widget $symbolicName($renWin).r1
      if {[info commands $widget] != ""} {
           $widget Render
      }
  }
}


