# Procedure: guiRUNSOLVER_processFlowHistDat_file
proc guiRUNSOLVER_processFlowHistDat_file { infn cvprefn solverinpfn period numStepsPerCycle outfn} {

  set timestep [expr 1.0*$period/$numStepsPerCycle]

  catch {unset mapping}
  set faces {}

  set solverfp [open $solverinpfn r]
  while {[gets $solverfp line] >= 0} {
    if {$line == ""} continue
    if {[string index $line 0] == "\#"} continue
    if [regexp -nocase "List of Output Surfaces" $line] {
      set firstindex [expr [string first ":" $line] + 1]
      set lastindex [expr [string first "#" $line] - 1]
      if {$lastindex < 0} {
       set lastindex end
      }
      set outids [string range $line $firstindex $lastindex]
      break
    }
  }
  close $solverfp

  set facefp [open $cvprefn r]
  while {[gets $facefp line] >= 0} {
    if {$line == ""} continue
    if {[string index $line 0] == "\#"} continue
    if [regexp -nocase set_surface_id $line] {
      set mapping([lindex $line 2]) [file rootname [file rootname [file tail [lindex $line 1]]]]
      if {[lindex $line 2] == "1"} continue
      lappend faces [file rootname [file rootname [file tail [lindex $line 1]]]]
    }
  }
  close $facefp

  set ordered_faces {}
  foreach i $outids {
    lappend ordered_faces $mapping($i)
  }

  set numFaces [llength $ordered_faces]
  puts "found ($numFaces) faces!"

  if {[llength $faces] != $numFaces} {
    puts "Warning: num faces ($numFaces) may not equal to those in cvpre file ([llength $faces])!"
  }

  set outfp [open $outfn w]
  puts -nonewline $outfp "step\ttime\t"
  foreach face $ordered_faces {
    puts -nonewline $outfp "$face\t"
  }
  puts $outfp ""

  set fp [open $infn r]
  set everything [read -nonewline $fp]
  close $fp

  set numSteps [lindex $everything 0]

  puts "numSteps: $numSteps"

  set maxLength [llength $everything]

  set firstIndex 1
  for {set i 0} {$i < $numSteps} {incr i} {
    set firstIndex [expr 1 + $i*$numFaces]
    set lastIndex [expr ($i+1)*$numFaces]
    if {$firstIndex >= $maxLength || $lastIndex >= $maxLength} {
      puts "incomplete line ($i, $firstIndex, $lastIndex)"
      break
    }
    set step_res [lrange $everything $firstIndex $lastIndex]
    puts -nonewline $outfp "$i\t[expr $i%$numStepsPerCycle*$timestep]\t"
    for {set j 0} {$j < $numFaces} {incr j} {
      puts -nonewline $outfp "[lindex $step_res $j]\t"
    }
    puts $outfp ""
  }
  close $outfp
}


# Procedure: guiRUNSOLVERcreateOutputSurfacesFile
proc guiRUNSOLVERcreateOutputSurfacesFile {} {

  global guiRUNSOLVERvars

  set infn        $guiRUNSOLVERvars(hist_dat_file)
