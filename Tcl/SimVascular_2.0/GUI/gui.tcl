# Procedure: bcGUIguessParams
proc bcGUIguessParams {} {
  # this code is a copy of the code found in bcGUIlssSlice
  # related to guessing parameters.  A change in one location
  # should be mirrored in other!

  global bcGUIthrValue
  if {$bcGUIthrValue == 0} {
      return -code error "ERROR:  To guess parameters you must set a non-zero threshold value!"
  }

  global bcGUIcurrentFrameNumber
  set frame $bcGUIcurrentFrameNumber

  set mag   /gBC/pcdata/mag/$frame
  set pot   /gBC/pcdata/pot/$frame

  global bcGUIctrX
  global bcGUIctrY
  global bcGUIradius
  global bcGUIlsParams
  global bcGUInumSamplePoints
  set numSamplePoints $bcGUInumSamplePoints

  # need header to convert voxel number into coord
  global gBC
  set imageInfo $gBC(phase_contrast_params)
  foreach i $imageInfo {
        set [lindex $i 0] [lindex $i 1]
  }

  set radius  $bcGUIradius
  set x [expr double([lindex $voxel_dims 0])*double($bcGUIctrX)]
  set y [expr double([lindex $voxel_dims 1])*double($bcGUIctrY)]
  set z 0
  set kt $bcGUIlsParams(Kthr)
  set klow $bcGUIlsParams(Klow)
  set kupp $bcGUIlsParams(Kupp)
  set magGradFactor $bcGUIlsParams(MagFactor)
  set gridType SparseGrid
  set maxTS $bcGUIlsParams(MaxSteps)
  set gridFactor $bcGUIlsParams(GridFactor)
  set isotropicFlag 1
  set goodnessStop $bcGUIlsParams(GoodnessStop)

    puts "Guessing parameters for frame $frame."
    #puts "  Reseting ctrX and ctrY to zero!"
    #set bcGUIctrX 0
    #set bcGUIctrY 0
    # threshold slice with current threshold setting
    bcGUIthrSlice
    set picked /gBC/pcdata/mag/$frame/thr/selected

    if {[repos_exists -obj $picked] == 0} {
        puts "  No selected contour found.  Skipping $picked."
        return
    }
    # need the center of the selected contour and the min radius
    # from it to the nearest point on the threshold section
    set center [geom_avgPt -obj $picked]
    # can't use closestPt since there are points in picked
    # not on the boundary!
    #set closestpt [geom_closestPt $picked $center]
    #set minr [math_distance $center $closestpt]
    set minr 1000000
    set closestPt {}
    foreach i [geom_getOrderedPts -obj $picked] {
      set tmpdist [math_distance $i $center]
      if  {$tmpdist < $minr} {
        set closestPt $i
  set minr $tmpdist
      }
    }
    if {$closestPt == ""} {
      return -code error "ERROR:  Could not find minimum radius."
    }

    puts "  center: $center closestPt: $closestPt minr: $minr"

    # reset the parameters if it makes sense.  All sorts of stuff
    # hard coded below.  As Ken would say, the weak of heart should
    # avert their eyes...

    if {$minr > 4.0} {
       set gridFactor 1.0
    } elseif {$minr > 2.0} {
       set gridFactor 0.5
    } elseif {$minr > 0.5} {
       set gridFactor 0.25
    } else {
       puts "  Minimum radius to small.  Skipping $frame."
       return
    }

    set bcGUIlsParams(gridFactor) $gridFactor
    set radius [expr 0.8*$minr]
    set bcGUIradius $radius
    set x [lindex $center 0]
    set y [lindex $center 1]
    set bcGUIctrX [expr double($x)/double([lindex $voxel_dims 0])]
    set bcGUIctrY [expr double($y)/double([lindex $voxel_dims 1])]
    set kt [expr 1.0/(0.9*$radius)]
    set klow 0.0
    set kupp [expr 1.2*$kt]
    set bcGUIlsParams(Klow) $klow
    set bcGUIlsParams(Kthr) $kt
    set bcGUIlsParams(Kupp) $kupp

    puts "  minr: $minr  radius: $radius  gridFactor: $gridFactor  x: $x  y: $y"
    puts "  bcGUIctrX: $bcGUIctrX  bcGUIctrY:  $bcGUIctrY"
    puts "  kthr: $kt  klow: $klow  kupp: $kupp"

  # update display
  bcGUIchangeFrame $frame

  return
}


# Procedure: bcGUIinterpCircle
proc bcGUIinterpCircle { type} {

  set tmpclean /bcGUI/interpCircle/clean
  set tmppd /bcGUI/interpCircle/clean/pd
  set tmpcir /bcGUI/interpCircle/cir

  catch {repos_delete -obj $tmpclean}
  catch {repos_delete -obj $tmppd}
  catch {repos_delete -obj $tmpcir}

  global bcGUIcurrentFrameNumber
  set frame $bcGUIcurrentFrameNumber

  # need to translate hand/analytic to proper type
  if {$type == "analytic"} {
    global bcGUIanalContourType
    set type $bcGUIanalContourType
  } elseif {$type == "hand"} {
    global bcGUIhandContourType
    set type $bcGUIhandContourType
  }

  if {$type == "levelset"} {
    set seg /gBC/pcdata/mag/$frame/ls
  } elseif {$type == "threshold"} {
    set seg /gBC/pcdata/mag/$frame/thr/selected
  } else {
    return -code error "Invalid segmentation type ($type)."
  }

  if {[repos_exists -obj $seg] == 0} {
    puts "WARNING:  No segmentation found!"
    return -code error "ERROR:  No oriented segmentation found!"
  }

  # clean up point list
  geom_mergePts -src $seg -dst $tmpclean

  # check that points define single region
  if {[geom_numClosedLineRegions -obj $tmpclean] != 1} {
       return -code error "Need just 1 closed region in $tmpclean"
  }

  set points [geom_getOrderedPts -obj $tmpclean]
  geom_polygonFromPts $points $tmppd
  set area [geom_surfArea -src $tmppd]

  set r [expr sqrt(double($area)/[math_pi])]
  set center [geom_avgPt -obj $seg]

  # keep the tags on the segmentation
  foreach key [repos_getLabelKeys -obj $seg] {
    set seg_keys($key) [repos_getLabel -obj $seg -key $key]
  }

  catch {repos_delete -obj $seg}

  # create the unoriented circle
  geom_circle $r 0 0 $tmpcir
  geom_translate -src $tmpcir -vec $center -dst $seg

  # reattach tags
  foreach key [array names seg_keys] {
    repos_setLabel -obj $seg -key $key -value $seg_keys($key)
  }
  if {[info exists seg_keys(replaced_with_circle)] == 0} {
    repos_setLabel -obj $seg         -key replaced_with_circle -value 1
  }

  # clean up
  catch {repos_delete -obj $tmpclean}
  catch {repos_delete -obj $tmppd}
  catch {repos_delete -obj $tmpcir}

  # update display
  bcGUIchangeFrame $frame
}


