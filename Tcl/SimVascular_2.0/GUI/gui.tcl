    puts "  kthr: $kt  klow: $klow  kupp: $kupp"

  }

  global bcGUIrunInParallel
  if {$bcGUIrunInParallel == 0} {

    set starttime [clock seconds]

    catch {lset2_main $mag $pot  $gridType $maxTS $gridFactor $stopV  $isotropicFlag $rebuildPhiFreq  $goodnessStop $acInterval  $radius $x $y $z $kt  $calcDecayConstBy $magGradFactor $potentialValueDecay  $klow $kupp   $calcPotConstBy $pFactor $potentialValuePotential  $lsres_unclean}

    #atch {lset_nate $mag $pot $radius $x $y $z  $kt $klow $kupp $magGradFactor  $gridType $maxTS $gridFactor  $isotropicFlag $goodnessStop $lsres_unclean}

    set totaltime  "[expr [clock seconds] - $starttime] seconds"

    # save the ls params with this slice
    if {[repos_exists -obj $lsres_unclean]} {

       # subsample the loop
       global bcGUInumSamplePoints
       #set numSamplePoints $bcGUInumSamplePoints
       geom_mergePts -src $lsres_unclean -dst $lsres -tol 0.001
       repos_delete -obj $lsres_unclean
       set pts [geom_getOrderedPts -obj $lsres]
       set curveLength [math_curveLength -pts $pts -closed 1]
       set numSamplePoints [expr int(ceil($curveLength/[lindex $voxel_dims 0]))]
       set bcGUInumSamplePoints $numSamplePoints
       set newpts [math_linearInterpCurve -pts $pts -closed 1 -numInterpPts $numSamplePoints]
       repos_delete -obj $lsres
       geom_mkLinesFromPts $newpts $lsres 1
       puts "curveLength: $curveLength  numSamplePoints: $numSamplePoints"

       foreach tagobj [list $lsres] {
         repos_setLabel -obj $tagobj -key radius                 -value $radius
         repos_setLabel -obj $tagobj -key center_x               -value $x
         repos_setLabel -obj $tagobj -key center_y               -value $y
         repos_setLabel -obj $tagobj -key center_z               -value $z
         repos_setLabel -obj $tagobj -key kt                     -value $kt
         repos_setLabel -obj $tagobj -key klow                   -value $klow
         repos_setLabel -obj $tagobj -key kupp                   -value $kupp
         repos_setLabel -obj $tagobj -key mag_grad_factor        -value $magGradFactor
         repos_setLabel -obj $tagobj -key grid_type              -value $gridType
         repos_setLabel -obj $tagobj -key max_num_time_steps     -value $maxTS
         repos_setLabel -obj $tagobj -key grid_factor            -value $gridFactor
         repos_setLabel -obj $tagobj -key isotropic_flag         -value $isotropicFlag
         repos_setLabel -obj $tagobj -key goodness_stop_criteria -value $goodnessStop
         repos_setLabel -obj $tagobj -key center_x_in_pixels     -value $bcGUIctrX
         repos_setLabel -obj $tagobj -key center_y_in_pixels     -value $bcGUIctrY
         repos_setLabel -obj $tagobj -key num_of_subsample_pts   -value $numSamplePoints
         repos_setLabel -obj $tagobj -key creation_method        -value "levelset"
         repos_setLabel -obj $tagobj -key creation_type          -value "levelset"
         repos_setLabel -obj $tagobj -key simulation_runtime     -value $totaltime
         repos_setLabel -obj $tagobj -key frame                  -value $frame
         repos_setLabel -obj $tagobj -key stop_velocity          -value $stopV
         repos_setLabel -obj $tagobj -key rebuild_phi_interval   -value $rebuildPhiFreq
         repos_setLabel -obj $tagobj -key area_check_interval    -value $acInterval
         repos_setLabel -obj $tagobj -key calc_decay_const_mtd   -value $calcDecayConstBy
         repos_setLabel -obj $tagobj -key explicit_pot_for_decay -value $potentialValueDecay
         repos_setLabel -obj $tagobj -key calc_pot_const_mtd     -value $calcPotConstBy
         repos_setLabel -obj $tagobj -key pFactor                -value $pFactor
         repos_setLabel -obj $tagobj -key explicit_pot_for_pot   -value $potentialValuePotential
       }

    }
    # update display
    bcGUIchangeFrame $frame

  } else {


    global tcl_platform
    if {$tcl_platform(platform) != "windows"} {
       return -code error "ERROR:  Running in Parallel not supported on UNIX."
    }
    set servers [getTclServers]
    if {[lsearch $servers "geodesic_batch_server"] < 0} {
      # check if the user wants to start the batch server
      set choicebs [tk_messageBox -title "Error: No batch server found." -message "Start a new batch server?" -icon question -type yesno]
      switch -- $choicebs {
        yes {
          batchGUIstartBatchServer
          # wait up to 30 seconds for the server to start, then return error
    for {set waitforserver 0} {$waitforserver < 30} {incr waitforserver} {
            after 1000
            set servers [getTclServers]
      if {[lsearch $servers "geodesic_batch_server"] > 0} {
              break
      }
    }
          if {$waitforserver == 30} {
            return -code error "ERROR:  server starting slowly!"
    }
  }
        no {
          return -code error "ERROR:  No ASPIRE2 batch server running!"
  }
      }
    }

    # set up directories
    global env
    global gFilenames
    file mkdir $gFilenames(tmp_dir)
    set batchdir [file join [pwd] $gFilenames(tmp_dir) pcmri]
    file mkdir $batchdir
    set baseScriptName $env(USER)-[file tail [pwd]]-[pid]-$frame
    set fullPathBSN [file join $batchdir $baseScriptName]


    # write out image slices
    repos_writeVtkStructuredPoints -obj $mag -type ascii -file $fullPathBSN-mag.vtk
    repos_writeVtkStructuredPoints -obj $pot -type ascii -file $fullPathBSN-pot.vtk

    # remove any previous seg results
    file delete $fullPathBSN-seg.vtk

    # create a script file to run in parallel
    set fp [open $fullPathBSN.tcl "w"]
    puts $fp "# geodesic_segment_autoscript 1.1"
    puts $fp ""
    puts $fp "set exit_when_done 0"
    puts $fp "if \{\[info exists batch_status\] == 0\} \{"
    puts $fp "  global tcl_rcFileName"
    puts $fp "  source \$tcl_rcFileName"
    puts $fp "  set exit_when_done 1"
    puts $fp "  set batch_status \[lindex \$argv 0\]"
    puts $fp "  catch \{set segment_number \[lindex \$argv 1\]\}"
    puts $fp "\}"
    puts $fp ""
    puts $fp "set lsres_unclean \{$lsres_unclean\}"
    puts $fp "set lsres \{$lsres\}"
    puts $fp "catch \{repos_delete -obj \$lsres\}"
    puts $fp "catch \{repos_delete -obj \$lsres_unclean\}"
    puts $fp "if \{\$batch_status == \"segment\"\} \{"
    puts $fp "  # read in magnitude and potential structured points"
    puts $fp "  catch \{tmp-sp-reader Delete\}"
    puts $fp "  vtkStructuredPointsReader tmp-sp-reader"
    puts $fp "  tmp-sp-reader SetFileName \{$baseScriptName-mag.vtk\}"
    puts $fp "  tmp-sp-reader Update"
    puts $fp "  catch \{repos_delete -obj $mag\}"
    puts $fp "  repos_importVtkSp -src \[tmp-sp-reader GetOutput\] -dst $mag"
    puts $fp "  catch \{tmp-sp-reader Delete\}"
    puts $fp "  vtkStructuredPointsReader tmp-sp-reader"
    puts $fp "  tmp-sp-reader SetFileName \{$baseScriptName-pot.vtk\}"
    puts $fp "  tmp-sp-reader Update"
    puts $fp "  catch \{repos_delete -obj $pot\}"
    puts $fp "  repos_importVtkSp -src \[tmp-sp-reader GetOutput\] -dst $pot"
    puts $fp "  file delete \{$baseScriptName-seg.vtk\}"
    puts $fp "  catch \{lset2_main $mag $pot $gridType $maxTS $gridFactor $stopV $isotropicFlag $rebuildPhiFreq $goodnessStop $acInterval $radius $x $y $z $kt $calcDecayConstBy $magGradFactor $potentialValueDecay $klow $kupp $calcPotConstBy $pFactor $potentialValuePotential $lsres_unclean\}"
    #puts $fp "  catch \{lset_nate $mag $pot $radius $x $y $z  $kt $klow $kupp $magGradFactor  $gridType $maxTS $gridFactor  $isotropicFlag $goodnessStop $lsres_unclean\}"
    puts $fp "  if \{\[repos_exists -obj $lsres_unclean\]\} \{"
