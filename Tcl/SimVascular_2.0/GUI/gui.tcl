# Procedure: img_blankVelocities
proc img_blankVelocities { velocityImg segmentation updatedImg rtnFlowRate rtnAvgVel rtnMinVel rtnMaxVel rtnArea} {

  upvar $rtnFlowRate flowRate
  upvar $rtnAvgVel avgVel
  upvar $rtnMinVel minVel
  upvar $rtnMaxVel maxVel
  upvar $rtnArea totalArea

  set flowRate 0
  set minVel 10000000.0
  set maxVel -10000000.0
  set avgVel 0
  set numCells 0

  set image [repos_exportToVtk -src $velocityImg]

  set origin [$image GetOrigin]
  set spacing [$image GetSpacing]
  set extent [$image GetExtent]
  set dims [list [expr [lindex $extent 1]+1] [expr [lindex $extent 3]+1]]
  set imageScalars [[$image GetPointData] GetScalars]
  set numScalars [$imageScalars GetNumberOfTuples]
  set area [expr double([lindex $spacing 0])*double([lindex $spacing 1])]

  set vScalars tmp-img_blankVelocities-scalars
  catch {$vScalars Delete}
  vtkFloatArray $vScalars
  $vScalars Allocate 100 100
  $vScalars SetNumberOfTuples $numScalars

  # do fake classify first so we can then call with usePrevPoly=1
  geom_ptInPoly -pgn $segmentation -pt {0 0} -usePrevPoly 0
  for {set pixel 0} {$pixel < $numScalars} {incr pixel} {
    set planeIx [expr $pixel / ([lindex $dims 0] * [lindex $dims 1])]
    set planeOffset [expr $pixel % ([lindex $dims 0] * [lindex $dims 1])]
    set rowIx [expr $planeOffset / [lindex $dims 0]]
    set colIx [expr $planeOffset % [lindex $dims 0]]

    set ptX [expr [lindex $origin 0] + ($colIx * [lindex $spacing 0])]
    set ptY [expr [lindex $origin 1] + ($rowIx * [lindex $spacing 1])]

    #puts "planeIx: $planeIx  rowIx: $rowIx  colIx: $colIx pt: $ptX $ptY"

    set classify [geom_ptInPoly -pgn $segmentation  -pt [list $ptX $ptY]  -usePrevPoly 1]
    #puts "pt: ($ptX,$ptY) classify: $classify"
    if {$classify == 1} {

      set v [$imageScalars GetTuple1 $pixel]
      $vScalars InsertTuple1 $pixel $v

      set flowRate [expr $flowRate + double($area) * double($v)]

      if {$v < $minVel} {
        set minVel $v
      }
      if {$v > $maxVel} {
        set maxVel $v
      }
      set avgVel [expr double($avgVel) + double($v)]
      incr numCells

    } else {

      $vScalars InsertTuple1 $pixel -100

    }

  }

  set avgVel [expr double($avgVel) / double($numCells)]

  set totalArea [expr double($area) * double($numCells)]

  set sp tmp-img_blankVelocitites-sp
  catch {$sp Delete}
  vtkStructuredPoints $sp
  $sp CopyStructure $image
  $sp CopyInformation $image
  [$sp GetPointData] SetScalars $vScalars
  set origin [$image GetOrigin]
  set spacing [$image GetSpacing]
  $sp SetOrigin [lindex $origin 0] [lindex $origin 1] [lindex $origin 2]
  $sp SetSpacing [lindex $spacing 0] [lindex $spacing 1] [lindex $spacing 2]
  repos_importVtkSp -src $sp -dst $updatedImg
  catch {$vScalars Delete}
  catch {$sp Delete}
}


