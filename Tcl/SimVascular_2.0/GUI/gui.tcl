# Procedure: volGUIupdateIsosurface
proc volGUIupdateIsosurface { context} {
  global gui3Dvars

  upvar #0 $context guiVars

  if {$guiVars(show_image) == 0} {return}

  # check to see if we should use seeded threshold
  if {$guiVars(use_seed_thr) == 0} {
  if { [info exists guiVars(use_alt_image)] && $guiVars(use_alt_image) } {
    set imgobj $guiVars(repos_image_obj_alt)  
    } else {
    set imgobj $guiVars(repos_image_obj)    
    }
  } else {
    set imgobj $guiVars(repos_seed_thr_obj)
  }

  set contourpd $guiVars(repos_isosurface)
  set decimator volGUIupdateIsosurface-decimator
  set connfilt volGUIupdateIsosurface-connfilt
  set smoother volGUIupdateIsosurface-smoother

  global gRen3d
  set ren $gRen3d

  set show_isosurface $guiVars(show_isosurface)
  set thresholdValue $guiVars(min_thr_value)

  # show the contour if desired
  if {$show_isosurface == 1} {
    catch {repos_delete -obj $contourpd}
    catch {vis_lodRm $gRen3d $contourpd}
    if {$guiVars(use_seed_thr) == 0} {
      set c [img_contour [repos_exportToVtk -src $imgobj] $thresholdValue]
    } else {
      set c [img_contour [repos_exportToVtk -src $imgobj] 1]
    }

    # optionally only display the largest region in the isosurface
    global gOptions
    if {$gOptions(display_only_largest_isosurface) == 1} {
      catch {$connfilt Delete}
      vtkPolyDataConnectivityFilter $connfilt
      $connfilt SetInputDataObject $c
      $connfilt SetExtractionModeToLargestRegion
      $connfilt Update
      set c [$connfilt GetOutput]
    }

    # optionally smooth isosurface
    if {$gOptions(smooth_isosurface) == 1} {
       catch {$smoother Delete}
       vtkSmoothPolyDataFilter $smoother
       $smoother SetInputDataObject $c
       $smoother SetNumberOfIterations $gOptions(smooth_num_iters)
       $smoother SetRelaxationFactor $gOptions(smooth_relax_factor)
       $smoother Update
       set c [$smoother GetOutput]
    }

    puts "num polys in original: [$c GetNumberOfPolys]"

    # we need to create a decimated version
    global gOptions
    set max_polys $gOptions(max_num_polys_for_isosurface)
    if {$max_polys != ""} {
      if {[$c GetNumberOfPolys] >= $max_polys} {
        catch {$decimator Delete}
        vtkDecimatePro $decimator
        $decimator AccumulateErrorOff
        $decimator SetInputDataObject $c
        $decimator SetTargetReduction [expr 1.0-double($max_polys)/double([$c GetNumberOfPolys])]
        $decimator Update
        puts "num polys in decimated: [[$decimator GetOutput] GetNumberOfPolys]"
        repos_importVtkPd -src [$decimator GetOutput] -dst $contourpd
        catch {$decimator Delete}
        catch {$connfilt Delete}
  catch {$smoother Delete}
      } else {
        repos_importVtkPd -src $c -dst $contourpd
  catch {$connfilt Delete}
  catch {$smoother Delete}
      }
    } else {
      repos_importVtkPd -src $c -dst $contourpd
      catch {$connfilt Delete}
      catch {$smoother Delete}
    }
    if {$gOptions(use_lod_for_isosurface)} {
      vis_lodRepos $ren $contourpd
      vis_lodSetColor $ren $contourpd 0 1 0
      vis_lodSetOpacity $ren $contourpd 1.0
    } else {
      set actor [vis_pRepos $ren $contourpd]
      eval [$actor GetProperty] SetColor [gdscGetUnixColor $gOptions(color_for_isosurface)]
      [$actor GetProperty] SetOpacity 0.33
      vis_render $ren
    }
  } else {
    catch {repos_delete -obj $contourpd}
    catch {vis_pRm $ren $contourpd}
    catch {vis_lodRm $ren $contourpd}
  }
  set gui3Dvars(smoothIsosurface) $contourpd
  set gui3Dvars(min_thr_value) $thresholdValue
}


# Procedure: volGUIupdateIsosurfaceUserCheck
proc volGUIupdateIsosurfaceUserCheck { context} {

   global guiVIB
   if {$guiVIB(show_isosurface) == 0} {
      volGUIupdateIsosurface $context
      return
   }

   set yesno [tk_messageBox -message "Creating an isosurface can take a long time.  Continue anyway?" -default no -icon question -type yesno]

   if  {$yesno == "yes"} {
     set guiVIB(show_isosurface) 1
     volGUIupdateIsosurface $context
   } else {
     set guiVIB(show_isosurface) 0
   }
}


# Procedure: volGUIupdatePointCloud
proc volGUIupdatePointCloud { context} {

    upvar #0 $context guiVars

    if {$guiVars(show_image) == 0} {return}

    set basename    $guiVars(vis_basename)
    set thrpts      $basename\_thrpts
    set thrmap      $basename\_thrmap
    set thract      $basename\_thract

    set point_cloud $guiVars(repos_point_cloud)

    # check to see if we should use seeded threshold
    if {$guiVars(use_seed_thr) == 0} {
    if { [info exists guiVars(use_alt_image)] && $guiVars(use_alt_image) } {
    set repos_image $guiVars(repos_image_obj_alt)  
      } else {
    set repos_image $guiVars(repos_image_obj)   
      }  
    } else {
      set repos_image $guiVars(repos_seed_thr_obj)
    }

    global gRen3d

    set onflag $guiVars(show_point_cloud)
    set thrMin $guiVars(min_thr_value)
    set thrMax $guiVars(max_thr_value)

    set ren $gRen3d

    if {[cmdExists $thract]} {
      if {[[$ren GetActors] IsItemPresent $thract] > 0} {
      vis_renRmActor $ren $thract
      }
    }
    if {[cmdExists $thrpts]} {
  $thrpts Delete
    }
    if {[cmdExists $thrmap]} {
  $thrmap Delete
    }
    if {![cmdExists $thract]} {
  vtkActor $thract
    }

    if {$onflag == 1} {
      catch {repos_delete -obj $point_cloud}
      if {$guiVars(use_seed_thr) == 0} {
        img_threshold -image $repos_image -result $point_cloud -min_value $thrMin  -max_value $thrMax -max_num_pts 2000000
      } else {
        img_threshold -image $repos_image -result $point_cloud -min_value 1  -max_value 1 -max_num_pts 2000000
      }
      set numthrpts [geom_numPts -obj $point_cloud]
      if {$numthrpts >= 2000000} {
         tk_messageBox -message "Your requested point cloud is to large to be fully displayed.  Please choose a more reasonable threshold value." -icon error
      }

      vtkPolyDataMapper $thrmap
      $thrmap SetInputDataObject [repos_exportToVtk -src $point_cloud]
      $thrmap ScalarVisibilityOff

      $thract SetMapper $thrmap
      [$thract GetProperty] SetPointSize 1

      vis_renAddActor $ren $thract

    }

   vis_render $ren
}


# Procedure: volGUIupdateSeed
proc volGUIupdateSeed { context centerBy} {

  upvar #0 $context guiVars

  set orgctr {}
  if {$centerBy == "manual"} {

       set orgctr $guiVars(seed_location)

  } elseif {$centerBy == "pathplan_cursor"} {

       global guiPPchooserXvar
       global guiPPchooserYvar
       global guiPPchooserZvar
       set orgctr [list $guiPPchooserXvar $guiPPchooserYvar $guiPPchooserZvar]

     } elseif {$centerBy == "interactor"} {

       set bounds $guiVars(resample_interactor_bounds)
       set llx [lindex $bounds 0]
       set lly [lindex $bounds 2]
       set llz [lindex $bounds 4]
       set urx [lindex $bounds 1]
       set ury [lindex $bounds 3]
       set urz [lindex $bounds 5]
       set orgctr [list [expr ($llx + $urx) / double(2.0)]  [expr ($lly + $ury) / double(2.0)]  [expr ($llz + $urz) / double(2.0)]]

    } elseif {$centerBy == "vol_sliders"} {
       # use of guiVIB is intentional
       global guiVIB
       set orgctr [list $guiVIB(r_coord) $guiVIB(a_coord) $guiVIB(s_coord)]
    } elseif {$centerBy == "path_point"} {
       global lsGUIpositionIdStr
       set orgctr $lsGUIpositionIdStr
    } elseif {$centerBy == "save_pt"} {
      global gImageVol
      set gImageVol(seed_pt) $guiVars(seed_location)
      return
    } elseif {$centerBy == "restore_pt"} {
      global gImageVol
      if [catch {set orgctr $gImageVol(seed_pt)}] {
         return -code error "ERROR: no saved seed location!"
      }
    } else {
       return -code error "ERROR:  Illegal centerBy code."
  }

  set guiVars(seed_location) $orgctr

  volGUIupdateSeededThreshold $context
}


# Procedure: volGUIupdateSeededThreshold
proc volGUIupdateSeededThreshold { context} {

  upvar #0 $context guiVars

  # seed contour 
  if {$guiVars(use_seed_thr) == 0} {
      catch {repos_delete -obj $guiVars(repos_seed_thr_obj)}
      return
  }

  if { [info exists guiVars(use_alt_image)] && $guiVars(use_alt_image) } {
  set imgobj $guiVars(repos_image_obj_alt)  
  } else {
  set imgobj $guiVars(repos_image_obj)    
  }
    
  # create a binary segmentation
  set thr mythr
  catch {$thr Delete}
  vtkImageThreshold $thr
  $thr SetInputDataObject [repos_exportToVtk -src $imgobj]
  $thr SetOutputScalarTypeToUnsignedChar
  $thr ThresholdBetween $guiVars(min_thr_value) $guiVars(max_thr_value)
  $thr ReplaceInOn
  $thr SetInValue 1
  $thr ReplaceOutOn
  $thr SetOutValue 0
  $thr Update

  set orgctr $guiVars(seed_location)

  set image [repos_exportToVtk -src $imgobj]
  set ijk [img_computeStructuredCoord -image $imgobj -pt $orgctr]
  puts "$ijk"
  puts "value: [lindex $ijk 2]"

  set seeder myseed
  catch {$seeder Delete}
  vtkImageSeedConnectivity $seeder
  $seeder SetDimensionality 3
  $seeder SetInputConnection [$thr GetOutputPort]
  $seeder AddSeed [lindex [lindex $ijk 0] 0]  [lindex [lindex $ijk 0] 1]  [lindex [lindex $ijk 0] 2]
  $seeder SetInputConnectValue 1
  $seeder SetOutputConnectedValue 1
  $seeder SetOutputUnconnectedValue 0
  $seeder Update

  catch {repos_delete -obj $guiVars(repos_seed_thr_obj)}
  repos_importVtkImg -src [$seeder GetOutput] -dst $guiVars(repos_seed_thr_obj)
}


# Procedure: volGUIupdateSlider
proc volGUIupdateSlider { context sliceDir numTicks} {

    upvar #0 $context guiVars

    if {$guiVars(show_image) == 0} {return}

    if { [info exists guiVars(use_alt_image)] && $guiVars(use_alt_image) } {
    set repos_image_obj $guiVars(repos_image_obj_alt)  
    } else {
    set repos_image_obj $guiVars(repos_image_obj)   
    }


    set image [repos_exportToVtk -src $repos_image_obj]
    set ldims [$image GetDimensions]
    set vdims [$image GetSpacing]

    # the calculations below assume the origin of the grid is
    # specified, NOT the centroid which is returned from
    # the structured points vtk object.  We there shift the
    # origin by half a pixel.
    #
    # Note that this is probably this shift is likely not
    # necessary since the code for the transforms looks like
    # it is using the centroids of the pixels anyway.  I just
    # didn't want to change Ken's code so I'm doing this
    # unnecessary shift.

    set imgOrigin [$image GetOrigin]
    set orig [list  [expr [lindex $imgOrigin 0] - ([lindex $vdims 0] / 2.0)]  [expr [lindex $imgOrigin 1] - ([lindex $vdims 1] / 2.0)]  [expr [lindex $imgOrigin 2] - ([lindex $vdims 2] / 2.0)] ]

    set basename $guiVars(vis_basename)
    switch -exact -- $sliceDir {
      RL {
        set ti $basename\_transformx
        set voii $basename\_voix
      }
      AP {
        set ti $basename\_transformy
        set voii $basename\_voiy
      }
      SI {
        set ti $basename\_transformz
        set voii $basename\_voiz
      }
      default { return -code error "ERR: volGUIupdateSlider"}
    }

    global gRen3d
    set ren $gRen3d

    switch -exact -- $sliceDir {
  RL {
      $ti Identity
      $ti Translate [expr ($numTicks + 0.5) * [lindex $vdims 0]  + [lindex $orig 0]]  [expr [lindex $ldims 1] * [lindex $vdims 1] / 2.0  + [lindex $orig 1]]  [expr [lindex $ldims 2] * [lindex $vdims 2] / 2.0  + [lindex $orig 2]]
      $ti Scale 1.0  [expr [lindex $ldims 1] * [lindex $vdims 1]]  [expr [lindex $ldims 2] * [lindex $vdims 2]]

      $voii SetVOI $numTicks $numTicks  0 [expr [lindex $ldims 1] - 1]  0 [expr [lindex $ldims 2] - 1]
            # vtk4.2.1 no longer allows this update directly, but things work if
            # we let the render window update it, bizarre
      #$voii Update
  }
  AP {
      $ti Identity
      $ti Translate [expr [lindex $ldims 0] * [lindex $vdims 0] / 2.0  + [lindex $orig 0]]  [expr ($numTicks + 0.5) * [lindex $vdims 1]  + [lindex $orig 1]]  [expr [lindex $ldims 2] * [lindex $vdims 2] / 2.0  + [lindex $orig 2]]
      $ti Scale [expr [lindex $ldims 0] * [lindex $vdims 0]]  1.0  [expr [lindex $ldims 2] * [lindex $vdims 2]]

      $voii SetVOI 0 [expr [lindex $ldims 0] - 1]  $numTicks $numTicks  0 [expr [lindex $ldims 2] - 1]
            # vtk4.2.1 no longer allows this update directly, but things work if
            # we let the render window update it, bizarre
      #$voii Update
  }
  SI {
      $ti Identity
      $ti Translate [expr [lindex $ldims 0] * [lindex $vdims 0] / 2.0  + [lindex $orig 0]]  [expr [lindex $ldims 1] * [lindex $vdims 1] / 2.0  + [lindex $orig 1]]  [expr ($numTicks + 0.5) * [lindex $vdims 2]  + [lindex $orig 2]]
      $ti Scale [expr [lindex $ldims 0] * [lindex $vdims 0]]  [expr [lindex $ldims 1] * [lindex $vdims 1]]  1.0

      $voii SetVOI 0 [expr [lindex $ldims 0] - 1]  0 [expr [lindex $ldims 1] - 1]  $numTicks $numTicks
            # vtk4.2.1 no longer allows this update directly, but things work if
            # we let the render window update it, bizarre
      #$voii Update
      set gVolImgBrowserParams(zv) $numTicks
  }
  default { puts "ERR: vis_img_MoveVolBrowserPlane"; return }
    }

    set guiVars(r_coord) [format {%6.03f} [lindex [$basename\_transformx GetPosition] 0]]
    set guiVars(a_coord) [format {%6.03f} [lindex [$basename\_transformy GetPosition] 1]]
    set guiVars(s_coord) [format {%6.03f} [lindex [$basename\_transformz GetPosition] 2]]


    global symbolicName
    set i $guiVars(sliderx_value)
    set j $guiVars(slidery_value)
    set k $guiVars(sliderz_value)

    set image [repos_exportToVtk -src $repos_image_obj]
    set ptId [$image ComputePointId $i $j $k]

    set guiVars(pixel_value) [[[$image GetPointData] GetScalars] GetTuple1 $ptId]

    #puts "ijk: $i $j $k ptId: $ptId scalar: $guiVars(pixel_value)"

    vis_render $ren
}


# Procedure: volGUIupdateVolRender
proc volGUIupdateVolRender { context update} {

  upvar #0 $context guiVars

  if {$guiVars(show_image) == 0} {return}

  global gRen3d
  set ren $gRen3d

  set flag $guiVars(show_vol_render)
  set opacityFn $guiVars(opacityFunction)
  set colorFn $guiVars(colorFunction)
  set gradientFn $guiVars(gradientFunction)
  set ambient $guiVars(ambientCoefficient)
  set diffuse $guiVars(diffuseCoefficient)
  set specular $guiVars(specularCoefficient)
  set specularPower $guiVars(specularPower)
  set enableShading $guiVars(enableShading)
  set sampleSize $guiVars(sampleStepSize)
 
  if { [info exists guiVars(use_alt_image)] && $guiVars(use_alt_image) } {
  set repos_image_obj $guiVars(repos_image_obj_alt)  
  } else {
  set repos_image_obj $guiVars(repos_image_obj)   
  }

  # turn on/off volume rendering
  if {$update == "all"} {
    if {$flag == 0} {
      vis_volRm $gRen3d $repos_image_obj
      return
    } elseif {$flag == 1} {
      catch {vis_volRm $gRen3d $repos_image_obj}
      vis_volRepos $gRen3d $repos_image_obj
      vis_volSetOpacityTransferFunction $ren $repos_image_obj $opacityFn
      vis_volSetColorTransferFunction $ren $repos_image_obj $colorFn
      vis_volSetGradientTransferFunction $ren $repos_image_obj $gradientFn
      vis_volSetShadingParameters $ren $repos_image_obj $ambient $diffuse $specular $specularPower
      if {$sampleSize != "auto"} {
         vis_volSetSampleDistance $ren $repos_image_obj $sampleSize
      }
      if {$enableShading} {
        vis_volShadeOn $ren $repos_image_obj
      }
      return
    }
  }

  # update select transfer function
  if {$update == "opacity"} {
      vis_volSetOpacityTransferFunction $ren $repos_image_obj $opacityFn
  } elseif {$update == "color"} {
      vis_volSetColorTransferFunction $ren $repos_image_obj $colorFn
  } elseif {$update == "gradient"} {
      vis_volSetGradientTransferFunction $ren $repos_image_obj $gradientFn
  } elseif {$update == "enable_shading"} {
      if {$enableShading} {
        vis_volShadeOn $ren $repos_image_obj
      } else {
        vis_volShadeOff $ren $repos_image_obj
      }
  } elseif {$update == "sample_size"} {
      vis_volSetSampleDistance $ren $repos_image_obj $sampleSize
  } elseif {$update == "shading"} {
      vis_volSetShadingParameters $ren $repos_image_obj $ambient $diffuse $specular $specularPower
  } else {
      return -code error "ERROR:  Illegal option ($update)"
  }

  return
}


