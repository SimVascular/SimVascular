      bcGUIflowCurByPixels
   }
}


# Procedure: bcGUIflowCur
proc bcGUIflowCur {} {

  global bcGUIcurrentFrameNumber
  global gFlow
  set frame $bcGUIcurrentFrameNumber
  global bcGUIcurrentGroup
  set group $bcGUIcurrentGroup
  global bcGUIzeroBoundVelFlag

  if {[group_idtaken $group $frame] != 1} {
    return -code error "ERROR:  id $frame doesn't exist in $group."
  }

  set object "/group/$group/$frame"

  #
  #
  # old code
  global gOptions
  if {$gOptions(calc_flow_with_constant_pixels) == 1} {
  #
  #
  #

  # only calculate flow params if object exists
  if {[repos_exists -obj $object] == 1} {
    global gBC
    if {$gBC(numVelComp) == 1} {
      set velocityData /gBC/pcdata/vel/$frame
      set warpVel /gBC/pcdata/warpVel/$frame
      catch {repos_delete -obj $warpVel}
      puts "Creating warp velocity object for $frame."
      img_blankVelocities $velocityData $object $warpVel flowRate AvgVel MinVel MaxVel totalArea
      #puts "approximate flow rate: $flowRate"
      set gFlow($frame) [list $flowRate $AvgVel $MinVel $MaxVel $totalArea]
      puts [format "%15s%15s%15s%15s%15s%15s" "Frame" "Flow (ml/min)"  "Avg V" "Min V" "Max V" "area (mm^2)"]
      set flowRate [expr double($flowRate)/1000.0*60.0]
      puts [format "%15i%15.3f%15.3f%15.3f%15.3f%15.3f" $frame $flowRate $AvgVel $MinVel $MaxVel $totalArea]
      bcGUIchangeFrame $frame
    } elseif {$gBC(numVelComp) == 3} {
      puts "Experimental 3-comp velocity flow calc code!"
      set velocityData /gBC/pcdata/vel/$frame/s
      set warpVel /gBC/pcdata/warpVel/$frame
      catch {repos_delete -obj $warpVel}
      puts "Creating warp velocity object for $frame."
      img_blankVelocities $velocityData $object $warpVel flowRate AvgVel MinVel MaxVel totalArea
      #puts "approximate flow rate: $flowRate"
      set gFlow($frame) [list $flowRate $AvgVel $MinVel $MaxVel $totalArea]
      puts [format "%15s%15s%15s%15s%15s%15s" "Frame" "Flow (ml/min)"  "Avg V" "Min V" "Max V" "area (mm^2)"]
      set flowRate [expr double($flowRate)/1000.0*60.0]
      puts [format "%15i%15.3f%15.3f%15.3f%15.3f%15.3f" $frame $flowRate $AvgVel $MinVel $MaxVel $totalArea]
      bcGUIchangeFrame $frame
    } else {
     return -code error "Illegal number of velocity components: $gBC(numVelComp)"
    }
  }
    return
  }

  #
  #
  #

  #
  #
  #
  # new code
  #
  #
  #

  if {[repos_exists -obj $object] == 1} {

    # calculate cross-sectional area (totalArea)

    set merged /tmp/bcGUIflowCur/merged
    set merged_pd /tmp/bcGUIflowCur/merged_pd
    catch {repos_delete -obj $merged}
    catch {repos_delete -obj $merged_pd}
    # clean up point list
    geom_mergePts -src $object -dst $merged
    # check that points define single region
    if {[geom_numClosedLineRegions -obj $merged] != 1} {
      return -code error "Need just 1 closed region in $merged"
    }
    set points [geom_getOrderedPts -obj $merged]
    geom_polygonFromPts $points $merged_pd
    set totalArea [geom_surfArea -src $merged_pd]
    # clean up
    catch {repos_delete -obj $merged}
    catch {repos_delete -obj $merged_pd}

    global gBC
    set warpVel /gBC/pcdata/warpVel/$frame
    catch {repos_delete -obj $warpVel}

    if {$gBC(numVelComp) == 1} {
      set velocityData /gBC/pcdata/vel/$frame
      img_createSegVelPD {} {} $velocityData 0 0 $gBC(baselineCorrectionZ) $gBC(directions) $object 0.05 $bcGUIzeroBoundVelFlag $warpVel
    } elseif {$gBC(numVelComp) == 3} {
      set vxFile /gBC/pcdata/vel/$frame/[lindex $gBC(xyz) 0]
      set vyFile /gBC/pcdata/vel/$frame/[lindex $gBC(xyz) 1]
      set vzFile /gBC/pcdata/vel/$frame/[lindex $gBC(xyz) 2]
      set helper(r) $gBC(baselineCorrectionX)
      set helper(a) $gBC(baselineCorrectionY)
      set helper(s) $gBC(baselineCorrectionZ)
      set correctX $helper([lindex $gBC(xyz) 0])
      set correctY $helper([lindex $gBC(xyz) 1])
      set correctZ $helper([lindex $gBC(xyz) 2])
      img_createSegVelPD $vxFile $vyFile $vzFile $correctX $correctY $correctZ $gBC(directions) $object 0.05 $bcGUIzeroBoundVelFlag $warpVel
    } else {
      return -code error "Illegal number of velocity components: $gBC(numVelComp)"
    }

    # calculate some parameters
    set flowRate [geom_integrateSurfaceFlux -obj $warpVel -nrm {0 0 1} -tensorType 0]
    set AvgVel [expr double($flowRate)/double($totalArea)]
    set minmax [[repos_exportToVtk -src $warpVel] GetScalarRange]
    set MinVel [lindex $minmax 0]
    set MaxVel [lindex $minmax 1]
    set gFlow($frame) [list $flowRate $AvgVel $MinVel $MaxVel $totalArea]
    puts [format "%15s%15s%15s%15s%15s%15s" "Frame" "Flow (ml/min)"  "Avg V" "Min V" "Max V" "area (mm^2)"]
    set flowRate [expr double($flowRate)/1000.0*60.0]
    puts [format "%15i%15.3f%15.3f%15.3f%15.3f%15.3f" $frame $flowRate $AvgVel $MinVel $MaxVel $totalArea]
    bcGUIchangeFrame $frame
  }

  #
  #
  #
}


# Procedure: bcGUIflowCurByPixels
proc bcGUIflowCurByPixels {} {

  global bcGUIcurrentFrameNumber
  global gFlow
  global gBC

  foreach param $gBC(phase_contrast_params) {
    set [lindex $param 0] [lindex $param 1]
  }
  set directions $gBC(directions)

  # can only tag a pixel once
  global gTaggedPixels
  if {[info exists gTaggedPixels] == 0} {
    return -code error "ERROR:  No pixels tagged!"
  }

  set frame $bcGUIcurrentFrameNumber

  # some parameters to track
  set flowRate 0
  set AvgVel  0
  set MinVel  999999
  set MaxVel -999999

  foreach pixel [lsort -dictionary [array names gTaggedPixels]] {
    set magslice /gBC/pcdata/mag/$frame
    set pt [[repos_exportToVtk -src $magslice] GetPoint $pixel]
    set x [lindex $pt 0]
    set y [lindex $pt 1]

   if {$gBC(numVelComp) == 1} {
      set velocityData /gBC/pcdata/vel/$frame
      set vx 0
      set vy 0
      set correctZ $gBC(baselineCorrectionZ)
      if {[string trim $correctZ] == ""} {
        set correctZ 0
      }
      set vz [[[[repos_exportToVtk -src $velocityData] GetPointData] GetScalars] GetTuple1 $pixel]
      #puts "vz: $vz"
      set vz [expr double([lindex $directions 2])*($vz-[expr $correctZ])]
      #puts "corrected vz: $vz"
    } elseif {$gBC(numVelComp) == 3} {
      set vxFile /gBC/pcdata/vel/$frame/[lindex $gBC(xyz) 0]
      set vyFile /gBC/pcdata/vel/$frame/[lindex $gBC(xyz) 1]
      set vzFile /gBC/pcdata/vel/$frame/[lindex $gBC(xyz) 2]
      set helper(r) $gBC(baselineCorrectionX)
      set helper(a) $gBC(baselineCorrectionY)
      set helper(s) $gBC(baselineCorrectionZ)
      set correctX $helper([lindex $gBC(xyz) 0])
      set correctY $helper([lindex $gBC(xyz) 1])
      set correctZ $helper([lindex $gBC(xyz) 2])
      # convert empty strings to zero values
      if {[string trim $correctX] == ""} {
        set correctX 0
      }
      if {[string trim $correctY] == ""} {
        set correctY 0
      }
      if {[string trim $correctZ] == ""} {
        set correctZ 0
      }
      set vx [[[[repos_exportToVtk -src $vxFile] GetPointData] GetScalars] GetTuple1 $pixel]
      set vy [[[[repos_exportToVtk -src $vyFile] GetPointData] GetScalars] GetTuple1 $pixel]
      set vz [[[[repos_exportToVtk -src $vzFile] GetPointData] GetScalars] GetTuple1 $pixel]
      set vx [expr double([lindex $directions 0])*($vx-[expr $correctX])]
      set vy [expr double([lindex $directions 1])*($vy-[expr $correctY])]
      set vz [expr double([lindex $directions 2])*($vz-[expr $correctZ])]
    } else {
      return -code error "Illegal number of velocity components: $gBC(numVelComp)"
    }

    # calculate some parameters
    set flowRate [expr $flowRate + $vz]
    set AvgVel [expr $AvgVel + $vz]
    if {$vz < $MinVel} {
      set MinVel $vz
    }
    if {$vz > $MaxVel} {
      set MaxVel $vz
    }

  }

  set numPixels [llength [array names gTaggedPixels]]
  set pixelArea [expr double([lindex $voxel_dims 0]) * double([lindex $voxel_dims 1])]
  #puts "numPixels: $numPixels  pixelArea: $pixelArea"
  set flowRate [expr double($flowRate) * double($pixelArea)]
  set AvgVel [expr double($AvgVel)/double($numPixels)]
  set totalArea [expr double($pixelArea)*double($numPixels)]
  set gFlow($frame) [list $flowRate $AvgVel $MinVel $MaxVel $totalArea]
  puts [format "%15s%15s%15s%15s%15s%15s" "Frame" "Flow (ml/min)"  "Avg V" "Min V" "Max V" "area (mm^2)"]
