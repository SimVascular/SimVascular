# Procedure: gui3DprobeData
proc gui3DprobeData {} {

  global gImageVol
  global gui3Dvars
  global symbolicName
  global gOptions

  if {$gOptions(orientImgVolToRAS) == 0} {
    # old xyz system
    set org_x [lindex $gImageVol(vtk_org_xyz) 0]
    set org_y [lindex $gImageVol(vtk_org_xyz) 1]
    set org_z [lindex $gImageVol(vtk_org_xyz) 2]
    set ext_i $gImageVol(ext_i)
    set ext_j $gImageVol(ext_j)
    set ext_k $gImageVol(ext_k)
    set vdims_x $gImageVol(vdims_x)
    set vdims_y $gImageVol(vdims_y)
    set vdims_z $gImageVol(vdims_z)
    set voi_x0 $gImageVol(voi_x0)
    set voi_x1 $gImageVol(voi_x1)
    set voi_y0 $gImageVol(voi_y0)
    set voi_y1 $gImageVol(voi_y1)
    set voi_z0 $gImageVol(voi_z0)
    set voi_z1 $gImageVol(voi_z1)
  } else {
    # properly oriented volume in ras space
    set org_x [lindex $gImageVol(vtk_org_ras) 0]
    set org_y [lindex $gImageVol(vtk_org_ras) 1]
    set org_z [lindex $gImageVol(vtk_org_ras) 2]
    set ext_i [lindex $gImageVol(ext_ras) 0]
    set ext_j [lindex $gImageVol(ext_ras) 1]
    set ext_k [lindex $gImageVol(ext_ras) 2]
    set vdims_x [lindex $gImageVol(vdims_ras) 0]
    set vdims_y [lindex $gImageVol(vdims_ras) 1]
    set vdims_z [lindex $gImageVol(vdims_ras) 2]
    set voi_x0 [lindex $gImageVol(voi_ras) 0]
    set voi_x1 [lindex $gImageVol(voi_ras) 1]
    set voi_y0 [lindex $gImageVol(voi_ras) 2]
    set voi_y1 [lindex $gImageVol(voi_ras) 3]
    set voi_z0 [lindex $gImageVol(voi_ras) 4]
    set voi_z1 [lindex $gImageVol(voi_ras) 5]
  }

  set increment $gui3Dvars(chooserIncrement)
  global gInteractorRen
  set curpos [vis_pointWidgetGetPosition $gInteractorRen gui3D]

  set orgcurpos $gui3Dvars(chooserCurPos)

  if {$gui3Dvars(chooserFixAxisX) != 0} {
    set x [lindex $orgcurpos 0]
  } else {
    set x [lindex $curpos 0]
  }
  if {$gui3Dvars(chooserFixAxisY) != 0} {
    set y [lindex $orgcurpos 1]
  } else {
    set y [lindex $curpos 1]
  }
  if {$gui3Dvars(chooserFixAxisZ) != 0} {
    set z [lindex $orgcurpos 2]
  } else {
    set z [lindex $curpos 2]
  }
  set curpos [list $x $y $z]

  set axes_x $gImageVol(axes-x)
  set axes_y $gImageVol(axes-y)
  set axes_z $gImageVol(axes-z)

  # need to do the inverse of the code from sliders, that
  # is calculate axes location give an interactor point

  set axes_x [expr int(([lindex $curpos 0] - $org_x)/($increment*$vdims_x))]
  set axes_y [expr int(([lindex $curpos 1] - $org_y)/($increment*$vdims_y))]
  set axes_z [expr int(([lindex $curpos 2] - $org_z)/($increment*$vdims_z))]

  set gImageVol(axes-x) $axes_x
  set gImageVol(axes-y) $axes_y
  set gImageVol(axes-z) $axes_z

  set gui3Dvars(chooserXscale) $axes_x
  set gui3Dvars(chooserYscale) $axes_y
  set gui3Dvars(chooserZscale) $axes_z

  gui3DchooserUpdateXYZ
}


# Procedure: gui3DrunLS
proc gui3DrunLS {} {

    global gui3Dvars
    global symbolicName

    set img  $gui3Dvars(ls_volume)

    set useDenseGrid $gui3Dvars(ls_use_dense_grid)
    set verbose 1
    set outPd /gui3D/lset/out/pd
    set connPd /gui3D/lset/conn/pd
    set tempPd /gui3D/lset/temp/pd
    set segPd $gui3Dvars(ls_finalSegmentation)

    # create initialization PD
    #todo: JTM make other seed types work
    if {$gui3Dvars(ls_init_option) == "seeds"} {
      set mytext {}
      set tbox $symbolicName(gui3DseedsTB)
      set numlines [lindex [split [$tbox index end] .] 0]
      set all_spheres {}
      for {set i 1} {$i < $numlines} {incr i} {
        set pd /tmp/gui3D/sphere/$i
        catch {repos_delete -obj $pd}
        set line [$tbox get $i.0 $i.end]
        set line [string trim $line]
        if {[string length $line] == 0} {
          continue
        }
        if {[string index $line 0] == "\#"} {
          continue
        }
        if {[lindex $line 0] == "sphere"} {

        geom_mkSphere [lindex $line 2] [lindex $line 3] [lindex $line 4] [lindex $line 1] $pd
      
          lappend all_spheres $pd
        } else {
          puts "WARNING: line ignored ($line)"
        }
      }

      if {[llength $all_spheres] == 0} {
         return -code error "ERROR: no seeds!"
      }

      set initPD /tmp/gui3D/initPD
      catch {repos_delete -obj $initPD}
      geom_appendPds $all_spheres $initPD
      set initData [list polydata $initPD]
    } elseif { $gui3Dvars(ls_init_option) == "smooth_poly" } {

      #set ignored [tk_messageBox -title "Done Segmentation" -type ok -message "Done!"]

      set initPD /tmp/gui3D/initPD
      catch {repos_delete -obj $initPD}
      geom_copy -src $segPd -dst $initPD
      set initData [list polydata $initPD]

    
    } elseif { $gui3Dvars(ls_init_option) == "selected_poly" } {

      #set ignored [tk_messageBox -title "Done Segmentation" -type ok -message "Done!"]
      set initPDs {}
      catch {repos_delete -obj $initPDs}
      set selected [guiSV_group_get_tree_current_groups_selected]
      if {$selected != ""} {
        foreach grp $selected {
          set inPD /tmp/gui3D/outPD/$grp
          catch {repos_delete -obj $inPD}
          global guiBOOLEANvars
          set sampling_default $guiBOOLEANvars(sampling_default)
          set lin_multiplier   $guiBOOLEANvars(linear_sampling_along_length_multiplier)
          set useLinearSampleAlongLength   $guiBOOLEANvars(use_linear_sampling_along_length)
          set numModes         $guiBOOLEANvars(num_modes_for_FFT)
          array set overrides  $guiBOOLEANvars(sampling_overrides)
          set useFFT           $guiBOOLEANvars(use_FFT)
          set sample_per_segment $guiBOOLEANvars(sampling_along_length_multiplier)
          set addCaps          1
          set noInterOut       1
	  set splineType       $guiBOOLEANvars(spline_type)

          set vecFlag 0
          set numOutPtsInSegs $sampling_default
          set numSegs [llength [group_get $grp]]
          set numOutPtsAlongLength [expr $sample_per_segment * $numSegs]
          set numPtsInLinearSampleAlongLength [expr $lin_multiplier *$numOutPtsAlongLength]

          if {$numSegs > 1} {
            polysolid_c_create_vessel_from_group $grp $vecFlag $useLinearSampleAlongLength  $numPtsInLinearSampleAlongLength $useFFT $numModes $numOutPtsInSegs $numOutPtsAlongLength $addCaps $splineType $inPD
            lappend initPDs $inPD
          }
        }
      }
      global gSeg3D
      set selected [guiSV_group_get_tree_current_surfaces_selected]
      if {$selected != ""} {
        foreach grp $selected {
          set objName $gSeg3D($grp)
          
          catch {cleaner Delete}
          vtkCleanPolyData cleaner
          cleaner ConvertPolysToLinesOff
          cleaner PointMergingOff
          cleaner ConvertLinesToPointsOff
          cleaner SetInputDataObject [repos_exportToVtk -src $objName]
          cleaner Update
          
          set inPD /tmp/gui3D/outPD2/$grp
          catch {repos_delete -obj $inPD}
          repos_importVtkPd -src [cleaner GetOutput] -dst $inPD
          #geom_copy -src [cleaner GetOutput] -dst $inPD
  
          
          lappend initPDs $inPD

        }
      }


      if { $gui3Dvars(ls_init_option_combine) == "boolean" } {
        set initSolid /tmp/gui3D/initSolid
        catch {repos_delete -obj $initSolid}
        geom_all_union -srclist $initPDs -intertype $noInterOut -result $initSolid

        set initPD /tmp/gui3D/initPD
        catch {repos_delete -obj $initPD}
        $initSolid GetPolyData -result $initPD
      } else {
        set initPD /tmp/gui3D/initPD
        catch {repos_delete -obj $initPD}
        puts "Appending PDs"
        geom_appendPds $initPDs $initPD
      }
      
      set initData [list polydata $initPD]

    }

    global gRen3d
    set rens $gRen3d
    if {[vis_pExists $gRen3d $segPd]} {
