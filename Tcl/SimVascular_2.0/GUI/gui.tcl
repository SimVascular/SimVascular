# Procedure: lsGUIthrSlice
proc lsGUIthrSlice {} {

  set mag   /tmp/lsGUI/mag
  set pot   /tmp/lsGUI/pot

  global lsGUIthrValue

  # set base string
  global lsGUIcurrentPositionNumber
  set posId $lsGUIcurrentPositionNumber
  global lsGUIcurrentPathNumber
  set pathId $lsGUIcurrentPathNumber
  set baseName /lsGUI/$pathId/$posId

  set thr $baseName/thr
  set thr_unclean $baseName/thr/unclean
  set picked $baseName/thr/selected
  set oriented $baseName/thr/oriented

  catch {repos_delete -obj $thr}
  catch {repos_delete -obj $thr_unclean}
  catch {repos_delete -obj $picked}
  catch {repos_delete -obj $oriented}

  # threshold
  global lsGUIthresholdType
  set type $lsGUIthresholdType

  if {$type == "mag"} {
    set img [repos_exportToVtk -src $mag]
    if {$lsGUIthrValue == 0} {
      return -code error "ERROR:  Must set a non-zero threshold value!"
    }
    repos_importVtkPd -src [img_contour $img $lsGUIthrValue]  -dst $thr_unclean
  } elseif {$type == "localMax"} {
    global lsGUIlsParams
    # find the range of the mangitude gradient
    set MGrng [[repos_exportToVtk -src $pot] GetScalarRange]
    set maxMGi [lindex $MGrng 1]
    set MGi_c [expr $lsGUIlsParams(MagFactor) * $maxMGi]
    repos_importVtkPd -src [img_contour [repos_exportToVtk -src $pot] $MGi_c]  -dst $thr_unclean
  } elseif {$type == "grad"} {
    global lsGUIlsParams
    repos_importVtkPd -src [img_contour [repos_exportToVtk -src $pot] $lsGUIlsParams(potentialValueDecay)]  -dst $thr_unclean
  } else {
    puts "ERROR:  Invalid threshold type $type."
    return -code error "ERROR:  Invalid threshold type $type."
  }

  global lsGUInumSamplePoints
  set numSamplePoints $lsGUInumSamplePoints
  geom_mergePts -src $thr_unclean -dst $thr -tol 0.001



  repos_delete -obj $thr_unclean
  repos_setLabel -obj $thr -key thresholdValue  -value $lsGUIthrValue

  # select contour
  global lsGUIctrX
  global lsGUIctrY
  # calculate center from voxel dims
  global gImageFoo
  set voxel_dims $gImageFoo(voxelDims)

  set center [list [expr double([lindex $voxel_dims 0])*double($lsGUIctrX)]  [expr double([lindex $voxel_dims 1])*double($lsGUIctrY)] 0]



  set tmpobj $picked

  set connfilt /gui3D/lset/connfilt
  catch {$connfilt Delete}
  vtkPolyDataConnectivityFilter $connfilt
  $connfilt SetInputData [repos_exportToVtk -src $thr]
  $connfilt SetExtractionModeToLargestRegion
  $connfilt Update

  catch {repos_delete -obj $segPd}
  repos_importVtkPd -src [$connfilt GetOutput] -dst $tmpobj

#  if [catch {geom_pick -result $tmpobj -obj $thr -pos $center} msg] {}
  if { 0 } {
    puts "Did not find closed boundary for $baseName.  ($msg)"
  } else {
    repos_setLabel -obj $picked -key center_x_in_pixels -value $lsGUIctrX
    repos_setLabel -obj $picked -key center_y_in_pixels -value $lsGUIctrY
    repos_setLabel -obj $picked -key center_x           -value [lindex $center 0]
    repos_setLabel -obj $picked -key center_y           -value [lindex $center 1]
    repos_setLabel -obj $picked -key threshold_value    -value $lsGUIthrValue
    repos_setLabel -obj $picked -key creation_method    -value "threshold"
    repos_setLabel -obj $picked -key creation_type      -value "threshold"

    # orient profile
    global symbolicName
    global lsGUIcurrentPathNumber
    set pathId $lsGUIcurrentPathNumber
    global gPathPoints
    set path $gPathPoints($pathId,splinePts)

    global lsGUIcurrentPositionNumber
    set posId $lsGUIcurrentPositionNumber

    array set items [lindex $path $posId]
    set pos [TupleToList $items(p)]
    set nrm [TupleToList $items(t)]
    set xhat [TupleToList $items(tx)]
    geom_orientProfile -src $picked -path_pos $pos -path_tan $nrm -path_xhat $xhat -dst $oriented

    repos_setLabel -obj $oriented -key center_x_in_pixels -value $lsGUIctrX
    repos_setLabel -obj $oriented -key center_y_in_pixels -value $lsGUIctrY
    repos_setLabel -obj $oriented -key center_x           -value [lindex $center 0]
    repos_setLabel -obj $oriented -key center_y           -value [lindex $center 1]
    repos_setLabel -obj $oriented -key threshold_value    -value $lsGUIthrValue
    repos_setLabel -obj $oriented -key creation_method    -value "threshold"
    repos_setLabel -obj $oriented -key creation_type      -value "threshold"
    repos_setLabel -obj $oriented -key pos                -value $pos
    repos_setLabel -obj $oriented -key nrm                -value $nrm
    repos_setLabel -obj $oriented -key xhat               -value $xhat
    repos_setLabel -obj $oriented -key pathId             -value $pathId
    repos_setLabel -obj $oriented -key posId              -value $posId

    # need these to be on seg also since this is what is used by the copy function
    repos_setLabel -obj $picked -key pos                -value $pos
    repos_setLabel -obj $picked -key nrm                -value $nrm
    repos_setLabel -obj $picked -key xhat               -value $xhat
    repos_setLabel -obj $picked -key pathId             -value $pathId
    repos_setLabel -obj $picked -key posId              -value $posId

  }

  # update display
  lsGUIchangeFrame 0
}


