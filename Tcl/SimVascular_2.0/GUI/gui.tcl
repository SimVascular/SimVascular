# Procedure: bcGUIfourierSmoothSeg
proc bcGUIfourierSmoothSeg { type} {

  global symbolicName
  global bcGUIcurrentFrameNumber
  set frame $bcGUIcurrentFrameNumber

  # need to translate hand/analytic to proper type
  if {$type == "analytic"} {
    global bcGUIanalContourType
    set type $bcGUIanalContourType
  } elseif {$type == "hand"} {
    global bcGUIhandContourType
    set type $bcGUIhandContourType
  }

  if {$type == "levelset"} {
    set seg /gBC/pcdata/mag/$frame/ls
  } elseif {$type == "threshold"} {
    set seg /gBC/pcdata/mag/$frame/thr/selected
  } else {
    return -code error "Invalid segmentation type ($type)."
  }

  if {[repos_exists -obj $seg] == 0} {
    return
  }

  # keep the tags on the segmentation
  foreach key [repos_getLabelKeys -obj $seg] {
    set seg_keys($key) [repos_getLabel -obj $seg -key $key]
  }

  set segpts [geom_getOrderedPts -obj $seg]

  catch {repos_delete -obj $seg}

  global bcGUIfourierNumModes
  set numModes $bcGUIfourierNumModes

  if {[expr [llength $segpts] * 2] < $numModes} {
    set numInterpPts [expr 3*$numModes]
  } else {
    set numInterpPts [llength $segpts]
  }

  set newpts [math_smoothCurve -pts $segpts -closed 1 -numModes $numModes -numInterpPts $numInterpPts]

  geom_mkLinesFromPts $newpts $seg 1

  # reattach tags
  foreach key [array names seg_keys] {
    repos_setLabel -obj $seg -key $key -value $seg_keys($key)
  }

  # update seg
  if {[info exists seg_keys(num_smooth_modes)] == 1} {
    repos_clearLabel -obj $seg -key num_smooth_modes
  }
  repos_setLabel -obj $seg -key num_smooth_modes -value $numModes

  # update display
  bcGUIchangeFrame $frame
}


# Procedure: bcGUIgroupLoad
proc bcGUIgroupLoad {} {

    global gFilenames
    set grpdir $gFilenames(vel_seg_dir)
    set filename [file join $grpdir group_contents.tcl]

    # check and see if the file exists
    if {[file exists $filename] == 0} {
      puts "ERROR: File $filename does not exist."
      return -code error "ERROR: File $filename does not exist."
    }

    # check for header
    set fp [open $filename "r"]
    set line {}
    catch {gets $fp line}
    close $fp

    if {[regexp geodesic_groups_file $line] == 0} {
      puts "ERROR: File $filename is not of the right format."
      return -code error "ERROR: File $filename is not of the right format."
    }

    if {[regexp 1.0 $line] == 0 && [regexp 2.0 $line] == 0} {
      puts "ERROR: File $filename is not version 1.0 or 2.0.  File being ignored!"
      return -code error "ERROR: File $filename is not version 1.0 or 2.0.  File being ignored!"
    }

    source $filename

    group_autoload
}


