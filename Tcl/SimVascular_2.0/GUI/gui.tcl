# Procedure: bcGUI_hand_tag_voxel
proc bcGUI_hand_tag_voxel { widget x y} {

  global bcGUImagWindow
  set ren $bcGUImagWindow(ren)

  set x [expr [winfo pointerx $widget] - [winfo rootx $widget]] 
  set y [expr [winfo pointery $widget] - [winfo rooty $widget]] 

  # y is flipped upside down
  set height [lindex [$widget configure -height] 4]
  set y [expr $height - $y]

  set coord tmp-add_point_cord
  catch {$coord Delete}
  vtkCoordinate $coord
  $coord SetCoordinateSystemToDisplay
  $coord SetValue $x $y
  set worldValue [$coord GetComputedWorldValue $ren]
  $coord Delete
  puts "x: $x  y: $y  worldValue: $worldValue"

  global bcGUIcurrentFrameNumber
  global gBC

  foreach param $gBC(phase_contrast_params) {
    set [lindex $param 0] [lindex $param 1]
  }

  set i [expr int(floor([lindex $worldValue 0]/[lindex $voxel_dims 0]))]
  set j [expr int(floor([lindex $worldValue 1]/[lindex $voxel_dims 1]))]

  if {$i < $gBC(voi_x0) || $i > $gBC(voi_x1) ||  $j < $gBC(voi_y0) || $j > $gBC(voi_y1)} {
      puts "$i $j out of range!"
      return
   }

  set pixel [expr ($i - $gBC(voi_x0)) + ($j - $gBC(voi_y0))*($gBC(voi_x1)-$gBC(voi_x0)+1)]

  # can only tag a pixel once
  global gTaggedPixels
  if {[info exists gTaggedPixels($pixel)] == 1} {
    return
  }

  set magslice /gBC/pcdata/mag/$bcGUIcurrentFrameNumber

  set gTaggedPixels($pixel) 1

  # vis the tagged pixel
  set box /tmp/bcGUI/taggedPixels/pixel/$pixel
  set boxpd /tmp/bcGUI/taggedPixels/pixel/$pixel/pd
  catch {repos_delete -obj $box}
  catch {repos_delete -obj $boxpd}
  solid_box3d -result $box -ctr [[repos_exportToVtk -src $magslice] GetPoint $pixel] -dims [list [expr [lindex $voxel_dims 0]]  [expr [lindex $voxel_dims 1]] [expr [lindex $voxel_dims 0]]]
  $box GetPolyData -result $boxpd
  repos_setLabel -obj $boxpd -key opacity -value 0.25
  repos_setLabel -obj $boxpd -key color -value yellow

  global bcGUImagWindow
  #global bcGUIpotWindow
  gdscGeneralView $bcGUImagWindow(ren) $boxpd
  #gdscGeneralView $bcGUIpotWindow(ren) $boxpd
  [$bcGUImagWindow(ren) GetRenderWindow] Render
  #[$bcGUIpotWindow(ren) GetRenderWindow] Render
}


