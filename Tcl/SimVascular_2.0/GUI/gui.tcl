# Procedure: bcGUIbaselineCalcEqn
proc bcGUIbaselineCalcEqn { order} {

  global gBC
  global bcGUIbaselineEqnRadioButton
  set comp $bcGUIbaselineEqnRadioButton

  # create a list of the velocity images
  set image_list {}
  for {set frame 1} {$frame <= $gBC(numSlices)} {incr frame} {
    # slightly different output depending on 1-comp or 3-comp
    # velocity data
    if {$gBC(numVelComp) == 1} {
      # only for thruplane
      set velslice /gBC/pcdata/vel/$frame
    } elseif {$gBC(numVelComp) == 3} {
      switch $comp {
        x {
           set velslice /gBC/pcdata/vel/$frame/r
    }
        y {
           set velslice /gBC/pcdata/vel/$frame/a
    }
        z {
           set velslice /gBC/pcdata/vel/$frame/s
    }
        default {
           return -code error "ERROR:  Improper value of EqnRadioButton!"
  }
      }
    } else {
      return -code error "ERROR:  Illegal number of velocity components"
    }
    lappend image_list $velslice
  }

  # we only use regions if the user has specified it for this direction
  global bcGUIbaselineRegions
  set region_list {}

  foreach i $bcGUIbaselineRegions {
      set obj [lindex $i 0]
      set id [file tail $obj]
      set uses [lindex $i 1]
      if {[regexp $comp $uses] == 1} {
         lappend region_list /bcGUI/region/$id
      }
  }

  # tcl function call
  #set fit [img_calcBaselineCorrectionEqn $region_list $image_list $order]
  set fit [img_calcCorrectionEqn -regions $region_list  -images $image_list -order $order]

  global gBC
  switch $comp {
    x {
      set gBC(baselineCorrectionX) $fit
    }
    y {
      set gBC(baselineCorrectionY) $fit
    }
    z {
      set gBC(baselineCorrectionZ) $fit
    }
  }
}


