# Procedure: guiRWHsetProperty
proc guiRWHsetProperty { property newvalue oldvalue} {

  DeselectPickedActor
  global guiRWH
  set old $guiRWH($oldvalue)
  set new $guiRWH($newvalue)
  #puts "old: $old  new: $new"
  if {$old == $new} {
    return
  }
  if {[llength $old] != [llength $new]} {
    puts "ERROR:  Wrong size of new value for $property!"
    return -code error "ERROR:  Wrong size of new value for $property!"
  }

  set actor $guiRWH(actor)
  set prop [$actor GetProperty]
  if {[llength $new] == 1} {
    $prop $property $new
  } elseif {[llength $new] == 3} {
    $prop $property [lindex $new 0] [lindex $new 1] [lindex $new 2]
  } else {
    puts "ERROR:  Invalid length to new value for $property!"
    return -code error "ERROR:  Invalid length to new value for $property!"
  }

  guiRWHgetProperties

  #$actor Update
  [$guiRWH(render) GetRenderWindow] Render
}


