}


# Procedure: guiPHASTAloadSupreFile
proc guiPHASTAloadSupreFile {} {

  global guiPHASTAvars
  global gFilenames
  global symbolicName

  set tbox $symbolicName(guiPHASTAsupreScript)
  $tbox delete 1.0 end
  set fp [open $gFilenames(supre_script_file) r]
  set i 1
  while {[gets $fp line] >= 0} {
    $tbox insert $i.0 "$line\n"
    incr i
  }
  close $fp
}


# Procedure: guiPHASTArunSupre
proc guiPHASTArunSupre {} {

  global gFilenames

  set yesno [tk_messageBox -message "Run cvpresolver?" -default no -icon question -type yesno]
  if {$yesno == "no"} {return}

  set yesno [tk_messageBox -message "Use currently loaded cvpresolver script?  This will save/overwrite the cvpresolver script file." -default yes -icon question -type yesno]
  if {$yesno == "yes"} {
    guiPHASTAsaveSupreFile
  }

  set fn $gFilenames(supre_script_file)

  global gExternalPrograms

  catch {exec $gExternalPrograms(cvpresolver) $fn} msg
  set fp [open "$fn.log" w]
  puts $fp "Executing:  $gExternalPrograms(cvpresolver) $fn >& $fn.log"
  puts $fp $msg
  close $fp
  #tk_messageBox -message $msg -title "cvpresolver output" -icon info -type ok
  guiSVMSGshow $msg
}


# Procedure: guiPHASTAsaveSupreFile
proc guiPHASTAsaveSupreFile {} {

  global guiPHASTAvars
  global gFilenames
  global symbolicName

  set tbox $symbolicName(guiPHASTAsupreScript)

  set fp [open $gFilenames(supre_script_file) w]
  puts $fp [$tbox get 1.0 end-1c]
  close $fp
}


