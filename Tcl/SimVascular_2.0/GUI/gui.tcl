# Procedure: bcGUIthrSlice
proc bcGUIthrSlice {} {

  global bcGUIthrValue
  global bcGUIcurrentFrameNumber
  set frame $bcGUIcurrentFrameNumber
  set thr /gBC/pcdata/mag/$frame/thr
  set thr_unclean /gBC/pcdata/mag/$frame/thr/unclean
  set picked /gBC/pcdata/mag/$frame/thr/selected
  catch {repos_delete -obj $thr}
  catch {repos_delete -obj $thr_unclean}
  catch {repos_delete -obj $picked}

  set mag  /gBC/pcdata/mag/$frame
  set pot  /gBC/pcdata/pot/$frame

  global gBC
  set imageInfo $gBC(phase_contrast_params)
  foreach i $imageInfo {
    set [lindex $i 0] [lindex $i 1]
  }

  # threshold
  global bcGUIthresholdType
  set type $bcGUIthresholdType

  if {$type == "mag"} {
    set img [repos_exportToVtk -src $mag]
    if {$bcGUIthrValue == 0} {
      return -code error "ERROR:  Must set a non-zero threshold value!"
    }
    repos_importVtkPd -src [img_contour $img $bcGUIthrValue]  -dst $thr_unclean
  } elseif {$type == "grad"} {
    global bcGUIlsParams
    # find the range of the mangitude gradient
    set MGrng [[repos_exportToVtk -src $pot] GetScalarRange]
    set maxMGi [lindex $MGrng 1]
    set MGi_c [expr $bcGUIlsParams(MagFactor) * $maxMGi]
    repos_importVtkPd -src [img_contour [repos_exportToVtk -src $pot] $MGi_c]  -dst $thr_unclean
  } else {
    puts "ERROR:  Invalid threshold type $type."
    return -code error "ERROR:  Invalid threshold type $type."
  }

  geom_mergePts -src $thr_unclean -dst $thr
  repos_delete -obj $thr_unclean

  repos_setLabel -obj $thr -key thresholdValue  -value $bcGUIthrValue

  # select contour
  global bcGUIctrX
  global bcGUIctrY

  set center [list [expr double([lindex $voxel_dims 0])*double($bcGUIctrX)]  [expr double([lindex $voxel_dims 1])*double($bcGUIctrY)] 0]

  if [catch {geom_pick -result $picked -obj $thr -pos $center} msg] {

    puts "Did not find closed boundary for $thr.  ($msg)"

  } else {

    # subsample the loop
    set pts [geom_getOrderedPts -obj $picked]
    set curveLength [math_curveLength -pts $pts -closed 1]
    set numSamplePoints [expr int(ceil($curveLength/[lindex $voxel_dims 0]))]
    global bcGUInumSamplePoints
    set bcGUInumSamplePoints $numSamplePoints
    set newpts [math_linearInterpCurve -pts $pts -closed 1 -numInterpPts $numSamplePoints]
    repos_delete -obj $picked
    geom_mkLinesFromPts $newpts $picked 1

    repos_setLabel -obj $picked -key center_x_in_pixels -value $bcGUIctrX
    repos_setLabel -obj $picked -key center_y_in_pixels -value $bcGUIctrY
    repos_setLabel -obj $picked -key center_x           -value [lindex $center 0]
    repos_setLabel -obj $picked -key center_y           -value [lindex $center 1]
    repos_setLabel -obj $picked -key threshold_value    -value $bcGUIthrValue
    repos_setLabel -obj $picked -key creation_method    -value "threshold"
    repos_setLabel -obj $picked -key creation_type      -value "threshold"
    repos_setLabel -obj $picked -key num_subsample_pts  -value $numSamplePoints
    repos_setLabel -obj $picked -key frame              -value $frame

  }

  # update display
  bcGUIchangeFrame $frame
}


# Procedure: bcGUItranslateSeg
proc bcGUItranslateSeg { type} {

  global bcGUIcurrentFrameNumber
  set frame $bcGUIcurrentFrameNumber

  # need to translate hand/analytic to proper type
  if {$type == "analytic"} {
    global bcGUIanalContourType
    set type $bcGUIanalContourType
  } elseif {$type == "hand"} {
    global bcGUIhandContourType
    set type $bcGUIhandContourType
  }

  if {$type == "levelset"} {
    set seg /gBC/pcdata/mag/$frame/ls
  } elseif {$type == "threshold"} {
    set seg /gBC/pcdata/mag/$frame/thr/selected
  } else {
    return -code error "Invalid segmentation type ($type)."
  }

  if {[repos_exists -obj $seg] == 0} {
    return
  }

  # keep the tags on the segmentation
  foreach key [repos_getLabelKeys -obj $seg] {
    set seg_keys($key) [repos_getLabel -obj $seg -key $key]
  }

  global bcGUItransX
  global bcGUItransY

  set beforetrans   /bcGUI/before-translate

  catch {repos_delete -obj $beforetrans}
  geom_copy -src $seg -dst $beforetrans
  catch {repos_delete -obj $seg}
  geom_translate -src $beforetrans -dst $seg -vec [list $bcGUItransX $bcGUItransY 0.0]

  # reattach tags
  foreach key [array names seg_keys] {
    repos_setLabel -obj $seg -key $key -value $seg_keys($key)
  }
  # update seg
  set translation_in_x 0.0
  set translation_in_y 0.0
  if {[info exists seg_keys(translation_in_x)] == 1} {
    repos_clearLabel -obj $seg -key translation_in_x
    set translation_in_x $seg_keys(translation_in_x)
  }
  repos_setLabel -obj $seg -key translation_in_x -value [expr $translation_in_x + $bcGUItransX]
  if {[info exists seg_keys(translation_in_y)] == 1} {
    repos_clearLabel -obj $seg -key translation_in_y
    set translation_in_y $seg_keys(translation_in_y)
  }
  repos_setLabel -obj $seg -key translation_in_y -value [expr $translation_in_y + $bcGUItransY]

  # update display
  bcGUIchangeFrame $frame
}


# Procedure: bcGUIupdate3dWindow
proc bcGUIupdate3dWindow {} {
    global bcGUI3dWindow
    global bcGUIcurrentFrameNumber
    global gRen3d
    set frame $bcGUIcurrentFrameNumber
    set magpd /gBC/pcdata/mag/hardcoded/pd
    set tmpobj /tmp/bcGUIupdate3dWindow/tmpobj
    set vectrepos /gBC/pcdata/vel/hardcoded/pd

    if {$bcGUI3dWindow(showWindow) == 1} {
      # create a polydata so we can visualize the
      # mag slice in the 3-d window
      set myfilt tmp-bcGUIupdate3dWindow-myfilt
      catch {$myfilt Delete}
      vtkImageDataGeometryFilter  $myfilt
      $myfilt SetInputDataObject [repos_exportToVtk -src /gBC/pcdata/mag/$frame]
      $myfilt Update
      global gFilenames

      set mymatrix tmp-bcgui-matrix
      catch {$mymatrix Delete}
      img_calcTransformMatrixToRAS $gFilenames(phase_contrast_geometric) $mymatrix

      catch {repos_delete -obj $magpd}
      catch {repos_delete -obj $tmpobj}
      repos_importVtkPd -src [$myfilt GetOutput] -dst $tmpobj
      geom_applyTransformMatrix $tmpobj $mymatrix $magpd

      repos_setLabel -obj $magpd -key showK -value 1

      global gRen3d
      catch {vis_pRm $gRen3d $magpd}
      catch {vis_vRm $gRen3d $vectrepos}

      # so we can visualize the vectors
      if {$bcGUI3dWindow(showRawVectors) == 1} {
        set vVectors gBC-pcdata-hardcoded-vectors
        set vPoly gBC-pcdata-hardcoded-pd
        catch {$vVectors Delete}
        catch {$vPoly Delete}

        vtkFloatArray $vVectors; $vVectors SetNumberOfComponents 3
        $vVectors Allocate 100 100
        vtkPolyData $vPoly
        $vPoly Allocate 100 100

        for {set pixel 0} {$pixel < [[repos_exportToVtk -src /gBC/pcdata/mag/$frame] GetNumberOfPoints]} {incr pixel} {
          global gBC
          if {$gBC(numVelComp) == 1} {
            # only for thruplane
            set velslice /gBC/pcdata/vel/$frame
            set velvalueX 0
            set velvalueY 0
            set velvalueZ [[[[repos_exportToVtk -src $velslice] GetPointData] GetScalars] GetTuple1 $pixel]
          } elseif {$gBC(numVelComp) == 3} {
            set velsliceX /gBC/pcdata/vel/$frame/[lindex $gBC(xyz) 0]
            set velsliceY /gBC/pcdata/vel/$frame/[lindex $gBC(xyz) 1]
            set velsliceZ /gBC/pcdata/vel/$frame/[lindex $gBC(xyz) 2]
            #set velsliceX /gBC/pcdata/vel/$frame/r
            #set velsliceY /gBC/pcdata/vel/$frame/a
            #set velsliceZ /gBC/pcdata/vel/$frame/s
            set velvalueX [[[[repos_exportToVtk -src $velsliceX] GetPointData] GetScalars] GetTuple1 $pixel]
            set velvalueY [[[[repos_exportToVtk -src $velsliceY] GetPointData] GetScalars] GetTuple1 $pixel]
            set velvalueZ [[[[repos_exportToVtk -src $velsliceZ] GetPointData] GetScalars] GetTuple1 $pixel]
            # flip directions if necessary
            set velvalueX [expr double([lindex $gBC(directions) 0])*$velvalueX]
            set velvalueY [expr double([lindex $gBC(directions) 1])*$velvalueY]
            set velvalueZ [expr double([lindex $gBC(directions) 2])*$velvalueZ]
      } else {
            return -code error
      }
          #puts "pixel: $pixel value: $velvalueX $velvalueY $velvalueZ"
          # flip directions if necessary
          
          $vVectors InsertNextTuple3 $velvalueX $velvalueY $velvalueZ
        }

        set vCreateNorm bcGUI-pcdata-hardcoded-norm
        catch {$vCreateNorm Delete}
        set tmpvectrepos /gBC/pcdata/vel/hardcoded/tmp/pd
 
        catch {repos_delete -obj $tmpvectrepos}
        catch {repos_delete -obj $vectrepos}

        $vPoly CopyStructure [$myfilt GetOutput]
        [$vPoly GetPointData] SetVectors $vVectors

        vtkVectorNorm $vCreateNorm
        $vCreateNorm SetInputDataObject $vPoly
        $vCreateNorm Update

        repos_importVtkPd -src [$vCreateNorm GetOutput] -dst $tmpvectrepos
        set velscalarrange [[$vCreateNorm GetOutput] GetScalarRange]
        $vCreateNorm Delete

        geom_applyTransformMatrix $tmpvectrepos $mymatrix $vectrepos
        catch {vis_vRm $gRen3d $vectrepos}
        set velactor [vis_vRepos $gRen3d $vectrepos]
        [$velactor GetMapper] ScalarVisibilityOn
        [$velactor GetMapper] SetLookupTable gBlueToRedLUT
        [$velactor GetMapper] SetScalarRange  [lindex $velscalarrange 0] [lindex $velscalarrange 1]
        global bcGUI3dWindow
        vis_vSetScale $gRen3d $vectrepos $bcGUI3dWindow(scaleFactor)
      }

      if {$bcGUI3dWindow(showMagSlice) == 1} {
        set actor [vis_pRepos $gRen3d $magpd]
        [$actor GetMapper] ScalarVisibilityOn

        set lookupGrayscale vis_img_g8bitGrayscaleLUT_3d_pcmri
        set lookupColor vis_img_gBlueToRedLUT_3d_pcmri

        if {[cmdExists $lookupGrayscale] == 0} {
          # 8-bit grayscale lookup table
          # ----------------------------
          vtkLookupTable $lookupGrayscale
          $lookupGrayscale SetHueRange 0.0 0.0
          $lookupGrayscale SetSaturationRange 0.0 0.0
          $lookupGrayscale SetValueRange 0.0 1.0
          $lookupGrayscale SetNumberOfColors 16384
          $lookupGrayscale Build

          # Red-to-blue colormap
          # --------------------
          vtkLookupTable $lookupColor
          $lookupColor SetHueRange 0.6667 0.0
          $lookupColor SetSaturationRange 1.0 1.0
          $lookupColor SetValueRange 1.0 1.0
          $lookupColor SetAlphaRange 1.0 1.0
          $lookupColor SetNumberOfColors 16384
          $lookupColor Build
        }

        [$actor GetMapper] SetLookupTable $lookupGrayscale

        set irng  [[repos_exportToVtk -src /gBC/pcdata/mag/$frame] GetScalarRange]
        [$actor GetMapper] SetScalarRange [lindex $irng 0] [lindex $irng 1]
        #[$actor GetMapper] SetLookupTable gBlueToRedLUT
        [$gRen3d GetRenderWindow] Render
        # set color here
      }

    } else {

       catch {vis_pRm $gRen3d $magpd}
       catch {vis_vRm $gRen3d $vectrepos}

    }
}


