# Procedure: guiVISupdateVol
proc guiVISupdateVol {} {

   global guiVISvars
   set theMesh $guiVISvars(mesh_vtk_obj)
   set pressurePD /guiVIS/vol/pressurePD
   set velocityPD /guiVIS/vol/velocityPD
   set surfacePD /guiVIS/vol/surface_mesh

   # update the display for volume
   global guiVISvars
   global guiVISvarsPrev

   set ren $guiVISvars(ren)
   global gRen3dFreeze

   if {$gRen3dFreeze > 0} {
      set alreadyfrozen 1
   } else {
      set alreadyfrozen 0
      set gRen3dFreeze 1
   }

   # toggle display of exterior mesh
   # note: the exterior volume mesh is independent of the
   #       actual step number
   if {$guiVISvarsPrev(vol_showSurfMesh) == 1 &&  $guiVISvars(vol_showSurfMesh) == 0} {
      vis_pRm $ren $surfacePD
      set guiVISvarsPrev(vol_showSurfMesh) 0
   } elseif {$guiVISvarsPrev(vol_showSurfMesh) == 0 &&  $guiVISvars(vol_showSurfMesh) == 1} {
      set actor [vis_pRepos $ren $surfacePD]
      set myprop [$actor GetProperty]
      $myprop SetRepresentationToWireframe
      $myprop SetInterpolationToFlat
      $myprop SetColor 0.8900 0.8100 0.3400
      $myprop SetSpecularColor 1 1 1
      $myprop SetSpecular 0.3
      $myprop SetSpecularPower 20
      $myprop SetAmbient 0.2
      $myprop SetDiffuse 0.8
      vis_render $ren
      # set the actor props here
      set guiVISvarsPrev(vol_showSurfMesh) 1
   }

   # the vol pressure and velocity depend on the step number,
   # if it appears the step number has changed since the last
   # display, we turn off all vis and let it update.  Note
   # that this code does not update the prev step number, that
   # should be done outside of this routine.

   if {$guiVISvarsPrev(currentStep) != $guiVISvars(currentStep)} {
     if {$guiVISvarsPrev(vol_displayPressure) != "off"} {
       puts "step change: vol:  remove old pressure display"
       vis_pRm $ren $pressurePD
       repos_delete -obj $pressurePD
       set guiVISvarsPrev(vol_displayPressure) "off"
     } else {
       puts "step change: vol:  pressure do nothing"
     }
     if {$guiVISvarsPrev(vol_displayVelocity) == "off"} {
          # dont need to do anything
          puts "step change: vol: velocity off, off -- do nothing"
     } elseif {$guiVISvarsPrev(vol_displayVelocity) == "as vectors"} {
          puts "step change: vol: turn off vectors"
          vis_vRm $ren $velocityPD
          set guiVISvarsPrev(vol_displayVelocity) "off"
          repos_delete -obj $velocityPD
     } else {
          puts "step change: vol: turn off velocity scalars"
          vis_pRm $ren $velocityPD
          set guiVISvarsPrev(vol_displayVelocity) "off"
          repos_delete -obj $velocityPD
     }
   }

   # toggle display of pressure
   if {$guiVISvarsPrev(vol_displayPressure) == "off" &&  $guiVISvars(vol_displayPressure) != "off"} {
       #puts "new display"
       catch {repos_delete -obj $pressurePD}
       set resultObj [repos_exportToVtk -src $guiVISvars(current_step)]
       set displayObj [repos_exportToVtk -src $surfacePD]
       [$displayObj GetPointData] SetScalars [[$resultObj GetPointData] GetScalars]
       repos_importVtkPd -src $displayObj -dst $pressurePD
       repos_setLabel -obj $pressurePD -key showK -value [list $guiVISvars(vol_minPressure) $guiVISvars(vol_maxPressure)]
       set guiVISvarsPrev(vol_minPressure) $guiVISvars(vol_minPressure)
       set guiVISvarsPrev(vol_maxPressure) $guiVISvars(vol_maxPressure)
       set guiVISvarsPrev(vol_displayPressure) $guiVISvars(vol_displayPressure)
       set actor [vis_pRepos $ren $pressurePD]
       [$actor GetMapper] ScalarVisibilityOn
       [$actor GetMapper] SetLookupTable gBlueToRedLUT
       [$actor GetMapper] SetScalarRange $guiVISvars(vol_minPressure) $guiVISvars(vol_maxPressure)
       vis_render $ren
   }  elseif {($guiVISvarsPrev(vol_displayPressure) != "off") &&  ($guiVISvars(vol_displayPressure) == "off")} {
       #puts "remove it!"
       vis_pRm $ren $pressurePD
       repos_delete -obj $pressurePD
       set guiVISvarsPrev(vol_displayPressure) $guiVISvars(vol_displayPressure)
   } elseif {($guiVISvarsPrev(vol_minPressure) != $guiVISvars(vol_minPressure)) ||  ($guiVISvarsPrev(vol_maxPressure) != $guiVISvars(vol_maxPressure))} {
       #puts "update"
       set guiVISvarsPrev(vol_minPressure) $guiVISvars(vol_minPressure)
       set guiVISvarsPrev(vol_maxPressure) $guiVISvars(vol_maxPressure)
       set guiVISvarsPrev(vol_displayPressure) $guiVISvars(vol_displayPressure)
       set actor [vis_pGetActor $ren $pressurePD]
       [$actor GetMapper] ScalarVisibilityOn
       [$actor GetMapper] SetLookupTable gBlueToRedLUT
       [$actor GetMapper] SetScalarRange $guiVISvars(vol_minPressure) $guiVISvars(vol_maxPressure)
       vis_render $ren
   }


   # toggle display of velocity
   # two cases, scalars and vectors
   if {$guiVISvars(vol_displayVelocity) == "off"} {

       if {$guiVISvarsPrev(vol_displayVelocity) == "off"} {
          # dont need to do anything
          puts "off, off -- do nothing"
       } elseif {$guiVISvarsPrev(vol_displayVelocity) == "as vectors"} {
          puts "turn off vectors"
          vis_vRm $ren $velocityPD
          set guiVISvarsPrev(vol_displayVelocity) $guiVISvars(vol_displayVelocity)
          repos_delete -obj $velocityPD
       } else {
          puts "turn off scalars"
          vis_pRm $ren $velocityPD
          set guiVISvarsPrev(vol_displayVelocity) $guiVISvars(vol_displayVelocity)
          repos_delete -obj $velocityPD
       }

   } else {

      if {($guiVISvarsPrev(vol_displayVelocity) == "as vectors") &&  ($guiVISvars(vol_displayVelocity) != "as vectors") } {
               puts "remove vectors from display"
               vis_vRm $ren $velocityPD
               set guiVISvarsPrev(vol_displayVelocity) "off"
      }  elseif {($guiVISvarsPrev(vol_displayVelocity) != "off") &&  ($guiVISvarsPrev(vol_displayVelocity) != "as vectors") &&  ($guiVISvars(vol_displayVelocity) == "as vectors") } {
                puts "remove scalars from display"
               vis_pRm $ren $velocityPD
               set guiVISvarsPrev(vol_displayVelocity) "off"
      }

      # two cases
      if {$guiVISvarsPrev(vol_displayVelocity) == "off" &&  $guiVISvars(vol_displayVelocity) == "as vectors"} {
          puts "new display as vectors"
          catch {repos_delete -obj $velocityPD}
          set resultObj [repos_exportToVtk -src $guiVISvars(current_step)]
          set displayObj [repos_exportToVtk -src $surfacePD]
          set vectnorm tmp-guiVISupdateVol-vn
          catch {$vectnorm Delete}
          vtkVectorNorm $vectnorm
          $vectnorm SetInputDataObject $resultObj
          $vectnorm Update
          [$displayObj GetPointData] SetScalars [[[$vectnorm GetOutput] GetPointData] GetScalars]
          [$displayObj GetPointData] SetVectors [[$resultObj GetPointData] GetVectors]
          repos_importVtkPd -src $displayObj -dst $velocityPD
          $vectnorm Delete
          repos_setLabel -obj $velocityPD -key showK -value [list $guiVISvars(vol_minVelMag) $guiVISvars(vol_maxVelMag)]
          set actor [vis_vRepos $ren $velocityPD]
          [$actor GetMapper] ScalarVisibilityOn
          [$actor GetMapper] SetLookupTable gBlueToRedLUT
          [$actor GetMapper] SetScalarRange $guiVISvars(vol_minVelMag) $guiVISvars(vol_maxVelMag)
          vis_vSetScale $ren $velocityPD $guiVISvars(vol_velocityScale)
          set guiVISvarsPrev(vol_velocityScale) $guiVISvars(vol_velocityScale)
          set guiVISvarsPrev(vol_displayVelocity) $guiVISvars(vol_displayVelocity)
          set guiVISvarsPrev(vol_minVelMag) $guiVISvars(vol_minVelMag)
          set guiVISvarsPrev(vol_maxVelMag) $guiVISvars(vol_maxVelMag)
      } elseif {$guiVISvarsPrev(vol_displayVelocity) == "off" &&  $guiVISvars(vol_displayVelocity) != "as vectors"} {
          puts "new display as scalars"
          catch {repos_delete -obj $velocityPD}
          set resultObj [repos_exportToVtk -src $guiVISvars(current_step)]
          set displayObj [repos_exportToVtk -src $surfacePD]
          set vectnorm tmp-guiVISupdateVol-vn
          catch {$vectnorm Delete}
          vtkVectorNorm $vectnorm
          $vectnorm SetInputDataObject $resultObj
          $vectnorm Update
          [$displayObj GetPointData] SetScalars [[[$vectnorm GetOutput] GetPointData] GetScalars]
          [$displayObj GetPointData] SetVectors [[$resultObj GetPointData] GetVectors]
          repos_importVtkPd -src $displayObj -dst $velocityPD
          $vectnorm Delete
          repos_setLabel -obj $velocityPD -key showK -value [list $guiVISvars(vol_minVelMag) $guiVISvars(vol_maxVelMag)]
          set guiVISvarsPrev(vol_minVelMag) $guiVISvars(vol_minVelMag)
          set guiVISvarsPrev(vol_maxVelMag) $guiVISvars(vol_maxVelMag)
          set guiVISvarsPrev(vol_displayVelocity) $guiVISvars(vol_displayVelocity)
          set actor [vis_pRepos $ren $velocityPD]
          [$actor GetMapper] ScalarVisibilityOn
          [$actor GetMapper] SetLookupTable gBlueToRedLUT
          [$actor GetMapper] SetScalarRange $guiVISvars(vol_minVelMag) $guiVISvars(vol_maxVelMag)
          vis_render $ren
      } elseif { ($guiVISvars(vol_displayVelocity) == "as vectors") } {

          if {$guiVISvarsPrev(vol_velocityScale) == $guiVISvars(vol_velocityScale)} {
             # do nothing
             puts "as vectors , as vectors -- do nothing"
          } else {
             puts "as vectors , as vectors -- scale"
             vis_vSetScale $ren $velocityPD $guiVISvars(vol_velocityScale)
             set guiVISvarsPrev(vol_velocityScale) $guiVISvars(vol_velocityScale)
    }

          if {($guiVISvarsPrev(vol_minVelMag) != $guiVISvars(vol_minVelMag)) ||  ($guiVISvarsPrev(vol_maxVelMag) != $guiVISvars(vol_maxVelMag))} {
            puts "scalar range update on vectors"
            set guiVISvarsPrev(vol_minVelMag) $guiVISvars(vol_minVelMag)
            set guiVISvarsPrev(vol_maxVelMag) $guiVISvars(vol_maxVelMag)
            set guiVISvarsPrev(vol_displayVelocity) $guiVISvars(vol_displayVelocity)
            set actor [vis_vGetActor $ren $velocityPD]
            [$actor GetMapper] ScalarVisibilityOn
            [$actor GetMapper] SetLookupTable gBlueToRedLUT
            [$actor GetMapper] SetScalarRange $guiVISvars(vol_minVelMag) $guiVISvars(vol_maxVelMag)
            vis_render $ren
    }

      } elseif { ($guiVISvarsPrev(vol_displayVelocity) != "as vectors") &&  ($guiVISvars(vol_displayVelocity) != "as vectors") } {

        if {($guiVISvarsPrev(vol_minVelMag) != $guiVISvars(vol_minVelMag)) ||  ($guiVISvarsPrev(vol_maxVelMag) != $guiVISvars(vol_maxVelMag))} {
            puts "scalars update"
            set guiVISvarsPrev(vol_minVelMag) $guiVISvars(vol_minVelMag)
            set guiVISvarsPrev(vol_maxVelMag) $guiVISvars(vol_maxVelMag)
            set guiVISvarsPrev(vol_displayVelocity) $guiVISvars(vol_displayVelocity)
            set actor [vis_pGetActor $ren $velocityPD]
            [$actor GetMapper] ScalarVisibilityOn
            [$actor GetMapper] SetLookupTable gBlueToRedLUT
            [$actor GetMapper] SetScalarRange $guiVISvars(vol_minVelMag) $guiVISvars(vol_maxVelMag)
            vis_render $ren
  } else {
            puts "not vectors, not vectors -- do nothing"
  }
      }
  }
  if {$alreadyfrozen == 0} {
    set gRen3dFreeze 0
    vis_render $ren
  }
}


# Procedure: guiVISvolFlowMultSliceCalcFlows
proc guiVISvolFlowMultSliceCalcFlows { writeSlices} {

  global guiVISvars

  # If "Save Multiple Slices" checked, then save slices for other
  # locations.
  if {$guiVISvars(volFlowSlice_mult_slice_flag) == 1} {
      # Subtract one plane for the original slice location, which
      # is computed last
      set num_planes [expr $guiVISvars(volFlowSlice_num_planes) - 1]
      set plane_incr $guiVISvars(volFlowSlice_plane_incr)
  
      # File names for other cut planes will be a variation of the
      # original output file name
      set orig_resultsDir $guiVISvars(volFlowSlice_resultsDir)
      set orig_resultsFilename $guiVISvars(volFlowSlice_resultsFilename)
      set orig_p1 $guiVISvars(p1)
      set orig_p2 $guiVISvars(p2)
      set orig_p3 $guiVISvars(p3)

      # Determine the number of sets of data that remain to be output.
      # If the number of remaining planes to output is not even,
      # then the "minus set" of planes will have one fewer plane
      # than the "plus set" of planes.
      set num_sets_plus [expr floor ([expr $num_planes / 2.0])]
      puts "Number of plus sets: $num_sets_plus"
      if {[expr $num_planes % 2] == 0} {
    set num_sets_minus $num_sets_plus
      } else {
    set num_sets_minus [expr $num_sets_plus - 1]
      }

      # Determine the correctly scaled normal to the cut plane
      set v1 [math_subVectors $guiVISvars(p2) $guiVISvars(p1)]
      set v2 [math_subVectors $guiVISvars(p3) $guiVISvars(p1)]
      set n [math_cross $v1 $v2]
      set n_unit [math_normalize $n]
      set n_incr [math_scaleVec $n_unit $plane_incr]

      # Write out the "plus set" planes
      for {set i 1} {$i <= $num_sets_plus} {incr i} {

    # Update the plane
    set plane_shift [math_scaleVec $n_incr $i]
    set guiVISvars(p1) [math_addVectors $orig_p1 $plane_shift]
    set guiVISvars(p2) [math_addVectors $orig_p2 $plane_shift]
    set guiVISvars(p3) [math_addVectors $orig_p3 $plane_shift]
    guiVISupdateCutPlane

    # Write out the results
    set specifier "_plus"
    set guiVISvars(volFlowSlice_resultsDir) "$orig_resultsDir$specifier$i" 
    set guiVISvars(volFlowSlice_resultsFilename) "$orig_resultsFilename$specifier$i"
    guiVISvolFlowSliceCalcFlows $writeSlices
      }


      # Write out the "subtraction/minus set" planes
      for {set i 1} {$i <= $num_sets_minus} {incr i} {

    # Update the plane
    set plane_shift [math_scaleVec $n_incr $i]
    set guiVISvars(p1) [math_subVectors $orig_p1 $plane_shift]
    set guiVISvars(p2) [math_subVectors $orig_p2 $plane_shift]
    set guiVISvars(p3) [math_subVectors $orig_p3 $plane_shift]
    guiVISupdateCutPlane

    # Write out the results
    set specifier "_sub"
    set guiVISvars(volFlowSlice_resultsDir) "$orig_resultsDir$specifier$i" 
    set guiVISvars(volFlowSlice_resultsFilename) "$orig_resultsFilename$specifier$i"
    guiVISvolFlowSliceCalcFlows $writeSlices
      }

      # Now set all values back to original
      set guiVISvars(volFlowSlice_resultsDir) $orig_resultsDir 
      set guiVISvars(volFlowSlice_resultsFilename) $orig_resultsFilename
      set guiVISvars(p1) $orig_p1
      set guiVISvars(p2) $orig_p2
      set guiVISvars(p3) $orig_p3
  }

  # Output results for original cut plane
  guiVISvolFlowSliceCalcFlows $writeSlices
}


# Procedure: guiVISvolFlowSliceCalcFlows
proc guiVISvolFlowSliceCalcFlows { writeSlices} {

  global guiVISvars
  set theMesh $guiVISvars(mesh_vtk_obj)
  set pressurePD /guiVIS/slice/pressurePD
  set velocityPD /guiVIS/slice/velocityPD
  set surfacePD /guiVIS/slice/mesh
  set flat /guiVIS/guiVISvolFlowSliceCalcFlows/flat
  set flat_0 /guiVIS/guiVISvolFlowSliceCalcFlows/flat/0

  # tmp obj names
  set clipped /tmp/guiVISvolFlowSlice/Clipped
  set sphere  tmp-guiVISvolFlowSlice-Sphere
  set clipper tmp-guiVISvolFlowSlice-Clipper
  set cleaner tmp-guiVISvolFlowSlice-Cleaner
  set pressureAndVelocity tmp-guiVISvolFlowSlice-PresAndVel

  catch {repos_delete -obj $clipped}
  catch {$sphere Delete}
  catch {$clipper Delete}
  catch {$cleaner Delete}
  catch {$pressureAndVelocity Delete}

  global guiVISvars
  global guiVISvarsPrev

  #file mkdir $gFilenames(tmp_dir)
  #set paramsfn [file join $gFilenames(tmp_dir) mpeg_encode_params_postproc]
  #set statfn [file join $gFilenames(tmp_dir) postproc.mpeg_encode]
  #file delete $statfn

  set integrateType $guiVISvars(volFlowSlice_integrateType)
  set sphere_center $guiVISvars(volFlowSlice_sphereCenter)
  set sphere_radius $guiVISvars(volFlowSlice_sphereRadius)
  set density $guiVISvars(volFlowSlice_density)

  if {$writeSlices == 1} {
    file mkdir $guiVISvars(volFlowSlice_resultsDir)
  }

  set step_start 0
  set step_stop [expr [llength $guiVISvars(all_results_names)]-1]
  set steps [string_parse $guiVISvars(volFlowSlice_selectedSteps) $step_start $step_stop]
  if {[llength $steps] == 0} {
     return -code error "ERROR:  no steps specified."
  }

  if {$integrateType == "pressure" &&  $guiVISvars(cutplane_displayPressure) == "off"} {
    return -code error "ERROR: Must turn ON pressure slice display first!!  (see Cut Plane Visualization)"
  }

  if {$integrateType == "velocity" &&  $guiVISvars(cutplane_displayVelocity) == "off"} {
    return -code error "ERROR: Must turn ON velocity slice display first!!  (see Cut Plane Visualization)"
  }

  if {$integrateType == "energy" &&  $guiVISvars(cutplane_displayPressure) == "off"} {
    return -code error "ERROR: Must turn ON pressure slice display first!!  (see Cut Plane Visualization)"
  }

  if {$integrateType == "energy" &&  $guiVISvars(cutplane_displayVelocity) == "off"} {
    return -code error "ERROR: Must turn ON velocity slice display first!!  (see Cut Plane Visualization)"
  }


    # create implicit sphere
  vtkSphere $sphere
  $sphere SetCenter [lindex $sphere_center 0]  [lindex $sphere_center 1]  [lindex $sphere_center 2]
  $sphere SetRadius $sphere_radius

   puts "cal vol flow for steps: $steps"
   foreach step $steps {
     guiVISchangeStep $step

     catch {repos_delete -obj $clipped}
     catch {$clipper Delete}
     catch {$cleaner Delete}

     # run it through another filter to only get the vessel of
     # interest
     vtkClipPolyData $clipper
     if {$integrateType == "pressure"} {
       $clipper SetInputDataObject [repos_exportToVtk -src $pressurePD]
     } elseif {$integrateType == "velocity"} {
       $clipper SetInputDataObject [repos_exportToVtk -src $velocityPD]
     } elseif {$integrateType == "energy"} {
       vtkPolyData $pressureAndVelocity
       set pts   [[repos_exportToVtk -src $pressurePD] GetPoints] 
       set polys [[repos_exportToVtk -src $pressurePD] GetPolys]
       set p [[[repos_exportToVtk -src $pressurePD] GetPointData] GetScalars]
       set v [[[repos_exportToVtk -src $velocityPD] GetPointData] GetVectors]
       $pressureAndVelocity SetPoints $pts
       $pressureAndVelocity SetPolys  $polys
       [$pressureAndVelocity GetPointData] SetScalars $p
       [$pressureAndVelocity GetPointData] SetVectors $v
       $clipper SetInputDataObject $pressureAndVelocity
     } else {
       return -code "ERROR: wrong integrateType ($integrateType)!"
     }

     $clipper SetClipFunction $sphere
     $clipper GenerateClippedOutputOn
     $clipper Update

     # need to eliminate unwanted points
     vtkCleanPolyData $cleaner
     $cleaner ConvertPolysToLinesOff
     $cleaner PointMergingOff
     $cleaner ConvertLinesToPointsOff
     $cleaner SetInputDataObject [$clipper GetClippedOutput]
     $cleaner Update

     # bring the results into Geodesic
     repos_importVtkPd -src [$cleaner GetOutput] -dst $clipped
     catch {$pressureAndVelocity Delete}

     # save clipped slice pd if desired
     if {$writeSlices == 1} {
        repos_writeVtkPolyData -obj $clipped -type ascii  -file [file join $guiVISvars(volFlowSlice_resultsDir) clipped-$step.vtk]
     }

     catch {repos_delete -obj $flat}
     catch {repos_delete -obj $flat_0}
     set boolean 1
     set rotated_norm {}
     set unitOutwardNormal [math_normalize $guiVISvars(volFlowSlice_surfaceNormal)]
     geom_flatten $unitOutwardNormal $boolean $clipped rotated_norm $flat
     set bbox [geom_bbox -obj $flat]
     geom_translate -src $flat  -vec [list 0 0 [expr -1.0*[lindex $bbox 4]]]  -dst $flat_0
     set area [geom_surfArea -src $flat_0]
     #puts "area: $area"
     # find the idealized outward normal
     if {[lindex $rotated_norm 2] > 0} {
        set inorm {0 0 1}
     } else {
        set inorm {0 0 -1}
     }

     #puts "original normal  : $unitOutwardNormal"
     #puts "rotated normal   : $rotated_norm"
     #puts "idealized normal : $inorm"

     if {$integrateType == "pressure"} {
       # get flux
       set flux [geom_integrateSurfaceFlux -obj $flat_0 -nrm $inorm -tensorType 0]
     } elseif {$integrateType == "velocity"} {
       # get flux
       set flux [geom_integrateSurfaceFlux -obj $flat_0 -nrm $inorm -tensorType 1]
     } elseif {$integrateType == "energy"} {
       set flux [geom_integrateEnergy -obj $flat_0 -nrm $inorm -rho $density] 
     } else {
       return -code "ERROR: wrong integrateType ($integrateType)!"
     }
     set flows($step) $flux
     set resfiles($step) $guiVISvars(currentResFileName)
     catch {repos_delete -obj $flat}
     catch {repos_delete -obj $flat_0}

   }

   # calc avg
   set total 0
   foreach step $steps {
     set total [expr double($total)+$flows($step)]
   }
   set total [expr $total/double([llength $steps])]
   # write out results
   set fp [open $guiVISvars(volFlowSlice_resultsFilename) w]
   if {$integrateType == "pressure"} {
     puts $fp "# geodesic results (pressure on slice)"
   } elseif {$integrateType == "velocity"} {
     puts $fp "# geodesic results (volumetric flow through slice)"
   } elseif {$integrateType == "energy"} {
     puts $fp "# geodesic results (energy for slice)" 
   } else {
     return -code "ERROR: wrong integrateType ($integrateType)!"
   }
   puts $fp "#"
   puts $fp "# p1: $guiVISvars(p1)"
   puts $fp "# p2: $guiVISvars(p2)"
   puts $fp "# p3: $guiVISvars(p3)"
   puts $fp "# sphere_center: $guiVISvars(volFlowSlice_sphereCenter)"
   puts $fp "# sphere_radius: $guiVISvars(volFlowSlice_sphereRadius)"
   puts $fp "# surfaceNormal: $guiVISvars(volFlowSlice_surfaceNormal)"
   puts $fp "# integrateType: $guiVISvars(volFlowSlice_integrateType)"
   puts $fp "#"
   puts $fp "# volumetric flow for files:"
   foreach step $steps {
     puts $fp "#   $step: $resfiles($step)"
   }
   if {$integrateType == "pressure"} {
     foreach step $steps {
        puts $fp "$step\t[expr $flows($step) / double ($area)]"
     }
     puts $fp "avg\t[expr $total / double ($area)]"
   } else {
     foreach step $steps {
        puts $fp "$step\t$flows($step)"
     }
     puts $fp "avg\t$total"
   }
   close $fp
}


# Procedure: guiVISvolFlowSliceDisplaySphere
