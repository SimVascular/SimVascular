# Procedure: guiWSSselectFaces
proc guiWSSselectFaces { widget} {
  set wsspd /guiWSS/wssPD
  set cutpd /guiWSS/cutPD
  global gFilenames
  global guiWSSvars
  global symbolicName
  set lb $symbolicName(guiWSSfaceLB)

  if {$guiWSSvars(useAllFaces) != 0} {
    catch {repos_delete -obj $cutpd}
    geom_copy -src $wsspd -dst $cutpd
  } else {
    catch {repos_delete -obj $cutpd}
    set numitems [llength [$lb get 0 end]]
    set faces {}
    for {set i 0} {$i < $numitems} {incr i} {
      set mytext [$lb get $i]
      if {[regexp "((( SELECTED )))" $mytext] == 1} {
        lappend faces [string range $mytext 0 end-17]
      }
    }
    puts "faces: $faces"
    set appender guiWSS-tmp-appender
    catch {$appender Delete}
    vtkAppendPolyData $appender
    foreach face $faces {
      puts "working on face: $face"
      set fn [file join $gFilenames(mesh_faces_dir) $face.vtk]
      set objname /guiWSS/mesh_surfaces/$face
      set tmpPD /guiWSS/tmp/replace_scalars/$face
      catch {repos_delete -obj $objname}
      catch {repos_delete -obj $tmpPD}
      repos_readVtkPolyData -file $fn -obj $objname
      geom_replacePointData -srcA $wsspd -srcB $objname -dst $tmpPD -scalars 1 -vectors 0
      $appender AddInputData [repos_exportToVtk -src $tmpPD]
    }
    $appender Update
    repos_importVtkPd -src [$appender GetOutput] -dst $cutpd
  }

  # update display if cut is shown
  guiWSSvis vis_cut_surface
}


