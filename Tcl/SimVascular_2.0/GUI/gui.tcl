# Procedure: lsGUI_hand_make_poly
proc lsGUI_hand_make_poly { widget x y} {

  set selectedPoints lsGUItmp-handdraw-pts1
  set selectedLines lsGUItmp-handdraw-lines1
  set cursorPoints lsGUItmp-handdraw-crsr-pts1

  # create a  closed polygon from the selected points

  set npts [$selectedPoints GetNumberOfPoints]

  if {$npts < 3} {
    return -code error "Need at 3 points to define a closed contour!"
  }

  # create the contour
  set pts {}
  for {set i 0} {$i < $npts} {incr i} {
    lappend pts [$selectedPoints GetPoint $i]
  }

  global lsGUIcurrentPathNumber
  set pathId $lsGUIcurrentPathNumber
  global gPathPoints
  set path $gPathPoints($pathId,splinePts)

  global lsGUIcurrentPositionNumber
  set posId $lsGUIcurrentPositionNumber

  global lsGUIhandContourType
  set type $lsGUIhandContourType

  if {$type == "levelset"} {
    set seg /lsGUI/$pathId/$posId/ls
    set segoriented $seg/oriented
  } elseif {$type == "threshold"} {
    set baseName /lsGUI/$pathId/$posId
    set seg $baseName/thr/selected
    set segoriented $baseName/thr/oriented
  } else {
    return -code error "Invalid segmentation type ($type)."
  }

  catch {repos_delete -obj $seg}
  catch {repos_delete -obj $segoriented}

  global gImageFoo
  set voxel_dim [math_minVec $gImageFoo(voxelDims)]

  # create the unoriented contour
  # subsample the loop
  set curveLength [math_curveLength -pts $pts -closed 1]
  set numSamplePoints [expr int(ceil($curveLength/double($voxel_dim)))]
  set newpts [math_linearInterpCurve -pts $pts -closed 1 -numInterpPts $numSamplePoints]
  geom_mkLinesFromPts $newpts $seg 1

  # orient the contour
  array set items [lindex $path $posId]
  set pos [TupleToList $items(p)]
  set nrm [TupleToList $items(t)]
  set xhat [TupleToList $items(tx)]
  geom_orientProfile -src $seg -path_pos $pos -path_tan $nrm -path_xhat $xhat -dst $segoriented

  # track creation parameters via labels
  foreach obj [list $seg $segoriented] {
    repos_setLabel -obj $obj -key creation_method     -value "hand_drawn"
    repos_setLabel -obj $obj -key num_hand_picked_pts -value $npts
    repos_setLabel -obj $obj -key creation_type       -value $type
    repos_setLabel -obj $obj -key num_subsample_pts   -value $numSamplePoints
  }

  repos_setLabel -obj $segoriented -key pos       -value $pos
  repos_setLabel -obj $segoriented -key nrm       -value $nrm
  repos_setLabel -obj $segoriented -key xhat      -value $xhat
  repos_setLabel -obj $segoriented -key pathId    -value $pathId
  repos_setLabel -obj $segoriented -key posId     -value $posId

  # update display
  lsGUIchangeFrame 0

  # clear the selection polygon
  $selectedLines Reset
  $selectedPoints Reset

  # clear the cursor if necessary
  $cursorPoints Reset

  foreach renWin {lsRenWinMag lsRenWinPot} {
      global symbolicName
      set widget $symbolicName($renWin).r1
      if {[info commands $widget] != ""} {
           $widget Render
      }
  }
}


