  set guiRWH(ambient) [$prop GetAmbient]
  set guiRWH(ambientColor) [$prop GetAmbientColor]
  set guiRWH(color) [$prop GetColor]
  set guiRWH(diffuse) [$prop GetDiffuse]
  set guiRWH(diffuseColor) [$prop GetDiffuseColor]
  set guiRWH(edgeColor) [$prop GetEdgeColor]
  set guiRWH(interpolation) [$prop GetInterpolationAsString]
  set guiRWH(lineWidth) [$prop GetLineWidth]
  set guiRWH(opacity) [$prop GetOpacity]
  set guiRWH(pointSize) [$prop GetPointSize]
  set guiRWH(representation) [$prop GetRepresentationAsString]
  set guiRWH(specular) [$prop GetSpecular]
  set guiRWH(specularColor) [$prop GetSpecularColor]
  set guiRWH(specularPower) [$prop GetSpecularPower]
  set guiRWH(edgeVisibility) [$prop GetEdgeVisibility]
  set guiRWH(backfaceCulling) [$prop GetBackfaceCulling]
  set guiRWH(frontfaceCulling) [$prop GetFrontfaceCulling]

  set guiRWH(newAmbient) $guiRWH(ambient)
  set guiRWH(newAmbientColor) $guiRWH(ambientColor)
  set guiRWH(newColor) $guiRWH(color)
  set guiRWH(newDiffuse) $guiRWH(diffuse)
  set guiRWH(newDiffuseColor) $guiRWH(diffuseColor)
  set guiRWH(newEdgeColor) $guiRWH(edgeColor)
  set guiRWH(newLineWidth) $guiRWH(lineWidth)
  set guiRWH(newOpacity) $guiRWH(opacity)
  set guiRWH(newPointSize) $guiRWH(pointSize)
  set guiRWH(newSpecular) $guiRWH(specular)
  set guiRWH(newSpecularColor) $guiRWH(specularColor)
  set guiRWH(newSpecularPower) $guiRWH(specularPower)
  set guiRWH(newEdgeVisibility) $guiRWH(edgeVisibility)
  set guiRWH(newBackfaceCulling) $guiRWH(backfaceCulling)
  set guiRWH(newFrontfaceCulling) $guiRWH(frontfaceCulling)

  set texture [$actor GetTexture]
  set allowLookupRadioButton disabled
  set guiRWH(lookupTable) ""
  if {$texture == ""} {
    if {[[$actor GetMapper] GetLookupTable] != ""} {
      set lookupTable [[$actor GetMapper] GetLookupTable]
      set guiRWH(lookupTable) $lookupTable
      set guiRWH(scalarMin) [lindex [[$actor GetMapper] GetScalarRange] 0]
      set guiRWH(scalarMax) [lindex [[$actor GetMapper] GetScalarRange] 1]
      set allowLookupRadioButton normal
    }
  } else {
    set lookupTable [$texture GetLookupTable]
    set guiRWH(lookupTable) $lookupTable
    set guiRWH(scalarMin) [lindex [$guiRWH(lookupTable) GetTableRange] 0]
    set guiRWH(scalarMax) [lindex [$guiRWH(lookupTable) GetTableRange] 1]
    if {$lookupTable != ""} {
      set allowLookupRadioButton normal
    }
  }
  global symbolicName
  $symbolicName(guiRWHlookupTableRadioButton1) configure -state $allowLookupRadioButton
  $symbolicName(guiRWHlookupTableRadioButton2) configure -state $allowLookupRadioButton
}


