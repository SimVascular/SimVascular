# Procedure: guiFILT
proc guiFILT { operation} {
  global guiFILTvars
  set image_obj $guiFILTvars(volume)
  set backup_obj "$image_obj/backup" 
  # restore original image volume
  if {$operation == "restore"} {
    if {![repos_exists -obj $backup_obj]} {
      return -code error "ERROR:  No backup currently stored!"
    }
    repos_delete -obj $image_obj
    repos_importVtkImg -src [repos_exportToVtk -src $backup_obj] -dst $image_obj
  } elseif {$operation == "save"} {
    set yesno [tk_messageBox -default yes  -message "Would you like to save this volume as a VTI file?"  -title "Save Volume as VTI"  -type yesno]
    switch -- $yesno {
       yes {
          set filename [tk_getSaveFile -filetypes {{VTI *.vti} {"All Files" *.*}} -title "Choose File Name for Image File In VTI Format"]
    if {$filename != ""} {
      if {[file extension $filename] == ""} {
              set filename "$filename.vti"
      }
            img_writeXML $image_obj $filename
          }
       }
       no {
         puts "Nothing done."
       }
    }

 } elseif {$operation == "delete_original"} {

   catch {repos_delete -obj $backup_obj}

 } elseif {$operation == "smooth"} {

   catch {repos_delete -obj $backup_obj}
   repos_importVtkImg -src [repos_exportToVtk -src $image_obj] -dst $backup_obj

   set caster tmp-guiFILT-caster
   catch {$caster Delete}

   vtkImageCast $caster
   $caster SetInputDataObject [repos_exportToVtk -src $backup_obj]
   $caster SetOutputScalarTypeToFloat
   $caster Update

   set floatImg guiFILT_volume_float
   set floatSmoothImg $floatImg\_smoothed

   catch {repos_delete -obj $floatImg}
   catch {repos_delete -obj $floatSmoothImg}

   repos_importVtkImg -src [$caster GetOutput] -dst $floatImg
   $caster Delete

   set num   $guiFILTvars(flow_num_iters)
   set r     $guiFILTvars(flow_stencil_radius)
   set steps $guiFILTvars(flow_time_step)

   itk_minMaxCurvSmooth -image $floatImg -numIterations $num -stencilRadius $r -timeStep $steps

   vtkImageCast $caster
   $caster SetInputDataObject [repos_exportToVtk -src $floatSmoothImg]
   $caster SetOutputScalarTypeToShort
   $caster Update

   repos_delete -obj $image_obj
   repos_importVtkImg -src [$caster GetOutput] -dst $image_obj
   
   $caster Delete
   repos_delete -obj $floatSmoothImg
   repos_delete -obj $floatImg

 } elseif {$operation == "diffuse"} {

   catch {repos_delete -obj $backup_obj}
   repos_importVtkImg -src [repos_exportToVtk -src $image_obj] -dst $backup_obj

   set diffV $guiFILTvars(aniso_diffusion_threshold)
   set flag $guiFILTvars(aniso_mag_grad_thr_flag)
   set num $guiFILTvars(aniso_num_iterations)

   set ansy tmp-guiFILT-ansy
   catch {$ansy Delete}

   vtkImageAnisotropicDiffusion3D $ansy
   $ansy SetInputDataObject [repos_exportToVtk -src $backup_obj]
   $ansy SetNumberOfIterations $num
   $ansy SetDiffusionThreshold $diffV
   $ansy SetGradientMagnitudeThreshold $flag
   $ansy Update

   repos_delete -obj $image_obj
   repos_importVtkImg -src [$ansy GetOutput] -dst $image_obj
   
   $ansy Delete

 } elseif {$operation == "shift_and_scale"} {

   catch {repos_delete -obj $backup_obj}
   repos_importVtkImg -src [repos_exportToVtk -src $image_obj] -dst $backup_obj

   set shift $guiFILTvars(shift_shift)
   set scale $guiFILTvars(shift_scale)
   set clamp $guiFILTvars(shift_clamp_overflow)
   set otype $guiFILTvars(shift_output_scalar_type)

   set shifty tmp-guiFILT-shifty
   catch {$shifty Delete}

   vtkImageShiftScale $shifty
   $shifty SetInputDataObject [repos_exportToVtk -src $backup_obj]
   $shifty SetShift $shift
   $shifty SetScale $scale
   $shifty SetClampOverflow $clamp
   $shifty SetOutputScalarTypeTo$otype
   $shifty Update

   repos_delete -obj $image_obj
   repos_importVtkImg -src [$shifty GetOutput] -dst $image_obj
   
   $shifty Delete

 } elseif {$operation == "threshold"} {

   catch {repos_delete -obj $backup_obj}
   repos_importVtkImg -src [repos_exportToVtk -src $image_obj] -dst $backup_obj

   set thr $guiFILTvars(threshold_value)
   if {$thr == 0 || $thr == ""} {
       if  {$image_obj == "volume_image"} {
         global guiVIB
         set thr $guiVIB(min_thr_value)
       } elseif {$image_obj == "resampled_image"} {
         global guiRIB
         set thr $guiRIB(min_thr_value)
       } else {
         return -code error "ERROR: valid volume ($image_obj)"
       }
   }

   repos_delete -obj $image_obj
   img_mkThrImg $backup_obj $thr $image_obj

 } else {

    return -code error "ERROR: invalid option ($operation)!"
 }
}


# Procedure: guiFILTsetShiftScaleParams
proc guiFILTsetShiftScaleParams {} {
   global guiFILTvars
   set min $guiFILTvars(shift_window_min)
   set max $guiFILTvars(shift_window_max)
   set guiFILTvars(shift_clamp_overflow) 1
   set guiFILTvars(shift_output_scalar_type) UnsignedChar

   set image_obj $guiFILTvars(volume)
   set range [[repos_exportToVtk -src $image_obj] GetScalarRange]
   set shift [expr -1.0*$min]
   set scale [expr 255.0/(1.0*($max - $min))]
   set guiFILTvars(shift_shift) $shift
   set guiFILTvars(shift_scale) $scale
}


# Procedure: guiFNMloadHandPaths
proc guiFNMloadHandPaths {} {
    global gFilenames
    set filename $gFilenames(path_file)

    # check and see if the file exists
    if {[file exists $filename] == 0} {
      puts "ERROR: File $filename does not exist."
      return -code error "ERROR: File $filename does not exist."
    }

    # check for header
    set fp [open $filename "r"]
    set line {}
    catch {gets $fp line}
    close $fp

    if {[regexp guiPP $line] == 0} {
      puts "ERROR: File $filename is not of the right format."
      return -code error "ERROR: File $filename is not of the right format."
    }
    global gPathPoints
    catch {unset gPathPoints}
    source $filename

    guiSV_path_update_tree
}


# Procedure: guiFNMloadPatientFile
proc guiFNMloadPatientFile {} {

    global gFilenames
    global gOptions

    set filename [tk_getOpenFile -filetypes {{"patient" .patient} {"all files" *}} -title "Options File" -initialfile $gFilenames(patient_file)]

    # check and see if the file exists
    if {[file exists $filename] == 0} {
      puts "ERROR: File $filename does not exist."
      return -code error "ERROR: File $filename does not exist."
    }
    set gFilenames(patient_file) $filename

    # check for header
    set fp [open $filename "r"]
    set line {}
    catch {gets $fp line}
    close $fp

    if {[regexp geodesic_params_file_format $line] == 0} {
      puts "ERROR: File $filename is not of the right format."
      return -code error "ERROR: File $filename is not of the right format."
    }

    if {[regexp 2.* $line] == 0} {
      puts "ERROR: File $filename is not version 2.0.  File being ignored!"
      return -code error "ERROR: File $filename is not version 2.0.  File being ignored!"
    }

    if {[regexp 2.1 $line] != 1} {
      puts "WARNING:  File $filename is not the latest version.  Please resave."
      puts "          Some previously selected options may have been lost."
    }

    source $filename
}


# Procedure: guiFNMloadSolidModel
proc guiFNMloadSolidModel { fn solid} {
    global gFilenames
    global gObjects
    global gOptions
    set filename $gFilenames($fn)
    set object $gObjects($solid)
    set solid_kernel $gOptions(meshing_solid_kernel)

    # check and see if the file exists
    if {[file exists $filename] == 0} {
      puts "ERROR: File $filename does not exist."
      return -code error "ERROR: File $filename does not exist."
    }

    catch {repos_delete -obj $object}
    global gOptions

    solid_setKernel -name $gOptions(meshing_solid_kernel)
    solid_readNative -file $filename -obj $object

    # if discrete model, try and load faces file or create defaults
    if {$gOptions(meshing_solid_kernel) == "Discrete"} {
      global gDiscreteModelFaceNames
      global gDiscreteModelFaceNamesInfo
      catch {unset gDiscreteModelFaceNames}
      catch {unset gDiscreteModelFaceNamesInfo}
      if [file exists $filename.facenames] {
        puts "sourcing $filename.facenames"
        source $filename.facenames
        package require md5
        set mymd5 [::md5::md5 -hex -file $filename]
        if {$mymd5 != $gDiscreteModelFaceNamesInfo(model_file_md5)} {
          return -code error "ERROR: dsm model ($filename) file doesn't match one used to generate facenames ($filename.facenames)!"
        }
      } else {
        set allids [$object GetFaceIds]
        foreach id $allids {
          set gDiscreteModelFaceNames($id) "noname_$id"
        }
      }
    } elseif {$gOptions(meshing_solid_kernel) == "PolyData"} {      
      global gPolyDataFaceNames
      global gPolyDataFaceNamesInfo
      catch {unset gPolyDataFaceNames}
      catch {unset gPolyDataFaceNamesInfo}
      if [file exists $filename.facenames] {
        puts "sourcing $filename.facenames"
        source $filename.facenames
        package require md5
        set mymd5 [::md5::md5 -hex -file $filename]
        #if {$mymd5 != $gPolyDataFaceNamesInfo(model_file_md5)} {
        #  return -code error "ERROR: polydata model ($filename) file doesn't match one used to generate facenames ($filename.facenames)!"
        #}
      } else {

        guiMMloadPolyData $object
  return

      }
    }

    if {$solid_kernel == "Parasolid" || $solid_kernel == "Discrete" || $solid_kernel == "PolyData"} {
  set pretty_names {}
  foreach i [$object GetFaceIds] {
    if {$solid_kernel == "Parasolid"} {
      catch {lappend pretty_names [$object GetFaceAttr -attr gdscName -faceId $i]}
    } elseif {$solid_kernel == "Discrete"} {
      lappend pretty_names $gDiscreteModelFaceNames($i)
    } elseif {$solid_kernel == "PolyData"} {
      lappend pretty_names $gPolyDataFaceNames($i)
    }
  }
  if {[llength [lsort -unique $pretty_names]] != [llength $pretty_names]} {
    set duplist [lsort -dictionary $pretty_names]
    foreach i [lsort -unique $pretty_names] {
      set idx [lsearch -exact $duplist $i]
      set duplist [lreplace $duplist $idx $idx]
    }
    set msg "Duplicate faces found!\n\n"
    foreach dup $duplist {
      set msg "$msg  name: $dup\n"
    }
    tk_messageBox -title "Duplicate Face Names" -type ok -message $msg
  }
    } else {
      return -code error "ERROR: invalid solid_kernel ($solid_kernel)"
    }
}


