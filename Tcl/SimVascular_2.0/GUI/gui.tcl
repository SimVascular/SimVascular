# Procedure: bcGUIscaleSeg
proc bcGUIscaleSeg { type} {

  global bcGUIcurrentFrameNumber
  set frame $bcGUIcurrentFrameNumber

  # need to translate hand/analytic to proper type
  if {$type == "analytic"} {
    global bcGUIanalContourType
    set type $bcGUIanalContourType
  } elseif {$type == "hand"} {
    global bcGUIhandContourType
    set type $bcGUIhandContourType
  }

  if {$type == "levelset"} {
    set seg /gBC/pcdata/mag/$frame/ls
  } elseif {$type == "threshold"} {
    set seg /gBC/pcdata/mag/$frame/thr/selected
  } else {
    return -code error "Invalid segmentation type ($type)."
  }

  if {[repos_exists -obj $seg] == 0} {
    return
  }

  # keep the tags on the segmentation
  foreach key [repos_getLabelKeys -obj $seg] {
    set seg_keys($key) [repos_getLabel -obj $seg -key $key]
  }

  global bcGUIscaleFactor

  set cntrseg   /bcGUI/scaled/centered
  set scaledseg /bcGUI/scaled/scaled

  catch {repos_delete -obj $cntrseg}
  catch {repos_delete -obj $scaledseg}

  # move center to origin
  set center [geom_avgPt -obj $seg]
  geom_translate -src $seg -dst $cntrseg -vec [math_scaleVec $center -1.0]
  # scale segmentation
  geom_scaleAvg -src $cntrseg -dst $scaledseg -factor $bcGUIscaleFactor
  catch {repos_delete -obj $seg}
  catch {repos_delete -obj $segoriented}
  # move back to center
  geom_translate -src $scaledseg -dst $seg -vec $center

  # reattach tags
  foreach key [array names seg_keys] {
    repos_setLabel -obj $seg -key $key -value $seg_keys($key)
  }
  # update seg
  set scale_factor 1.0
  if {[info exists seg_keys(scale_factor)] == 1} {
    repos_clearLabel -obj $seg -key scale_factor
    set scale_factor $seg_keys(scale_factor)
  }
  repos_setLabel -obj $seg -key scale_factor -value [expr double($scale_factor) * $bcGUIscaleFactor]

  # update display
  bcGUIchangeFrame $frame
}


# Procedure: bcGUIsegBatch
proc bcGUIsegBatch { choice} {

  global bcGUIbatchEntry

  set entry $bcGUIbatchEntry

  # find min/max frame number
  global symbolicName
  global gBC
  set minFrame 1
  set maxFrame $gBC(numSlices)

  # we allow a special file to be specified which comes from vcalc
  # in the batch space for thresholding

  if {$choice == "threshold"} {

    set isFile 0
    if {[file exists $entry] == 1} {
      set isFile 1
    }
    if {$isFile == 1} {
      set allframes {}
      set fp [open $entry "r"]
      while {[gets $fp line] >= 0} {
        scan $line "Frame %i, threshold = %i" frame value
        set threshValues($frame) $value
        lappend allframes $frame
      }
      close $fp
    } else {
      set allframes [string_parse $bcGUIbatchEntry $minFrame $maxFrame]
    }
  } elseif {$choice == "levelset"} {

    set allframes [string_parse $bcGUIbatchEntry $minFrame $maxFrame]

  } else {

    return -code error "ERROR:  Illegal type $choice."

  }

  if {[llength $allframes] == 0} {
     puts "ERROR:  No frames specified."
     return -code error "ERROR:  No frames specified."
  }
  set yesno [tk_messageBox -message "Do you want to attempt [llength $allframes] segmentations?" -default no -icon question -type yesno]
  if {$yesno == "no"} {
        return -code error "User halted."
  }
  if {[llength $allframes] >= 5} {
     set yesno [tk_messageBox -message "Are you really, really sure?  You will be attempting [llength $allframes] segmentations." -default no -icon question -type yesno]
     if {$yesno == "no"} {
        return -code error "User halted."
     }
  }

  set starttime [clock seconds]
  foreach frame $allframes {
    $symbolicName(bcGUIframeScrollbar) set $frame
    if {$choice == "threshold"} {
      # only set the threshold value if we've read in a file of them
      if {$isFile == 1} {
        global bcGUIthrValue
        set bcGUIthrValue $threshValues($frame)
      }
      bcGUIthrSlice
    } elseif {$choice == "levelset"} {
      bcGUIlsSlice
    } else {
      return -code error "ERROR:  $type not valid."
    }
  }
  puts "time for batch run: [expr [clock seconds] - $starttime] seconds."
}


