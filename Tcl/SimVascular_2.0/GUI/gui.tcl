    foreach i [array names gPathPoints] {
      set ab [split $i ,]
      set a [lindex $ab 0]
      set b [lindex $ab 1]
      if {$a == $pathID} {
  if {$b == "name"} {
          set pathName $gPathPoints($i)
        } elseif {$b == "numSplinePts"} {
          set numSplinePts $gPathPoints($i)
  } elseif {$b == "splinePts"} {
          # skip splined path
  } elseif {$b == "creationTimestamp"} {
          # skip
        } elseif {$b == "lastUpdateTimestamp"} {
          # skip
        } else {
          lappend pointIds $b
  }
      }
    }

    $tv insert .paths.all end -id .paths.all.$pathID -text "$pathName     (ID: $pathID)  (Number of Spline Pts: $numSplinePts)"
    foreach pt [lsort -integer $pointIds] {
     $tv insert .paths.all.$pathID end -id .paths.all.$pathID.$pt -text "$gPathPoints($pathID,$pt)"
    }
  }

  $tv yview moveto 0
}


