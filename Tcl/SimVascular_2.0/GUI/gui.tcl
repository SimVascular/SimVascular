  bind $w <ButtonRelease-1> {guiSVIMG_canvas_done   %W}
  bind $w <Button-3>        {guiSVIMG_canvas_delete %W}
  interp alias {} tags$w {} $w itemcget current -tags
}


# Procedure: guiSVIMG_canvas_calc_histogram
proc guiSVIMG_canvas_calc_histogram { bounds} {

    # Get the size of the histogram window    
    set width [expr [lindex $bounds 1] - [lindex $bounds 0]]
    set height [expr [lindex $bounds 3] - [lindex $bounds 2]]
    
    # Setup the bins of the accumulate filter from the range of input data
    set accumulate tmp-guiSVIMG_canvas_add_histogram-accum
    catch {$accumulate Delete}
    vtkImageAccumulate $accumulate
    $accumulate SetInputDataObject [repos_exportToVtk -src volume_image]

    set numBins [expr $width / 2]

    global guiCVIMGvars
    set xmin $guiCVIMGvars(vol_ren_min_range)
    set xmax $guiCVIMGvars(vol_ren_max_range)
    set inputRange [list $xmin $xmax]       

    set origin [lindex $inputRange 0]

    set spacing [expr 1.0 * ([lindex $inputRange 1] - $origin) / $numBins]

    $accumulate SetComponentExtent 0 [expr $numBins - 1] 0 0 0 0
    $accumulate SetComponentOrigin $origin 0.0 0.0
    $accumulate SetComponentSpacing $spacing 1.0 1.0
    $accumulate Update

    # Get the histogram data

    set data [$accumulate GetOutput]

    # Scale the histogram max to fit the window

    set histRange [[[$data GetPointData] GetScalars] GetRange]
    set scale [expr 0.9 * $height / [lindex $histRange 1]]

    set rtnpts {}
    for {set idx 0} {$idx < $numBins} {incr idx} {
        set y [$data GetScalarComponentAsDouble $idx 0 0 0]
        set y1 [expr $y * $scale]
        set y2 [lindex [split $y1 .] 0]
        set x [expr $idx * 2]
        set plotX [expr [lindex $bounds 0] + $x]
        lappend rtnpts "$plotX [lindex $bounds 3] $plotX [expr [lindex $bounds 3] - $y2]"
    }
    $accumulate Delete
    return $rtnpts
}


# Procedure: guiSVIMG_canvas_delete
proc guiSVIMG_canvas_delete { w {all "0"}} {
    #-- delete a node of, or a whole polygon
    set tags [tags$w]
    if {[regexp {of:([^ ]+)} $tags -> poly]} {
        if {$all} {
            $w delete $poly of:$poly
        } else {
            regexp {at:([^ ]+)} $tags -> pos
            $w coords $poly [lreplace [$w coords $poly] $pos [incr pos]]
            guiSVIMG_canvas_markNodes $w $poly
        }
    }
    $w delete poly0 ;# possibly clean up unfinished polygon
    catch {unset ::polydraw(item$w)}
}


# Procedure: guiSVIMG_canvas_done
proc guiSVIMG_canvas_done { args} {
  global guiCVIMGvars
  set xmin $guiCVIMGvars(vol_ren_min_range)
  set xmax $guiCVIMGvars(vol_ren_max_range)
  global symbolicName
  set Cblue $symbolicName(guiSVIMG_blue_canvas)
  set Cgreen $symbolicName(guiSVIMG_green_canvas)
  set Copacity $symbolicName(guiSVIMG_opacity_canvas)
  set Cred $symbolicName(guiSVIMG_red_canvas)
  if {[lindex $args 0] == $Copacity} {
     set opacityPts [$Copacity coords line0]
     puts "opacityPts: $opacityPts"
     set numPts [expr [llength $opacityPts] / 2]
     # normalize here
     for {set i 0} {$i < $numPts} {incr i} {
   set px [lindex $opacityPts [expr 2*$i]]
         set py [lindex $opacityPts [expr 2*$i+1]]
         set x [math_linInterp1D 50.0 350.0 $xmin $xmax $px]
         # remember y is in screen (upside down) coords
         set y [math_linInterp1D 10.0 110.0 1.0 0.0 $py]
         if {$y < 0} {set y 0}
         #puts "pt $i: $x $y"
         lappend newPts [list $x $y]         
     }
     global guiVIB
     set guiVIB(opacityFunction) $newPts
     volGUIupdateVolRender guiVIB opacity
     return
  }

  package require struct
  package require math::interpolate

  if {[lindex $args 0] == $Cblue || [lindex $args 0] == $Cgreen ||  [lindex $args 0] == $Cred} {

    # need to create equal number of pts for all colors
    set redPts [$Cred coords line0]
    set greenPts [$Cgreen coords line0]
    set bluePts [$Cblue coords line0]
  
    set id 0
    foreach ptset [list $redPts $greenPts $bluePts] {
      set numPts [expr [llength $ptset] / 2]
      # normalize here
      set orgpts($id) ""
      for {set i 0} {$i < $numPts} {incr i} {
        set px [lindex $ptset [expr 2*$i]]
        set py [lindex $ptset [expr 2*$i+1]]
        set x [math_linInterp1D 50.0 350.0 $xmin $xmax $px]
        # remember y is in screen (upside down) coords
        set y [math_linInterp1D 10.0 110.0 1.0 0.0 $py]
  if {$y < 0} {set y 0}
        lappend allX $x
        set orgpts($id) "$orgpts($id) $x $y"         
      }
      incr id
    }

    set useX [lsort -real -unique $allX]
    puts "useX: $useX"
    puts "$orgpts(0)"
    foreach i $useX {
      set r [::math::interpolate::interp-linear $orgpts(0) $i]
      set g [::math::interpolate::interp-linear $orgpts(1) $i]
      set b [::math::interpolate::interp-linear $orgpts(2) $i]
      lappend colorFn [list $i $r $g $b]
    }
    global guiVIB
    set guiVIB(colorFunction) $colorFn
    volGUIupdateVolRender guiVIB color   

  }
}


# Procedure: guiSVIMG_canvas_has
proc guiSVIMG_canvas_has { list element} {
expr {[lsearch $list $element]>=0}
}


# Procedure: guiSVIMG_canvas_insert
proc guiSVIMG_canvas_insert { w} {
    #-- create a new node halfway to the previous node
    set tags [tags$w]
    if {[guiSVIMG_canvas_has $tags node]} {
        regexp {of:([^ ]+)} $tags -> poly
        regexp {at:([^ ]+)} $tags -> pos
        set coords [$w coords $poly]
        set pos2 [expr {$pos==0? [llength $coords]-2 : $pos-2}]
        foreach {x0 y0} [lrange $coords $pos end] break
        foreach {x1 y1} [lrange $coords $pos2 end] break
        if {$x0 > 1} {
          puts "coords: $x0 $y0 $x1 $y1"
          set x [expr {($x0 + $x1) / 2}]
          set y [expr {($y0 + $y1) / 2}]
          $w coords $poly [linsert $coords $pos $x $y]
          guiSVIMG_canvas_markNodes $w $poly
  }
    }
}


# Procedure: guiSVIMG_canvas_mark
proc guiSVIMG_canvas_mark { w x y} {
    # prepare a node for moving
    set x [$w canvasx $x]; set y [$w canvasy $y]
    catch {unset ::polydraw(current$w)}
    if {[guiSVIMG_canvas_has [tags$w] node]} {
        set ::polydraw(current$w) [$w find withtag current]
        set ::polydraw(x$w)       $x
        set ::polydraw(y$w)       $y
    }
}


# Procedure: guiSVIMG_canvas_markNodes
proc guiSVIMG_canvas_markNodes { w item} {
    #-- decorate a polygon with square marks at its nodes
    $w delete of:$item
    set pos 0
    foreach {x y} [$w coords $item] {
        set coo [list [expr $x-3] [expr $y-3] [expr $x+3] [expr $y+3]]
        $w create rect $coo -fill blue -tag "node of:$item at:$pos"
        incr pos 2
    }
}


# Procedure: guiSVIMG_canvas_move
proc guiSVIMG_canvas_move { w x y bounds tol {all "0"}} {
    #-- move a node or whole line
    set x [$w canvasx $x]; set y [$w canvasy $y]
    if {[info exists ::polydraw(current$w)]} {
        set dx [expr {$x - $::polydraw(x$w)}]
        set dy [expr {$y - $::polydraw(y$w)}]

        set ::polydraw(x$w) $x
        set ::polydraw(y$w) $y
        if {!$all} {
            # the actual motion may get changed in the redraw routine
            set dxdy [guiSVIMG_canvas_redraw $w $dx $dy $bounds]
            $w move $::polydraw(current$w) [lindex $dxdy 0] [lindex $dxdy 1]
            #puts "$w move $::polydraw(current$w) [lindex $dxdy 0] [lindex $dxdy 1]"
        } elseif [regexp {of:([^ ]+)} [tags$w] -> poly] {
            $w move $poly    $dx $dy
            $w move of:$poly $dx $dy
        }
    }
}


# Procedure: guiSVIMG_canvas_redraw
proc guiSVIMG_canvas_redraw { w dx dy bounds} {
    #-- update a polygon when one node was moved
    set tags [tags$w]
    if [regexp {of:([^ ]+)} $tags -> poly] {
        regexp {at:([^ ]+)} $tags -> from
        #puts "from ($from)  poly($poly)"
        set coords [$w coords $poly]
        set to [expr {$from + 1}]
        # don't allow end points to move in x-direction
        if {$from == 0 || $from == [expr [llength $coords] - 2]} {
    set x [lindex $coords $from]
          set dx 0
  } else {
          set x [expr {[lindex $coords $from] + $dx}]
  }
        set y [expr {[lindex $coords $to]   + $dy}]
        # check bounds of motion before actually updating
        # bound the possible motion
        if {$x <= [lindex $bounds 0]} {
    set x [lindex $bounds 0]
    set dx [expr $x - [lindex $coords $from]]
    #puts "x: $x dx: $dx coords: [lindex $coords $from]"
  } elseif {$x >= [lindex $bounds 1]} {
    set x [lindex $bounds 1]
    set dx [expr $x - [lindex $coords $from]]
    #puts "x: $x dx: $dx coords: [lindex $coords $from]"
        } 
        if {$y <= [lindex $bounds 2]} {
    set y [lindex $bounds 2]
    set dy [expr $y - [lindex $coords $to]]
    #puts "y: $y dy: $dy coords: [lindex $coords $to]"
        } elseif {$y >= [lindex $bounds 3]} {
    set y [lindex $bounds 3]
    set dy [expr $y - [lindex $coords $to]]
    #puts "y: $y dy: $dy coords: [lindex $coords $to]"
  }
        $w coords $poly [lreplace $coords $from $to $x $y]
    }
  set ::polydraw(x$w) $x
  set ::polydraw(y$w) $y
  global guiCVIMGvars
  set xmin $guiCVIMGvars(vol_ren_min_range)
  set xmax $guiCVIMGvars(vol_ren_max_range)
  set x [math_linInterp1D 50.0 350.0 $xmin $xmax $x]
  # remember y is in screen (upside down) coords
  set y [math_linInterp1D 10.0 110.0 1.0 0.0 $y]
  if {$y < 0} {set y 0}
  set guiCVIMGvars(canvas_value) [format "intensity %.1f  fvalue %.3f" $x $y]
  return [list $dx $dy]
}


