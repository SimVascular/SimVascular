# Procedure: guiWSSvis
proc guiWSSvis { flag} {

  global guiWSSvars
  global gRen3d

  set ren $gRen3d

  global gRen3dFreeze
  if {$gRen3dFreeze > 0} {
    set alreadyfrozen 1
  } else {
    set alreadyfrozen 0
    set gRen3dFreeze 1
  }

  set wsspd /guiWSS/wssPD
  set cutpd /guiWSS/cutPD

  if {$flag == "vis_boxInteractor"} {
    if {$guiWSSvars(vis_boxInteractor) == 0} {
      catch {vis_boxWidgetRm $gRen3d guiWSS}
    } else {
      catch {vis_boxWidgetRm $gRen3d guiWSS}
      vis_boxWidgetAdd $gRen3d [repos_exportToVtk -src $wsspd] guiWSS  guiWSSboxEnableInteraction  guiWSSboxBeginInteraction  guiWSSboxInteract  guiWSSboxEndInteraction
      vis_boxWidgetOn $gRen3d guiWSS
    }
  }

  if {$flag == "vis_planeInteractor"} {
    if {$guiWSSvars(vis_planeInteractor) == 0} {
      catch {vis_planeWidgetRm $gRen3d guiWSS}
    } else {
      catch {vis_planeWidgetRm $gRen3d guiWSS}
      vis_planeWidgetAdd $gRen3d [repos_exportToVtk -src $wsspd] guiWSS  guiWSSplaneEnableInteraction  guiWSSplaneBeginInteraction  guiWSSplaneInteract  guiWSSplaneEndInteraction
      vis_planeWidgetOn $gRen3d guiWSS
    }
  }

  if {$flag == "vis_initial_surface"} {
    if {$guiWSSvars(vis_initial_surface) == 0} {
      catch {vis_pRm $gRen3d $wsspd}
    } else {
      catch {vis_pRm $gRen3d $wsspd}
      vis_pRepos $gRen3d $wsspd
      set actor [vis_pGetActor $ren $wsspd]
      [$actor GetMapper] ScalarVisibilityOn
      [$actor GetMapper] SetLookupTable gBlueToRedLUT
      [$actor GetMapper] SetScalarRange $guiWSSvars(area_min_value) $guiWSSvars(area_max_value)
    }
  }


  if {$flag == "vis_cut_surface"} {
    if {$guiWSSvars(vis_cut_surface) == 0} {
      catch {vis_pRm $gRen3d $cutpd}
    } else {
      catch {vis_pRm $gRen3d $cutpd}
      vis_pRepos $gRen3d $cutpd
      set actor [vis_pGetActor $ren $cutpd]
      [$actor GetMapper] ScalarVisibilityOn
      [$actor GetMapper] SetLookupTable gBlueToRedLUT
      [$actor GetMapper] SetScalarRange $guiWSSvars(area_min_value) $guiWSSvars(area_max_value)
    }
  }

  if {$alreadyfrozen == 0} {
    set gRen3dFreeze 0
    vis_render $ren
  }
}


