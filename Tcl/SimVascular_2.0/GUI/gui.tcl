# Procedure: guiBOUNDARIESfillWithIds
proc guiBOUNDARIESfillWithIds {} {
  global gOptions
  global guiPDvars
  global gPolyDataFaceNames
  global smasherInputName
  global gui3Dvars

  if {$smasherInputName == "none"} {
    tk_messageBox -title "Load Solid Model Please"  -type ok -message "WARNING: You need to load the model with face ids before filling with ids"
    return
  }

  set maxid 0
  set allids [$smasherInputName GetFaceIds]
  set numfaceids [llength $allids]
  if {$numfaceids == 0} {
    tk_messageBox -title "No Faces on Solid"  -type ok -message "WARNING: You need to apply faceids to the surface before filling with ids"
    return
  }
  foreach id $allids {
    if {$id > $maxid} {
      set maxid $id
    }
  }
  set fillpd /guiBOUNDARIES/fillpd 
  set outfillpd /guiBOUNDARIES/outfillpd 
  catch {repos_delete -obj $fillpd}
  catch {repos_delete -obj $outfillpd}

  $smasherInputName GetPolyData -result $fillpd

  set filltype 2
  geom_fillHolesWithIds $fillpd $outfillpd $maxid $filltype

  $smasherInputName SetVtkPolyData -obj $outfillpd
  set newids [$smasherInputName GetFaceIds]
  foreach id $newids {
    if {$id > $maxid} {
      set gPolyDataFaceNames($id) "noname_$id"
    }
  }

  crd_ren gRenWin_3D_ren1
  smasherGUIupdateViewWindow
  set guiPDvars(updatedSolid) 1
}

