    set guiVISresults($filename) [list 0 {} {}]

  }
}


# Procedure: guiVISflowThruFace
proc guiVISflowThruFace { meshFaceObj unitOutwardNormal type rtnFlow} {

  # NOTE: this proc has been generalized to calculate mean pressure
  #       as well, so the name is not really correct

  global guiVISvars
  set theMesh $guiVISvars(mesh_vtk_obj)

  upvar $rtnFlow flux

  set faceCopy /guiVIS/guiVISflowThruFace/copy
  set faceCopy2 /guiVIS/guiVISflowThruFace/copy2
  set flat /guiVIS/guiVISflowThruFace/flat
  set flat_0 /guiVIS/guiVISflowThruFace/flat/0
  set myVectors tmp-guiVISflowThruFace-vec
  set myScalars tmp-guiVISflowThruFace-sca
  catch {repos_delete -obj $flat}
  catch {repos_delete -obj $flat_0}
  catch {repos_delete -obj $faceCopy}
  catch {repos_delete -obj $faceCopy2}
  catch {$myVectors Delete}
  catch {$myScalars Delete}

  #now calculate flow rate for each time point

  set numPts [geom_numPts -obj $meshFaceObj]
  set nodeIds [[[repos_exportToVtk -src $meshFaceObj] GetPointData] GetScalars]

  geom_copy -src $meshFaceObj -dst $faceCopy

  if {$type == "flow"} {

    # create an array for the velocity vectors
    vtkFloatArray $myVectors; $myVectors SetNumberOfComponents 3
    $myVectors Allocate 1000 1000

    set objVectors [[$theMesh GetPointData] GetVectors]

    for {set i 0} {$i < $numPts} {incr i} {
      set node [expr int([$nodeIds GetValue $i])]
      # nodes in vtk start at 0, so subtract 1 off
      set node [expr $node - 1]
      set vec [$objVectors GetTuple3 $node]
      #puts "processing vtk node: $node vec: $vec"
      $myVectors InsertNextTuple3 [lindex $vec 0] [lindex $vec 1] [lindex $vec 2]
    }

    # associate vectors with polydata
    [[repos_exportToVtk -src $faceCopy] GetPointData] SetVectors $myVectors

  } elseif {$type == "pressure"} {

    # create an array for the velocity vectors
    vtkFloatArray $myScalars; $myScalars SetNumberOfComponents 1
    $myScalars Allocate 1000 1000
    $myScalars SetName "scalars"

    set objScalars [[$theMesh GetPointData] GetScalars]

    for {set i 0} {$i < $numPts} {incr i} {
      set node [expr int([$nodeIds GetValue $i])]
      # nodes in vtk start at 0, so subtract 1 off
      set node [expr $node - 1]
      set scalar [$objScalars GetTuple1 $node]
      #puts "processing vtk node: $node vec: $vec"
      $myScalars InsertNextTuple1 $scalar
    }

    # associate scalars with polydata
    [[repos_exportToVtk -src $faceCopy] GetPointData] SetScalars ""
    [[repos_exportToVtk -src $faceCopy] GetPointData] SetScalars $myScalars
    [[repos_exportToVtk -src $faceCopy] GetPointData] SetActiveScalars scalars

  } elseif {$type == "energy"} {

    # create an array for the velocity vectors
    vtkFloatArray $myVectors; $myVectors SetNumberOfComponents 3
    $myVectors Allocate 1000 1000

    set objVectors [[$theMesh GetPointData] GetVectors]

    for {set i 0} {$i < $numPts} {incr i} {
      set node [expr int([$nodeIds GetValue $i])]
      # nodes in vtk start at 0, so subtract 1 off
      set node [expr $node - 1]
      set vec [$objVectors GetTuple3 $node]
      #puts "processing vtk node: $node vec: $vec"
      $myVectors InsertNextTuple3 [lindex $vec 0] [lindex $vec 1] [lindex $vec 2]
    }

    # associate vectors with polydata
    [[repos_exportToVtk -src $faceCopy] GetPointData] SetVectors $myVectors

    # create an array for the pressure
    vtkFloatArray $myScalars; $myScalars SetNumberOfComponents 1
    $myScalars Allocate 1000 1000

    set objScalars [[$theMesh GetPointData] GetScalars]

    for {set i 0} {$i < $numPts} {incr i} {
      set node [expr int([$nodeIds GetValue $i])]
      # nodes in vtk start at 0, so subtract 1 off
      set node [expr $node - 1]
      set scalar [$objScalars GetTuple1 $node]
      #puts "processing vtk node: $node vec: $vec"
      $myScalars InsertNextTuple1 $scalar
    }

    # need to wait to associate scalars with polydata, see below

  } else {

    return -code error "ERROR:  invalid type ($type)."

  }

  # get flux
  if {$type == "flow"} {
    set tensorType 1
    set fluxarea [geom_integrateSurface2 -obj $faceCopy -tensorType $tensorType]
    set flux [lindex $fluxarea 0]
    set area [lindex $fluxarea 1]
    catch {repos_delete -obj $faceCopy}
    return
  } elseif {$type == "pressure"} {
    set tensorType 0
    set fluxarea [geom_integrateSurface2 -obj $faceCopy -tensorType $tensorType]
    set flux [lindex $fluxarea 0]
    set area [lindex $fluxarea 1]
    # intentionally divide by area for pressure calc
    set flux [expr $flux / $area]
    catch {repos_delete -obj $faceCopy}
    catch {repos_delete -obj $faceCopy2}
    return
  }

  #
  #  can't get here - dead code
  # 

  set boolean 1
  set rotated_norm {}
  geom_flatten $unitOutwardNormal $boolean $meshFaceObj rotated_norm $flat

  set bbox [geom_bbox -obj $flat]

  if {[expr abs([lindex $bbox 5] - [lindex $bbox 4])] > 0.1} {
    catch {repos_delete -obj $flat}
    set flux non-planar
    puts "NOTE:  face not flat, value of zero being returned!"
    return
  }

  geom_translate -src $flat  -vec [list 0 0 [expr -1.0*[lindex $bbox 4]]]  -dst $flat_0
  set area [geom_surfArea -src $flat_0]
  #puts "area: $area"

  # find the idealized outward normal
  if {[lindex $rotated_norm 2] > 0} {
      set inorm {0 0 1}
  } else {
      set inorm {0 0 -1}
  }

  # intentionally wait to define scalars on flat object so we don't overwrite
  # scalar node id values!

  if {$type == "pressure" || $type == "energy"} {
    # associate scalars with polydata
    [[repos_exportToVtk -src $flat_0] GetPointData] SetScalars $myScalars
  }

  #puts "original normal  : $unitOutwardNormal"
  #puts "rotated normal   : $rotated_norm"
  #puts "idealized normal : $inorm"

  # get flux
  if {$type == "flow"} {
     return -code error "ERROR: should not reach this code \"flow\""
     set tensorType 1
    set flux [geom_integrateSurfaceFlux -obj $flat_0 -nrm $inorm -tensorType $tensorType]
  } elseif {$type == "pressure"} {
     return -code error "ERROR: should not reach this code \"pressure\""
     set tensorType 0
     set flux [geom_integrateSurfaceFlux -obj $flat_0 -nrm $inorm -tensorType $tensorType]
     set flux [expr $flux / $area]
  } elseif {$type == "energy"} {
     set density $guiVISvars(volFlowSlice_density)
     global gOptions
     if {$gOptions(meshing_solid_kernel) == "Discrete"} {
       return -code error "ERROR:  Energy for Discrete not implemented yet!"
     }
     set flux [geom_integrateEnergy -obj $flat_0 -nrm $inorm -rho $density]
     #set flux [expr $flux / $area]
  } else {
     return -code error "ERROR: invalid type ($type)."
  }

  catch {repos_delete -obj $flat}
  catch {repos_delete -obj $flat_0}
  catch {repos_delete -obj $faceCopy}

  if {$type == "flow" || $type == "energy"} {
    # unassociate vectors with polydata
    [[repos_exportToVtk -src $meshFaceObj] GetPointData] SetVectors ""
    $myVectors Delete
  } elseif {$type == "pressure" || $type == "energy"} {
    #[[repos_exportToVtk -src $meshFaceObj] GetPointData] SetScalars ""
    $myScalars Delete
  } else {
    return -code error "ERROR: invalid type ($type)."
  }
}


# Procedure: guiVISgetStepData
proc guiVISgetStepData { step} {
;#fixed

  global guiVISvars
  global gOptions

  set theMesh $guiVISvars(mesh_vtk_obj)
  set thePressure guiVISpressureObject
  set theVelocity guiVISvelocityObject
  set thePD guiVISpolyObject
  set baseResName /guiVIS/result_
  set surfacePDwithNormals /guiVIS/vol/surface_mesh/with_normals

  # can't read the data if we haven't read a mesh
  # note: the reason this returns okay is that
  #       it is called automatically by "scale"
  #       when the gui is first created.
  global guiVISvars
  if {$guiVISvars(mesh_loaded) == 0} {
    return GDSC_OK
  }

  # this procedure either just returns the object
  # name for the step if its already loaded or loads
  # it if necessary.

  global guiVISresults

  set resFilenames $guiVISvars(all_results_names)

  set numFilenames [llength $resFilenames]
  if {$step < 0 || $step >= $numFilenames} {
    return -code error "STEP NUMBER $step OUT OF RANGE."
  }

  set filename [lindex $resFilenames $step]
  set guiVISvars(currentResFileName) [file tail $filename]

  #debug
  #puts "filename: $filename"
  #puts "stepData: $baseResName$step"

  # delete any previously loaded data
  if {$guiVISvars(retainAllResInMemory) == 0} {
     puts "deleting results"
     for {set dstep 0} {$dstep < $numFilenames} {incr dstep} {
       if {[lindex $guiVISresults([lindex $resFilenames $dstep]) 0] == 1} {
   catch {repos_delete -obj $baseResName$dstep}
         puts "$guiVISresults([lindex $resFilenames $dstep])"
         set guiVISresults([lindex $resFilenames $dstep]) [lreplace $guiVISresults([lindex $resFilenames $dstep]) 0 0 0]
         puts "$guiVISresults([lindex $resFilenames $dstep])"
       }
     }
  }

  # if the step has already been loaded, just return it.
  if {[lindex $guiVISresults($filename) 0] == 1} {

    set guiVISvars(currentStep_pressureRange) [lindex $guiVISresults($filename) 1]
    set guiVISvars(currentStep_velMagRange) [lindex $guiVISresults($filename) 2]

    return $baseResName$step

  } else {

    # load it here
    puts "Reading $filename..."

    # if the file is vtk, just read it in.  If not, then
    # use post_ procs to get info from Vis files
    if {[file ext $filename] == ".vtk"} {

      catch {repos_delete -obj $baseResName$step}
      repos_readVtkPolyData -obj $baseResName$step -file $filename

    } else {

      catch {repos_delete -obj $baseResName$step}

          # completely ugly hack to get to see scalar transport
          set presandvelobj /tmp/guiVIS/pressureAndVelocity
          set transportobj /tmp/guiVIS/scalarTransport
          set stressobj  /tmp/guiVIS/stresses
          set wallshear  /tmp/guiVIS/wallshear
          catch {repos_delete -obj $presandvelobj}
          catch {repos_delete -obj $transportobj}
          catch {repos_delete -obj $stressobj}
          catch {repos_delete -obj $wallshear}

          global gOptions
          set readingoptions {}
          if [info exists gOptions(post_traction_nodes_file)] {
             set tractfp [open $gOptions(post_traction_nodes_file) r]
             set traction_nodes {}
             while {[gets $tractfp line] > 0} {
               lappend traction_nodes $line
             }
             close $tractfp
             set traction_nodes [lsort -integer -unique $traction_nodes]
             set readingoptions "$readingoptions -traction_nodes [list $traction_nodes]"
             #puts "$readingoptions"
    }
          if {($gOptions(post_read_transport) == 0) && ($gOptions(post_calc_wall_shear) == 0)} {
             if [catch {eval post_readVisRes -file $filename -result $baseResName$step -grid analysis_mesh $readingoptions} msg] {
               return -code error "Could not read vis result file ($msg)."
       }
    } else {
             if {$gOptions(post_read_transport) != 0} {
                set readingoptions "-transport $transportobj"
       }
             if {$gOptions(post_calc_wall_shear) == 0} {
                # do nothing
       } elseif {$gOptions(post_calc_wall_shear) == 1} {
                # spectrum style
                set readingoptions "$readingoptions -stress $stressobj"
       } elseif {$gOptions(post_calc_wall_shear) == 2} {
                # phasta style
    if {$gOptions(post_solver) == "phasta"} {
                  set readingoptions "$readingoptions -traction $wallshear"
    } elseif {$gOptions(post_solver) == "acusolve"} {
                  set readingoptions "$readingoptions -traction $stressobj"
    } else {
                  return -code error "invalid solver option ($gOptions(post_solver))"
    }
       } elseif {$gOptions(post_calc_wall_shear) == 3} {
                set readingoptions "$readingoptions -displacement $wallshear"
       } elseif {$gOptions(post_calc_wall_shear) == 4} {
                set readingoptions "$readingoptions -wss $wallshear"
       } else {
                return -code error "ERROR: Invalid gOptions(calc_wall_shear) value"
       }

             if [catch {eval post_readVisRes -file $filename -result $presandvelobj -grid analysis_mesh $readingoptions} msg] {
                   return -code error "Could not read vis result file ($msg)."
       }

             # spectrum style
       if {$gOptions(post_calc_wall_shear) == 1} {
               post_calcWallShear -surfaceMesh $surfacePDwithNormals -tensors $stressobj -result $wallshear
       }
             if {($gOptions(post_calc_wall_shear) == 2) && ($gOptions(post_solver) == "acusolve")} {
               post_calcWallShear -surfaceMesh $surfacePDwithNormals -tractions $stressobj -result $wallshear
       }

             if {$gOptions(post_read_transport) == 1} {
                set usescalars $transportobj
       } else {
                set usescalars $presandvelobj
       }
             if {$gOptions(post_calc_wall_shear) != 0} {
                set usevectors $wallshear
       } else {
                set usevectors $presandvelobj
       }

              set tmppdobj tmp-guiReadVisResults-change-res
        catch {$tmppdobj Delete}
              vtkPolyData $tmppdobj
              $tmppdobj SetPoints [[repos_exportToVtk -src $presandvelobj] GetPoints]
              [$tmppdobj GetPointData] SetScalars [[[repos_exportToVtk -src $usescalars] GetPointData] GetScalars]
              [$tmppdobj GetPointData] SetVectors [[[repos_exportToVtk -src $usevectors] GetPointData] GetVectors]
              repos_importVtkPd -src $tmppdobj -dst $baseResName$step
              $tmppdobj Delete
              repos_delete -obj $presandvelobj
              catch {repos_delete -obj $transportobj}
              catch {repos_delete -obj $stressobj}
              catch {repos_delete -obj $wallshear}

    }
    }

    set scalarRange [[[[repos_exportToVtk -src $baseResName$step] GetPointData] GetScalars] GetRange]

    set vectnorm tmp-guiVISgetStepData-vn
    catch {$vectnorm Delete}
    vtkVectorNorm $vectnorm
    $vectnorm SetInputDataObject [repos_exportToVtk -src $baseResName$step]
    $vectnorm Update
    set vecMagRange [[[[$vectnorm GetOutput] GetPointData] GetScalars] GetRange]
    set guiVISresults($filename) [list 1 $scalarRange $vecMagRange]
    $vectnorm Delete

    # update global ranges
    set gpress $guiVISvars(global_pressureRange)
    if {$gpress == ""} {
       set gpress $scalarRange
    } else {
  if {[lindex $scalarRange 0] < [lindex $gpress 0]} {
          set gpress [list [lindex $scalarRange 0] [lindex $gpress 1]]
  }
  if {[lindex $scalarRange 1] > [lindex $gpress 1]} {
          set gpress [list [lindex $gpress 0] [lindex $scalarRange 1]]
  }
    }
    set guiVISvars(global_pressureRange) $gpress
    # update global ranges
    set gvecm $guiVISvars(global_velMagRange)
    if {$gvecm == ""} {
       set gvecm $vecMagRange
    } else {
  if {[lindex $vecMagRange 0] < [lindex $gvecm 0]} {
          set gvecm [list [lindex $vecMagRange 0] [lindex $gvecm 1]]
  }
  if {[lindex $vecMagRange 1] > [lindex $gvecm 1]} {
          set gvecm [list [lindex $gvecm 0] [lindex $vecMagRange 1]]
  }
    }
    set guiVISvars(global_velMagRange) $gvecm

    set guiVISvars(currentStep_pressureRange) [lindex $guiVISresults($filename) 1]
    set guiVISvars(currentStep_velMagRange) [lindex $guiVISresults($filename) 2]

    return $baseResName$step

  }
}


# Procedure: guiVISloadResults
proc guiVISloadResults {} {
;#fixed

  global guiVISvars
  set theMesh $guiVISvars(mesh_vtk_obj)
  set surfacePD /guiVIS/vol/surface_mesh
  set surfacePDwithNormals /guiVIS/vol/surface_mesh/with_normals

  global guiVISvars
  global guiVISresults

  catch {$theMesh Delete}
  if {[info exists guiVISresults] == 1} {
     set j 0
     foreach i [array names guiVISresults] {
       guiVISdeleteStepData $j
       incr j
     }
  }

  puts "Load the mesh."
  # assume that if a filename was given it is in the correct directory
  set dir $guiVISvars(resultsDir)
  if {[file isdirectory $dir] == 0} {
    set dir [file dirname $dir]
  }

  # we will first look for the results in vtk form, if they
  # don't exist we'll switch to vis files
  set meshvtk {}
  set meshvisgz {}
  set meshvisZ {}
  set meshvis {}
  catch {set meshvtk [glob [file join $dir *_mesh.vtk]]}
  catch {set meshvisgz [glob [file join $dir *_mesh.vis.gz]]}
  catch {set meshvisZ [glob [file join $dir *_mesh.vis.Z]]}
  catch {set meshvis [glob [file join $dir *_mesh.vis]]}
  if {$meshvtk == "" && $meshvisgz == "" && $meshvisZ == "" && $meshvis == ""} {
      puts "ERROR:  Could not find a mesh with a .vtk, .vis, , vis.gz, or .vis.Z extension."
      return -code error "COULD NOT FIND MESH FILE."
  }

  if {$meshvtk != ""} {
    # read vtk file
    puts "Reading $meshvtk..."
    set reader tmp-guiVISloadResults-reader
    catch {$reader Delete}
    vtkUnstructuredGridReader $reader
    $reader SetFileName $meshvtk
    $reader Update
    set guiVISvars(mesh_vtk_obj) guiVISmeshObject
    set theMesh $guiVISvars(mesh_vtk_obj)
    catch {$theMesh Delete}
    vtkUnstructuredGrid $theMesh
    $theMesh Allocate 200 400
    $theMesh Initialize
    $theMesh CopyStructure [$reader GetOutput]
    $theMesh SetPoints [[$reader GetOutput] GetPoints]
    $theMesh CopyInformation [$reader GetOutput]
    catch {$reader Delete}
  } elseif {$meshvis != ""} {
      # read vis file
      puts "Reading $meshvis..."
      catch {repos_delete -obj analysis_mesh}
      if [catch {post_readVisMesh -file $meshvis -obj analysis_mesh}] {
        return -code error "COULD NOT READ THE MESH."
      }
      set guiVISvars(mesh_vtk_obj) [repos_exportToVtk -src analysis_mesh]
      set theMesh $guiVISvars(mesh_vtk_obj)
  } elseif {$meshvisgz != ""} {
      # read vis file
      puts "Reading $meshvisgz..."
      catch {repos_delete -obj analysis_mesh}
      if [catch {post_readVisMesh -file $meshvisgz -obj analysis_mesh}] {
        return -code error "COULD NOT READ THE MESH."
      }
      set guiVISvars(mesh_vtk_obj) [repos_exportToVtk -src analysis_mesh]
      set theMesh $guiVISvars(mesh_vtk_obj)
  } elseif {$meshvisZ != ""} {
      return -code error "compress compression not supported!"
  } else {
    return -code error "ERROR:  could not find mesh file."
  }

  set guiVISvars(mesh_loaded) 1

  # extract the surface mesh
  puts "Extracting the exterior surface mesh..."
  catch {repos_delete -obj $surfacePD}
  post_getSurfMesh $theMesh $surfacePD
  set guiVISvars(surface_mesh_exists) 1

  # calculate the surface normals (needed for wall shear stress code)
  set mknrm tmp-guiVISloadResults-mknrm
  catch {$mknrm Delete}
  vtkPolyDataNormals $mknrm
  $mknrm ConsistencyOff
  $mknrm SplittingOff
  $mknrm ComputePointNormalsOn
  $mknrm FlipNormalsOff

  $mknrm SetInputDataObject [repos_exportToVtk -src $surfacePD]
  $mknrm Update

  catch {repos_delete -obj $surfacePDwithNormals}
  repos_importVtkPd -src [$mknrm GetOutput] -dst $surfacePDwithNormals
  $mknrm Delete

  set resultsvtk {}
  set resultsvis {}
  set resultsvisgz {}
  set resultsvisZ {}
  catch {set resultsvtk [glob [file join $dir *_res*.vtk]]}
  catch {set resultsvisgz [glob [file join $dir *_res*.vis.gz]]}
  catch {set resultsvisZ [glob [file join $dir *_res*.vis.Z]]}
  catch {set resultsvis [glob [file join $dir *_res*.vis]]}
  if {$resultsvtk == "" && $resultsvisgz == "" && $resultsvisZ == "" && $resultsvis == ""} {
      puts "ERROR:  Could not find results with a .vtk, .vis, .vis.gz, or .vis.Z extension."
      return -code error "COULD NOT FIND RESULTS FILES."
  }

  # we will first look for the results in vtk form, if they
  # don't exist we'll switch to vis files
  if {$resultsvtk != ""} {
    set resFilenames $resultsvtk
  } elseif {$resultsvis != ""} {
    set resFilenames $resultsvis
  } elseif {$resultsvisgz != ""} {
    set resFilenames $resultsvisgz
  } elseif {$resultsvisZ != ""} {
    set resFilenames $resultsvisZ
  } else {
    return -code error "COULD NOT FIND RESULTS FILES."
  }

  set resFilenames [lsort -dictionary $resFilenames]
  set guiVISvars(all_results_names) $resFilenames

  # store the names of the results into an array, and their
  # current load status
  catch {unset guiVISresults}
  # just set up the array if we don't need to read them all
  foreach i $resFilenames {
    set guiVISresults($i) [list 0 {} {}]
  }

  # read all the files if desired
  if {$guiVISvars(readAllResFiles) == 1} {
    for {set i 0} {$i < [llength $resFilenames]} {incr i} {
      guiVISgetStepData $i
    }
  }

  # make sure the graphics window is displayed
  global gRen3d
  set gRen3d [vis_gRenWin_3D]
  #if {[info exists gRen3d] == 0} {
  #  set gRen3d [vis_initTKgr gRenWin_3D "Geodesic 3-D Graphics Window" {}]
  #}
  #if {[info commands $gRen3d] == ""} {
  #  set gRen3d [vis_initTKgr gRenWin_3D "Geodesic 3-D Graphics Window" {}]
  #}
  set guiVISvars(ren) $gRen3d

  # we will track the last value of the variables in case
  # we need them
  global guiVISvarsPrev
  catch {unset guiVISvarsPrev}
  foreach i [array names guiVISvars] {
    set guiVISvarsPrev($i) $guiVISvars($i)
  }

  # setup step scale
  global symbolicName
  set scale $symbolicName(guiVISstepScale)
  $scale configure -from 0
  $scale configure -to [expr [llength $resFilenames]-1]

  set guiVISvarsPrev(currentStep) -1
  guiVISchangeStep 0

  return GDSC_OK
}


