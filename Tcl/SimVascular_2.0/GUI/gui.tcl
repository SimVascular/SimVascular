# Procedure: guiVIScalcPathResults
proc guiVIScalcPathResults { choice} {

    global guiVISvars
    set theMesh $guiVISvars(mesh_vtk_obj)

    if {$choice == "plot"} {
    return -code error "ERROR:  not implemented yet!"
    } elseif {$choice != "saveFile"} {
    return -code error "ERROR:  invalid choice \"$choice\""
    }

   global lsGUIcurrentPathNumber
   global gPathPoints
   global guiVISvars

   set pathId $lsGUIcurrentPathNumber
   set outType $guiVISvars(plotpath_type)
   if {[info exists gPathPoints($pathId,splinePts)] == 0} {
      return -code error "ERROR:  no spline for pts."
   }

   set splinePts $gPathPoints($pathId,splinePts)

   set pathLen [llength $splinePts]

   # find steps
   set step_start 0
   set step_stop [expr [llength $guiVISvars(all_results_names)]-1]
   set steps [string_parse $guiVISvars(plotpath_selectedSteps) $step_start $step_stop]
   if {[llength $steps] == 0} {
     return -code error "ERROR:  no steps specified."
   }

   catch {repos_delete -obj /guiVIS/probe}
   path_MakePolyData $splinePts /guiVIS/probe

   foreach step $steps {

     guiVISchangeStep $step
     set resfiles($step) $guiVISvars(currentResFileName)

     set probefilt tmp-guiVIS-probe
     catch  {$probefilt Delete}
     vtkProbeFilter $probefilt
     $probefilt SetInputDataObject [repos_exportToVtk -src /guiVIS/probe]
     $probefilt SetSource $theMesh
     $probefilt Update
     catch {repos_delete -obj /guiVIS/probe/$step}
     repos_importVtkPd -src [$probefilt GetOutput] -dst /guiVIS/probe/$step

     set vectors [[[$probefilt GetOutput] GetPointData] GetVectors]
     set scalars [[[$probefilt GetOutput] GetPointData] GetScalars]

     set all {}
     set id 0
     foreach pos $splinePts {
  array set items $pos
  set pt [TupleToList $items(p)]
        set ptcoord($id) $pt
        if {$outType == "velocity"} {
          set v [$vectors GetTuple3 $id]
          #puts "v: $v"
          lappend all $v
        } elseif {$outType == "pressure"} {
    set p [$scalars GetTuple1 $id]
          #puts "p: $p"
          lappend all $p
  } else {
          return -code error "ERROR: invalid $outType."
  }
        incr id
     }
     set results($step) $all
   }

   set fp [open $guiVISvars(plotpath_file) "w"]

   puts $fp "# geodesic results (velocity or pressure along path)"
   puts $fp "#"
   puts $fp "# pathId: $pathId"
   puts $fp "# pathname: $gPathPoints($pathId,name)"
   puts $fp "#"
   puts $fp "# results files:"
   foreach step $steps {
     puts $fp "#   $step: $resfiles($step)"
   }

   if {$outType == "velocity"} {
     puts $fp "#step\tptId\tR\tA\tS\tdistance\tv_mag\tv_x\tv_y\tv_z"
     foreach step $steps {
        set vlist $results($step)
        set distance 0
        set prevpt $ptcoord(0)
        for {set i 0} {$i < [llength $vlist]} {incr i} {
          set v [lindex $vlist $i]
          set pt $ptcoord($i)
          set distance [expr double ($distance) + [math_magnitude [math_subVectors $pt $prevpt]]]
          set prevpt $pt
          puts $fp "$step\t$i\t[lindex $pt 0]\t[lindex $pt 1]\t[lindex $pt 2]\t$distance\t[math_magnitude $v]\t[lindex $v 0]\t[lindex $v 1]\t[lindex $v 2]"
        }
      }
   } else {
      puts $fp "#step\tptId\tR\tA\tS\tdistance\tpressure"
      foreach step $steps {
       set plist $results($step)
       set distance 0
       set prevpt $ptcoord(0)
       for {set i 0} {$i < [llength $plist]} {incr i} {
         set p [lindex $plist $i]
         set pt $ptcoord($i)
         set distance [expr double ($distance) + [math_magnitude [math_subVectors $pt $prevpt]]]
         set prevpt $pt
         puts $fp "$step\t$i\t[lindex $pt 0]\t[lindex $pt 1]\t[lindex $pt 2]\t$distance\t$p"
       }
      }
   }
   close $fp
}


