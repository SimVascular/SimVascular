  set flag $guiPPchooserSplineInteractorFlag

  catch {vis_splineWidgetRm $gRen3d guiPP}
  catch {vis_splineImageWidgetRm $gRen3d guiPP}

  if {$flag == 0} {
    return
  }

  set ptpd /tmp/guiPP/splineinteractor
