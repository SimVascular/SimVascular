# Procedure: bcGUIflowBatch
proc bcGUIflowBatch {} {
  global bcGUIcurrentFrameNumber
  global bcGUIctrlCurrentFrameChoice
  global bcGUIbatchEntry

  set entry $bcGUIbatchEntry
  set isFile 0
  if {[file exists $entry] == 1} {
    set isFile 1
  }

  # find min/max frame number
  global symbolicName
  global gBC
  set minFrame 1
  set maxFrame $gBC(numSlices)

  global bcGUIctrlBatchChoice

  set choice $bcGUIctrlBatchChoice

  if {$choice == "threshold"} {
    # check if entry widget contains filename first.
    # for now, assume it is a vcalc threshold file.
    if {$isFile == 1} {
      set allframes {}
      set fp [open $entry "r"]
      while {[gets $fp line] >= 0} {
         scan $line "Frame %i, threshold = %i" frame value
         set threshValues($frame) $value
         lappend allframes $frame
      }
      close $fp
    } else {
      global gBC
      set allframes [string_parse $bcGUIbatchEntry 1 $gBC(numSlices)]
    }
    if {[llength $allframes] == 0} {
       puts "ERROR:  No frames specified."
       return -code error "ERROR:  No frames specified."
    }
    foreach frame $allframes {
      set bcGUIcurrentFrameNumber $frame
      global bcGUIctrlCurrentFrameChoice
      set bcGUIctrlCurrentFrameChoice "Threshold"
      bcGUIflowCur
    }
  } else {
    global gBC
    set allframes [string_parse $bcGUIbatchEntry 1 $gBC(numSlices)]
    if {[llength $allframes] == 0} {
       puts "ERROR:  No frames specified."
       return -code error "ERROR:  No frames specified."
    }
    foreach frame $allframes {
      set bcGUIcurrentFrameNumber $frame
      global bcGUIctrlCurrentFrameChoice
      set bcGUIctrlCurrentFrameChoice "Level Set"
      bcGUIflowCur
    }
  }
}


# Procedure: bcGUIflowBatchByPixels
proc bcGUIflowBatchByPixels {} {
  global bcGUIcurrentFrameNumber
  global bcGUIbatchEntry

  set entry $bcGUIbatchEntry

  # find min/max frame number
  global gBC
  set minFrame 1
  set maxFrame $gBC(numSlices)

  set allframes [string_parse $bcGUIbatchEntry $minFrame $maxFrame]

   if {[llength $allframes] == 0} {
       puts "ERROR:  No frames specified."
