# Procedure: guiRESULTS3DsetColor
proc guiRESULTS3DsetColor { side type} {

  global guiRESULTSvars
  global gRen3d
  global gRenWin_3D_vars

  set obj $guiRESULTSvars($side\_selected_solid_model)
  set show $guiRESULTSvars($side\_show)
  set opacity $guiRESULTSvars($side\_opacity_scale_value)
  set color $guiRESULTSvars($side\_color)

  set  color [tk_chooseColor]
  if {$color == ""} return
  set r [expr (0x[string range $color 1 2])/255.0]
  set g [expr (0x[string range $color 3 4])/255.0]
  set b [expr (0x[string range $color 5 6])/255.0]

  set guiRESULTSvars($side\_color) [list $r $g $b]

  set guiRESULTSvars(visopts_solid_$obj) [list show $show opacity $opacity r $r g $g b $b]

  set useLOD $gRenWin_3D_vars(solid_use_LOD)

  if {$show == 1} {
    catch {vis_lodSetColor $gRen3d $obj $r $g $b}
    catch {[[vis_pGetActor $gRen3d $obj] GetProperty] SetColor $r $g $b}
    catch {vis_render $gRen3d}
  }
}


