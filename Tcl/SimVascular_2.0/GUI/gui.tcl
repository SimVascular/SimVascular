# Procedure: guiVIScalcOutletFlows
proc guiVIScalcOutletFlows { type choice} {

    global guiVISvars
    set theMesh $guiVISvars(mesh_vtk_obj)

    if {$choice == "plot"} {
    return -code error "ERROR:  not implemented yet!"
    } elseif {$choice != "saveFile"} {
    return -code error "ERROR:  invalid choice \"$choice\""
    }

   global guiVISvars
   global gFilenames

   # find steps
   set step_start 0
   set step_stop [expr [llength $guiVISvars(all_results_names)]-1]
   set steps [string_parse $guiVISvars(outlet_selectedSteps) $step_start $step_stop]
   if {[llength $steps] == 0} {
     return -code error "ERROR:  no steps specified."
   }

   # loop over the named faces on the specified solid model

   set solid $guiVISvars(solidModelName)
   if {[repos_exists -obj $solid] == 0} {
      return -code error "ERROR: $solid does not exist."
   }
   if {[repos_type -obj $solid] != "SolidModel"} {
      return -code error "ERROR: repos object $solid not of type SolidModel."
   }

   # find the named faces on the solid model
   set faces {}

   global gOptions
   if {$gOptions(meshing_solid_kernel) == "Parasolid"} {
     foreach id [$solid GetFaceIds] {
       set face [$solid GetFaceAttr -attr gdscName -faceId $id]
       if {$face != ""} {
         lappend faces $face
         set unitOutwardNormal [$solid GetFaceNormal -face $id -u 0 -v 0]
         set unitOutwardNormal [math_normalize $unitOutwardNormal]
         set normals($face) $unitOutwardNormal
       }
     }
   } elseif {$gOptions(meshing_solid_kernel) == "Discrete"} {
     foreach id [$solid GetFaceIds] {
       set face noname_$id
       lappend faces $face
       # normal is ignored now
       set normals($face) "0 0 0"
     }
   } else {
     return -code error "ERROR:  invalid solid model type!"
   }

   if {[llength $faces] == 0} {
     return -code error "ERROR:  no named faces found on solid."
   }

   global gOptions
   # need to make sure this variable exists!!
   if {$gOptions(exclude_walls_from_flows) == 1} {
     set original_faces $faces
     set faces {}
     foreach face $original_faces {
       if {[string tolower [string range $face 0 3]] != "wall"} {
         lappend faces $face
       }
     }
   }

   set faces [lsort -dictionary $faces]

   # read in the vtk objects for the named faces
   repos_deleteList [repos_subList /guiVIS/outlet/face/*]
   foreach face $faces {
      set fn [file join $gFilenames(mesh_faces_dir) $face.vtk]
      puts "Reading ($face) from file ($fn)."
      repos_readVtkPolyData -file $fn -obj /guiVIS/outlet/face/$face
   }

   foreach step $steps {

     guiVISchangeStep $step

     foreach face $faces {
       set flow {}
       guiVISflowThruFace /guiVIS/outlet/face/$face $normals($face) $type flow
       set flow_results($face,$step) $flow
     }

   }

   # output results

   if {$type == "flow"} {
     set fp [open $guiVISvars(outlet_flow_results_file) w]
   } elseif {$type =="pressure"} {
     set fp [open $guiVISvars(outlet_pressure_results_file) w]
   } elseif {$type =="energy"} {
     set fp [open $guiVISvars(energy_flux_results_file) w]
   } else {
     return -code error "ERROR: invalid type ($type)"
   }

   fconfigure $fp -translation lf

   puts -nonewline $fp "step\t"
   foreach face $faces {
     puts -nonewline $fp "$face\t"
   }
   puts $fp "total"

   foreach face $faces {
     set avgs($face) 0.0
   }

   foreach step $steps {
     puts -nonewline $fp "$step\t"
     set total 0.0
     foreach face $faces {
       if {$flow_results($face,$step) == "non-planar"} {
         puts -nonewline $fp [format "%12s\t" $flow_results($face,$step)]
       } else {
   puts -nonewline $fp [format "%+012.5E\t" $flow_results($face,$step)]
         set total [expr double($total) + double ($flow_results($face,$step))]
         set avgs($face) [expr double($avgs($face)) + double ($flow_results($face,$step))]
       }
     }
     if {$type == "flow"} {
       puts $fp [format "%+012.5E\t" $total]
     } else {
       puts $fp ""
     }
   }
   puts -nonewline $fp "average\t"
   foreach face $faces {
     puts -nonewline $fp [format "%+012.5E\t" [expr $avgs($face)/double([llength $steps])]]
   }
   puts $fp ""

   close $fp
}


# Procedure: guiVIScalcOutletResistances
proc guiVIScalcOutletResistances {} {

  global guiVISvars
  set flowfn $guiVISvars(outlet_flow_results_file)
  set meanfn $guiVISvars(outlet_pressure_results_file)
  set outfn $guiVISvars(outlet_resistance_results_file)

  set flowfp [open $flowfn r]
  set meanfp [open $meanfn r]

  catch {unset flow}
  catch {unset mean}
  catch {unset resistance}
  catch {unset skipped}

  gets $flowfp flow_title
  gets $meanfp mean_title

  set numPieces [expr [llength $flow_title] - 1]
  for {set i 0} {$i < $numPieces} {incr i} {
     set skipped($i) 0
  }

  while {[gets $flowfp flowline] >= 0} {
    gets $meanfp meanline
    set step [lindex $flowline 0]
    set flows {}
    set means {}
    set resistances {}
    for {set i 1} {$i < $numPieces} {incr i} {
      set q [lindex $flowline $i]
      set p [lindex $meanline $i]
      if {$q == "non-planar"} {
        set skipped($i) 1
      } else {
        if {$step == "average"} {
    if {$skipped($i) != 0} {
            continue
    }
        }
  if {[expr abs($q)] < 0.001} {
          set r 0.0
  } elseif [catch {set r [expr $p / $q]} msg] {
          puts "ERROR: $msg"
    puts "P: $p Q: $q column: [lindex $flow_title $i]"
          set r 9999999999.99
  }
        lappend flows $q
        lappend means $p
        lappend resistances $r
      }
    }
    set flow($step) $flows
    set mean($step) $means
    set resistance($step) $resistances
  }

  close $flowfp
  close $meanfp

  set fp [open $outfn w]

  puts $fp "pressure"
  for {set i 0} {$i < [expr [llength $flow_title] - 1]} {incr i} {
     if {$skipped($i) == 0} {
       puts -nonewline $fp "[lindex $flow_title $i]\t"
     }
  }
  puts $fp ""

  set numSteps [llength [array names mean]]
  for {set i 0} {$i < $numSteps} {incr i} {
    if {$i == [expr $numSteps -1]} {
      set step average
    } else {
      set step $i
    }
    puts -nonewline $fp "$step"
    for {set j 0} {$j < [llength $mean($step)]} {incr j} {
      puts -nonewline $fp "\t[lindex $mean($step) $j]"
    }
    puts $fp ""
