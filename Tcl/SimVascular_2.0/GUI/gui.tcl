# Procedure: guiGROUPSsplineInteractionEvent
proc guiGROUPSsplineInteractionEvent {} {
  global guiGROUPSvars
  global gRen3d
  global gPathBrowser
  set numHandles $gPathBrowser(solid_sample)

  global gInteractorRen

  set pts [vis_splineClosedWidgetGetPts $gInteractorRen guiGROUPS]

  set orgSplinePts $guiGROUPSvars(orgSplinePts)

  set fixAxisX 0
  set fixAxisY 0
  set fixAxisZ 0
  set fixPlane 1

  for {set i 0} {$i < [llength $pts]} {incr i} {
    set orgpt [lindex $orgSplinePts $i]
    set newpt [lindex $pts $i]
    if {$fixAxisX != 0} {
       set x [lindex $orgpt 0]
    } else {
       set x [lindex $newpt 0]
    }
    if {$fixAxisY != 0} {
       set y [lindex $orgpt 1]
    } else {
       set y [lindex $newpt 1]
    }
    if {$fixAxisZ != 0} {
       set z [lindex $orgpt 2]
    } else {
       set z [lindex $newpt 2]
    }
    lappend fixedpts [list $x $y $z]
  }

  if {$fixPlane == 0} {
    vis_splineClosedWidgetUpdatePts $gRen3d guiGROUPS $fixedpts
  } else {
    vis_splineClosedWidgetUpdatePtsPlaneOnly $gRen3d guiGROUPS $fixedpts
  }

  global guiGROUPSvars

  if {$guiGROUPSvars(show_surface) != 0} {
    global gRen3dFreeze
    set gRen3dFreeze 1
    if {$guiGROUPSvars(seg_before_id) != ""} {
      set vtkobj [repos_exportToVtk -src $guiGROUPSvars(seg_before_id)]
      set ptobj [$vtkobj GetPoints]
      for {set i 0} {$i < [llength $fixedpts]} {incr i} {
        eval $ptobj SetPoint [expr $i + [llength $fixedpts]] [lindex $fixedpts $i]
      }
      vis_pRm $gRen3d $guiGROUPSvars(seg_before_id)
      set a [vis_pRepos $gRen3d $guiGROUPSvars(seg_before_id)]
      [$a GetProperty] SetOpacity $guiGROUPSvars(surface_opacity)
    }
    if {$guiGROUPSvars(seg_after_id) != ""} {
      set vtkobj [repos_exportToVtk -src $guiGROUPSvars(seg_after_id)]
      set ptobj [$vtkobj GetPoints]
      for {set i 0} {$i < [llength $fixedpts]} {incr i} {
        eval $ptobj SetPoint $i [lindex $fixedpts $i]
      }
      vis_pRm $gRen3d $guiGROUPSvars(seg_after_id)
      set a [vis_pRepos $gRen3d $guiGROUPSvars(seg_after_id)]
      [$a GetProperty] SetOpacity $guiGROUPSvars(surface_opacity)
    }
    set gRen3dFreeze 0
    vis_render $gRen3d
  }
}


