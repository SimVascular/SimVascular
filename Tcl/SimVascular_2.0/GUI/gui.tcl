# Procedure: guiVISsetCutFromCurrentPathLoc
proc guiVISsetCutFromCurrentPathLoc {} {
    global gPathPoints
    global lsGUIcurrentPathNumber
    global lsGUIcurrentPositionNumber
    set pathId $lsGUIcurrentPathNumber
    set path $gPathPoints($pathId,splinePts)
    set posId $lsGUIcurrentPositionNumber

    array set items [lindex $path $posId]
    set pos [TupleToList $items(p)]
    set nrm [TupleToList $items(t)]
    set xhat [TupleToList $items(tx)]

    set n1 [lindex $nrm 0]
    set n2 [lindex $nrm 1]
    set n3 [lindex $nrm 2]

    set a1 [lindex $pos 0]
    set a2 [lindex $pos 1]
    set a3 [lindex $pos 2]

    global guiVISvars

    set guiVISvars(p1) $pos

    set p1 [expr 1.01*$a1+1]
    set p2 $a2
    set p3 [expr $a3 - ($n1*double($p1-$a1)+$n2*double($p2-$a2))/double($n3)]

    set guiVISvars(p2) [list $p1 $p2 $p3]

    set p1 $a1
    set p2 [expr 1.01*$a2+1]
    set p3 [expr $a3 - ($n1*double($p1-$a1)+$n2*double($p2-$a2))/double($n3)]

    set guiVISvars(p3) [list $p1 $p2 $p3]

    # cross pollenation with the gui to calc vol flow
    set guiVISvars(volFlowSlice_sphereCenter) $pos
    set guiVISvars(volFlowSlice_surfaceNormal) $nrm

    return
}


# Procedure: guiVISsetCutFromPC
proc guiVISsetCutFromPC {} {
;#fixed

  set filename [tk_getOpenFile -title "Choose Phase Contrast Filename"]

  if {$filename == ""} {return}

  global guiVISvars

  set params [img_readHeader -file $filename]

  foreach i $params {
    set option [lindex $i 0]
    set value [lindex $i 1]
    if {$option == "top_right_corner"} {
       set guiVISvars(p1) $value
    } elseif {$option == "bottom_right_corner"} {
       set guiVISvars(p2) $value
    } elseif {$option == "top_left_corner"} {
       set guiVISvars(p3) $value
    }
  }

  return GDSC_OK
}


# Procedure: guiVISupdateCutPlane
proc guiVISupdateCutPlane {} {

   global guiVISvars
   set theMesh $guiVISvars(mesh_vtk_obj)
   set pressurePD /guiVIS/slice/pressurePD
   set velocityPD /guiVIS/slice/velocityPD
   set surfacePD /guiVIS/slice/mesh

   # update the display for volume
   global guiVISvars
   global guiVISvarsPrev

   set ren $guiVISvars(ren)

   global gRen3dFreeze
   if {$gRen3dFreeze > 0} {
      set alreadyfrozen 1
   } else {
      set alreadyfrozen 0
      set gRen3dFreeze 1
   }

   # although the mesh slice topology is technically independent
   # of the time step, since it is regenerated by the vtkCutter
   # we will assume here that it is not constant over time

   # the vol pressure and velocity depend on the step number,
   # if it appears the step number has changed since the last
   # display, we turn off all vis and let it update.  Note
   # that this code does not update the prev step number, that
   # should be done outside of this routine.  The graphics
   # also depend on the slice location, so if its changed
   # we also need to update.

   if {($guiVISvarsPrev(currentStep) != $guiVISvars(currentStep)) ||  ($guiVISvarsPrev(p1) != $guiVISvars(p1)) ||  ($guiVISvarsPrev(p2) != $guiVISvars(p2)) ||  ($guiVISvarsPrev(p3) != $guiVISvars(p3)) ||  ($guiVISvarsPrev(slice_offset) != $guiVISvars(slice_offset))} {
     if {$guiVISvarsPrev(cutplane_displayPressure) != "off"} {
       puts "step change: cutplane:  remove old pressure display"
       vis_pRm $ren $pressurePD
       repos_delete -obj $pressurePD
       set guiVISvarsPrev(cutplane_displayPressure) "off"
     } else {
       puts "step change: cutplane:  pressure do nothing"
     }
     if {$guiVISvarsPrev(cutplane_displayVelocity) == "off"} {
          # dont need to do anything
          puts "step change: cutplane: velocity off, off -- do nothing"
     } elseif {$guiVISvarsPrev(cutplane_displayVelocity) == "as vectors"} {
          puts "step change: cutplane: turn off vectors"
          vis_vRm $ren $velocityPD
          set guiVISvarsPrev(cutplane_displayVelocity) "off"
          repos_delete -obj $velocityPD
     } elseif {$guiVISvarsPrev(cutplane_displayVelocity) == "warp vectors"} {
          puts "step change: cutplane: turn off warp vectors"
          vis_pRm $ren $velocityPD
          set guiVISvarsPrev(cutplane_displayVelocity) "off"
          repos_delete -obj $velocityPD
     } else {
          puts "step change: cutplane: turn off velocity scalars"
          vis_pRm $ren $velocityPD
          set guiVISvarsPrev(cutplane_displayVelocity) "off"
          repos_delete -obj $velocityPD
     }
     if {$guiVISvarsPrev(cutplane_showMesh) == 1} {
        vis_pRm $ren $surfacePD
        set guiVISvarsPrev(cutplane_showMesh) 0
     }
     # create the slice
     catch {repos_delete -obj $surfacePD}
     guiVIScreateCutPlane $theMesh $surfacePD
   }

   # toggle display of exterior mesh
   # note: the exterior volume mesh is independent of the
   #       actual step number
   if {$guiVISvarsPrev(cutplane_showMesh) == 1 &&  $guiVISvars(cutplane_showMesh) == 0} {
      vis_pRm $ren $surfacePD
      set guiVISvarsPrev(cutplane_showMesh) 0
   } elseif {$guiVISvarsPrev(cutplane_showMesh) == 0 &&  $guiVISvars(cutplane_showMesh) == 1} {
      set actor [vis_pRepos $ren $surfacePD]
      set myprop [$actor GetProperty]
      $myprop SetRepresentationToWireframe
      $myprop SetInterpolationToFlat
      $myprop SetColor 0.8900 0.8100 0.3400
      $myprop SetSpecularColor 1 1 1
      $myprop SetSpecular 0.3
      $myprop SetSpecularPower 20
      $myprop SetAmbient 0.2
      $myprop SetDiffuse 0.8
      vis_render $ren
      # set the actor props here
      set guiVISvarsPrev(cutplane_showMesh) 1
   }


   # toggle display of pressure
   if {$guiVISvarsPrev(cutplane_displayPressure) == "off" &&  $guiVISvars(cutplane_displayPressure) != "off"} {
       puts "cutplane: new display"
       catch {repos_delete -obj $pressurePD}
       geom_copy -src $surfacePD -dst $pressurePD
       set guiVISvarsPrev(cutplane_minPressure) $guiVISvars(cutplane_minPressure)
       set guiVISvarsPrev(cutplane_maxPressure) $guiVISvars(cutplane_maxPressure)
       set guiVISvarsPrev(cutplane_displayPressure) $guiVISvars(cutplane_displayPressure)
       set actor [vis_pRepos $ren $pressurePD]
       [$actor GetMapper] ScalarVisibilityOn
       [$actor GetMapper] SetLookupTable gBlueToRedLUT
       [$actor GetMapper] SetScalarRange $guiVISvars(cutplane_minPressure) $guiVISvars(cutplane_maxPressure)
       vis_render $ren
   }  elseif {($guiVISvarsPrev(cutplane_displayPressure) != "off") &&  ($guiVISvars(cutplane_displayPressure) == "off")} {
       puts "cutplane: remove it!"
       vis_pRm $ren $pressurePD
       repos_delete -obj $pressurePD
       set guiVISvarsPrev(cutplane_displayPressure) $guiVISvars(cutplane_displayPressure)
   } elseif {($guiVISvarsPrev(cutplane_minPressure) != $guiVISvars(cutplane_minPressure)) ||  ($guiVISvarsPrev(cutplane_maxPressure) != $guiVISvars(cutplane_maxPressure))} {
       puts "cutplane: update"
       set guiVISvarsPrev(cutplane_minPressure) $guiVISvars(cutplane_minPressure)
       set guiVISvarsPrev(cutplane_maxPressure) $guiVISvars(cutplane_maxPressure)
       set guiVISvarsPrev(cutplane_displayPressure) $guiVISvars(cutplane_displayPressure)
       set actor [vis_pGetActor $ren $pressurePD]
       [$actor GetMapper] ScalarVisibilityOn
       [$actor GetMapper] SetLookupTable gBlueToRedLUT
       [$actor GetMapper] SetScalarRange $guiVISvars(cutplane_minPressure) $guiVISvars(cutplane_maxPressure)
       vis_render $ren
   }


   # toggle display of velocity
