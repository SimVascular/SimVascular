# Procedure: createPREOPloadsaveVolFile
proc createPREOPloadsaveVolFile {} {

  global gImageVol

  set filename [tk_chooseDirectory -mustexist true -title "Choose Volumetric Image Data Directory" -initialdir [pwd]]
  set gImageVol(tmpfilename) [string trim $filename]
  
  if {[string length [string trim $filename]] != 0} {
    set gImageVol(filename) $filename
    set gImageVol(data_dirname) $filename
    #  check to make sure image files exist
    if {[glob -nocomplain [file join $filename I.???]] != ""} {
     set gImageVol(filePattern) "\%s.\%03d"
    } elseif {[glob -nocomplain [file join $filename I.????]] != ""} {
     set gImageVol(filePattern) "\%s.\%04d"
    } elseif {[glob -nocomplain [file join $filename I.???.dcm]] != ""} {
     set gImageVol(filePattern) "\%s.\%03d.dcm"
    } elseif {[glob -nocomplain [file join $filename I.????.dcm]] != ""} {
     set gImageVol(filePattern) "\%s.\%04d.dcm"
    } elseif {[glob -nocomplain [file join $filename IM-????-????.dcm]] != ""} {
     set gImageVol(filePattern) "\%s-\%04d-\%04d.dcm"
    } else {
      puts "ERROR:  Directory ($filename) does not appear to contain image files."
      return -code error "ERROR:  Directory ($filename) does not appear to contain image files."
    }

    if [catch {img_guessVolParams $filename} msg] {
      return -code error "ERROR:  ($msg)"
    } else {
        img_guessRASmin $filename
    }

  }
}


