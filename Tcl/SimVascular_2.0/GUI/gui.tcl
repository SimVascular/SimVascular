# Procedure: guiTRIMcreateCutPlane
proc guiTRIMcreateCutPlane { side} {

  global gui3Dvars
  global guiTRIMvars
  global gRen3d
  global guiPDvars
  global smasherInputName

  set obj  $gui3Dvars(polydata_change_obj)
  set Bobj $gui3Dvars(polydata_change_backup_obj)
#  set tmpcutpd /guiTRIM/tmpcutpd
  catch {repos_delete -obj $Bobj}
  if {$guiPDvars(updatedSolid) == 1} {
     if {[catch {$smasherInputName GetPolyData -result $Bobj} errmsg] != 0} {
      tk_messageBox -title "Load Solid Model Please"  -type ok -message "WARNING: You need to load a solid model before cutting with plane"
      return
    }
  } else {
     if {[catch {geom_copy -src $obj -dst $Bobj} errmsg] != 0} {
      tk_messageBox -title "Load Solid Model Please"  -type ok -message "WARNING: You need to load a solid model before cutting with plane"
      return
    }
  }

  set clipper tmp-guiTRIM-clipper

  set plane guiTRIMcreateCutPlane-Plane
  set impPlane guiTRIMcreateCutPlane-Imp

  set reposPlane /guiTRIM/slice_plane

  catch {$clipper Delete}
  catch {$plane Delete}
  catch {$impPlane Delete}

  catch {repos_delete -obj $reposPlane}

  set topRight $guiTRIMvars(plane_p1)
  set botRight $guiTRIMvars(plane_p2)
  set topLeft  $guiTRIMvars(plane_p3)

  if {[llength $topRight] != 3 ||
      [llength $botRight] != 3 ||
      [llength $topLeft] != 3} {
    return
    #return -code error "ERROR:  incorrect length of corner pts."
  }

  # create the explicit cut plane

  vtkPlaneSource $plane
  $plane SetOrigin [lindex $topRight 0] [lindex $topRight 1] [lindex $topRight 2]
  $plane SetPoint1 [lindex $topLeft 0] [lindex $topLeft 1] [lindex $topLeft 2]
  $plane SetPoint2 [lindex $botRight 0] [lindex $botRight 1] [lindex $botRight 2]
  $plane Update
  set nrm [$plane GetNormal]
  repos_importVtkPd -src [$plane GetOutput] -dst $reposPlane

  # create the implicit cut plane
  vtkPlane $impPlane
  $impPlane SetOrigin [lindex $topRight 0] [lindex $topRight 1] [lindex $topRight 2]
  $impPlane SetNormal [lindex $nrm 0] [lindex $nrm 1] [lindex $nrm 2]

  # do the clipping
  #puts "Using clipper..."
  catch {$clipper Delete}
  vtkClipPolyData $clipper
  $clipper SetInputDataObject [repos_exportToVtk -src $Bobj]
  $clipper GenerateClippedOutputOn
  $clipper SetClipFunction $impPlane
  $clipper SetValue $guiTRIMvars(plane_offset)
  $clipper Update

  global gRen3d

  set ren $gRen3d

  global gRen3dFreeze
  if {$gRen3dFreeze > 0} {
    set alreadyfrozen 1
  } else {
    set alreadyfrozen 0
    set gRen3dFreeze 1
  }

  if {$guiTRIMvars(vis_cut_surface) != 0} {
    vis_pRm $gRen3d $obj
  }

  set triangulator tmp-guiTRIM-triangulator
  catch {$triangulator Delete}
  vtkFillHolesFilter $triangulator

  if {$side == "above"} {
    $triangulator SetInputDataObject [$clipper GetOutput]
  } else {
    $triangulator SetInputDataObject [$clipper GetClippedOutput]
  }
  $triangulator Update
  catch {repos_delete -obj $obj}
  repos_importVtkPd -src [$triangulator GetOutput] -dst $obj

  global guiTRIMvars
  set guiTRIMvars(num_tris) [[repos_exportToVtk -src $obj] GetNumberOfCells]

  set guiPDvars(updatedSolid) 0
  guiTRIMvis vis_cut_surface

  $triangulator Delete
  $clipper Delete
  $plane Delete

  if {$alreadyfrozen == 0} {
    set gRen3dFreeze 0
    vis_render $ren
  }
}


# Procedure: guiTRIMcreateSolid
proc guiTRIMcreateSolid {} {
  set cutpd /guiTRIM/cutPD
  set outModel trimmed_discrete_model
  catch {repos_delete -obj $outModel}
  set curKernel [solid_getKernel]
  solid_setKernel -name Discrete
  global guiTRIMvars
  if [catch {solid_poly3dSolid -result $outModel -src $cutpd -facet Union -angle $guiTRIMvars(discrete_angle)} msg] {
    return -code error "ERROR creating solid ($msg)"
  }
  tk_messageBox -message "Model creation complete.\nModel has [llength [$outModel GetFaceIds]] faces."
  #$outModel WriteNative -file $fn
  global gDiscreteModelFaceNames
  global gPolyDataFaceNames
  set allids [$outModel GetFaceIds]
  foreach id $allids {
#     set gDiscreteModelFaceNames($id) "noname_$id"
      set gDiscreteModelFaceNames($id) $gPolyDataFaceNames($id)
  }
  solid_setKernel -name $curKernel

  global smasherInputName
  set smasherInputName trimmed_discrete_model

  global gOptions
  set gOptions(meshing_solid_kernel) Discrete

  if {$smasherInputName == "trimmed_discrete_model"} {
    smasherGUIupdateViewWindow
  }
}


# Procedure: guiTRIMcutRefresh
proc guiTRIMcutRefresh {} {
  # replace object with original and then
  # process the current cut file
  set wsspd /guiTRIM/initialPD
  set cutpd /guiTRIM/cutPD

  catch {repos_delete -obj $cutpd}
  if {[catch {geom_copy -src $wsspd -dst $cutpd} errmsg] != 0} {
   tk_messageBox -title "Load Solid Model Please"  -type ok -message "WARNING: You need to load a solid before viewing"
   return
  }

  # update display if cut is shown
  guiTRIMvis vis_cut_surface

  global symbolicName
  global guiTRIMvars
  set mytext {}
  set tbox $symbolicName(guiTRIMcutFileTB)
  set numlines [lindex [split [$tbox index end] .] 0]
  for {set i 1} {$i < $numlines} {incr i} {
    set line [$tbox get $i.0 $i.end]
    set line [string trim $line]
    if {[string length $line] == 0} {
      continue
    }
    if {[string index $line 0] == "\#"} {
      continue
    }
    if {[lindex $line 0] == "plane"} {
      set side [lindex $line 1]
      set offset [lindex $line 2]
      set topRight [lrange $line 3 5]
      set botRight [lrange $line 6 8]
      set topLeft  [lrange $line 9 11]
      set guiTRIMvars(plane_p1) $topRight
      set guiTRIMvars(plane_p2) $botRight
      set guiTRIMvars(plane_p3) $topLeft
      set guiTRIMvars(plane_offset) $offset
      guiTRIMcreateCutPlane $side
    } elseif {[lindex $line 0] == "box_planes"} {
      set side [lindex $line 1]
      set offset [lindex $line 2]
      set vals [lrange $line 3 end]
      guiTRIMcreateCutBox $side $offset $vals
    }
  }
}


# Procedure: guiTRIMloadCutsFile
proc guiTRIMloadCutsFile {} {

  global guiTRIMvars
  global gFilenames
  global symbolicName

   global gFilenames
   set fn [tk_getOpenFile -filetypes {{CUTS *.cuts} {"All Files" *.*}} -title "Choose Cuts File"]
   if {$fn == ""} {
     return
   }

  set gFilenames(trim_cuts_file) $fn
  
  set mytext {}
  set tbox $symbolicName(guiTRIMcutFileTB)
  $tbox delete 1.0 end
  set fp [open $gFilenames(trim_cuts_file) r]
  set i 1
  while {[gets $fp line] >= 0} {
    $tbox insert $i.0 "$line\n"
    set mytext "$mytext$line\n"
    incr i
  }
  set guiTRIMvars(loaded_cut_file_text) $mytext

  close $fp
}


